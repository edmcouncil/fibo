     xmlns:fibo-fnd-aap-agt="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"
     xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"
     xmlns:fibo-fnd-org-fm="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Organizations/FormalOrganizations/"
     xmlns:fibo-fnd-oac-own="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/"
     xmlns:fibo-be-le-lp="http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/"
     xmlns:fibo-be-corp-corp="http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/Corporations/Corporations/"
