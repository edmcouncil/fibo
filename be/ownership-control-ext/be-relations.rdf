    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/hasShareholderFormalOrganization">
        <rdfs:label>has shareholder formal organization</rdfs:label>
        <skos:definition>Some Formal Organization which part owns the Incorporated Company by means of shares.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/whollyOwnedBy">
        <rdfs:label>wholly owned by</rdfs:label>
        <skos:definition>Incorporated Company holding all of the voting shares in the Incorporated Company.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/majorityControllingInterestBy">
        <rdfs:label>majority controlling interest by</rdfs:label>
        <skos:definition>Entity having controlling ownership of the Formal Organization.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/majorityControllingOwnershipBy">
        <rdfs:label>majority controlling ownership by</rdfs:label>
        <skos:definition>Incorporated Company holding over fifty percent of the voting shares in this Incorporated Company.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/whollyControlledBy">
        <rdfs:label>wholly controlled by</rdfs:label>
        <skos:definition>Entity having controlling ownership of the Formal Organization.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/constitutionallyPartOwnedBy">
        <rdfs:label>constitutionally part owned by</rdfs:label>
        <skos:definition>Some entity which part owns the Formal Organization by means of some formal instrument such as shares or stockholder equity.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/totalVotingShareholdingBy">
        <rdfs:label>total voting shareholding by</rdfs:label>
        <skos:definition>Formal Organization holding all of the voting shares in the Incorporated Company.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/partOwnedBy">
        <rdfs:label>part owned by</rdfs:label>
        <skos:definition>The entity is owned in some part by this kind of entity.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/significantVotingShareholdingBy">
        <rdfs:label>significant voting shareholding by</rdfs:label>
        <skos:definition>Formal Organization having significant voting ownership in the Incorporated Company.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/hasShareholder">
        <rdfs:label>has shareholder</rdfs:label>
        <skos:definition>Some entity which part owns the Incorporated Company by means of shares.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/hasLegalControlOf">
        <rdfs:label>has legal control of</rdfs:label>
        <skos:definition>The entity has legal control over this organization.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/be-relations/controlledBy">
        <rdfs:label>controlled by</rdfs:label>
        <skos:definition>Some entity which controls the Formal Organization, this being some kind of autonomous agent.</skos:definition>
