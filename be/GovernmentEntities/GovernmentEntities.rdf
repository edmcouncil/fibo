    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/hasFullSovereigntyOver -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;hasFullSovereigntyOver">
        <rdfs:label>has full sovereignty over</rdfs:label>
        <skos:definition>relates a polity to a geopolitical entity where the polity has absolute, supreme and ultimate dominion and authority of a political state subject to no higher power, expressed within its territory in full self-government and in complete freedom from any outside influence.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-ge-ge;hasSovereigntyOver"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/hasGovernmentalAgency -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;hasGovernmentalAgency">
        <rdfs:label rdf:datatype="&xsd;string">has governmental agency</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies the relation between a governmental agency and the governmental entity to which the agency belongs.</skos:definition>
        <rdfs:range rdf:resource="&fibo-be-ge-ge;GovernmentAgency"/>
        <rdfs:domain rdf:resource="&fibo-be-ge-ge;GovernmentalEntity"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/hasGovernmentalBranch -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;hasGovernmentalBranch">
        <rdfs:label rdf:datatype="&xsd;string">has governmental branch</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies the relation between a governmental branch and the government.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-ge-ge;Government"/>
        <rdfs:range rdf:resource="&fibo-be-ge-ge;GovernmentBranch"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/hasGovernmentalDepartment -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;hasGovernmentalDepartment">
        <rdfs:label>has governmental department</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies the relation between a governmental department and the governmental entity to which the department belongs.</skos:definition>
        <rdfs:range rdf:resource="&fibo-be-ge-ge;GovernmentDepartment"/>
        <rdfs:domain rdf:resource="&fibo-be-ge-ge;GovernmentalEntity"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/hasPartialSovereigntyOver -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;hasPartialSovereigntyOver">
        <rdfs:label>has partial sovereignty over</rdfs:label>
        <skos:definition>relates a polity to a geopolitical entity where the polity exercises partial dominion and authority of a political state.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-ge-ge;hasSovereigntyOver"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/hasSharedSovereigntyOver -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;hasSharedSovereigntyOver">
        <rdfs:label>has shared sovereignty over</rdfs:label>
        <skos:definition>relates a polity to a geopolitical entity where the polity exercises shared dominion and authority of a political state.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-ge-ge;hasSovereigntyOver"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/hasSovereigntyOver -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;hasSovereigntyOver">
        <rdfs:label>has sovereignty over</rdfs:label>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/sovereignty.html</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition>relates a polity to a geopolitical entity where the polity exercises dominion and authority of a political state.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-ge-ge;Polity"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Countries/GeopoliticalEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/instrumentOf -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;instrumentOf">
        <rdfs:label>instrument of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the relation between a government and a government intrumentality.</skos:definition>
        <rdfs:range rdf:resource="&fibo-be-ge-ge;Government"/>
        <rdfs:domain rdf:resource="&fibo-be-ge-ge;GovernmentInstrumentality"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/isElectedBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;isElectedBy">
        <rdfs:label>is elected by</rdfs:label>
        <skos:definition>chooses someone to hold office or some other position by voting</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/isRepresentedBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-ge-ge;isRepresentedBy">
        <rdfs:label>is represented by</rdfs:label>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://dictionary.reference.com/browse/representative</fibo-fnd-utl-av:definitionOrigin>
        <skos:definition>of or relating to a system of governance by chosen representatives</skos:definition>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/ExecutiveBranch -->

    <owl:Class rdf:about="&fibo-be-ge-ge;ExecutiveBranch">
        <rdfs:label>executive branch</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-ge-ge;GovernmentBranch"/>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">The executive branch executes, or enforces the law. </fibo-fnd-utl-av:explanatoryNote>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Executive_(government)</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition rdf:datatype="&xsd;string">the part of the government that has its authority and responsibility for the daily administration of the state. </skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/FederalGovernment -->

    <owl:Class rdf:about="&fibo-be-ge-ge;FederalGovernment">
        <rdfs:label>federal government</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-ge-ge;NationalGovernment"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;governs"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Countries/FederalCapitalArea"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;governs"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Countries/FederalState"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">A federation is a political entity characterized by a union of partially self-governing states or regions under a central (federal) government. In a federation, the self-governing status of the component states, as well as the division of power between them and the central government, are typically constitutionally entrenched and may not be altered by a unilateral decision of either party, the states or the federal political body</fibo-fnd-utl-av:explanatoryNote>
        <skos:definition rdf:datatype="&xsd;string">a union of states under a central government distinct from the individual governments of the separate states</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Federation</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/Federation -->

    <owl:Class rdf:about="&fibo-be-ge-ge;Federation">
        <rdfs:label>federation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-ge-ge;SovereignState"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-be-ge-ge;isRepresentedBy"/>
                        <owl:onClass rdf:resource="&fibo-be-ge-ge;RegionalGovernment"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-be-ge-ge;isRepresentedBy"/>
                        <owl:onClass rdf:resource="&fibo-be-ge-ge;FederalGovernment"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-be-ge-ge;hasFullSovereigntyOver"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/Country"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-be-ge-ge;hasSharedSovereigntyOver"/>
                        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Countries/FederalState"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">Federal entity in the sense of a legal entity, that is, what it is that incurs debt for a federal government, i.e. the Federal entity in the sense of the legal entity, as distinct from the Federal government. It is identified as a legal entity because it is able to incur debt in its own right.</fibo-fnd-utl-av:explanatoryNote>
        <skos:definition rdf:datatype="&xsd;string">a political entity characterized by a union of partially self-governing states or regions under a central (federal) government.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Federation</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/Government -->

    <owl:Class rdf:about="&fibo-be-ge-ge;Government">
        <rdfs:label>government</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-ge-ge;GovernmentalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-ge-ge;hasGovernmentalDepartment"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-ge-ge;GovernmentDepartment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;governs"/>
                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Countries/GeopoliticalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-ge-ge;hasGovernmentalAgency"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-ge-ge;GovernmentAgency"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-ge-ge;hasGovernmentalBranch"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-ge-ge;GovernmentBranch"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">In the Commonwealth of Nations, the word government is also used more narrowly to refer to the collective group of people that exercises executive authority in a state. This usage is analogous to what is called an &quot;administration&quot; in American English. Furthermore, especially in American English, the concepts of the state and the government may be used synonymously to refer to the person or group of people exercising authority over a politically organized territory</fibo-fnd-utl-av:explanatoryNote>
        <skos:definition rdf:datatype="&xsd;string">the system by which a state or community is controlled. </skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/GovernmentAgency -->

    <owl:Class rdf:about="&fibo-be-ge-ge;GovernmentAgency">
        <rdfs:label rdf:datatype="&xsd;string">government agency</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-ge-ge;GovernmentalEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-ge-ge;GovernmentAppointee"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">There is a notable variety of agency types. Although usage differs, a government agency is normally distinct both from a department or ministry, and other types of public body established by government. The functions of an agency are normally executive in character, since different types of organizations (such as commissions) are most often constituted in an advisory roleâ€”this distinction is often blurred in practice however.</fibo-fnd-utl-av:explanatoryNote>
        <skos:definition rdf:datatype="&xsd;string">a permanent or semi-permanent organization, often an appointed commission, in the machinery of government that is responsible for the oversight and administration of specific functions.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Government_agency</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/GovernmentAppointee -->

    <owl:Class rdf:about="&fibo-be-ge-ge;GovernmentAppointee">
        <rdfs:label>government appointee</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/Executives/Executive"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isAppointedBy"/>
                        <owl:someValuesFrom rdf:resource="&fibo-be-ge-ge;GovernmentalEntity"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>an individual appointed by government decree to lead, or participate in some capacity, in a government agency.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/GovernmentEntities/GovernmentEntities/GovernmentBranch -->

    <owl:Class rdf:about="&fibo-be-ge-ge;GovernmentBranch">
        <rdfs:label>government branch</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-ge-ge;GovernmentalEntity"/>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Separation_of_powers</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition rdf:datatype="&xsd;string">the division of the government of a state, each with separate and independent powers and areas of responsibility so that the powers of one branch are not in conflict with the powers associated with the other branches. </skos:definition>
