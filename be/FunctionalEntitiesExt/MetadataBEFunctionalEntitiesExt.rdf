		<dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/FunctionalEntitiesExt/StateEntities/"/>
		<dct:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dct:license>
		<dct:title>Financial Industry Business Ontology (FIBO) Business Entities (BE) Functional Entities Extensions Module</dct:title>
