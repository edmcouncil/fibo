    <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/ControlParties/">
        <rdfs:label>Control Parties Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO BE Control Parties Ontology -->

