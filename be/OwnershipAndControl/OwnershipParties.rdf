   <owl:Ontology rdf:about="http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/OwnershipAndControl/OwnershipParties/">
        <rdfs:label>Ownership Parties Ontology</rdfs:label>
    

    <!-- Curation and Rights Metadata for the FIBO BE Ownership Parties Ontology -->

