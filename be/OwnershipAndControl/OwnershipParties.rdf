    <owl:ObjectProperty rdf:about="&fibo-be-oac-opty;isPartiallyOwnedBy">
        <rdfs:label>is partially owned by</rdfs:label>
        <skos:definition>links an interest/position in something (e.g, a formal business organization) to the party that holds or owns it, in whole or in part</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;isOwnedBy"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-be-oac-opty;EntityOwner"/>
        <owl:inverseOf rdf:resource="&fibo-be-oac-opty;holdsAnInterestIn"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-oac-opty;hasConstitutionalOwner">
        <rdfs:label>has constitutional owner</rdfs:label>
        <skos:definition>links a constitutional interest/position in something (e.g., a formal business organization) to the party that holds or owns it, in whole or in part</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-opty;isPartiallyOwnedBy"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-be-oac-opty;ConstitutionalOwner"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-oac-opty;hasInvestor">
        <rdfs:label>has investor</rdfs:label>
        <skos:definition>links an investment interest in something (e.g., a formal business organization) to the party that holds or owns it, in whole or in part</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-opty;isPartiallyOwnedBy"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-be-oac-opty;Investor"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-oac-opty;holdsAnInterestIn">
        <rdfs:label>holds an interest in</rdfs:label>
        <skos:definition>links a party to some organization it holds an interest in</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;owns"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-opty;EntityOwner"/>
        <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-oac-opty;holdsEquity">
        <rdfs:label>holds equity</rdfs:label>
        <skos:definition>links a party to some equity it holds</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;owns"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-opty;EntityOwner"/>
        <rdfs:range rdf:resource="&fibo-fnd-acc-aeq;Equity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-oac-opty;isEquityHeldBy">
        <rdfs:label>is equity held by</rdfs:label>
        <skos:definition>links some equity to the party that holds it</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;isOwnedBy"/>
        <rdfs:range rdf:resource="&fibo-be-oac-opty;EntityOwner"/>
        <owl:inverseOf rdf:resource="&fibo-be-oac-opty;holdsEquity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-oac-opty;guarantees">
        <rdfs:label>guarantees</rdfs:label>
        <skos:definition>provide a formal assurance or promise, esp. that certain conditions shall be fulfilled relating to a product, service, or transaction</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">https://www.google.com/#q=guarantees</fibo-fnd-utl-av:adaptedFrom>
    </owl:ObjectProperty>


