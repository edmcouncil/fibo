     xmlns:fibo-be-oac-cpty="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/ControlParties/"
     xmlns:fibo-be-oac-cown="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/CorporateOwnership/"
     xmlns:fibo-be-oac-cctl="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/CorporateControl/">


   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/CorporateControl/">
        <rdfs:label>Corporate Control Ontology</rdfs:label>


