    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
	
    <owl:Class rdf:about="&fibo-be-sps-sps;SoleProprietor">
        <rdfs:label>sole proprietor</rdfs:label>
        <fibo-fnd-utl-av:synonym rdf:datatype="&xsd;string">sole owner</fibo-fnd-utl-av:synonym>
        <fibo-fnd-utl-av:synonym rdf:datatype="&xsd;string">sole trader</fibo-fnd-utl-av:synonym>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-own;Owner"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
			<owl:onClass rdf:resource="&fibo-fnd-aap-ppl;LegallyCapablePerson"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
		 </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-oac-oac;ownsAndControls"/>
                        <owl:someValuesFrom rdf:resource="&fibo-be-sps-sps;SoleProprietorship"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a party that owns a business, has the rights to all profits from that business and is considered a single entity (unincorporated) together with that business for tax and liability purposes</skos:definition>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">A sole proprietor has unlimited liability with respect to any business debts.</fibo-fnd-utl-av:explanatoryNote>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
    <owl:Class rdf:about="&fibo-be-sps-sps;SoleProprietorship">
        <rdfs:label>sole proprietorship</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-le-lp;BusinessEntity"/>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-law-lcap;LiabilityCapacity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onClass rdf:resource="&fibo-fnd-aap-ppl;LegallyCapablePerson"/>
                <owl:onProperty rdf:resource="&fibo-fnd-oac-oac;isOwnedAndControlledBy"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">an unincorporated business owned by a single person</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>

</rdf:RDF>