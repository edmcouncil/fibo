<rdf:RDF
	xml:base="http://www.omg.org/spec/EDMC-FIBO/BE/SoleProprietorships/SoleProprietorships/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-be-le-lp="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/"
	xmlns:fibo-be-sps-sps="http://www.omg.org/spec/EDMC-FIBO/BE/SoleProprietorships/SoleProprietorships/"
	xmlns:fibo-fnd-aap-agt="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-oac-own="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/"
	xmlns:fibo-fnd-org-fm="http://www.omg.org/spec/EDMC-FIBO/FND/Organizations/FormalOrganizations/"
	xmlns:fibo-fnd-pty-pty="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/"
	xmlns:fibo-fnd-pty-rl="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/"
	xmlns:fibo-fnd-rel-rel="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/SoleProprietorships/SoleProprietorships/">
		<rdfs:label>Sole Proprietorships Ontology</rdfs:label>
		<dct:license
			rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright
			rdf:datatype="&xsd;string">Copyright (c) 2015 EDM Council, Inc.
Copyright (c) 2015 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/FND/</sm:dependsOn>
		<sm:fileAbbreviation
			rdf:datatype="&xsd;string">fibo-be-sps-sps</sm:fileAbbreviation>
		<sm:fileAbstract
			rdf:datatype="&xsd;string">This ontology defines the fundamental concepts for representing sole proprietorships -- i.e., organizations that are owned by an individual that is responsible for the liabilities of the organization.</sm:fileAbstract>
		<sm:filename
			rdf:datatype="&xsd;string">SoleProprietorships.rdf</sm:filename>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/BE/AboutBE/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/EDMC-FIBO/BE/SoleProprietorships/AboutSoleProprietorships/</rdfs:seeAlso>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/"/>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Organizations/FormalOrganizations/"/>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/"/>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/"/>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/"/>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"/>
		<owl:versionIRI
			rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/20150501/SoleProprietorships/SoleProprietorships/"/>
		<skos:changeNote
			rdf:datatype="&xsd;string">The http://www.omg.org/spec/EDMC-FIBO/BE/20150501/SoleProprietorships/SoleProprietorships.rdf version of this ontology was added to Business Entities, including both per the issue resolutions identified in the FIBO BE 1.0 FTF report.</skos:changeNote>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-be-sps-sps;SoleProprietor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-oac-own;Owner"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/People/LegallyCapablePerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:minQualifiedCardinality
							rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
						<owl:onClass
							rdf:resource="&fibo-be-sps-sps;SoleProprietorship"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-oac-own;owns"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>sole proprietor</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">A sole proprietor has unlimited liability with respect to any business debts.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">sole owner</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a party that owns a business, has the rights to all profits from that business and is considered a single entity (unincorporated) together with that business for tax and liability purposes</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-sps-sps;SoleProprietorship">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-le-lp;BusinessEntity"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom>
					<owl:Restriction>
						<owl:onProperty
							rdf:resource="&fibo-fnd-pty-rl;playsRole"/>
						<owl:someValuesFrom
							rdf:resource="&fibo-fnd-oac-own;Owner"/>
					</owl:Restriction>
				</owl:allValuesFrom>
				<owl:onProperty
					rdf:resource="&fibo-be-le-lp;allowsUnlimitedLiabilityOn"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-sps-sps;SoleProprietorship"/>
				<owl:onProperty
					rdf:resource="&fibo-be-le-lp;allowsUnlimitedLiabilityOnEntity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Law/LegalCapacity/LiabilityCapacity"/>
				<owl:onProperty
					rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Law/LegalCapacity/hasCapacity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/People/LegallyCapablePerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-oac-own;isOwnedBy"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>sole proprietorship</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an unincorporated business owned by a single person</skos:definition>
	</owl:Class>

</rdf:RDF>