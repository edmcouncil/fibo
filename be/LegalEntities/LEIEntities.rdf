    <!ENTITY fibo-fnd-pty-rl "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Parties/Roles/" >
    <!ENTITY fibo-fnd-law-jur "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Law/Jurisdiction/" >
    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/" >
