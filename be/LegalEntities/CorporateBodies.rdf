    <!-- Ontology/File-Level Metadata for the FIBO BE Corporate Bodies Ontology -->

        <sm:filename rdf:datatype="&xsd;string">CorporateBodies.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-be-le-cb</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/20150201/LegalEntities/CorporateBodies/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology defines the basic mechanisms that establish legal personhood for judicial or artificial persons, specifically those that are corporate bodies, including bodies incorporated by equity, by guarantee, and by agreement.</sm:fileAbstract>

        <skos:changeNote rdf:datatype="&xsd;string">The http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/20131101/LegalEntities/CorporateBodies.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.0 FTF report.</skos:changeNote>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/</sm:dependsOn>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>

