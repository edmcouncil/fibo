<rdf:RDF xml:base="http://edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://edmcouncil.org/techprocess/ab/SpecificationMetadata/"
     xmlns:fibo-fnd-utl-av="http://edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-utl-bt="http://edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"
     xmlns:fibo-fnd-rel-rel="http://edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"
     xmlns:fibo-fnd-aap-agt="http://edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"
     xmlns:fibo-fnd-plc-adr="http://edmcouncil.org/spec/EDMC-FIBO/FND/Places/Addresses/"
     xmlns:fibo-fnd-org-fm="http://edmcouncil.org/spec/EDMC-FIBO/FND/Organizations/FormalOrganizations/"
     xmlns:fibo-fnd-pty-rl="http://edmcouncil.org/spec/EDMC-FIBO/FND/Parties/Roles/"
     xmlns:fibo-fnd-pty-pty="http://edmcouncil.org/spec/EDMC-FIBO/FND/Parties/Parties/"
     xmlns:fibo-fnd-acc-aeq="http://edmcouncil.org/spec/EDMC-FIBO/FND/Accounting/AccountingEquity/"
     xmlns:fibo-fnd-agr-ctr="http://edmcouncil.org/spec/EDMC-FIBO/FND/Agreements/Contracts/"
     xmlns:fibo-be-le-lp="http://edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/"
     xmlns:fibo-be-le-fbo="http://edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/">


    <owl:Ontology rdf:about="http://edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/">
        <rdfs:label>Formal Business Organizations Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO BE Formal Business Organizations Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2013-2015 EDM Council, Inc.
Copyright (c) 2013-2015 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://edmcouncil.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the FIBO BE Formal Business Organizations Ontology -->

        <sm:filename rdf:datatype="&xsd;string">FormalBusinessOrganizations.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-be-le-fbo</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/BE/20150201/LegalEntities/FormalBusinessOrganizations/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology defines formal business organizations and related concepts. The ontology covers parts of organizations, membership, classification, address relations and other properties which are applicable to formal business organizations generally. The concept of a formal business organization forms the basis for articulation of types of organization, both incorporated and non-incorporated, in other FIBO-BE ontologies.</sm:fileAbstract>

        <skos:changeNote rdf:datatype="&xsd;string">The http://edmcouncil.org/spec/EDMC-FIBO/BE/20131101/LegalEntities/FormalBusinessOrganizations.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.0 FTF report.</skos:changeNote>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/</sm:dependsOn>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://edmcouncil.org/spec/ODM/</sm:contentLanguage>

        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://edmcouncil.org/spec/EDMC-FIBO/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://edmcouncil.org/spec/EDMC-FIBO/BE/AboutBE/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/AboutLegalEntities/</rdfs:seeAlso>

        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Places/Addresses/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Organizations/FormalOrganizations/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Parties/Roles/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Parties/Parties/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Accounting/AccountingEquity/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/FND/Agreements/Contracts/"/>
        <owl:imports rdf:resource="http://edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/"/>

   </owl:Ontology>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

   <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasEquity">
        <rdfs:label>has equity</rdfs:label>
      <skos:definition rdf:datatype="&xsd;string">the formally constituted organization has some form of stockholders equity</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-aeq;StockholdersEquity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasInternalEntity">
        <rdfs:label>has internal entity</rdfs:label>
      <skos:definition rdf:datatype="&xsd;string">the organization has some part which is formally defined as an Internal Entity, for example for reporting, management or accounting purposes</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasSubUnit"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;OrganizationalSubUnit"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasOperatingAddress">
        <rdfs:label>has operating address</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">address at which the organization carries out its operations</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasPrimaryAddress">
        <rdfs:label>has primary address</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The main address at which communications to the organization may be addressed; its headquarters address.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasRegisteredAddress">
        <rdfs:label>has registered address</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The organization has an address at which it is registered and at which legal papers may be served on it.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-be-le-fbo;RegisteredAddress"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasSignatory">
        <rdfs:label>has signatory</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">has a party which is authorized to sign contracts on behalf of the entity</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-be-le-lp;Signatory"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasSubUnit">
        <rdfs:label>has sub-unit</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The company has a branch, which is part of that company. This may be in another country or in the country in which that company is registered (incorporated).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-be-le-fbo;OrganizationalSubUnit"/>
        <owl:inverseOf rdf:resource="&fibo-be-le-fbo;isSubUnitOf"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;isSubUnitOf">
        <rdfs:label>is sub-unit of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The organization is a sub-unit of a formally constituted organization and has the role and function of being a formal part of that organization, such as a branch.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;OrganizationalSubUnit"/>
        <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isPartOf"/>
   </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-be-le-fbo;Branch">
        <rdfs:label>branch</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">A part of a business organization or company, identified as a branch.</skos:definition>
        <skos:editorialNote rdf:datatype="&xsd;string">This is not a separate legal entity in its own right, but a functional part of the entity.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="&fibo-be-le-fbo;OrganizationalSubUnit"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;Division">
        <rdfs:label>division</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a part of a company, such as a line of business, that may have separate accounting or reporting requirements</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-be-le-fbo;OrganizationalSubUnit"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;FormallyConstitutedOrganization">
        <rdfs:label>formally constituted organization</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Any organization with some formal contractual agreement among its principals</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isClassifiedBy"/>
                <owl:onClass rdf:resource="&fibo-be-le-fbo;OrganizationIndustrySectorClassification"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isIdentifiedBy"/>
                <owl:onClass rdf:resource="&fibo-be-le-fbo;OrganizationIdentificationCode"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
                <owl:onClass rdf:resource="&fibo-be-le-fbo;OrganizationCoveringAgreement"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;FormallyConstitutedOrganizationMember">
        <rdfs:label>formally constituted organization member</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">A member of some formally constituted organization.</skos:definition>
        <skos:editorialNote rdf:datatype="&xsd;string">This party may hold some equity in the formally constituted organization and may or may not have some responsibilities or potential liabilitiy in resect of the activities of that organization. Terms setting out the position of the member in these and other matters are defined in the organization covering agreement, to which the member may be a signatory.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;OrganizationMember"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;hasRole"/>
                <owl:someValuesFrom>              
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isMemberOf"/>
                        <owl:allValuesFrom rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
                    </owl:Restriction>
                </owl:someValuesFrom>              
            </owl:Restriction>
        </rdfs:subClassOf>
   </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;JointVenture">
        <rdfs:label>joint venture</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">A formal venture between two or more business entities</skos:definition>
        <skos:editorialNote rdf:datatype="&xsd;string">Detailed properties still to be modeled. This will be similar to Partnership in that it will have two or more venture partners (need to determine best label for these), and some formal standing. Also to research: whether JVs are only instituted via mutual share ownership and therefore may only be between limited companies (or may only be a limited company but may have other types of legal person and/or legal entity as venture partners).</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;OrganizationCoveringAgreement">
        <rdfs:label>organization covering agreement</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">A formal agreement between the principals in a formal organization which covers the relationship between the principals, and between the principals and the entity.</skos:definition>
        <skos:editorialNote rdf:datatype="&xsd;string">Also covers the aims and purposes of the Entity.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;WrittenContract"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;OrganizationIdentificationCode">
        <rdfs:label>organization identification code</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">A textual code which uniquely identifies a formal organization; Further Notes: Things which may identified by this kind of code Include but are not restricted to Legal Entity. At this most general level, this may be the identifier for a Fund, for a Legal Entity, for a Trust, or for the LEI scope of Artificial Person plus Trust.</skos:definition>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasUniqueIdentifier"/>
                <owl:onDataRange rdf:resource="&fibo-fnd-utl-bt;text"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;OrganizationIdentificationScheme">
        <rdfs:label>organization identification scheme</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">A formal scheme defining identification codes for a business entity.</skos:definition>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;defines"/>
                <owl:allValuesFrom rdf:resource="&fibo-be-le-fbo;OrganizationIdentificationCode"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;OrganizationIndustrySectorClassification">
        <rdfs:label>organization industry sector classification</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The classification of an organization in terms of the industry sector in which it carries out business.</skos:definition>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;classifies"/>
                <owl:allValuesFrom rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;RegisteredAddress">
        <rdfs:label>registered address</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The address at which an entity is registered and at which legal papers may be served.</skos:definition>
        <skos:editorialNote rdf:datatype="&xsd;string">This usually has to not be a postal address (except in some jurisdictions). So there should be a common superclass.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-be-le-fbo;OrganizationalSubUnit">
        <rdfs:label>organizational sub-unit</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">an organization that is part of a larger formal organization and only has full recognition within the context of that formal organization; In other words, it is not a legal entity in its own right.</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
    </owl:Class>

</rdf:RDF>