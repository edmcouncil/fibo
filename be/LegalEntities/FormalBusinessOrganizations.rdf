  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/isSubUnitOf">
    <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/isPartOf"/>
