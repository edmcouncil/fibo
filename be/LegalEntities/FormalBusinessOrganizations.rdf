    </owl:inverseOf>
    <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/OrganizationalSubUnit"/>
    <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
    <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasPart"/>
