    <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
    <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/has"/>
    <skos:definition>the formally constituted organization has some form of stockholders equity</skos:definition>
    <rdfs:label>has equity</rdfs:label>
