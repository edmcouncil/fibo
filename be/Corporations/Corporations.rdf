     xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-utl-bt="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"
     xmlns:fibo-fnd-aap-agt="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"
     xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"
     xmlns:fibo-fnd-acc-cur="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Accounting/CurrencyAmount/"     
     xmlns:fibo-fnd-law-jur="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Law/Jurisdiction/"
     xmlns:fibo-be-le-fbo="http://spec.edmcouncil.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/"
