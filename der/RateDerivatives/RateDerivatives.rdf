		<rdfs:seeAlso
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/AboutDER/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/"/>
