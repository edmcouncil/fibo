        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/IND/InterestRates/InterestRates/"/>
