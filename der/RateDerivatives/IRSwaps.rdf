    <!-- http://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/Swap -->

    <owl:Class rdf:about="&fibo-der-drc-swp;Swap">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-der-rtd-irswp;isNotional"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapLeg -->

    <owl:Class rdf:about="&fibo-der-drc-swp;SwapLeg"/>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/BasisSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;BasisSwap">
        <rdfs:label>basis swap</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;FloatFloatInterestRateSwap"/>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">A basis rate swap is a type of swap in which two parties swap variable interest rates based on different money markets, and this is usually done to limit interest-rate risk that a company faces as a result of having differing lending and borrowing rates.</fibo-fnd-utl-av:explanatoryNote>
        <skos:editorialNote rdf:datatype="&xsd;string">Basis swap can be the same tenor but different indices e.g. 3 month LIBOR v 3 month TIBOR. Then the difference is not driven from a different interest rate period but from different markets, so I am swapping based on the difference in efficiency between the two markets. In cross ccy sense, would swap a Jap index for a US index. (is this a basis swap?) Protecting ourselves fom basis risk which is the difference in price between two markets. Review Conclusion: Basis swap is distinguished by being related to two markets. Floating Floating is also sometimes referred to as a Basis Swap as well though, but this is a different meaning (so we model it somewhere else). See also forward swap, as a means for controlling interest rate.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">a floating-floating interest rate swap under which the floating rate payments are referenced to different bases</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.investopedia.com/terms/b/basisrateswap.asp</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/CrossCurrencyInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;CrossCurrencyInterestRateSwap">
        <rdfs:label>cross-currency interest rate swap</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateSwap"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:onClass rdf:resource="&fibo-fnd-acc-cur;Currency"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a swap in which the two streams of interest payments are in different currencies</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/ExplicitInterestAmountCalculationEvent -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;ExplicitInterestAmountCalculationEvent">
        <rdfs:label>explicit interest amount calculation event</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateStreamEvent"/>
        <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sch;ScheduledCalculationPeriodEndEvent"/>
        <skos:definition rdf:datatype="&xsd;string">the explicit representation of the calculation event in a given period, in which an interest payment is calculated based on the rate (fixed or floating) and the notional amount (in the payment currency, and factored for Fx if necessary), on a given date</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FixedFloatCrossCurrencyInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FixedFloatCrossCurrencyInterestRateSwap">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;CrossCurrencyInterestRateSwap"/>
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;FixedFloatInterestRateSwap"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;CrossCurrencyInterestRateSwap"/>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;FixedFloatInterestRateSwap"/>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FixedFloatInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FixedFloatInterestRateSwap">
        <rdfs:label>fixed float interest rate swap</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;InterestRateSwap"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-der-drc-swp;hasLeg"/>
                        <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;FixedInterestRateLeg"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-der-drc-swp;hasLeg"/>
                        <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;FloatingInterestRateLeg"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateSwap"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-der-drc-swp;exchanges"/>
                        <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;FixedInterestRateLeg"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-der-drc-swp;exchanges"/>
                        <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;FloatingInterestRateLeg"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">an interest rate swap in which fixed interest payments on the notional are exchanged for floating interest payments</skos:definition>
        <fibo-fnd-utl-av:synonym rdf:datatype="&xsd;string">vanilla interest rate swap</fibo-fnd-utl-av:synonym>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FixedFloatSingleCurrencyInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FixedFloatSingleCurrencyInterestRateSwap">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;FixedFloatInterestRateSwap"/>
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;SingleCurrencyInterestRateSwap"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;FixedFloatInterestRateSwap"/>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FixedInterestRateLeg -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FixedInterestRateLeg">
        <rdfs:label>fixed interest rate leg</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;InterestRateSwapLeg"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fbc-dae-dbt;hasInterestRate"/>
                        <owl:someValuesFrom rdf:resource="&fibo-fbc-dae-dbt;FixedInterestRate"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateSwapLeg"/>
        <owl:disjointWith rdf:resource="&fibo-der-rtd-irswp;FloatingInterestRateLeg"/>
        <skos:editorialNote rdf:datatype="&xsd;string">This may be the funding leg of some swaps (i.e. one party agrees to pay fixed interest amounts in exchange for whatever is the other leg) or it may be one or both sides of an Interest Rate Swap, where the two parties exchange different interest streams. Note that the FpML term on which this Swapstream term was based, is the one for asset return swaps, where this is the payment leg. This is the leg which has interest amounts, interest calculations and calculation dates, and optional stub calculation terms. It should be possible to relate this to the specific interest payments of an underlier if there is one.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">a swap stream that specifies fixed interest amounts and terms for the payment of that interest</skos:definition>
        <fibo-fnd-utl-av:synonym rdf:datatype="&xsd;string">fixed interest rate swap stream terms</fibo-fnd-utl-av:synonym>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FixedRateInterestCalculation -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FixedRateInterestCalculation">
        <rdfs:label>fixed rate interest calculation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-utl-alx;Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
                <owl:someValuesFrom rdf:resource="&fibo-fbc-dae-dbt;FixedInterestRate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">calculation of the amount of interest due in a given period on a fixed rate IR swap leg</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FloatFloatCrossCurrencyInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FloatFloatCrossCurrencyInterestRateSwap">
        <rdfs:label>float-float cross-currency interest rate swap</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;CrossCurrencyInterestRateSwap"/>
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;FloatFloatInterestRateSwap"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;FloatFloatInterestRateSwap"/>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FloatFloatInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FloatFloatInterestRateSwap">
        <rdfs:label>float-float interest rate swap</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;InterestRateSwap"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-der-drc-swp;hasLeg"/>
                        <owl:onClass rdf:resource="&fibo-der-rtd-irswp;FloatingInterestRateLeg"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateSwap"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-der-drc-swp;exchanges"/>
                <owl:onClass rdf:resource="&fibo-der-rtd-irswp;FloatingInterestRateLeg"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a swap that exchanges cashflows based on two different interest rates</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FloatFloatSingleCurrencyInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FloatFloatSingleCurrencyInterestRateSwap">
        <rdfs:label>float-float single currency interest rate swap</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;FloatFloatInterestRateSwap"/>
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;SingleCurrencyInterestRateSwap"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;FloatFloatInterestRateSwap"/>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FloatingInterestRateLeg -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FloatingInterestRateLeg">
        <rdfs:label>floating interest rate leg</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-rtd-irswp;InterestRateSwapLeg"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fbc-dae-dbt;hasInterestRate"/>
                        <owl:someValuesFrom rdf:resource="&fibo-fbc-dae-dbt;FloatingInterestRate"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateSwapLeg"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-der-rtd-irswp;hasInterestRateResetSchedule"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestRateResetSchedule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestRateReset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-der-rtd-irswp;hasInterestCalculationSchedule"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestCalculationSchedule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestRateFixing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:editorialNote rdf:datatype="&xsd;string">Instead of an absolute rate you have an underlier and an offset called a spread (same as margin in floating rate notes).</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">a swapstream in which variable interest is paid on some notional amount, linked to some underlying interest reference rate</skos:definition>
        <fibo-fnd-utl-av:synonym rdf:datatype="&xsd;string">floating interest rate swap stream terms</fibo-fnd-utl-av:synonym>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FloatingRateFixingValue -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FloatingRateFixingValue">
        <rdfs:label>floating rate fixing value</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-utl-alx;Variable"/>
        <skos:editorialNote rdf:datatype="&xsd;string">Forms an explicit set of terms at a point in time, and is part of the explicit schedule information.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">rdf:datatype=&quot;http://www.w3.org/2001/XMLSchema#string&quot;&gt;value of floating interest rate amount during a given calculation period, along with the relevant parameters in its calculation</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/FloatingRateInterestCalculation -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;FloatingRateInterestCalculation">
        <rdfs:label>floating rate interest calculation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-utl-alx;Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;FloatingRateFixingValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/InterestRateStreamEvent -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;InterestRateStreamEvent">
        <rdfs:label>interest rate stream event</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-der-drc-swp;SwapStreamEvent"/>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/InterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;InterestRateSwap">
        <rdfs:label>interest rate swap</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-drc-swp;Swap"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-der-drc-swp;hasLeg"/>
                        <owl:onClass rdf:resource="&fibo-der-rtd-irswp;InterestRateSwapLeg"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-drc-swp;Swap"/>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-rtd;InterestRateDerivativeInstrument"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-der-drc-swp;exchanges"/>
                <owl:onClass rdf:resource="&fibo-der-rtd-irswp;InterestRateSwapLeg"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a swap in which the underlier for one or both legs is an interest rate</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/InterestRateSwapLeg -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;InterestRateSwapLeg">
        <rdfs:label>interest rate swap stream terms</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-der-drc-swp;SwapLeg"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fbc-dae-dbt;hasInterestRate"/>
                        <owl:someValuesFrom rdf:resource="&fibo-fnd-acc-cur;InterestRate"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&fibo-der-drc-swp;SwapLeg"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-der-drc-swp;hasPaymentSchedule"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestPaymentSchedule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestCalculation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestPayment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-dae-dbt;hasPrincipal"/>
                <owl:onClass rdf:resource="&fibo-fbc-dae-dbt;Principal"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;InterestRateStreamEvent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;NotionalStepChangeEvent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:editorialNote rdf:datatype="&xsd;string">In IR markets there is no real distinction between an IR leg and a return leg, rather there are 2 funding legs.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">the components specifiying an interest rate stream, including both a parametric and cashflow representation for the stream of payments</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/NotionalStepAmount -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;NotionalStepAmount">
        <rdfs:label>notional step amount</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-acc-cur;hasNotionalAmount"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:editorialNote rdf:datatype="&xsd;string">Note that this is an actual concrete sum of money, which may be specified either as a monetary amount (e.g. dollars and cents) or as a percentage of either the original notional amount or the previous notional amount.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">the amount of money which is removed from the notional on each step date</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/NotionalStepChangeEvent -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;NotionalStepChangeEvent">
        <rdfs:label>notional step change event</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateStreamEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-dt-fd;CalculatedDate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:editorialNote rdf:datatype="&xsd;string">This alwys occurs on a Calculation date (that is, one of the Calculation Period End dates). Therefore the frequency / period length of the steps in the step schedule is a multiple of the Calculation Period or frequency. For example, if the notional is recalculated on every Calculation Date, applying a new interest rate to the new notional amount, then the two frequencies are the same. If notional is updated every second calculation period, then the step schedule specifies periods that are twice as long, and so on.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">a step change in the notional amount of the swap stream</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/NotionalStepPercentageAmount -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;NotionalStepPercentageAmount">
        <rdfs:label>notional step percentage amount</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-cur;PercentageMonetaryAmount"/>
        <skos:editorialNote rdf:datatype="&xsd;string">The percentage is either a percentage applied to the initial notional amount or the previous outstanding notional, and may be positive or negative.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">the percentage amount by which the notional changes on each step date</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/NotionalStepPeriodLength -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;NotionalStepPeriodLength">
        <rdfs:label>notional step period length</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-dt-fd;RecurrenceInterval"/>
        <skos:definition rdf:datatype="&xsd;string">a recurrence interval indicating the frequency with which step changes occur, which is a multiple of the calculation period in the calculation schedule</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/NotionalStepSchedule -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;NotionalStepSchedule">
        <rdfs:label>notional step schedule</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sch;ParametricSchedule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;NotionalStepAmount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;refersTo"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestCalculationSchedule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:editorialNote rdf:datatype="&xsd;string">For the Notional Step Schedule there are no stubs.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">schedule of changes in the notional amount on which interest is paid, comprising the regular sequence of step events</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/SingleCurrencyInterestRateSwap -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;SingleCurrencyInterestRateSwap">
        <rdfs:subClassOf rdf:resource="&fibo-der-rtd-irswp;InterestRateSwap"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;specifies"/>
                <owl:onClass rdf:resource="&fibo-fnd-acc-cur;Currency"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/SwapStreamCalculationRelativeDate -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;SwapStreamCalculationRelativeDate">
        <rdfs:label>swap stream calculation relative date</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-dt-fd;RelativeDate"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-dt-fd;isRelativeTo"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestRateResetSchedule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/SwapStreamInterestCalculation -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;SwapStreamInterestCalculation">
        <rdfs:label>swapstream interest calculation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-utl-alx;Expression"/>
        <skos:definition rdf:datatype="&xsd;string">calculation of the interest due</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/SwapStreamInterestCalculationSchedule -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;SwapStreamInterestCalculationSchedule">
        <rdfs:label>swapstream interest calculation schedule</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sch;ParametricSchedule"/>
        <skos:definition rdf:datatype="&xsd;string">parametric schedule of calculation dates</skos:definition>
    </owl:Class>
    


    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/SwapStreamInterestFixingRelativeDate -->

    <owl:Class rdf:about="&fibo-der-rtd-irswp;SwapStreamInterestFixingRelativeDate">
        <rdfs:label>swapstream interest fixing relative date</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-dt-fd;RelativeDate"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-dt-fd;isRelativeTo"/>
                <owl:someValuesFrom rdf:resource="&fibo-der-rtd-irswp;SwapStreamInterestRateReset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


