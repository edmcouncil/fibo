    <!-- http://spec.edmcouncil.org/fibo/DER/RateDerivatives/IRSwaps/NotionalStepRelativeToSelection -->

    <rdfs:Datatype rdf:about="&fibo-der-rtd-irswp;NotionalStepRelativeToSelection">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">Initial</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">Previous</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
        <skos:definition rdf:datatype="&xsd;string">a specification of whether a percentage rate change, used to calculate a change in notional outstanding, is expressed as a percentage of the initial notional amount or of the previously outstanding notional amount</skos:definition>
    </rdfs:Datatype>
    


