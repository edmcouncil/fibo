			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">share option o t c contract</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An option in which the underlyer is a single publicly issued and traded share.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-ass-eqo;ShareOptionStrikeTermsSet">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-ass-eqo;EquityOptionExerciseTermsSet"/>
