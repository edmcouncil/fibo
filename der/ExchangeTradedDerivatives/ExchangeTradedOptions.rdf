			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-etd-eto;identity.1"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-der-etd-std;DerivativesExchange"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-der-etd-ostd;OptionsClearingEntity"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">novated option contract principal</rdfs:label>
	</owl:Class>
	
