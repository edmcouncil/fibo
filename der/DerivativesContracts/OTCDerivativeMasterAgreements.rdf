		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">o t c master agreement element</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Terms specific to a Master Agreement governing transactions of Over the Counter derivatives between parties.</skos:definition>
