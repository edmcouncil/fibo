    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/PaymentObligationAsDelivery"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
    >payment in kind delivery obligation</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:ObjectProperty>
