	</rdf:Description>
	
	<rdf:Description
		rdf:about="&fibo-der-der-ma;masterAgreementCurrency">
		<rdf:type
			rdf:resource="&owl;DatatypeProperty"/>
		<rdf:type
			rdf:resource="&owl;ObjectProperty"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">master agreement currency</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-der-ma;CurrencySpecificationTerm"/>
