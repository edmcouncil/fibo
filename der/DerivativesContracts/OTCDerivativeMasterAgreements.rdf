  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/invokedInEventOf.1">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/CreditEvents/CreditEvent"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/OTCMasterAgreementRightToTerminationFollowingDefaultEvent"/>
