    >payable by counterparty</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/setsOut">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/OTCMasterAgreementEarlyTerminationRight"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/MasterAgreementEarlyTerminationProvisions"/>
    <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"
