  <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/hasTerms">
    <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/OTCMasterAgreementElement"/>
    <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/OTCDerivativeTransactionMasterAgreement"/>
