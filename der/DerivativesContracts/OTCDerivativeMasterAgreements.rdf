		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">tax withholding obligation</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An obligation with regard to the payment of taxes.</skos:definition>
	</owl:Class>
	
