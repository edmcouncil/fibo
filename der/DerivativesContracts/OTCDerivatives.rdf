<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-der-der-dbx "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasicsExt/">
	<!ENTITY fibo-der-der-ma "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCDerivativeMasterAgreements/">
	<!ENTITY fibo-der-der-otc "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCDerivatives/">
	<!ENTITY fibo-fnd-occ-prod "https://spec.edmcouncil.org/fibo/ontology/FND/Occurrent/Production/">
	<!ENTITY fibo-fnd-txn-rea "https://spec.edmcouncil.org/fibo/ontology/FND/Transactions/REATransactions/">
	<!ENTITY fibo-fnd-txn-txnx "https://spec.edmcouncil.org/fibo/ontology/FND/Transactions/TransactionsExtended/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCDerivatives/"
	xmlns:fibo-der-der-dbx="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasicsExt/"
	xmlns:fibo-der-der-ma="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCDerivativeMasterAgreements/"
	xmlns:fibo-der-der-otc="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCDerivatives/"
	xmlns:fibo-fnd-occ-prod="https://spec.edmcouncil.org/fibo/ontology/FND/Occurrent/Production/"
	xmlns:fibo-fnd-txn-rea="https://spec.edmcouncil.org/fibo/ontology/FND/Transactions/REATransactions/"
	xmlns:fibo-fnd-txn-txnx="https://spec.edmcouncil.org/fibo/ontology/FND/Transactions/TransactionsExtended/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCDerivatives/">
		<rdfs:label
			xml:lang="en">OTCDerivatives</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasicsExt/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCDerivativeMasterAgreements/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Transactions/REATransactions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Transactions/TransactionsExtended/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-der-der-otc;OTCDerivativeContract">
		<rdfs:subClassOf
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/OTCInstruments/OTCInstrument"/>
		<rdfs:subClassOf
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/DerivativeInstrument"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-der-ma;governs"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-der-der-otc;OTCDerivativeTransaction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">o t c derivative contract</rdfs:label>
		<skos:definition
			xml:lang="en">A derivative contract which is struck between two parties &quot;Over the Counter&quot;.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">This is both a Derivative Contract (not all of which are struck OTC) and an OTC contract (not all of which are Derivatives.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-otc;OTCDerivativeTransaction">
		<rdfs:subClassOf
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/DerivativeTransaction"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-txnx;CoveredTransaction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-der-ma;embodies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-der-der-otc;OTCDerivativeContract"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-txn-txnx;governedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-der-der-ma;OTCDerivativeTransactionMasterAgreement"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">o t c derivative transaction</rdfs:label>
		<skos:definition
			xml:lang="en">A derivative transaction which is struck over the counter between two parties.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">This is both a derivative transaction and a Covered Transaction, that is, the OTC transaction is transacted under some previously agreed Master Agreement between the two parties.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-otc;OTCProduct">
		<rdfs:label
			xml:lang="en">o t c product</rdfs:label>
		<skos:definition
			xml:lang="en">An Over the Counter Derivative. This is made up of one or more transactions, with a corresponding contract or contracts.</skos:definition>
	</owl:Class>

</rdf:RDF>