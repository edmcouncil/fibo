	<owl:ObjectProperty
		rdf:about="&fibo-der-der-dbx;underlierHasIdentity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">underlier has identity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-der-dbx;Underlier"/>
		<rdfs:range>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-der-der-dbx;Basket"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-der-der-dbx;FinancialInstrument"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-der-der-dbx;MarketRate"/>
										<rdf:rest>
											<rdf:Description>
												<rdf:first
													rdf:resource="&fibo-fnd-bus-pasx;NegotiableCommodity"/>
												<rdf:rest
													rdf:resource="&rdf;nil"/>
											</rdf:Description>
										</rdf:rest>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</rdfs:range>
	</owl:ObjectProperty>
	
