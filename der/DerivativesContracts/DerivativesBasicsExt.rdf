		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/DerivativeContractParty"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-agr-ctr;hasContractParty"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
