		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">derivatives close time</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-fwd;ForwardCashConditionalDeliveryCommitment">
