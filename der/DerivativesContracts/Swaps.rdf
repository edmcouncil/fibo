	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapContract">
		<rdfs:subClassOf
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DerivativeInstrument"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Swaps/SwapParty"/>
