	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/BE/LegalEntitiesExt/LegalPersonsExtended/">
		<rdfs:label
			xml:lang="en">LegalPersonsExtended</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
