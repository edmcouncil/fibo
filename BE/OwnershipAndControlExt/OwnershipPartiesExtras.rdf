	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/BE/OwnershipAndControlExt/OwnershipPartiesExtras/">
		<rdfs:label
			xml:lang="en">OwnershipPartiesExtras</rdfs:label>
