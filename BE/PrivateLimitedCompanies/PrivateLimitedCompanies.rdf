			rdf:resource="https://spec.edmcouncil.org/fibo/BE/AboutBE/"/>
		<rdfs:seeAlso
			rdf:resource="https://spec.edmcouncil.org/fibo/BE/PrivateLimitedCompanies/AboutPrivateLimitedCompanies/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"/>
