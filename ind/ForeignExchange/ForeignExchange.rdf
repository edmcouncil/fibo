<!DOCTYPE rdf:RDF [
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY fibo-fbc-pas-fpas "https://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/">
	<!ENTITY fibo-fnd-acc-cur "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/">
	<!ENTITY fibo-fnd-dt-fd "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-bt "https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/">
	<!ENTITY fibo-ind-fx-fx "https://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/">
	<!ENTITY fibo-ind-ind-ind "https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-fbc-pas-fpas="https://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"
	xmlns:fibo-fnd-acc-cur="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-bt="https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"
	xmlns:fibo-ind-fx-fx="https://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/"
	xmlns:fibo-ind-ind-ind="https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/">
		<rdfs:label>Foreign Exchange Ontology</rdfs:label>
		<dct:license
			rdf:resource="&sm;MITLicense"/>
		<sm:contentLanguage
			rdf:resource="http://www.omg.org/spec/ODM/"/>
		<sm:contentLanguage
			rdf:resource="http://www.w3.org/standards/techs/owl#w3c_all"/>
		<sm:copyright
			rdf:datatype="&xsd;string">Copyright (c) 2014-2017 EDM Council, Inc.
Copyright (c) 2014-2017 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/"/>
		<sm:dependsOn
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/"/>
		<sm:dependsOn
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"/>
		<sm:fileAbbreviation
			rdf:datatype="&xsd;string">fibo-ind-fx-fx</sm:fileAbbreviation>
		<sm:fileAbstract
			rdf:datatype="&xsd;string">This ontology provides the parameters for foreign exchange rates, covering spot and forward rates, as well as Fx spot rate volatilities.</sm:fileAbstract>
		<sm:filename
			rdf:datatype="&xsd;string">ForeignExchange.rdf</sm:filename>
		<rdfs:seeAlso
			rdf:resource="https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"/>
		<rdfs:seeAlso
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/AboutIND/"/>
		<rdfs:seeAlso
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/ForeignExchange/AboutForeignExchange/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"/>
		<owl:versionIRI
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/20170201/ForeignExchange/ForeignExchange/"/>
		<skos:changeNote
			rdf:datatype="&xsd;string">The https://spec.edmcouncil.org/fibo/IND/20140601/ForeignExchange/ForeignExchange.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 1 report.</skos:changeNote>
		<skos:changeNote
			rdf:datatype="&xsd;string">The https://spec.edmcouncil.org/fibo/IND/20150501/ForeignExchange/ForeignExchange.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 2 report, namely, to take advantage of content added via the FIBO FND 1.1 with respect to higher-level concepts of Rate, ExchangeRate, and InterestRate.</skos:changeNote>
		<skos:changeNote
			rdf:datatype="&xsd;string">The https://spec.edmcouncil.org/fibo/IND/20160801/ForeignExchange/ForeignExchange.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.1 RTF report.</skos:changeNote>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencyForwardRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;Date"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-pas-fpas;hasSettlementDate"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>currency forward rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a rate of exchange between two currencies for settlement at some future point in time, expressed as a premium on the spot rate</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencySpotBuyRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<rdfs:label>currency spot buy rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an indicative spot buying market rate as observed by the reporting source</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencySpotMidRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<rdfs:label>currency spot mid rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an indicative middle market (mean of spot buying and selling) rate as observed by the reporting source</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencySpotSellRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<rdfs:label>currency spot sell rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an indicative spot selling market rate as observed by the reporting source</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;FxSpotVolatility">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ind-ind;Volatility"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;ExchangeRate"/>
				<owl:onProperty
					rdf:resource="&fibo-ind-ind-ind;isVolatilityOf"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>FX spot volatility</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a measure of exchange rate fluctuation</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">OptionsEducation.org</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Mathematically, volatility is the annualized standard deviation of the daily changes in the exchange rate.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;QuotedExchangeRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;ExchangeRate"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ind-ind;MarketRate"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-acc-cur;Currency"/>
				<owl:onProperty
					rdf:resource="&fibo-ind-fx-fx;hasQuoteCurrency"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>quoted exchange rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an exchange rate quoted at a specific point in time, for a given block amount of currency as quoted against another (base) currency</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">An exchange rate of R represents a rate of R units of the quoted currency to 1 unit of the base currency.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;hasQuotationBlockAmountBasis">
		<rdfs:label>has quotation block amount basis</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the amount of the dealt currency which would be exchanged in a trade for which the stated spot rate applies</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;hasQuotationSettlementBasis">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasDuration"/>
		<rdfs:label>has quotation settlement basis</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;Duration"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the settlement period for a trade for which the stated spot rate applies</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-ind-fx-fx;hasQuotationSettlementBasisInDays">
		<rdfs:label>has quotation settlement basis in days</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;number"/>
		<owl:deprecated
			rdf:datatype="&xsd;boolean">true</owl:deprecated>
		<skos:definition
			rdf:datatype="&xsd;string">the settlement period in days for a trade for which the stated spot rate applies</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;hasQuoteCurrency">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-acc-cur;hasDealtCurrency"/>
		<rdfs:label>has quote currency</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;Currency"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the quote currency in an exchange rate; R units of this currency represent one unit of the base currency</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;hasSettlementDate">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label>has settlement date</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<owl:deprecated
			rdf:datatype="&xsd;boolean">true</owl:deprecated>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating the settlement date of a given transaction</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;isPremiumOn">
		<rdfs:label>is premium on</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-fx-fx;CurrencyForwardRate"/>
		<rdfs:range
			rdf:resource="&fibo-ind-fx-fx;QuotedExchangeRate"/>
		<skos:definition
			rdf:datatype="&xsd;string">an exchange rate expressed as a premium on the spot rate for the currency pair</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF><!-- touch -->