    <owl:DatatypeProperty rdf:about="&fibo-ind-ir-ir;hasRateResetTimeOfDay">
        <rdfs:label>has rate reset time of day</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the time of day when the rate is reset e.g., 11:00</skos:definition>
        <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterbankRate"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-ind-ir-ir;hasTenorInDays">
        <rdfs:label>has tenor in days</rdfs:label>
        <skos:editorialNote rdf:datatype="&xsd;string">This is given as a whole number representing the number of days, because the concept of a duration is not yet modeled semantically, otherwise this term would refer to duration as its range instead. The name of this property reflects this compromise and would be changed to &quot;Tenor&quot; once a suitable range exists for this property.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the length of time for which the interbank rate is quoted expressed as a number of days</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
        <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterbankRate"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-ind-ir-ir;hasTenorInMonths">
        <rdfs:label>has tenor in months</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the length of time for which the interbank rate is quoted, e.g., 3 months, 6 months expressed as a number of months</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
        <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterbankRate"/>
    </owl:DatatypeProperty>
    

