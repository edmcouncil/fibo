			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has indicator value</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-qt-qtu;QuantityValue"/>
		<skos:definition
			rdf:datatype="&xsd;string">specifies quantity value for a given indicator</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-ind-ei-ei;hasMinimumLegalWorkingAge">
		<rdfs:label>has minimum legal working age</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://www.investopedia.com/terms/w/working-age-population.asp"/>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">The working-age population is the total population in a region, within a set range of ages, that is considered to be able and likely to work. The working-age population measure is used to give an estimate of the total number of potential workers within an economy. For example, in the U.S., it is 16, whereas in Canada it is 15.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:range
			rdf:resource="&xsd;integer"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating the legal working age (minimum), in years, of people that are counted as members of the working population</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-ei-ei;hasPeriodicity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasRecurrenceInterval"/>
		<rdfs:label>has periodicity</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;RecurrenceInterval"/>
		<skos:definition
			rdf:datatype="&xsd;string">specifies a recurrence interval (monthly, quarterly, annual) that an indicator reflects</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-ind-ei-ei;hasPopulationSize">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-acc-cur;hasAmount"/>
		<rdfs:label>has population size</rdfs:label>
		<rdfs:range
			rdf:resource="&xsd;decimal"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating the number of elements in a given population</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-ei-ei;hasReferencePeriod">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasRecurrenceInterval"/>
		<rdfs:label>has reference period</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;RecurrenceInterval"/>
		<skos:definition
			rdf:datatype="&xsd;string">specifies a reference (baseline) recurrence interval for which a given economic indicator applies</skos:definition>
	</owl:ObjectProperty>
	
