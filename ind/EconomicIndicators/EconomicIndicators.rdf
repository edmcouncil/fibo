	<owl:Class
		rdf:about="&fibo-ind-ei-ei;StatisticalPopulation">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalUniverse"/>
		<rdfs:subClassOf>
