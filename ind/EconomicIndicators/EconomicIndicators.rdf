		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalUniverse"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasTimeContext"/>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;ExplicitDatePeriod"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasRegionalContext"/>
				<owl:allValuesFrom
					rdf:resource="&fibo-ind-ei-ei;StatisticalArea"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasPopulationSize"/>
				<owl:allValuesFrom
					rdf:resource="&xsd;decimal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>statistical population</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://stats.oecd.org/glossary/detail.asp?ID=2079"/>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">A common aim of statistical analysis is to produce information about some chosen population. In statistical inference, a subset of the population (a statistical sample) is chosen to represent the population in a statistical analysis. If a sample is chosen properly, characteristics of the entire population that the sample is drawn from can be estimated from corresponding characteristics of the sample.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a statistical universe filtered by time and region</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;StatisticalProgram">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-fct-pub;Publication"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasReferencePeriod"/>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-bd;BusinessRecurrenceInterval"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasReleaseDate"/>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;Date"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasReportingPeriod"/>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;ExplicitDatePeriod"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fbc-fct-ra;specifies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;StatisticalUniverse"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>statistical program</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://www.oxforddictionaries.com/definition/english/study"/>
		<skos:definition
			rdf:datatype="&xsd;string">a publication program that presents a detailed investigation and analysis of a subject or situation involving one or more studies or surveys</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;StatisticalUniverse">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-arr;Collection"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasUniverseSize"/>
				<owl:allValuesFrom
					rdf:resource="&xsd;decimal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasContext"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;StatisticalProgram"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>statistical universe</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://stats.oecd.org/glossary/detail.asp?ID=2087"/>
		<skos:definition
			rdf:datatype="&xsd;string">a collection representing the total membership, or &apos;universe&apos;, of people, resources, products, services, events, or entities of interest for some question, experiment, survey or statistical program</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">A statistical universe can be a group of actually existing objects (e.g. the set of all stars within the Milky Way galaxy) or a hypothetical and potentially infinite group of objects conceived as a generalization from experience (e.g. the set of all possible hands in a game of poker).</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;Total">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;QuantityValue"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Expression"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;StatisticalMeasure"/>
		<rdfs:label>total</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">https://wiki.edmcouncil.org/display/IND/Specifying+Statistical+Quantities</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a sum of the values for some characteristic of all units</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;UnemployedPopulation">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;CivilianLaborForce"/>
		<rdfs:label>unemployed population</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Persons who were waiting to be recalled to a job from which they had been laid off need not have been looking for work to be classified as unemployed.</fibo-fnd-utl-av:explanatoryNote>
		<owl:disjointWith
			rdf:resource="&fibo-ind-ei-ei;EmployedPopulation"/>
		<skos:definition
			rdf:datatype="&xsd;string">a subset of the civilian labor force that is considered to have had no employment but was available for work, except for temporary illness, and had made specific efforts to find employment sometime during a specified period, during the reporting period</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;UnemploymentRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Expression"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;CivilianLaborForce"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;hasIndicatorValue"/>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;RatioValue"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;UnemployedPopulation"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;UnemployedPopulation"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>unemployment rate</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.bls.gov/cps/faq.htm#Ques3</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Persons are classified as unemployed if they do not have a job, have actively looked for work in the prior 4 weeks, and are currently available for work. Workers expecting to be recalled from layoff are counted as unemployed, whether or not they have engaged in a specific jobseeking activity. In all other cases, the individual must have been engaged in at least one active job search activity in the 4 weeks preceding the interview and be available for work (except for temporary illness).</fibo-fnd-utl-av:explanatoryNote>
		<fibo-ind-ei-ei:actualExpression
			rdf:datatype="&xsd;string">unemployed population ÷ civilian labor force</fibo-ind-ei-ei:actualExpression>
		<rdfs:seeAlso
			rdf:resource="http://www.bls.gov/news.release/pdf/empsit.pdf"/>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing the ratio of the unemployed population with respect to the civilian labor force of a given economy for some specified period</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;ValueAddedProducerPriceIndex">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;ProducerPriceIndex"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;InputProducerPriceIndex"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;OutputProducerPriceIndex"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>value-added producer price index</rdfs:label>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">value-added PPI</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">https://www.imf.org/external/pubs/ft/ppi/2010/manual/ppi.pdf</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing a weighted average of the input and output producer price indices</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;Variance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;QuantityValue"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Expression"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;StatisticalMeasure"/>
		<rdfs:label>variance</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.statcan.gc.ca/edu/power-pouvoir/glossary-glossaire/5214842-eng.htm#v</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a measure of spread, calculated as the average squared deviation of each number from the mean of a data set</skos:definition>
	</owl:Class>
	
