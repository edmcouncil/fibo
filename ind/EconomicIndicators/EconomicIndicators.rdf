	<owl:DatatypeProperty
		rdf:about="&fibo-ind-ei-ei;excludesEnergyAndFood">
		<rdfs:label>excludes energy and food</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-ei-ei;ConsumerPriceIndex"/>
		<rdfs:range
