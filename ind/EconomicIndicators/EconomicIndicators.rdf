	<owl:Class
		rdf:about="&fibo-ind-ei-ei;Civilian">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-aap-ppl;LegallyCapablePerson"/>
		<rdfs:label>civilian</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a person that is not a member of the military (i.e., that is not on active duty)</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;CivilianLaborForce">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;CivilianNonInstitutionalPopulation"/>
		<rdfs:label>civilian labor force</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041</fibo-fnd-utl-av:adaptedFrom>
		<owl:disjointWith
			rdf:resource="&fibo-ind-ei-ei;PopulationNotInLaborForce"/>
		<skos:definition
			rdf:datatype="&xsd;string">a subset of the civilian, non-institutional population considered to be part of the labor force during a given reporting period</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;CivilianLaborForceParticipationRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Expression"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;CivilianLaborForce"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;CivilianNonInstitutionalPopulation"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;CivilianLaborForce"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>civilian labor force participation rate</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.bls.gov/bls/cps_fact_sheets/lfp_mock.htm</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">The labor force participation rate is the percentage of the population that is either employed or unemployed (that is, either working or actively seeking work).</fibo-fnd-utl-av:explanatoryNote>
		<fibo-ind-ei-ei:actualExpression
			rdf:datatype="&xsd;string">civilian labor force ÷ civilian non-institutional population</fibo-ind-ei-ei:actualExpression>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing the rate of participation the labor force of a given economy for some specified period</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;CivilianNonInstitutionalPopulation">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalUniverse"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;InstitutionalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-ind-ei-ei;Civilian"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first>
											<owl:Restriction>
												<owl:allValuesFrom
													rdf:resource="&xsd;integer"/>
												<owl:onProperty
													rdf:resource="&fibo-ind-ei-ei;hasMinimumLegalWorkingAge"/>
											</owl:Restriction>
										</rdf:first>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>civilian non-institutional population</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a statistical universe consisting of people of a certain age who reside in a given region, do not live in institutions (for example, correctional facilities, long-term care hospitals, and nursing homes), and are not on active military duty</skos:definition>
		<skos:scopeNote
			rdf:datatype="&xsd;string">The civilian non-institutional population is typically reported in thousands.</skos:scopeNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;CombinedStatisticalArea">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalArea"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;StatisticalArea"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>combined statistical area</rdfs:label>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">CSA</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="https://en.wikipedia.org/wiki/Combined_statistical_area"/>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="https://www.census.gov/population/metro/"/>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="https://www.whitehouse.gov/sites/default/files/omb/assets/fedreg_2010/06282010_metro_standards-Complete.pdf"/>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">These areas that combine retain their own designations as metropolitan or micropolitan statistical areas within the larger combined statistical area.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a combination of adjacent metropolitan and micropolitan areas with economic ties measured by commuting patterns</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;Consumer">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-aap-ppl;Person"/>
		<rdfs:label>consumer</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">For the purposes of the CPI, the definition of consumer is limited to humans.  In general, a consumer could include a pet, as the consumer of pet food, for example, although the pet owner would likely be the purchaser and target of advertising.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">The consumer is not always the purchaser of the product.  Consumers are considered to be the users of the final product.  For example, purchasers of building products are interim users of these products while constructing the finished product, which then may be purchased by the consumer.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a person that is the ultimate user of a product or service</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;ConsumerPriceIndex">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Expression"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-ei;CivilianNonInstitutionalPopulation"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;CivilianNonInstitutionalPopulation"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
