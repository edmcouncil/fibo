	<owl:Class
		rdf:about="&fibo-ind-ei-usei;AmericanStatisticsPublisher">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-pub;StatisticalInformationPublisher"/>
		<rdfs:label>American statistics publisher</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a statistical information publisher comprised of the government agencies and departments that publish statistical information on behalf of the U.S. Federal Government</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">Examples of American statistical information publishers include the Department of Commerce, Bureau of Economic Analysis (Commerce), Census Bureau (Commerce), Bureau of Labor Statistics (Labor), Department of the Treasury, and the Federal Reserve Board, among others.</skos:example>
	</owl:Class>
	
	<fibo-be-ge-ge:GovernmentAgency
		rdf:about="&fibo-ind-ei-usei;BureauOfLaborStatistics">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Bureau of Labor Statistics</rdfs:label>
		<fibo-be-ge-ge:hasJurisdiction
			rdf:resource="&fibo-be-ge-usj;UnitedStatesJurisdiction"/>
		<fibo-fnd-rel-rel:isPartOf
			rdf:resource="&fibo-ind-ei-usei;UnitedStatesDepartmentOfLabor"/>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">BLS</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.bls.gov/bls/infohome.htm</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Its mission is to collect, analyze, and disseminate essential economic information to support public and private decision-making. As an independent statistical agency, BLS serves its diverse user communities by providing products and services that are objective, timely, accurate, and relevant.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:seeAlso
			rdf:resource="http://www.bls.gov/"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the Bureau of Labor Statistics, the principal Federal agency responsible for measuring labor market activity, working conditions, and price changes in the economy</skos:definition>
	</fibo-be-ge-ge:GovernmentAgency>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;ConsumerExpenditureSurvey">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalProgram"/>
		<rdfs:label>consumer expenditure survey</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://www.bls.gov/cex/"/>
		<skos:definition
			rdf:datatype="&xsd;string">a program conducted on a regular basis that provides information on the buying habits of consumers, including data on their expenditures, income, and consumer unit (families and single consumers) characteristics</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;CurrentEmploymentStatistics">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalProgram"/>
		<rdfs:label>current employment statistics</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://www.bls.gov/ces/"/>
		<skos:definition
			rdf:datatype="&xsd;string">a survey conducted on a regular basis that presents analytical information related to businesses and government agencies, in order to provide detailed industry data on employment, hours, and earnings of workers</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;CurrentPopulationSurvey">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalProgram"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fbc-fct-ra;specifies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;CivilianNonInstitutionalPopulation"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>current population survey</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://www.bls.gov/cps/"/>
		<skos:definition
			rdf:datatype="&xsd;string">a survey conducted on a regular basis that presents analytical information related to the general population of a given statistical area with respect to labor force, employment, unemployment, persons not in the labor force, hours of work, earnings, and other demographic and labor force characteristics</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;EmploymentSituationEstablishmentSurvey">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-usei;EmploymentSituationSurvey"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasContext"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;Establishment"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>employment situation establishment survey</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041</fibo-fnd-utl-av:adaptedFrom>
		<rdfs:seeAlso
			rdf:resource="http://www.bls.gov/web/empsit/ces_cps_trends.pdf"/>
		<skos:definition
			rdf:datatype="&xsd;string">a survey conducted on a regular basis that presents analytical information related to the labor force of a given statistical area, surveyed with respect to businesses, and is, for the most part, seasonally adjusted</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;EmploymentSituationHouseholdSurvey">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-usei;EmploymentSituationSurvey"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasContext"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;Household"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>employment situation household survey</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041</fibo-fnd-utl-av:adaptedFrom>
		<rdfs:seeAlso
			rdf:resource="http://www.bls.gov/web/empsit/ces_cps_trends.pdf"/>
		<skos:definition
			rdf:datatype="&xsd;string">a survey conducted on a regular basis that presents analytical information related to the labor force of a given statistical area, surveyed with respect to households, and is, for the most part, seasonally adjusted</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;EmploymentSituationSurvey">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-usei;CurrentEmploymentStatistics"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-usei;CurrentPopulationSurvey"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onDataRange
					rdf:resource="&xsd;boolean"/>
				<owl:onProperty
					rdf:resource="&fibo-ind-ei-ei;isSeasonallyAdjusted"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>employment situation survey</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://www.bls.gov/opub/reports/about.htm"/>
		<skos:definition
			rdf:datatype="&xsd;string">a survey conducted on a regular basis that presents analytical information focused on the employment characteristics of a given statistical area</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;PointOfPurchaseSurvey">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;StatisticalProgram"/>
		<rdfs:label>point of purchase survey</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:resource="http://www.bls.gov/respondents/cpi/tpops/"/>
		<skos:definition
			rdf:datatype="&xsd;string">a program conducted on a regular basis that provides information on purchases of various items and services by consumers</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;USProducerPriceIndex">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;ProducerPriceIndex"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:hasValue
					rdf:resource="&fibo-ind-ei-usei;BureauOfLaborStatistics"/>
				<owl:onProperty
					rdf:resource="&fibo-be-fct-pub;isPublishedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-be-fct-pub;hasPublisher"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-usei;AmericanStatisticsPublisher"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>U.S. producer price index</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.bls.gov/ppi/</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing a measure of the change over time in the selling prices received by domestic producers for their output</skos:definition>
	</owl:Class>
	
	<fibo-be-ge-ge:GovernmentDepartment
		rdf:about="&fibo-ind-ei-usei;UnitedStatesDepartmentOfLabor">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>United States Department of Labor</rdfs:label>
		<fibo-be-ge-ge:hasJurisdiction
			rdf:resource="&fibo-be-ge-usj;UnitedStatesJurisdiction"/>
		<fibo-fnd-rel-rel:isPartOf
			rdf:resource="&fibo-be-ge-usj;UnitedStatesGovernment"/>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">DOL</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">https://www.dol.gov/general/aboutdol/mission</fibo-fnd-utl-av:adaptedFrom>
		<rdfs:seeAlso
			rdf:resource="https://www.dol.gov/"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the U.S. Department of Labor, a government department whose mission is to foster, promote, and develop the welfare of the wage earners, job seekers, and retirees of the United States; improve working conditions; advance opportunities for profitable employment; and assure work-related benefits and rights</skos:definition>
	</fibo-be-ge-ge:GovernmentDepartment>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;UrbanConsumerPriceIndex">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Expression"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;ConsumerPriceIndex"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:hasValue
					rdf:resource="&fibo-ind-ei-usei;AmericanStatisticsPublisher"/>
				<owl:onProperty
					rdf:resource="&fibo-be-fct-pub;hasPublisher"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:hasValue
					rdf:resource="&fibo-ind-ei-usei;BureauOfLaborStatistics"/>
				<owl:onProperty
					rdf:resource="&fibo-be-fct-pub;isPublishedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-ind-ei-usei;UrbanConsumersUniverse"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-usei;UrbanConsumersUniverse"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fbc-pas-fpas;Basket"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>consumer price index</rdfs:label>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">CPI-U</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.bls.gov/cpi/</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing a measure of the average change over time in the prices paid by urban consumers for a market basket of consumer goods and services</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-usei;UrbanConsumersUniverse">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;CivilianNonInstitutionalPopulation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasContext"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-usei;ConsumerExpenditureSurvey"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasContext"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-usei;PointOfPurchaseSurvey"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-ind-ei-ei;Consumer"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>urban consumers universe</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">U.S. Bureau of Labor Statistics, http://www.bls.gov/cpi/</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">In the United States, the CPI-U population, which covers about 88 percent of the U.S. population, covers households in all areas of the United States except people living in rural nonmetropolitan areas, in farm households, on military installations, in religious communities, and in institutions such as prisons and mental hospitals.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a statistical universe for consumer expenditure surveys consisting of people within a household that make joint expenditure decisions</skos:definition>
