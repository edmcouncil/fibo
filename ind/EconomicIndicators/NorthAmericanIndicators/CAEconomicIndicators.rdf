	xmlns:fibo-be-fct-pub="http://spec.edmcouncil.org/fibo/BE/FunctionalEntities/Publishers/"
	xmlns:fibo-be-ge-caj="http://spec.edmcouncil.org/fibo/BE/GovernmentEntities/NorthAmericanJurisdiction/CAGovernmentEntitiesAndJurisdictions/"
	xmlns:fibo-be-ge-ge="http://spec.edmcouncil.org/fibo/BE/GovernmentEntities/GovernmentEntities/"
	xmlns:fibo-fnd-arr-cls="http://spec.edmcouncil.org/fibo/FND/Arrangements/ClassificationSchemes/"
	xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-alx="http://spec.edmcouncil.org/fibo/FND/Utilities/Analytics/"
	xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-ind-ei-caei="http://spec.edmcouncil.org/fibo/IND/EconomicIndicators/NorthAmericanIndicators/CAEconomicIndicators/"
	xmlns:fibo-ind-ei-ei="http://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"
	xmlns:fibo-ind-ei-pub="http://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicatorPublishers/"
