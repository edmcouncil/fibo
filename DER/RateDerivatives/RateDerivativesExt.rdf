<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-der-der-dbx "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasicsExt/">
	<!ENTITY fibo-der-rat-ratx "https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivativesExt/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-tim-tim "https://spec.edmcouncil.org/fibo/FND/Time/Time/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivativesExt/"
	xmlns:fibo-der-der-dbx="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasicsExt/"
	xmlns:fibo-der-rat-ratx="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivativesExt/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-tim-tim="https://spec.edmcouncil.org/fibo/FND/Time/Time/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivativesExt/">
		<rdfs:label
			xml:lang="en">RateDerivativesExt</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasicsExt/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivatives/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/IND/InterestRates/InterestRates/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivatives/EconomicRateObservable">
		<owl:equivalentClass
			rdf:resource="&fibo-der-rat-ratx;EconomicRateObservable"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivatives/IndexDerivativeInstrument">
		<owl:equivalentClass
			rdf:resource="&fibo-der-rat-ratx;IndexDerivativeInstrument"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivatives/InterestRateDerivativeInstrument">
		<owl:equivalentClass
			rdf:resource="&fibo-der-rat-ratx;InterestRateDerivativeInstrument"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivatives/InterestRateObservable">
		<owl:equivalentClass
			rdf:resource="&fibo-der-rat-ratx;InterestRateObservable"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/DER/RateDerivatives/RateDerivatives/RateBasedObservable">
		<owl:equivalentClass
			rdf:resource="&fibo-der-rat-ratx;RateBasedObservable"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;BondEquivalentYield">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-rat-ratx;InterestRateTreatment"/>
		<rdfs:label
			xml:lang="en">bond equivalent yield</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;EconomicRateObservable">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-rat-ratx;RateBasedObservable"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-rat-ratx;observableIdentity"/>
				<owl:someValuesFrom
					rdf:resource="https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/EconomicIndicator"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">economic rate observable</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-der-rat-ratx;InterestRateObservable"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;IndexDerivativeInstrument">
		<rdfs:label
			xml:lang="en">index derivative instrument</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;InterestRateDerivativeInstrument">
		<rdfs:label
			xml:lang="en">interest rate derivative instrument</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;InterestRateObservable">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-rat-ratx;RateBasedObservable"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-rat-ratx;observableIdentity"/>
				<owl:someValuesFrom
					rdf:resource="https://spec.edmcouncil.org/fibo/IND/InterestRates/InterestRates/ReferenceInterestRate"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">interest rate observable</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;InterestRateTreatment">
		<rdfs:label
			xml:lang="en">interest rate treatment</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;MoneyMarketYield">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-rat-ratx;InterestRateTreatment"/>
		<rdfs:label
			xml:lang="en">money market yield</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-rat-ratx;RateBasedObservable">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-der-dbx;ObservableUnderlier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-rat-ratx;observableIdentity"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/EconomicIndicator"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/MarketRate"/>
										<rdf:rest>
											<rdf:Description>
												<rdf:first
													rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/BasketOfIndices"/>
												<rdf:rest
													rdf:resource="&rdf;nil"/>
											</rdf:Description>
										</rdf:rest>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">rate based observable</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-rat-ratx;hasRateTreatment">
		<rdfs:label
			xml:lang="en">has rate treatment</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-rat-ratx;InterestRateObservable"/>
		<rdfs:range
			rdf:resource="&fibo-der-rat-ratx;InterestRateTreatment"/>
		<skos:definition
			xml:lang="en">The specification of any rate conversion which needs to be applied to the observed rate before being used in any calculations.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-rat-ratx;indexTenor">
		<rdfs:label
			xml:lang="en">index tenor</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-rat-ratx;InterestRateObservable"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-tim-tim;KnownDateDuration"/>
		<skos:definition
			xml:lang="en">The tenor of the index, that is the length of time for which the index is quoted, e.g. 3 months, 6 months and so on.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-rat-ratx;observableIdentity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
		<rdfs:label
			xml:lang="en">observable identity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-rat-ratx;RateBasedObservable"/>
		<rdfs:range>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/EconomicIndicator"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="https://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/MarketRate"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/BasketOfIndices"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</rdfs:range>
	</owl:ObjectProperty>

</rdf:RDF>