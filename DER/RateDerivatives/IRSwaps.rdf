										<owl:someValuesFrom
											rdf:resource="&fibo-der-rtd-irswp;FixedInterestRateLeg"/>
									</owl:Restriction>
								</rdf:first>
								<rdf:rest>
									<rdf:Description>
										<rdf:first>
											<owl:Restriction>
												<owl:onProperty
													rdf:resource="&fibo-der-drc-swp;hasLeg"/>
												<owl:someValuesFrom
													rdf:resource="&fibo-der-rtd-irswp;FloatingInterestRateLeg"/>
											</owl:Restriction>
										</rdf:first>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
