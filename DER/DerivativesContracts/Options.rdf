	<owl:Class rdf:about="&fibo-der-der-opt;OptionWriterCommitment">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
		<rdfs:label xml:lang="en">option writer commitment</rdfs:label>
