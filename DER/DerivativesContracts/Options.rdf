			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-der-der-opt;hasSeller"/>
				<owl:someValuesFrom rdf:resource="&fibo-der-der-opt;OptionWritingParty"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-der-der-opt;hasStrikeTerms"/>
