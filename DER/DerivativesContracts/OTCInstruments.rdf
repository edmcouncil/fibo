<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-der-der-ma "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/">
	<!ENTITY fibo-der-der-otc "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivatives/">
	<!ENTITY fibo-der-der-oti "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/">
	<!ENTITY fibo-der-drc-bsc "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/">
	<!ENTITY fibo-fnd-acc-cur "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/">
	<!ENTITY fibo-fnd-agr-ctr "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/">
	<!ENTITY fibo-fnd-dt-fd "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/">
	<!ENTITY fibo-fnd-txn-rea "https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-sec-sec-sec "https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/">
	<!ENTITY junk "http://www.exmample.org/test#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/"
	xmlns:fibo-der-der-ma="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/"
	xmlns:fibo-der-der-otc="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivatives/"
	xmlns:fibo-der-der-oti="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/"
	xmlns:fibo-der-drc-bsc="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/"
	xmlns:fibo-fnd-acc-cur="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
	xmlns:fibo-fnd-agr-ctr="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-txn-rea="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-sec-sec-sec="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/"
	xmlns:junk="http://www.exmample.org/test#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCInstruments/">
		<rdfs:label
			xml:lang="en">OTCInstruments</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivativeMasterAgreements/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/OTCDerivatives/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/Securities/"/>
	</owl:Ontology>
	
	<rdfs:Datatype
		rdf:about="&xsd;String">
	</rdfs:Datatype>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;CashDeliveryCommitment">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-drc-bsc;DeliveryCommitment"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-der-oti;commitsTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-der-der-oti;DeliverCash"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">cash delivery commitment</rdfs:label>
		<skos:definition
			xml:lang="en">Commitment to deliver cash at the earliest available date as per settlement convention. This is the same date as a settlement date when the corresponding cash is made available in the other currency, in settlement for this amount.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">FpML exchangedCurrency1 referred to as a &quot;Currency Stream&quot;</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;DeliverCash">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-drc-bsc;DerivativeDeliveryTermsSet"/>
		<rdfs:label
			xml:lang="en">deliver cash</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;OTCContractSettlementTerms">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractTermsCompositeDescription"/>
		<rdfs:label
			xml:lang="en">o t c contract settlement terms</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;OTCDerivativeAgreement">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-drc-bsc;DerivativeAgreement"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-der-der-oti;OTCTransactionAgreement"/>
		<rdfs:label
			xml:lang="en">o t c derivative agreement</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;OTCInstrument">
		<rdfs:label>otcinstrument</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;OTCInstrumentConfirmation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;WrittenContract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-der-ma;governs"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-der-der-otc;OTCDerivativeTransaction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-agr-ctr;supersedes"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-der-der-oti;OTCInstrumentContract"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">o t c instrument confirmation</rdfs:label>
		<skos:definition
			xml:lang="en">A formal confirmation document which constitutes a written contract between the parties to the transaction, setting out the terms and conditions of the transaction.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">This effectively supersedes the (implied) contract that is brought into being by the transaction. It makes reference to the terms and conditions under which the transaction is deemed to have been conducted. The Confirmation references the Master Agreement under which the transaction has been conducted along with the ISDA standard terms referred to (if applicable), and adds terms and conditions specific to that one Transaction. This Confirmation generally supersedes all terms in the ISDA standard Definitions and the Master Agreement in the event of any inconsistency between the three.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;OTCInstrumentContract">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-der-der-oti;oTCInstrumentContractEmbodies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-der-der-oti;OTCTransactionAgreement"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">o t c instrument contract</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-sec-sec-sec;Security"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-oti;OTCTransactionAgreement">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
		<rdfs:label
			xml:lang="en">o t c transaction agreement</rdfs:label>
		<skos:definition
			xml:lang="en">The agreement which forms the basis of any Over the Counter (OTC) transaction, including OTC derivatives and bilateral debt instruments.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-der-oti;commitsTo">
		<rdfs:label
			xml:lang="en">commits to</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-der-oti;CashDeliveryCommitment"/>
		<rdfs:range
			rdf:resource="&fibo-der-der-oti;DeliverCash"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-der-oti;deliveryAmount">
		<rdfs:label
			xml:lang="en">delivery amount</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-der-oti;DeliverCash"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-der-oti;deliveryDate">
		<rdfs:label
			xml:lang="en">delivery date</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-der-oti;DeliverCash"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<skos:definition
			xml:lang="en">&apos;The date on which both currencies traded will settle.&apos;</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-der-oti;oTCInstrumentContractEmbodies">
		<rdfs:label
			xml:lang="en">o t c instrument contract embodies</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-der-oti;OTCInstrumentContract"/>
		<rdfs:range
			rdf:resource="&fibo-der-der-oti;OTCTransactionAgreement"/>
		<skos:definition
			xml:lang="en">The economic agreement (transaction related agreement) which the OTC Instrument Contract describes as obtaining between the parties to the Contract.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-der-der-oti;paymentType">
		<rdfs:label
			xml:lang="en">payment type</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-der-oti;DeliverCash"/>
		<rdfs:range
			rdf:resource="&xsd;String"/>
		<skos:definition
			xml:lang="en">FpML = paymentType &apos;A classification of the type of fee or additional payment, e.g. brokerage, upfront fee etc. FpML does not define domain values for this element.&apos;</skos:definition>
	</owl:DatatypeProperty>

</rdf:RDF>