		<rdfs:label
			xml:lang="en">deliverable underlier</rdfs:label>
		<skos:definition
			xml:lang="en">The Underlying of a Derivatives Contract, which may also be delivered on exercise or expiry of that contract.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-der-dbx;DerivativeContract">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasics/DerivativeContractParty"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-agr-ctr;hasContractParty"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
