		rdf:about="&fibo-der-ass-eqo;hasAnnex">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-agr-ctr;hasContractualElement"/>
		<rdfs:label
			xml:lang="en">has annex</rdfs:label>
		<rdfs:domain
