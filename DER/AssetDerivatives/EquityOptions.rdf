		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-agr-ctr;hasGoverningJurisdiction"/>
		<rdfs:label
			xml:lang="en">has relevant jurisdiction</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-der-ass-eqo;EquityOptionOTCContract"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
	</owl:ObjectProperty>
	
