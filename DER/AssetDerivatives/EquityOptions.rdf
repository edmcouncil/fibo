		<rdfs:range
			rdf:resource="&fibo-der-ass-bas;EquityBasketOptionAnnex"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-ass-eqo;hasLocalJurisdiction">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-agr-ctr;hasGoverningJurisdiction"/>
		<rdfs:label
			xml:lang="en">has local jurisdiction</rdfs:label>
		<rdfs:domain
