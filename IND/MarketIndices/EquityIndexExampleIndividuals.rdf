	<owl:NamedIndividual rdf:about="&fibo-ind-mkt-eqind;SPDowJonesIndices">
		<rdf:type rdf:resource="&fibo-fbc-pas-fpas;FinancialServiceProvider"/>
		<rdf:type rdf:resource="&fibo-ind-ir-pub;MarketDataProvider"/>
		<rdfs:label>S &amp; P Dow Jones Indices</rdfs:label>
		<skos:definition>financial service and market data provider that is a global provider of index-based data and research</skos:definition>
		<fibo-fnd-rel-rel:hasIdentity rdf:resource="&fibo-ind-mkt-eqind;SPDowJonesIndicesLLC-US-DE"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-ind-mkt-eqind;SPDowJonesIndicesLLC-US-DE">
		<rdf:type rdf:resource="&fibo-be-plc-plc;PrivateCompanyWithLimitedLiability"/>
		<rdfs:label>S &amp; P Dow Jones Indices LLC US-DE</rdfs:label>
		<skos:definition>the S &amp; P Dow Jones Indices LLC legal entity that is a Delaware limited liability company</skos:definition>
		<fibo-be-oac-cctl:hasDomesticUltimateParent rdf:resource="&fibo-fbc-fct-usfsind;SPGlobal"/>
		<fibo-fbc-fct-breg:hasHeadquartersAddress rdf:resource="&fibo-fbc-fct-usfsind;SPGlobalHeadquartersAddress"/>
