<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-civ-fnd-civ "https://spec.edmcouncil.org/fibo/ontology/CIV/Funds/CIV/">
	<!ENTITY fibo-civ-fnd-clx "https://spec.edmcouncil.org/fibo/ontology/CIV/Funds/CIVClassification/">
	<!ENTITY fibo-fnd-inf-inf "https://spec.edmcouncil.org/fibo/ontology/FND/Information/InfoCore/">
	<!ENTITY fibo-fnd-inf-sch "https://spec.edmcouncil.org/fibo/ontology/FND/Information/Schemes/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/ontology/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/ontology/CIV/Funds/CIVClassification/"
	xmlns:fibo-civ-fnd-civ="https://spec.edmcouncil.org/fibo/ontology/CIV/Funds/CIV/"
	xmlns:fibo-civ-fnd-clx="https://spec.edmcouncil.org/fibo/ontology/CIV/Funds/CIVClassification/"
	xmlns:fibo-fnd-inf-inf="https://spec.edmcouncil.org/fibo/ontology/FND/Information/InfoCore/"
	xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/ontology/FND/Information/Schemes/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/ontology/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/ontology/CIV/Funds/CIVClassification/">
		<rdfs:label
			xml:lang="en">CIVClassification</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/CIV/Funds/CIV/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Information/InfoCore/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Information/Schemes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="&fibo-civ-fnd-civ;Fund">
		<owl:equivalentClass
			rdf:resource="&fibo-civ-fnd-clx;Fund"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-civ-fnd-clx;Fund">
		<rdfs:label
			xml:lang="en">fund</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-civ-fnd-clx;FundClassification">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;Classification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;classifies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-civ-fnd-civ;Fund"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">fund classification</rdfs:label>
		<skos:definition
			xml:lang="en">The category of the fund according to the asset class. This is a published code by which the Fund is classified. Further notes: Could be EFCF codes. This is the way in which &quot;Type of fund&quot; is articulated.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-civ-fnd-clx;FundClassificationScheme">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;ClassificationScheme"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;defines.1"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-civ-fnd-clx;FundClassification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">fund classification scheme</rdfs:label>
		<skos:definition
			xml:lang="en">A published scheme for the category of a fund according to the asset class.</skos:definition>
	</owl:Class>

</rdf:RDF>