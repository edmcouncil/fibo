<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-bp-reg-req "https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/">
	<!ENTITY fibo-fnd-law-jur "https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/">
	<!ENTITY fibo-fnd-lawx-rst "https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/">
	<!ENTITY fibo-fnd-tim-tim "https://spec.edmcouncil.org/fibo/FND/Time/Time/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-val "https://spec.edmcouncil.org/fibo/FND/Utilities/Values/">
	<!ENTITY fibo-sec-sec-resx "https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/"
	xmlns:fibo-bp-reg-req="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/"
	xmlns:fibo-fnd-law-jur="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"
	xmlns:fibo-fnd-lawx-rst="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/"
	xmlns:fibo-fnd-tim-tim="https://spec.edmcouncil.org/fibo/FND/Time/Time/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
	xmlns:fibo-sec-sec-resx="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictionsExt/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">SecuritiesRestrictionsExt</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Time/Time/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/FinancialInstrument">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-sec-resx;FinancialInstrument"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-sec-resx;Security"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;FinancialInstrument">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">financial instrument</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;InvestmentRegulatoryLaw">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">investment regulatory law</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A body of law dealing with investment regulation.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;InvestmentRegulatoryRequirement">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-lawx-rst;RegulatoryRequirement"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-bp-reg-req;mandatedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-resx;InvestmentRegulatoryLaw"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">investment regulatory requirement</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A requirement laid down in law for investment regulation.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;InvestmentRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-lawx-rst;LegalRestriction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-lawx-rst;restrictionAppliesIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">investment restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Restrictions applicable to a financial instrument as provided for in a body of Law.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;InvestorDomicileType">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">investor domicile type</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;InvestorType">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">investor type</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;Security">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-sec-sec-resx;SecurityContractualRestriction"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-resx;contractuallyDefinesRestrictions"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-resx;holderRestrictions"/>
				<owl:someValuesFrom
					rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">security</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;SecurityContractualRestriction">
		<rdfs:subClassOf
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/ContractualRestriction"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">security contractual restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Contractual terms setting out restrictions on either the holder or the issuer of the security.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">These are restrictions defined in the contractual terms of the security itself and form part of the terms of the security, as distinct from restrictions defined in some body of law and being applicable to the security, which are defined separately.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-resx;SecurityRegistrationRequirement">
		<rdfs:subClassOf
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/RegistrationRequirement"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-resx;InvestmentRegulatoryRequirement"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/appliesTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-resx;Security"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">security registration requirement</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A Requirement for Registraton of a Security within some Country, Jurisdiction or Federal Province.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-resx;contractuallyDefinesRestrictions">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contractually defines restrictions</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-resx;Security"/>
		<rdfs:range
			rdf:resource="&fibo-sec-sec-resx;SecurityContractualRestriction"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-resx;hasEffectivePeriod">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has effective period</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-resx;InvestmentRestriction"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-tim-tim;DatePeriod"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Period during which the restriction applies.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-resx;hasRegistrationRequirement">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has registration requirement</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-resx;Security"/>
		<rdfs:range
			rdf:resource="&fibo-sec-sec-resx;SecurityRegistrationRequirement"/>
		<owl:inverseOf
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/appliesTo"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-resx;hasRestriction">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has restriction</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-resx;Security"/>
		<rdfs:range
			rdf:resource="&fibo-sec-sec-resx;InvestmentRestriction"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-resx;hasRestrictions">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has restrictions  (please review)</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-resx;FinancialInstrument"/>
		<rdfs:range
			rdf:resource="&fibo-sec-sec-resx;InvestmentRestriction"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-resx;holderRestrictions">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">holder restrictions</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-resx;Security"/>
		<rdfs:range
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-resx;isInclusive">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is inclusive</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-resx;SecurityRegistrationRequirement"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-val;TrueFalseValue"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Whether the Restriction is defined inclusively (the stated terms put something within the Restriction), or Exclusively (the stated terms put something outside the reach or remit of the Restriction). Yes = Inclusive.</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF><!-- touch -->