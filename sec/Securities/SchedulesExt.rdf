<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-dt-fd "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/">
	<!ENTITY fibo-fnd-dt-oc "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/">
	<!ENTITY fibo-fnd-occ-ev "https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-sec-sec-schx "https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-dt-oc="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/"
	xmlns:fibo-fnd-occ-ev="https://spec.edmcouncil.org/fibo/FND/Occurrent/Events/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-sec-sec-schx="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/SchedulesExt/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">SchedulesExt</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="&fibo-fnd-dt-fd;AdHocSchedule">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-sec-schx;AdHocSschedule"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="&fibo-fnd-dt-fd;RegularSchedule">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-sec-schx;RegularSchedule"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-schx;AdHocSschedule">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-schx;listsScheduledEvent"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-schx;ExplicitlyScheduledEvent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">ad hoc schedule</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-schx;ExplicitlyScheduledEvent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-oc;OccurrenceKind"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-schx;hasEventDate"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-schx;ScheduledEventDate"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">explicitly scheduled event</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">An event which is scheduled in advance to take place on some specific date.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">This is as distinct from an event the date for which is specified in terms of some formula or heuristic for determining that date.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-schx;RegularSchedule">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-schx;schedules"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-schx;ScheduledEvent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">regular schedule</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-schx;ScheduledEvent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-oc;OccurrenceKind"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-schx;scheduledBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-schx;RegularSchedule"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">scheduled event</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-schx;ScheduledEventDate">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">scheduled event date</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-schx;hasEventDate">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has event date</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-schx;ExplicitlyScheduledEvent"/>
		<rdfs:range
			rdf:resource="&fibo-sec-sec-schx;ScheduledEventDate"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-schx;listsScheduledEvent">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;comprises"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">lists scheduled event</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-schx;AdHocSschedule"/>
		<rdfs:range
			rdf:resource="&fibo-sec-sec-schx;ExplicitlyScheduledEvent"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-schx;scheduledBy">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">scheduled by</rdfs:label>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-schx;schedules">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">schedules</rdfs:label>
	</owl:ObjectProperty>

</rdf:RDF>