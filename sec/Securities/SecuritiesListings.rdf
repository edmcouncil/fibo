	xmlns:fibo-fbc-fct-mkt="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/Markets/"
	xmlns:fibo-fbc-fct-ra="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/"
