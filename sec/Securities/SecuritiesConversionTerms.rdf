<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-sec-sec-con "https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesConversionTerms/">
	<!ENTITY fibo-sec-sec-sec "https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/Securities/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesConversionTerms/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-sec-sec-con="https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesConversionTerms/"
	xmlns:fibo-sec-sec-sec="https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/Securities/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesConversionTerms/">
		<rdfs:label
			xml:lang="en">SecuritiesConversionTerms</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/Securities/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-con;ConversionTerms">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-sec;SecurityContractTerms"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-con;specifiesConversionInto"/>
				<owl:someValuesFrom
					rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/Security"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">conversion terms</rdfs:label>
		<skos:definition
			xml:lang="en">Contractual specifications when a security can be converted to another security (usually of the same issuer).</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">Not to be confused with the underlying indicators of a derivative. i.e. convertible bonds to common stock.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-con;specifiesConversionInto">
		<rdfs:label
			xml:lang="en">specifies conversion into  (please review)</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-con;ConversionTerms"/>
		<rdfs:range
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/Security"/>
	</owl:ObjectProperty>

</rdf:RDF>