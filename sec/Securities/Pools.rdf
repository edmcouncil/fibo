		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"/>
