    <!ENTITY fibo-fnd-plc-fac "http://spec.edmcouncil.org/fibo/FND/Places/Facilities/" >
    <!ENTITY fibo-fnd-plc-vrt "http://spec.edmcouncil.org/fibo/FND/Places/VirtualPlaces/" >
    <!ENTITY fibo-fnd-pty-rl "http://spec.edmcouncil.org/fibo/FND/Parties/Roles/" >
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/FND/Parties/Parties/" >
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/" >
    <!ENTITY fibo-fnd-pas-pas "http://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/" >

    <!ENTITY fibo-fbc-fi-fi "http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/" >
    <!ENTITY fibo-fbc-fct-mkt "http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/Markets/" >
