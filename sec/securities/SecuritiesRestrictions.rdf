	<owl:Class
		rdf:about="&fibo-fbc-fi-fi;FinancialInstrument">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-sec-sec-rst;ContractualRestriction"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;defines"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-sec-sec-rst;RegistrationRequirement"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-rst;hasRegistrationRequirement"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-sec-sec-rst;SecuritiesRestriction"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-rst;hasRestriction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">extends the definition of a financial instrument to identify any relevant restrictions or registration requirements</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-lst;SecurityListing">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-sec-sec-rst;SecuritiesRestriction"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-rst;hasRestriction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">extends the definition of a security listing to identify any relevant restrictions</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;BlueSkyLaw">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;SecuritiesRegulation"/>
		<rdfs:label>Blue-Sky law</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Finance and Investment Terms, Ninth Edition, 2014</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">This allows investors to base their judgments on trustworthy data.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a kind of securities regulation passed by various states, designed to protect investors against securities fraud by requiring sellers of new issues to register their offerings and provide financial details</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;ContractualRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;SecuritiesRestriction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isMandatedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>contractual restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">Contractual terms setting out restrictions on either the holder or the issuer of the security.  has to apply to a financial instrument (context)  ... part of contract terms set for the financial instrument</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">These are restrictions defined in the contractual terms of the security itself and form part of the terms of the security, as distinct from restrictions defined in some body of law and being applicable to the security, which are defined separately.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;IndividualInvestor">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-oac-opty;Investor"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-aap-ppl;Person"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>individual investor</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an investor that is a person, rather than an organization, who may or may not be considered a qualified investor from a legal perspective</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;InstitutionalInvestor">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-oac-opty;Investor"/>
		<rdfs:label>institutional investor</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Finance and Investment Terms, Ninth Edition, 2014</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Typically more than 70 percent of the daily trading on the New York Stock Exchange is conducted on behalf of institutional investors.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">an organization that trades large volumes of securities</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">Example institutional investors include banks, insurance companies, mutual funds, pension funds, and other similar large funds.</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;InvestorsDomicileRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;LegalHoldingRestriction"/>
		<rdfs:label>investors&apos; domicile restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a legal holding restriction that specifies where holders of a security may be domiciled</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Needs review and provenance</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;LegalHoldingRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;SecuritiesRestriction"/>
		<rdfs:label>legal holding restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a security restriction applicable to holding the security</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;LegalRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;SecuritiesRestriction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-law-cor;Law"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-sec-sec-rst;SecuritiesRegulation"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:allValuesFrom>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isDefinedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-law-cor;Law"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-sec-sec-rst;SecuritiesRegulation"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:allValuesFrom>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isMandatedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>legal restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a securities restriction that is mandated by law or regulation</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;QualifiedInvestorRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;LegalHoldingRestriction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;DatePeriod"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-rst;hasHoldingPeriod"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>qualified investor restriction</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">If a holding period is not defined, then the period for which the restriction applies is indefinite.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a legal holding restriction specifying that only qualified investors may hold the security, optionally for a defined holding period (if this is not defined then the period is indefinite)</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Needs review and provenance</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;RegistrationRequirement">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;SecuritiesRegulation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fbc-fi-fi;Security"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>registration requirement</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a requirement for registraton of a security within some country, jurisdiction or federal province  ... applies to a registered security, is a function of registration regulations</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Needs review and provenance</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;RegulationS">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;InvestorsDomicileRestriction"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;QualifiedInvestorRestriction"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;SecuritiesRegulation"/>
		<rdfs:label>Regulation S</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a securities regulation defining an exemption through which corporations can issue unregistered securities to qualified foreign investors and foreign institutions</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Needs review and provenance</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;Restriction144A">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-rst;LegalRestriction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;DatePeriod"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-rst;hasHoldingPeriod"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Restriction 144A</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a Securities &amp; Exchange Commission rule modifying a two-year holding period requirement on privately placed securities to permit qualified institutional buyers to trade these positions among themselves</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Needs review and provenance</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;SecuritiesRegulation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fct-rga;Regulation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-agr-ctr;hasGoverningJurisdiction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-law-cor;isInForceIn"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-sec-sec-rst;SecuritiesRestriction"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;defines"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>securities regulation</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a regulation codified in law specific to securities and investments</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-rst;SecuritiesRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-agr-ctr;hasGoverningJurisdiction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-law-cor;isInForceIn"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
