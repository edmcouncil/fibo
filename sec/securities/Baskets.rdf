    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-sec-sec-bsk;Basket">
        <rdfs:label>basket</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a tradable container, a financial instrument in its own right, and collection of two or more things</skos:definition>
        <fibo-fnd-utl-av:definitionOrigin>Nordea Capital Markets</fibo-fnd-utl-av:definitionOrigin>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-arr-arr;Collection"/>
        <rdfs:subClassOf rdf:resource="&fibo-fbc-fi-fi;FinancialInstrument"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
                <owl:onClass rdf:resource="&fibo-sec-sec-bsk;BasketConstituent"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-sec-sec-bsk;BasketConstituent">
        <rdfs:label>basket constituent</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a component of a basket</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-rl;ThingInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&fibo-fbc-pas-pas;Commodity"/>
                            <rdf:Description rdf:about="&fibo-fbc-fi-fi;FinancialInstrument"/>
                            <rdf:Description rdf:about="&fibo-ind-ind-ind;MarketRate"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
                <owl:someValuesFrom>              
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isMemberOf"/>
                        <owl:onClass rdf:resource="&fibo-sec-sec-bsk;Basket"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:someValuesFrom>              
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>

    <owl:Class rdf:about="&fibo-sec-sec-bsk;BasketOfIndices">
        <rdfs:label>market basket</rdfs:label>
        <skos:definition>A basket made up of indices.</skos:definition>
      <skos:editorialNote>For example certain equities derivatives have an underlying basket which is a basket of more than one index, not a basket of securities.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;Basket"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
                <owl:onClass rdf:resource="&fibo-sec-sec-bsk;BasketOfIndicesConstituent"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-bsk;BasketOfIndicesConstituent">
      <rdfs:label>Basket Of Indices Constituent</rdfs:label>
      <skos:definition>One constituent of a basket of indices. This is a published index, defined as a part of the basket. Further notes: Not to be confused with the constituent of an index of securities.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;BasketConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-bsk;BasketOfSecurities">
        <rdfs:label>basket Of securities</rdfs:label>
        <skos:definition>a basket of traded securities</skos:definition>
        <skos:editorialNote>This does not include baskets of indices, which are modeled separately. also does not include Basket of Baskets. Also need to break down by Country, by Region (e.g. non Japan Asia sub-indices). See MSCI World Index.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;Basket"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
                <owl:onClass rdf:resource="&fibo-sec-sec-bsk;SecuritiesBasketConstituent"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-sec-sec-bsk;MixedBasket">
        <rdfs:label>Mixed Basket</rdfs:label>
        <skos:definition>A basket that has a mix of constituents which include securities and indices.</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;Basket"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
                <owl:onClass> 
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Class rdf:about="&fibo-sec-sec-bsk;BasketOfIndicesConstituent"/>
                            <owl:Class rdf:about="&fibo-sec-sec-bsk;SecuritiesBasketConstituent"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass> 
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-sec-sec-bsk;SecuritiesBasketConstituent">
        <rdfs:label>Securities Basket Constituent</rdfs:label>
        <skos:definition>a Security defined as one of the components of a basket of Securities.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;BasketConstituent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
                <owl:allValuesFrom rdf:resource="&fibo-fbc-fi-fi;Security"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

</rdf:RDF>