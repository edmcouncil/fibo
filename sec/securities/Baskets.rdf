<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY fibo-fbc-fi-fi "http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/">
	<!ENTITY fibo-fbc-pas-fpas "http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/">
	<!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-ind-ei-ei "http://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/">
	<!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/">
	<!ENTITY fibo-sec-sec-bsk "http://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="http://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-fbc-fi-fi="http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"
	xmlns:fibo-fbc-pas-fpas="http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"
	xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-ind-ei-ei="http://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"
	xmlns:fibo-ind-ind-ind="http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"
	xmlns:fibo-sec-sec-bsk="http://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="http://spec.edmcouncil.org/fibo/SEC/Securities/Baskets/">
		<rdfs:label>Baskets Ontology</rdfs:label>
		<dct:license
			rdf:resource="&sm;MITLicense"/>
		<sm:contentLanguage
			rdf:resource="http://www.omg.org/spec/ODM/"/>
		<sm:contentLanguage
			rdf:resource="http://www.w3.org/standards/techs/owl#w3c_all"/>
		<sm:copyright
			rdf:datatype="&xsd;string">Copyright (c) 2016-2017 EDM Council, Inc.
Copyright (c) 2016-2017 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/"/>
		<sm:dependsOn
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/"/>
		<sm:dependsOn
			rdf:resource="http://spec.edmcouncil.org/fibo/IND/"/>
		<sm:fileAbbreviation
			rdf:datatype="&xsd;string">fibo-sec-sec-bsk</sm:fileAbbreviation>
		<sm:fileAbstract
			rdf:datatype="&xsd;string">This ontology defines the concept of a tradable container of securities, indices, and/or market rates, and identifies the elements that can be constituents of a such a basket.</sm:fileAbstract>
		<sm:filename
			rdf:datatype="&xsd;string">Baskets.rdf</sm:filename>
		<rdfs:seeAlso
			rdf:resource="http://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"/>
		<rdfs:seeAlso
			rdf:resource="http://spec.edmcouncil.org/fibo/SEC/AboutSEC/"/>
		<rdfs:seeAlso
			rdf:resource="http://spec.edmcouncil.org/fibo/SEC/Securities/AboutSecurities/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"/>
		<owl:versionIRI
			rdf:resource="http://spec.edmcouncil.org/fibo/SEC/20170201/Securities/Baskets/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-bsk;BasketOfIndices">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-fpas;Basket"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fbc-pas-fpas;hasConstituent"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-bsk;BasketOfIndicesConstituent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>market basket</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a basket made up of market indices</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">For example, certain equities derivatives have an underlying basket which is a basket of more than one index, not a basket of securities.</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-bsk;BasketOfIndicesConstituent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-fpas;BasketConstituent"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-ind-ind-ind;MarketRate"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:allValuesFrom>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>basket Of indices constituent</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">This is a published index, defined as a part of the basket. Further notes: Not to be confused with the constituent of an index of securities.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a single constituent of a basket of indices</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-bsk;BasketOfSecurities">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-fpas;Basket"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fbc-pas-fpas;hasConstituent"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-sec-bsk;SecuritiesBasketConstituent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>basket Of securities</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a basket of traded securities</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">This does not include baskets of indices, which are modeled separately. also does not include basket of baskets. Also need to break down by Country, by Region (e.g. non Japan Asia sub-indices). See MSCI World Index.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-bsk;MixedBasket">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-fpas;Basket"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fbc-pas-fpas;hasConstituent"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-sec-sec-bsk;BasketOfIndicesConstituent"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-sec-sec-bsk;SecuritiesBasketConstituent"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>mixed basket</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a basket that has a mix of constituents, including securities and indices</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-bsk;SecuritiesBasketConstituent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-fpas;BasketConstituent"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fbc-fi-fi;Security"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>securities basket constituent</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a security identified as a component of a basket of securities</skos:definition>
	</owl:Class>

</rdf:RDF>