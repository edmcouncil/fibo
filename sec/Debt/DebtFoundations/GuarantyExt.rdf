<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-be-le-fbo "https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/">
	<!ENTITY fibo-fnd-agr-ctr "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/">
	<!ENTITY fibo-fnd-pty-pty "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/">
	<!ENTITY fibo-fnd-pty-rl "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-sec-dbt-dbt-gtyx "https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/"
	xmlns="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/"
	xmlns:fibo-be-le-fbo="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"
	xmlns:fibo-fnd-agr-ctr="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"
	xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"
	xmlns:fibo-fnd-pty-rl="https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-sec-dbt-dbt-gtyx="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/GuarantyExt/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">GuarantyExt</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2002/07/owl#"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/Lattice/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/Guaranty">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-dbt-dbt-gtyx;Guaranty"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="&fibo-fnd-agr-ctr;Contract">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-dbt-dbt-gtyx;Contract"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbt-gtyx;Contract">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contract</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbt-gtyx;ContractGuarantor">
		<rdfs:subClassOf
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/Guarantor"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractThirdParty"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contract guarantor</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A Party which guarantees to underwrite some cash flow obligation on behalf of some other party.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbt-gtyx;Guaranty">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&owl;Thing"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-dbt-dbt-gtyx;guarantyTakesFormOf"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">guaranty</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Arrangement whereby cash flows on the debt security, e.g., interest payment, are guaranteed by a firm other than the issuer.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbt-gtyx;guarantorHasIdentity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">guarantor has identity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbt-gtyx;ContractGuarantor"/>
		<rdfs:range
			rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbt-gtyx;guarantyTakesFormOf">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">guaranty takes form of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbt-gtyx;Guaranty"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbt-gtyx;hasContractGuarantor">
		<rdfs:subPropertyOf
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/hasGuarantor"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has contract guarantor</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbt-gtyx;Contract"/>
		<rdfs:range
			rdf:resource="&fibo-sec-dbt-dbt-gtyx;ContractGuarantor"/>
	</owl:ObjectProperty>

</rdf:RDF>