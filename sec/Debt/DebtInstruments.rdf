	<owl:Class
		rdf:about="&fibo-fbc-fi-fi;DebtInstrument">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-iss;hasOffering"/>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>
	
	<fibo-sec-dbt-dbti:RelativePrice
		rdf:about="&fibo-sec-dbt-dbti;AtADiscount">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>at a discount</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.investopedia.com/terms/a/at-a-discount.asp</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">below par</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a selling price that is less than the face or nominal value</skos:definition>
	</fibo-sec-dbt-dbti:RelativePrice>
	
	<fibo-sec-dbt-dbti:RelativePrice
		rdf:about="&fibo-sec-dbt-dbti;AtAPremium">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>at a premium</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.investopedia.com/terms/a/at-a-premium.asp</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">above par</fibo-fnd-utl-av:synonym>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">premium</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a selling price significantly above the stated face or redemption value due to high demand or timing of redemption</skos:definition>
	</fibo-sec-dbt-dbti:RelativePrice>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;CallEvent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-oc;OccurrenceKind"/>
		<rdfs:label>call event</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an event associated with the call schedule for a debt instrument</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;CallFeature">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrumentRedemptionProvision"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;CallNotificationProvision"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;CallSchedule"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>call feature</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Most corporate and municipal bonds have ten-year call features (termed call protection by holders); government securities typically have none.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">call provision</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a redemption provision defining the schedule and price of redemption prior to maturity</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;CallNotificationProvision">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrumentRedemptionProvision"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;CallWindow"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-dt-fd;hasDatePeriod"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>call notification provision</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a provision of a call feature defining notification requirements</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;CallPremium">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MonetaryPrice"/>
		<rdfs:label>call premium</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an amount over par that an issuer must pay an investor to redeem the security early</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;CallPrice">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MonetaryPrice"/>
		<rdfs:label>call price</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">redemption price</fibo-fnd-utl-av:synonym>
		<fibo-ind-ei-ei:actualExpression
			rdf:datatype="&xsd;string">par value + call premium</fibo-ind-ei-ei:actualExpression>
		<skos:definition
			rdf:datatype="&xsd;string">a price at which a debt instrument with a call feature may be redeemed by the issuer</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;CallSchedule">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-fd;Schedule"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;CallEvent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-sec-dbt-dbti;CallPremium"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-sec-dbt-dbti;CallPrice"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>call schedule</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a schedule that defines the events associated with the call feature of a debt instrument, i.e, the dates on which the debt instrument may be called (recalled) at what price by the issuer</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;CallWindow">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-fd;DatePeriod"/>
		<rdfs:label>call window</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an explicit period of time prior to a call date during which an issuer or agent must give notice to holders</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;DebtOffering">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-sec-iss;Offering"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;CallFeature"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-dbt-dbti;hasCallFeature"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fbc-fi-fi;DebtInstrument"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-dbt-dbti;hasInterestPaymentTerms"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fbc-dae-dbt;InterestPaymentTerms"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-dbt-dbti;hasRedemptionProvision"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fbc-dae-dbt;RedemptionProvision"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>debt offering</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an offering of one or more debt instruments for sale</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;IssuedDebt">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-dae-dbt;Debt"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fbc-fi-fi;DebtInstrument"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isConferredBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-dae-dbt;hasDebtAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>issued debt</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">debt that is set out by the borrower in some form of financial security in which the lender is the holder or counterparty of that security</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;MakeWholeFeature">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-dbti;CallFeature"/>
		<rdfs:label>make whole feature</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.investopedia.com/terms/m/make-wholecall.asp</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">The issuer typically has to make a lump sum payment to the investor derived from a formula based on the net present value (NPV) of future interest or coupon payments that will not be paid incrementally because of the call combined with the principal payment the investor would have received at maturity.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">make whole provision</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a call provision allowing the issuer to pay off remaining debt early</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;NonTradableDebtInstrument">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fi-fi;DebtInstrument"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;MutualContractualAgreement"/>
		<rdfs:label>non-tradable debt instrument</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrument"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;NonTradableDebtRedemptionProvision">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-dae-dbt;RedemptionProvision"/>
		<rdfs:label>non-tradable debt redemption terms</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">terms for repayment of principal on debt instruments that are not traded securities, such as over the counter, bilateral paper</skos:definition>
	</owl:Class>
	
	<fibo-sec-dbt-dbti:RelativePrice
		rdf:about="&fibo-sec-dbt-dbti;ParValue">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>par value</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.investopedia.com/terms/p/par.asp</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">face value</fibo-fnd-utl-av:synonym>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">nominal value</fibo-fnd-utl-av:synonym>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">par</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">the stated value of a negotiable instrument, stock, or bond, as compared with the value that instrument might receive when sold</skos:definition>
	</fibo-sec-dbt-dbti:RelativePrice>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PubliclyIssuedDebt">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-dbti;IssuedDebt"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrument"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isConferredBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>publicly issued debt</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an issued debt in the form of a tradable debt instrument (security)</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PutEvent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-oc;OccurrenceKind"/>
		<rdfs:label>put event</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an event associated with the put schedule for a debt instrument, i.e., an event involving the &apos;put&apos;, or surrender of the instrument by the holder</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PutFeature">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrumentRedemptionProvision"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;PutNotificationProvision"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;PutSchedule"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>put feature</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">FIBIM has term &quot;Putable Date&quot; which (by implication, and comparing with definition for &quot;Next Call Date&quot;) is presumably a single calendar date in the future, at a given point in time. That does not cover the definition of formal terms defining when and how the issue may be put, which is what is modeled here.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">put provision</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a redemption provision giving the holder the right, but not the obligation, to sell a specified amount of the debt instrument (i.e., redeem it), prior to maturity</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PutNotificationProvision">
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrumentRedemptionProvision"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;PutWindow"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-dt-fd;hasDatePeriod"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>put notification provision</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a provision of a put feature defining notification requirements</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PutPremium">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MonetaryPrice"/>
		<rdfs:label>put premium</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.investopedia.com/terms/p/put.asp</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an amount over par that a debt instrument holder must pay to sell the security early</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PutPrice">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MonetaryPrice"/>
		<rdfs:label>put price</rdfs:label>
		<fibo-ind-ei-ei:actualExpression
			rdf:datatype="&xsd;string">par value + put premium</fibo-ind-ei-ei:actualExpression>
		<skos:definition
			rdf:datatype="&xsd;string">a price at which a debt instrument with a put feature may be sold by the holder</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">needs review - should the expression be exercise value vs. par value?</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PutSchedule">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-fd;Schedule"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;PutEvent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-sec-dbt-dbti;PutPremium"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-sec-dbt-dbti;PutPrice"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>put schedule</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.investopedia.com/terms/p/put.asp</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a schedule that defines the events associated with the put feature of a debt instrument, i.e, the dates on which the debt instrument may be sold at what price by the holder</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;PutWindow">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-fd;DatePeriod"/>
		<rdfs:label>put window</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an explicit period of time prior to a put date during which holder or agent must give notice to an issuer</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;RelativePrice">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MonetaryPrice"/>
		<rdfs:label>relative market price</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.investopedia.com/terms/m/market-price.asp</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.investopedia.com/terms/p/par.asp</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a relative price with respect to either a stated or market value for a debt instrument at some point in time, defined as par, premium, or discount</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;TradableDebtInstrument">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fi-fi;DebtInstrument"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fi-fi;Security"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:maxQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrument"/>
				<owl:onProperty
					rdf:resource="&fibo-sec-dbt-dbti;mayBeSubordinatedTo"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fbc-dae-gty;hasGuarantor"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fbc-dae-gty;Guarantor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-iss;hasOffering"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>tradable debt instrument</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Details from Ecofin: A [debt] instrument can be traded, if its features depend only on one borrower. If the instrument has no bilateral or multilateral obligations, the investor can easily transfer it to another investor without asking the borrower (except the terms prohibit this explicitly). This is simplified with securitised instruments, where the debt is already split into handy denominations which trade easily (e.g. in round thousands or millions as with bonds, commercial paper, etc.). But in principle it works also with interbank loans and similar instruments. FIBIM Definition: Financial instruments evidencing moneys owed by the issuer to the holder on terms as specified.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a debt instrument that is also a security, i.e., that can be bought and sold by the holder</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-dbt-dbti;TradableDebtInstrumentRedemptionProvision">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-dae-dbt;RedemptionProvision"/>
		<rdfs:label>tradable debt instrument redemption provision</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Debt represented by a tradable debt security may be paid as a one off payment at maturity, by paying down principal amounts periodically, or by a call of the issue by the issuer. Debt that is defined by a convertible bond are redeemed by conversion into an equity security issued by the same company. More notes from the EDMC SME Review: Distinction between call and amortizing is whether you pay off everybody at the same time or via a lottery. SF can be either. either amortize the payment of a SF or pay it off by a lottery. The terms of the instrument determine that. Mandatory - all in the schedule. Mandatory to the bond issuer. Issuer has to pay off a certain amount at a certain time - this is in the Schedule. Callable - nothing mandatory.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">terms for the redemption of the debt represented by a debt security</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasCallFeature">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-sec-dbt-dbti;hasRepaymentTerms"/>
		<rdfs:label>has call terms</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&fibo-sec-dbt-dbti;CallFeature"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-sec-dbt-dbti;hasDefaultLotSize">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
		<rdfs:label>has default lot size</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">This is the minimum denomination required for transfer or change of ownership of a tradable debt security.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&xsd;integer"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the default number of units of the security that may be held at any one time</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasDurationToMaturity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasDuration"/>
		<rdfs:label>has duration to maturity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;ExplicitDuration"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the lifespan of a tradable debt instrument, from the issue date to the scheduled maturity date</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasInterestPaymentTerms">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-agr-ctr;hasTerms"/>
		<rdfs:label>has interest payment terms</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&fibo-fbc-dae-dbt;InterestPaymentTerms"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-sec-dbt-dbti;hasPoolNumber">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
		<rdfs:label>has pool number</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;PubliclyIssuedDebt"/>
		<rdfs:range
			rdf:resource="&xsd;integer"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the pool number of the debt</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasPriceAndYieldDayCountConvention">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-bd;hasBusinessRecurrenceIntervalConvention"/>
		<rdfs:label>has price and yield day count convention</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&fibo-fbc-dae-dbt;DayCountConvention"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the convention used to determine the number of days in a month and days in a year that are counted when performing calculations for yield and price figures</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasRedemptionProvision">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-agr-ctr;hasTerms"/>
		<rdfs:label>has redemption terms</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&fibo-fbc-dae-dbt;RedemptionProvision"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasRelativePriceAtIssue">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-acc-cur;hasPrice"/>
		<rdfs:label>has value at issue</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-sec-dbt-dbti;RelativePrice"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates a relative price with respect to the face value at which an instrument is issued, namely par, premium or discount</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasRelativePriceAtRedemption">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-acc-cur;hasPrice"/>
		<rdfs:label>has value at redemption</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-sec-dbt-dbti;RelativePrice"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates a relative price with respect to the face value at the time an instrument is redeemed, namely par, discount or premium</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;hasRepaymentTerms">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-agr-ctr;hasTerms"/>
		<rdfs:label>has repayment terms</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&fibo-fbc-dae-dbt;PrincipalRepaymentTerms"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-sec-dbt-dbti;isCallable">
		<rdfs:label>is callable</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&xsd;boolean"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates whether the issuer (writer) of the debt contract (the debtor) can call the debt prior to maturity</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-sec-dbt-dbti;isExchangeable">
		<rdfs:label>is exchangeable</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&xsd;boolean"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates whether the security can be exchanged for another security at the issuers discretion</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-sec-dbt-dbti;isPutable">
		<rdfs:label>is putable</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&xsd;boolean"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates whether the holder has the right to ask for redemption of the security prior to final maturity</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-sec-dbt-dbti;isStrippable">
		<rdfs:label>is strippable</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&xsd;boolean"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates whether or not the instrument can be stripped, so that interest and principal may be traded separately</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-sec-dbt-dbti;isSubordinated">
		<rdfs:label>is subordinated</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;DebtOffering"/>
		<rdfs:range
			rdf:resource="&xsd;boolean"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates whether the security is a subordinated security, meaning that the security has a lower priority than another security so that when the assets are liquidated this one is not first in line</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-dbti;mayBeSubordinatedTo">
		<rdfs:label>may be subordinated to</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrument"/>
		<rdfs:range
			rdf:resource="&fibo-sec-dbt-dbti;TradableDebtInstrument"/>
	</owl:ObjectProperty>

</rdf:RDF>