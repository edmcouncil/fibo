    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/.UnionClass.6">
        <skos:definition>Something that is widely accepted in payment for goods and services and in settling debts.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/isMeasuredIn">
        <rdfs:label>is measured in</rdfs:label>
        <skos:definition>The unit of measure in which the Measure is formally expressed.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/ParametricThing">
        <rdfs:label>parametric thing</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/amount">
        <rdfs:label>amount</rdfs:label>
        <skos:definition>The amount of whatever this is a quantity of.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/Unit">
        <rdfs:label>unit</rdfs:label>
        <skos:definition>A unit of measure ofa quantity, time or anything else which is measurable. NB don&apos;t confuse this with units in a Trust Fund!!</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/mayBeExpressedAs">
        <rdfs:label>may be expressed as</rdfs:label>
        <skos:definition>A measure in which the material amount may be expressed.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/reciprocal">
        <rdfs:label>reciprocal</rdfs:label>
        <skos:definition>A measure stated in terms of the recpirocal of another measure.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/MaterialAmount">
        <rdfs:label>material amount</rdfs:label>
        <skos:definition>A qualified amount of some continuous substance or some thing measured in units. Material Amount is always qualified by some unit in which the substance is measured.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/MathematicalThing">
        <rdfs:label>mathematical thing</rdfs:label>
        <skos:definition>A Mathematical Thing is something in the universe of mathematics. That is, something that is a parameter (mathematical quantity) or something that is used to express that parameter, such as a value or components of a formula.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/hasBaseUnit">
        <rdfs:label>has base unit</rdfs:label>
        <skos:definition>That in which the material amount is measured.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/Measure">
        <rdfs:label>measure</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/QuantitativeAmount">
        <rdfs:label>quantitative amount</rdfs:label>
        <skos:definition>A measured amount of anything (material or time). This is an actual quantity of something rather than the abstract concept of the amount that something is of. Hence it is concrete not abstract.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/isQuantityOf">
        <rdfs:label>is quantity of</rdfs:label>
        <skos:definition>That which the amount is an amount of.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/NumericAmount">
        <rdfs:label>numeric amount</rdfs:label>
        <skos:definition>An amount of some discrete thing Note: does not require units as the individual things are counted. Tech note: in other (data) standards this stands alongside (material) Amount rather than above it see e.g. CCTS, ISO 20022 v1.5</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/isAmountOf">
        <rdfs:label>is amount of</rdfs:label>
        <skos:definition>That which the Material Amount is an amount of.</skos:definition>
    </rdf:Description>
