<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-arrx-meth "https://spec.edmcouncil.org/fibo/FND/Arrangements/MethodsAndRules/">
	<!ENTITY fibo-fnd-inf-sch "https://spec.edmcouncil.org/fibo/FND/Information/Schemes/">
	<!ENTITY fibo-fnd-occ-occ "https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Arrangements/MethodsAndRules/"
	xmlns="https://spec.edmcouncil.org/fibo/FND/Arrangements/MethodsAndRules/"
	xmlns:fibo-fnd-arrx-meth="https://spec.edmcouncil.org/fibo/FND/Arrangements/MethodsAndRules/"
	xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"
	xmlns:fibo-fnd-occ-occ="https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/MethodsAndRules/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">MethodsAndRules</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/Lattice/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;AdjustmentMethod">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;Method"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">adjustment method</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The method by which something is adjusted, for example the terms of a contract, in reaction to events.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;DeliveryMethod">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;Method"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">delivery method</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The means or process used in delivering something.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;DistributionMethod">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;Method"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">distribution method</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The means by which monies or benefits accruing from the possession of something are distributed to the holders of that thing.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;Method">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;ContinuantThing"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;IndependentThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-meth;isWayOfDoing"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-occ;IntendedOccurrent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">method</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A way in which something is done.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;Preference">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">preference</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;PriceDeterminationMethod">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;VariableDeterminationMethod"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">price determination method</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The way in which the price of something is determined or is to be determined.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;Rule">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;Abstract"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;ContinuantThing"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">rule</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A Rule is any statement which determines how something must be. It is defined in relation to that something (which may be an event or a parameter), and with some reference to the way in which that Rule is defined, e.g. whether it forms part of some legally binding contract, is defined by physics and so on.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;RuleCode">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;SchematicCode"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-meth;standsFor"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-meth;Rule"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">rule code</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;RuleScheme">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;Scheme"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;defines.1"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-arrx-meth;RuleCode"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">rule scheme</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;SaleMethod">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;Method"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">sale method</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The means by which something is sold.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-meth;VariableDeterminationMethod">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;Method"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">variable determination method</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The way in which some variable amount, quantity or value is determined or is to be determined.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-meth;isWayOfDoing">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is way of doing</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-meth;Method"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-occ;IntendedOccurrent"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">What the Method is a means of carrying out.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-meth;standsFor">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">stands for</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-meth;RuleCode"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-meth;Rule"/>
	</owl:ObjectProperty>

</rdf:RDF>