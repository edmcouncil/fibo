    <owl:ObjectProperty rdf:about="&fibo-fnd-aap-agt;identifies">
        <rdfs:label>identifies</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">is the relationship between something and that which provides a unique reference for it</skos:definition>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-aap-agt;isIdentifiedBy">
        <rdfs:label>is identified by</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">provides a unique identifier for something</skos:definition>
        <owl:inverseOf rdf:resource="&fibo-fnd-aap-agt;identifies"/>
    </owl:ObjectProperty>


