    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-aap-agt;hasName">
        <rdfs:label>has name</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">that by which some thing is known; may apply to anything</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
    </owl:DatatypeProperty>


