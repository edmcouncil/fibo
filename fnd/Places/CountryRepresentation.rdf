    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/hasAlpha2Code -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;hasAlpha2Code">
        <rdfs:label>has alpha-2 code</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">relates a country to its ISO 3166-1 alpha-2 code</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cr;Alpha2Code"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasDenotation"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/hasAlpha3Code -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;hasAlpha3Code">
        <rdfs:label>has alpha-3 code</rdfs:label>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cr;Alpha3Code"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasDenotation"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/hasApproximateCoordinates -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;hasApproximateCoordinates">
        <rdfs:label>has approximate coordinates</rdfs:label>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.fao.org/countryprofiles/geoinfo/en/</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition rdf:datatype="&xsd;string">relates a country to its approximate coordinates per the UN FAO geopolitical ontology</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cr;ApproximateCoordinates"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/hasSubdivision -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;hasSubdivision">
        <rdfs:label>has subdivision</rdfs:label>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cr;CountrySubdivision"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/hasSubdivisionCode -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;hasSubdivisionCode">
        <rdfs:label>has subdivision code</rdfs:label>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;CountrySubdivision"/>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cr;SubdivisionCode"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasDenotation"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/hasSubdivisionKind -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;hasSubdivisionKind">
        <rdfs:label>has subdivision kind</rdfs:label>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;CountrySubdivision"/>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cr;SubdivisionKind"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/hasTerritory -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;hasTerritory">
        <rdfs:label>has territory</rdfs:label>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">The isIndependent flag indicates whether or not a &apos;country&apos; is in fact independent, but does not indicate whether or not it is self-governing.  The UN FAO ontology distinguishes between these two cases, which can be added as an extension if required in a subsequent version of this specification.</fibo-fnd-utl-av:explanatoryNote>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.fao.org/countryprofiles/geoinfo/en/</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition rdf:datatype="&xsd;string">relates a country to another uniquely represented entity in ISO 3166 that is a territory of that country</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cty;GeopoliticalEntity"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/isSubdivisionOf -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;isSubdivisionOf">
        <rdfs:label>is subdivision of</rdfs:label>
        <rdfs:range rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;CountrySubdivision"/>
        <owl:inverseOf rdf:resource="&fibo-fnd-plc-cr;hasSubdivision"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/usesAdministrativeLanguageDigraph -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;usesAdministrativeLanguageDigraph">
        <rdfs:label>uses administrative language digraph</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">relates a country code to the alpha-2 ISO 639 code element of each administrative language of the country</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-lng-lr;Alpha2Code"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;uses"/>
    </owl:ObjectProperty>
    


    <!-- http://spec.edmcouncil.org/fibo/FND/Places/CountryRepresentation/usesAdministrativeLanguageTrigraph -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-cr;usesAdministrativeLanguageTrigraph">
        <rdfs:label>uses administrative language trigraph</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">relates a country code to the alpha-3 (terminological version) ISO 639 code element of each administrative language of the country</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-lng-lr;Alpha3Code"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;uses"/>
    </owl:ObjectProperty>
    


