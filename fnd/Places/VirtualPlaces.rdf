    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/" >
    <!ENTITY fibo-fnd-utl-av "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/" >
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/" >
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/" >
    <!ENTITY fibo-fnd-aap-agt "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/" >
    <!ENTITY fibo-fnd-plc-loc "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Locations/" >
    <!ENTITY fibo-fnd-plc-adr "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Addresses/" >
    <!ENTITY fibo-fnd-plc-vrt "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/" >
]>

<rdf:RDF xml:base="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-utl-bt="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"
     xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"
     xmlns:fibo-fnd-aap-agt="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"
     xmlns:fibo-fnd-plc-loc="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Locations/"
     xmlns:fibo-fnd-plc-adr="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Addresses/"
     xmlns:fibo-fnd-plc-vrt="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/">


    <owl:Ontology rdf:about="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/">
        <rdfs:label>Virtual Places Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO FND Virtual Places Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2014 EDM Council, Inc.
Copyright (c) 2014 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the FIBO FND Virtual Places Ontology -->

        <sm:filename rdf:datatype="&xsd;string">VirtualPlaces.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-fnd-plc-vrt</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/20141101/Places/VirtualPlaces/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology provides scaffolding for use in describing virtual location-oriented concepts.</sm:fileAbstract>

        <skos:changeNote rdf:datatype="&xsd;string">This ontology was added to Foundations in advance of the December 2014 Long Beach meeting in support of the SEC specification.</skos:changeNote>

        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Locations/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Addresses/</sm:dependsOn>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>

        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AboutFND/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/AboutPlaces/</rdfs:seeAlso>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Locations/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Places/Addresses/"/>

    </owl:Ontology>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-vrt;hasURL">
        <rdfs:label>has url</rdfs:label>
        <skos:prefLabel rdf:datatype="&xsd;string">has URL</skos:prefLabel>
        <skos:altLabel rdf:datatype="&xsd;string">has uniform resource locator</skos:altLabel>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;URI"/>
    </owl:DatatypeProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-fnd-plc-vrt;NetworkLocation">
        <rdfs:label>network location</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-plc-vrt;VirtualLocation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
                <owl:onClass rdf:resource="&fibo-fnd-plc-adr;Address"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">A network address is location in a telecommunications network that may be identified by a network address (an identifier for a node or interface)</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-plc-vrt;NotionalPlace">
        <rdfs:label>notional place</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-plc-loc;Location"/>
        <skos:definition rdf:datatype="&xsd;string">A place described in terms of some abstract description or as a list of commonly understood concepts such as domestic, Eurozone etc.</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-plc-vrt;VirtualLocation">
        <rdfs:label>virtual location</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-plc-loc;Location"/>
        <skos:definition rdf:datatype="&xsd;string">A place which has no physical location.</skos:definition>
    </owl:Class>

</rdf:RDF>