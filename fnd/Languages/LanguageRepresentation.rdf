        <skos:definition rdf:datatype="&xsd;string">name, reference name, appellation in English; a linguistic expression used to designate an individual concept</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-lng-lr;HumanLanguage"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Section 3.4, ISO 639-3</fibo-fnd-utl-av:adaptedFrom>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-lng-lr;hasFrenchName">
        <rdfs:label>has French name</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">name, reference name, appellation in French; a linguistic expression used to designate an individual concept</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-lng-lr;HumanLanguage"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Section 3.4, ISO 639-3</fibo-fnd-utl-av:adaptedFrom>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-lng-lr;hasGermanName">
        <rdfs:label>has German name</rdfs:label>
