    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-lng-lr;hasAlpha3Code">
        <rdfs:label>has alpha-3 code</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasDenotation"/>
        <rdfs:range rdf:resource="&fibo-fnd-lng-lr;Alpha3Code"/>
        <rdfs:domain rdf:resource="&fibo-fnd-lng-lr;HumanLanguage"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-lng-lr;hasEarlierForm">
