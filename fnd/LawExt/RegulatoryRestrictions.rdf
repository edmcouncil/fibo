<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-bp-reg-req "https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/">
	<!ENTITY fibo-fnd-agr-ctr "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/">
	<!ENTITY fibo-fnd-law-cor "https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/">
	<!ENTITY fibo-fnd-law-jur "https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/">
	<!ENTITY fibo-fnd-lawx-rst "https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/">
	<!ENTITY fibo-fnd-occ-act "https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY owl1 "http://www.w3.org/2002/07/owl/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/"
	xmlns="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/"
	xmlns:fibo-bp-reg-req="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/"
	xmlns:fibo-fnd-agr-ctr="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"
	xmlns:fibo-fnd-law-cor="https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/"
	xmlns:fibo-fnd-law-jur="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"
	xmlns:fibo-fnd-lawx-rst="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/"
	xmlns:fibo-fnd-occ-act="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:owl1="http://www.w3.org/2002/07/owl/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/LawExt/RegulatoryRestrictions/">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">RegulatoryRestrictions</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2002/07/owl"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2002/07/owl#"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BP/Regulatory/RegulatoryRequirements/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/UO/Lattice/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="&fibo-fnd-agr-ctr;Contract">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-lawx-rst;Contract"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="&fibo-fnd-law-jur;StatuteLaw">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-lawx-rst;StatuteLaw"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="&fibo-fnd-law-cor;Law">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-lawx-rst;Law"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;Contract">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contract</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;ContractualRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-lawx-rst;Restriction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-lawx-rst;contractuallyImposedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contractual restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A restriction defined in a Contract as opposed to being defined in a body of Law.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;Instrumentality">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">instrumentality</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-fnd-lawx-rst;Contract"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-lawx-rst;Law"/>
								<rdf:rest
									rdf:resource="&rdf;nil"/>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">some law or contract which is instrumental in the existence of some recognized construct such as a restriction</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;Law">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">law</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;LegalRestriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-lawx-rst;Restriction"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-lawx-rst;legallyImposedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-lawx-rst;StatuteLaw"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="https://spec.edmcouncil.org/fibo/FND/LawExt/Restrictions/restrictionAppliesIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">legal restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A Restriction defined in a body of Law.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;RegulatoryRequirement">
		<rdfs:subClassOf
			rdf:resource="&fibo-bp-reg-req;LegalRequirement"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">regulatory requirement</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">A requirement of some sort by which something is to be regulated.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">There is not presently a common &quot;Financial&quot; sub-type of this, since regulations like investment, consumer protection, systemic risk and so on are quite different and quite far reaching. However, readers of this model should keep in mind that this term does not apply only to financial types of regulation. For instance, insurance regulation would also be a sub-type of this item.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;Restriction">
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;ContinuantThing"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-uo-lat;RelativeThing"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-lawx-rst;restricts"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-lawx-rst;restrictsActivity"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-occ-act;Activity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">restriction</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">Anything which prevents something being done which might otherwise be done. The Restriction is defined relative to the thing which might otherwise be done, which may be an activity of some sort. It is therefore a Relative Thing, with a property identifying that act or event.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-lawx-rst;StatuteLaw">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">statute law</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;contractuallyImposedBy">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-lawx-rst;imposedBy"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contractually imposed by</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;ContractualRestriction"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;contractuallyImposes">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-lawx-rst;imposes"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">contractually imposes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;Contract"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-lawx-rst;ContractualRestriction"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;imposedBy">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">imposed by</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;Restriction"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-lawx-rst;Instrumentality"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;imposes">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">imposes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;Instrumentality"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-lawx-rst;Restriction"/>
		<owl:inverseOf
			rdf:resource="&fibo-fnd-lawx-rst;imposedBy"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">some instrumentality such as a contract or law imposes a restriction on some entity in regard to some subject matter</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;legallyImposedBy">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-lawx-rst;imposedBy"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">legally imposed by</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;LegalRestriction"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-lawx-rst;StatuteLaw"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;legallyImposes">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-lawx-rst;imposes"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">legally imposes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;StatuteLaw"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-lawx-rst;LegalRestriction"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;restricts">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">restricts</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;Restriction"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-lawx-rst;restrictsActivity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-lawx-rst;restricts"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">restricts activity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-lawx-rst;Restriction"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-occ-act;Activity"/>
	</owl:ObjectProperty>

</rdf:RDF>