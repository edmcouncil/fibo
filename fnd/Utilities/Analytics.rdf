    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY sm "http://spec.edmcouncil.org/techprocess/ab/SpecificationMetadata/" >
    <!ENTITY fibo-fnd-utl-av "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/" >
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/" >
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/" >
    <!ENTITY fibo-fnd-dt-fd "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/DatesAndTimes/FinancialDates/" >
    <!ENTITY fibo-fnd-utl-alx "http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/Analytics/" >
]>

<rdf:RDF xml:base="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/Analytics/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://spec.edmcouncil.org/techprocess/ab/SpecificationMetadata/"
     xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-utl-bt="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"
     xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"
     xmlns:fibo-fnd-dt-fd="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/DatesAndTimes/FinancialDates/"
     xmlns:fibo-fnd-utl-alx="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/Analytics/">


    <owl:Ontology rdf:about="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/Analytics/">
        <rdfs:label>Analytics Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO FND Analytics Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the FIBO FND Analytics Ontology -->

        <sm:filename rdf:datatype="&xsd;string">Analytics.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-fnd-utl-alx</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/20141101/Utilities/Analytics/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology provides mathematical abstractions for use in other ontologies, including for example the basic components of formulae, parameters and values.</sm:fileAbstract>

        <skos:changeNote rdf:datatype="&xsd;string">The http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/20140501/Utilities/Analytics.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/.</skos:changeNote>
        <skos:changeNote rdf:datatype="&xsd;string">This ontology was added to Foundations in advance of the June 2014 Boston meeting in support of the IND RFC.</skos:changeNote>

        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/DatesAndTimes/FinancialDates/</sm:dependsOn>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/ODM/</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>

        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/AboutFND/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AboutUtilities/</rdfs:seeAlso>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/DatesAndTimes/FinancialDates/"/>

    </owl:Ontology>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-utl-alx;forYear">
        <rdfs:label>for calendar year</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the calendar year for which the statistical measure is applicable</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-dt-fd;ExplicitDatePeriod"/>
        <rdfs:domain rdf:resource="&fibo-fnd-utl-alx;StatisticalMeasure"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-utl-alx;hasApplicableDatePeriod">
        <rdfs:label>has applicable date period</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the date period for which the statistical measure is applicable</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-dt-fd;DatePeriod"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDatePeriod"/>
        <rdfs:domain rdf:resource="&fibo-fnd-utl-alx;StatisticalMeasure"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-utl-alx;hasArgument">
        <rdfs:label>has argument</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">indicates a specific input to a function, formula or expression, also known as an independent variable</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:range rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-utl-alx;hasExpression">
        <rdfs:label>has expression</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">indicates a mathematical or other formal expression, which may be part of a formula</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-alx;Expression"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-utl-alx;hasFormula">
        <rdfs:label>has formula</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">indicates a concise way of expressing information symbolically, as in a mathematical or chemical formula</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-alx;Formula"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-utl-alx;hasOperand">
        <rdfs:label>has operand</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">indicates the operand of a mathematical argument, parameter or other similar concept</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:range rdf:resource="&fibo-fnd-rel-rel;Referent"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-utl-alx;isMeasureOf">
        <rdfs:label>is a measure of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the concept being measured</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
        <rdfs:domain rdf:resource="&fibo-fnd-utl-alx;Measure"/>
    </owl:ObjectProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-utl-alx;hasApplicablePeriodEnd">
        <rdfs:label>has applicable period end</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the end of the period for which the statistical measure is applicable</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDateTimeValue"/>
        <rdfs:domain rdf:resource="&fibo-fnd-utl-alx;StatisticalMeasure"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-utl-alx;hasApplicablePeriodStart">
        <rdfs:label>has applicable period start</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the start of the period for which the statistical measure is applicable</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDateTimeValue"/>
        <rdfs:domain rdf:resource="&fibo-fnd-utl-alx;StatisticalMeasure"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-utl-alx;hasMeasurementDate">
        <rdfs:label>has measurement date</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the date on which the measure was taken</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-dt-fd;dateValue"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDateValue"/>
        <rdfs:domain rdf:resource="&fibo-fnd-utl-alx;Measure"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-utl-alx;hasMeasurementPeriodInMonths">
        <rdfs:label>has measurement period in months</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a predicate indicating the coverage period for which the measure is applicable expressed in months</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-utl-alx;Measure"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
    </owl:DatatypeProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-fnd-utl-alx;Constant">
        <rdfs:label>constant</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <skos:definition rdf:datatype="&xsd;string">a symbol that represents a constant in a formula or expression</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-utl-alx;Expression">
        <rdfs:label>expression</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
                <owl:onClass rdf:resource="&fibo-fnd-utl-alx;Constant"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-utl-alx;hasArgument"/>
                <owl:onClass rdf:resource="&fibo-fnd-utl-alx;Variable"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a finite combination of symbols that are well-formed according to applicable rules</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-utl-alx;Formula">
        <rdfs:label>formula</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-utl-alx;hasExpression"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-utl-alx;Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a general fact or rule expressed in letters and symbols; may consist of one or more expressions</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.merriam-webster.com/dictionary/formula</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-utl-alx;Measure">
        <rdfs:label>measure</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <skos:definition rdf:datatype="&xsd;string">an amount or degree of something; the dimensions, capacity, or amount of something ascertained by measuring</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.merriam-webster.com/dictionary/measure</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-utl-alx;StatisticalMeasure">
        <rdfs:label>statistical measure</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-utl-alx;Measure"/>
        <skos:definition rdf:datatype="&xsd;string">A summary (means, mode, total, index, etc.) of the individual quantitative variable values for the statistical units in a specific group (study domains).</skos:definition>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://stats.oecd.org/glossary/detail.asp?ID=5068</fibo-fnd-utl-av:definitionOrigin>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-utl-alx;Variable">
        <rdfs:label>variable</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <skos:definition rdf:datatype="&xsd;string">a symbol that represents a parameter in a formula or expression</skos:definition>
    </owl:Class>

</rdf:RDF>