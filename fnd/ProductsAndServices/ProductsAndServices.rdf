		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Places/Facilities/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Places/Locations/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Relations/Relations/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/</sm:dependsOn>
		<sm:fileAbbreviation>fibo-fnd-pas-pas</sm:fileAbbreviation>
		<sm:fileAbstract>This ontology defines fundamental concepts for buyers, sellers, clients, customers, products, goods and services for use in other FIBO ontologies.</sm:fileAbstract>
		<sm:filename>ProductsAndServices.rdf</sm:filename>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"/>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/FND/AboutFND/"/>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/AboutProductsAndServices/"/>
