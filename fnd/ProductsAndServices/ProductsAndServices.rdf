	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Buyer">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Product"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-pas;buys"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>buyer</rdfs:label>
		<skos:altLabel
			rdf:datatype="&xsd;string">purchaser</skos:altLabel>
		<skos:definition
			rdf:datatype="&xsd;string">a party that acquires, or agrees to acquire, ownership (in case of goods), or benefit or usage (in case of services), in exchange for money or other consideration under a contract of sale</skos:definition>
		<skos:prefLabel
			rdf:datatype="&xsd;string">buyer</skos:prefLabel>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/buyer.html</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Client">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;ClientIdentifier"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:minQualifiedCardinality
							rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
						<owl:onClass
							rdf:resource="&fibo-fnd-pas-pas;ServiceAgreement"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-pty-pty;isAPartyTo"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>client</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a party that purchases professional services from, or has a formal relationship to purchase services from another party</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;ClientIdentifier">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-id;Identifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Client"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;identifies"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>client identifier</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an identifier for a client</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Commodity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pas-pas;Good"/>
		<rdfs:label>commodity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a basic good used in commerce that is interchangeable with other commodities of the same type</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.investopedia.com/terms/c/commodity.asp</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Commodities are most often used as inputs in the production of other goods or services. The quality of a given commodity may differ slightly, but it is essentially uniform across producers.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Customer">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pas-pas;Buyer"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-pas-pas;CustomerIdentifier"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Supplier"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-pas;buysFrom"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>customer</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a buyer that receives or consumes products (goods or services) and has the ability to choose between different products and suppliers</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/customer.html</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;CustomerIdentifier">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-id;Identifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Customer"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;identifies"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>customer identifier</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an identifier for a customer</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Good">
		<rdfs:label>good</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-cur;AmountOfMoney"/>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
		<skos:definition
			rdf:datatype="&xsd;string">any tangible thing that is not money or real estate</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/good.html</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">An inherently useful and relatively scarce tangible item produced from agricultural, construction, manufacturing, or mining activities. According to the UN Convention On Contract For The International Sale Of Goods, the term &apos;good&apos; does not include (1) items bought for personal use, (2) items bought at an auction or foreclosure sale, (3) aircraft or oceangoing vessels.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Producer">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Product"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-pas;produces"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>producer</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">the manufacturer of a product, also called maker</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.investorwords.com/3872/producer.html</fibo-fnd-utl-av:definitionOrigin>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Product">
		<rdfs:subClassOf>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-fnd-pas-pas;Good"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-pas-pas;Service"/>
								<rdf:rest
									rdf:resource="&rdf;nil"/>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-pas-pas;ProductIdentifier"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Producer"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-pas;isProducedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>product</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">A commercially distributed good that is (1) tangible property, (2) the output or result of a fabrication, manufacturing, or production process, or (3) something that passes through a distribution channel before being consumed or used.</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/product.html</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;ProductIdentifier">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-id;Identifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Product"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;identifies"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>product identifier</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an identifier for a product</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Seller">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-pas;Product"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-pas;sells"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>seller</rdfs:label>
		<skos:altLabel
			rdf:datatype="&xsd;string">vendor</skos:altLabel>
		<skos:definition
			rdf:datatype="&xsd;string">a party that makes, offers or contracts to make a sale to an actual or potential buyer</skos:definition>
		<skos:prefLabel
			rdf:datatype="&xsd;string">seller</skos:prefLabel>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/seller.html</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-pas;Service">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-pas-pas;ServiceProvider"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-pas;isProvisionedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-pas-pas;ServiceProvider"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isProvidedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-plc-fac;Capability"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;provides"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>service</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a type of economic activity that is intangible, is not stored and does not result in ownership; a service is consumed at the point of sale</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">Services include intangible products, such as accounting, banking, cleaning, consultancy, education, insurance, expertise, medical treatment, or transportation services.</skos:example>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/services.html</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.investorwords.com/6664/service.html</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Sometimes services are difficult to identify because they are closely associated with a good; such as the combination of a diagnosis with the administration of a medicine. No transfer of possession or ownership takes place when services are sold, and they (1) cannot be stored or transported, (2) are instantly perishable, and (3) come into existence at the time they are bought and consumed.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
