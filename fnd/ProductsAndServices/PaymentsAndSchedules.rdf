    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-pas-psch;fulfillsObligation">
        <rdfs:label>fulfills obligation</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">satisfies a requirement for payment of some claim, debt, or other obligation</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-pas-psch;Payment"/>
        <rdfs:range rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-pas-psch;hasObligation">
        <rdfs:label>has obligation</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies a duty or obligation that a given party has taken on</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-pas-psch;Payer"/>
        <rdfs:range rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
        <owl:inverseOf rdf:resource="&fibo-fnd-pas-psch;isObligationOf"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-pas-psch;hasPaymentAmount">
        <rdfs:label>has payment amount</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">specifies the amount of money involved in a payment</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-pas-psch;Payment"/>
        <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fnd-pas-psch;isObligationOf">
        <rdfs:label>is obligation of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies a party that has incurred a given obligation</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
        <rdfs:range rdf:resource="&fibo-fnd-pas-psch;Payer"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isConferredOn"/>
    </owl:ObjectProperty>

    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-fnd-pas-psch;Payee">
        <rdfs:label>payee</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-pty-pty;isAPartyTo"/>
                        <owl:onClass rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a party to whom a debt should be paid, or to whose order a bill of exchange, note, or check is made payable</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-pas-psch;Payer">
        <rdfs:label>payer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pas-psch;hasObligation"/>
                <owl:allValuesFrom rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a party who pays a bill or fees</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-pas-psch;Payment">
        <rdfs:label>payment</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-dt-oc;Occurrence"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pas-psch;fulfillsObligation"/>
                <owl:onClass rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-pas-psch;Payee"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-pas-psch;Payer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pas-psch;hasPaymentAmount"/>
                <owl:allValuesFrom rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">delivery of money in fulfillment of an obligation, such as to satisfy a claim or debt</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-pas-psch;PaymentEvent">
        <rdfs:label>payment event</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;TransactionEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
                <owl:onClass rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;involves"/>
                <owl:onClass rdf:resource="&fibo-fnd-pas-psch;Payment"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">an event that involves delivery of money in fulfillment of an obligation</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-pas-psch;PaymentObligation">
        <rdfs:label>payment obligation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-agr;Commitment"/>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-law-lcap;Duty"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isConferredBy"/>
                <owl:allValuesFrom rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pas-psch;isObligationOf"/>
                <owl:allValuesFrom rdf:resource="&fibo-fnd-pas-psch;Payer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a legally enforceable duty to pay a sum of money, or agree to do something (or not to do something), according to the terms stated in a contract</skos:definition>
        <skos:example rdf:datatype="&xsd;string">the duty of a borrower to repay a loan, and the legal right of a lender to enforce payment</skos:example>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-pas-psch;PaymentSchedule">
        <rdfs:label>payment schedule</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-dt-fd;Schedule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
                <owl:onClass rdf:resource="&fibo-fnd-pas-psch;PaymentEvent"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">schedule for delivery of money in fulfillment of an obligation, such as a coupon payment schedule, loan payment schedule, interest payment schedule</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
</rdf:RDF>