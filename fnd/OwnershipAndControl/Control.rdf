    <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Control/">
        <rdfs:label>Control Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO FND Control Ontology -->

