     -->

    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/Control -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;Control">
        <rdfs:label>control</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-oac-ctl;hasPartyInControl"/>
                <owl:onClass rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-oac-ctl;isInControlOfThing"/>
                <owl:onClass rdf:resource="&fibo-fnd-pty-rl;ThingInRole"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">The possession by a party, direct or indirect, of the power to direct or cause the direction of the management and policies of a thing, whether through the ownership of voting shares, by contract, or otherwise.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/ControlledThing -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;ControlledThing">
        <rdfs:label>controlled thing</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-rl;ThingInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isControlledBy"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">Thing over which some party exercises some form of control in some context.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/ControllingCapacity -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;ControllingCapacity">
        <rdfs:label>controlling capacity</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;involves"/>
                <owl:onClass rdf:resource="&fibo-fnd-oac-ctl;ControlledThing"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;involves"/>
                <owl:onClass rdf:resource="&fibo-fnd-oac-ctl;ControllingParty"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">the possession, direct or indirect, of the power to direct or cause the direction of the thing which is controlled</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/ControllingParty -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;ControllingParty">
        <rdfs:label rdf:datatype="&xsd;string">controlling  party</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;controls"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:editorialNote rdf:datatype="&xsd;string">At this level of abstraction it is not defined whether the control is some degree of controlling interest, or some level of actual control (asserted or calculated) in some entity.</skos:editorialNote>
        <skos:definition rdf:datatype="&xsd;string">Party which exercises some form of control in some context.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/DeFactoControl -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;DeFactoControl">
        <rdfs:label>de facto control</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-ctl;ControllingCapacity"/>
        <owl:disjointWith rdf:resource="&fibo-fnd-oac-ctl;DeJureControl"/>
        <fibo-fnd-utl-av:usageNote rdf:datatype="&xsd;string">Use this element for relationships where there is asserted to be control but the type of control isI either unknown or immaterial. This includes where the existence of real control of some thing or some company is inferred by running inferencing on some set of de jure controlling interests, and also where control is baldly asserted and there is nothing known about how or why such control comes about.</fibo-fnd-utl-av:usageNote>
        <skos:definition rdf:datatype="&xsd;string">control that is understood, due to condition or situation treated as standard or official, even if not explicitly stated (or actually standardized)</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.investorwords.com/1349/de_facto.html</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/DeJureControl -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;DeJureControl">
        <rdfs:label>de jure control</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-ctl;ControllingCapacity"/>
        <fibo-fnd-utl-av:usageNote rdf:datatype="&xsd;string">Use this class of thing for all types of controlling interest, such as share ownership, holding of general partner equity, or being the beneficiary of any side contract in which control is ceded, as well as types of control more generally in which there is some instrument or means of control. This class defines control as a kind of legal capacity, conferred on something by something via some formal means i.e. some instrument of control. When asserting that there is some de jure controlling interest (for example in some company by some other company) we do not at the same time assert the conclusions that would be drawn as a result of observing those controlling interests â€“ that is covered under de facto control.</fibo-fnd-utl-av:usageNote>
        <skos:definition rdf:datatype="&xsd;string">control that is formalized in law, or codified in some legal instrument</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.businessdictionary.com/definition/de-jure.html</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


