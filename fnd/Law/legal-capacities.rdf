    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/LiabilityCapacity">
        <rdfs:label>liability capacity</rdfs:label>
        <skos:definition>The ability to be sued at law.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/StatutoryResponsibility">
        <rdfs:label>statutory responsibility</rdfs:label>
        <skos:definition>An obligation which is defined under some body of law (statute).</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/.UnionClass.5">
        <skos:definition>A logical union of Law, Contract and Constitution.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/LegalCapacity">
        <rdfs:label>legal capacity</rdfs:label>
        <skos:definition>The capacity to carry out certain actions or to have certain rights.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/SignatoryCapacity">
        <rdfs:label>signatory capacity</rdfs:label>
        <skos:definition>The capacity of some natural person to sign agreements on the part of some entity.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/isCapacityOf">
        <rdfs:label>is capacity of</rdfs:label>
        <skos:definition>That upon which the legal capacity has been conferred.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/DelegatedLegalAuthority">
        <rdfs:label>delegated legal authority</rdfs:label>
        <skos:definition>Some capacity vested in some party to give them legal control of some entity, that is, a capacity to undertake legally binding commitments on the part of that entity.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/isConferredBy">
        <rdfs:label>is conferred by</rdfs:label>
        <skos:definition>That which confers the construct or brings it into being.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/statutoryResponsibilityIsMandatedBy">
        <rdfs:label>statutory responsibility is mandated by</rdfs:label>
        <skos:definition>The obligation is mandated under some body of statute law.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/LegalConstruct">
        <rdfs:label>legal construct</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/Duty">
        <rdfs:label>duty</rdfs:label>
        <skos:definition>Some obligation which exists and is imposed on some individual.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/legalConstructIsConferredOn">
        <rdfs:label>legal construct is conferred on</rdfs:label>
        <skos:definition>That to upon which the conferred thing is conferred.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/hasCapacity">
        <rdfs:label>has capacity</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/ContractualCapability">
        <rdfs:label>contractual capability</rdfs:label>
        <skos:definition>The capacity to enter into legally binding contracts.</skos:definition>
    </rdf:Description>
