    <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/">
        <rdfs:label>Legal Capacity Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO FND Legal Capacity Ontology -->

