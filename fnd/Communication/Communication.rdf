<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY fibo-fnd-aap-agt "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/">
	<!ENTITY fibo-fnd-arr-doc "https://spec.edmcouncil.org/fibo/FND/Arrangements/Documents/">
	<!ENTITY fibo-fnd-com-com "https://spec.edmcouncil.org/fibo/FND/Communication/Communication/">
	<!ENTITY fibo-fnd-dt-fd "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/">
	<!ENTITY fibo-fnd-dt-oc "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/">
	<!ENTITY fibo-fnd-pas-pas "https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/">
	<!ENTITY fibo-fnd-pty-rl "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-ind-ei-ei "https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.edmcouncil.org/fibo/FND/Communication/Communication/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-arr-doc="https://spec.edmcouncil.org/fibo/FND/Arrangements/Documents/"
	xmlns:fibo-fnd-com-com="https://spec.edmcouncil.org/fibo/FND/Communication/Communication/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-dt-oc="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/"
	xmlns:fibo-fnd-pas-pas="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/"
	xmlns:fibo-fnd-pty-rl="https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-ind-ei-ei="https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.edmcouncil.org/fibo/FND/Communication/Communication/">
		<rdfs:label>Communication Ontology</rdfs:label>
		<dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
		<sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
		<sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright>Copyright (c) 2014-2017 EDM Council, Inc.
Copyright (c) 2014-2017 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/</sm:dependsOn>
		<sm:fileAbbreviation>fibo-fnd-arr-arr</sm:fileAbbreviation>
		<sm:fileAbstract>This ontology defines concepts around communication, including requests, responses, submissions etc.</sm:fileAbstract>
		<sm:filename>Communication.rdf</sm:filename>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"/>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/FND/AboutFND/"/>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/AboutArrangements/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Arrangements/Documents/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"/>
		<owl:versionIRI rdf:resource="https://spec.edmcouncil.org/fibo/FND/Communication/Communication/"/>
		<fibo-fnd-utl-av:hasMaturityLevel rdf:resource="&fibo-fnd-utl-av;Provisional"/>
	</owl:Ontology>
	
	<owl:Class rdf:about="&fibo-fnd-com-com;Application">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-com-com;Request"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-dt-fd;hasDateReceived"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-dt-fd;Date"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>application</rdfs:label>
		<skos:definition>a request submitted by an applicant to receive something (e.g. a licence, a loan, a membership) accompanied by documentation of pertinent information to determine whether the request should be granted</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fnd-com-com;Request">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-dt-oc;Occurrence"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-com-com;isRequestFor"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-com-com;isSentFrom"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-com-com;isSentTo"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>request</rdfs:label>
		<skos:definition>an occurrence whereby an agent asks another agent for something or to do something</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-com-com;isRequestFor">
		<rdfs:label>is request for</rdfs:label>
		<skos:definition>relates a request to that which is being requested, e.g. a report</skos:definition>
		<skos:editorialNote>Future consideration: move this property to somewhere in FND.</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-com-com;isResponseTo">
		<rdfs:label>is response to</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fnd-dt-oc;Occurrence"/>
		<rdfs:range rdf:resource="&fibo-fnd-dt-oc;Occurrence"/>
		<skos:definition>relates an occurrence to another occurrence that it is a response to, most often in the context of communication</skos:definition>
		<skos:editorialNote>Future consideration: move this property to somewhere in FND.</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-com-com;isSentFrom">
		<rdfs:label>is from</rdfs:label>
		<skos:definition>relates e.g. a communication to its source or origin (e.g. the agent originating a request)</skos:definition>
		<skos:editorialNote>Future consideration: move this property to the Relations ontology (fibo-fnd-rel-rel).</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-com-com;isSentTo">
		<rdfs:label>is to</rdfs:label>
		<skos:definition>relates e.g. a communication to a target or destination (e.g. the agent receiving a request)</skos:definition>
		<skos:editorialNote>Future consideration: move this property to the Relations ontology (fibo-fnd-rel-rel).</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-com-com;isSubmittedBy">
		<rdfs:label>is submitted by</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<skos:definition>relates something to that agent that provided or submitted it (e.g. the person or organization that submitted a report)</skos:definition>
		<skos:editorialNote>Future consideration: move this property to the Relations ontology (fibo-fnd-rel-rel).</skos:editorialNote>
		<skos:editorialNote>Ideally, this would be a subproperty of isProvidedBy, but that is reserved for use with ThingInRole.  This property is therefore a subproperty of an unnamed property chain from the thing submitted to the submitting institution that goes through an instance of a class that means PartyInProviderRole.</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-com-com;isSubmittedTo">
		<rdfs:label>is submitted to</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<skos:definition>relates something to which it is submitted (e.g. the organization receiving a report)</skos:definition>
		<skos:editorialNote>Future consideration: move this property to the Relations ontology (fibo-fnd-rel-rel).</skos:editorialNote>
	</owl:ObjectProperty>

</rdf:RDF>