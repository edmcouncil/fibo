        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Arrangements/Arrangements/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/spec/EDMC-FIBO/FND/Relations/Relations/"/>
