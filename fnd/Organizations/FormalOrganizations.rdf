	<owl:Ontology
		rdf:about="http://spec.edmcouncil.org/fibo/FND/Organizations/FormalOrganizations/">
		<rdfs:label>Formal Organizations Ontology</rdfs:label>
		<dct:license
			rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
