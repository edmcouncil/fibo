    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-fnd-rel-rel;Reference">
        <rdfs:label>reference</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a concept that refers to (or stands in for) another concept</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://grammar.about.com/od/rs/g/referenceterm.htm</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fnd-rel-rel;Referent">
        <rdfs:label>referent</rdfs:label>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://grammar.about.com/od/rs/g/referentterm.htm</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition rdf:datatype="&xsd;string">the concept that another concept stands for or refers to</skos:definition>
    </owl:Class>

</rdf:RDF>
