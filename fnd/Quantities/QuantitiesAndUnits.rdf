			rdf:resource="http://spec.edmcouncil.org/fibo/FND/20160201/Quantities/QuantitiesAndUnits/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;BaseQuantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;QuantityKind"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;SystemOfQuantities"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasDefinition"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>base quantity</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<skos:definition
			rdf:datatype="&xsd;string">a quantity kind in a conventionally chosen subset of a given system of quantities, where no subset quantity can be expressed in terms of the others</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">The International System of Quantities (ISQ) comprises these base quantities (with their SI base measurement units): length (meter), mass (kilogram), duration (second), electric current (ampere), thermodynamic temperature (kelvin), amount of substance (mole), and luminous intensity (candela).  These base quantities are not mutually comparable.  All quantities of any one of these kinds are, however, mutually comparable.</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;BaseUnit">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;SystemOfUnits"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasDefinition"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>base unit</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Quantity units that are not base units are derived units.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a measurement unit that is defined by a system of units to be the reference measurement unit for a base quantity</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;DerivedQuantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;QuantityKind"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;BaseQuantity"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-qt-qtu;isDerivedFrom"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>derived quantity</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<skos:definition
			rdf:datatype="&xsd;string">a quantity kind, in a system of quantities, that is not a base quantity of the system but may be defined in terms of base quantities of the system</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">velocity (length/time), mass density (mass/length3)</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;DerivedUnit">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;BaseUnit"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-qt-qtu;isDerivedFrom"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>derived unit</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Every derived unit is defined in terms of base units.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a measurement unit for a derived quantity</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">1 minute = 60 seconds</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;MeasurementUnit">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;Quantity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;QuantityKind"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;SystemOfUnits"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasDefinition"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>measurement unit</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<skos:definition
			rdf:datatype="&xsd;string">a quantity, defined and adopted by convention, with which any other quantity of the same kind can be compared to express the ratio of the two quantities as a number</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">week, day, hour, minute, second, kilogram, joule, meter</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;ParticularQuantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Measure"/>
		<rdfs:label>particular quantity</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">A particular quantity is given by a definite description, which identifies the individual thing and the property. Particular quantities are properties of particular things and are generally expressed by a term for the property and a quantity value.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a property that is of an individual thing and is quantifiable as an instance of some quantity kind</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">The weight of a given person, the mass of the Earth, the speed of light, and the distance between and are said to be “particular quantities.”</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;Quantity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Measure"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-qt-qtu;hasQuantityKind"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-qt-qtu;QuantityKind"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>quantity</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">A quantity as defined here is said to be a “scalar” as distinct from a “vector.” However, a vector or a tensor whose components are quantities is also considered to be a quantity.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a property of a phenomenon, body, or substance, to which a number can be assigned with respect to a reference</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">The term quantity is used here to refer to the abstraction of the properties - the amount of measurable stuff that can be compared between particular quantities. The height of the something refers to a particular quantity; 555 ft 5 inches refers to a quantity value.</skos:editorialNote>
		<skos:example
			rdf:datatype="&xsd;string">second, kilogram, joule, meter. These are quantities in a general sense, which is what is meant here by quantity.</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;QuantityKind">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-cls;Classifier"/>
		<rdfs:label>quantity kind</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Every instance of ‘quantity kind’ is also a specialization of ‘quantity’. So the concept ‘duration’ is an instance of ‘quantity kind’ and it is a specialization of ‘quantity’, i.e., it is a classifier of actual quantities. But a given duration (i.e., the duration of something) is an instance of ‘duration’ and thus a ‘particular quantity,’ not an instance of ‘quantity kind’. For example, a ‘year’ is not an instance of quantity kind; it is an instance of quantity, but not a category of quantity.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a categorization type for ‘quantity’ that characterizes quantities as being mutually comparable</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">duration, mass, energy, length</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;QuantityValue">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Measure"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-qt-qtu;hasMeasurementUnit"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onDataRange
					rdf:resource="&xsd;decimal"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>quantity value</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">The quantity expressed by a quantity value is the quantity whose ratio to the measurement unit is the number.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">number and measurement unit together giving magnitude of a quantity</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">2 days, 3.5 hours, 150 lb, 45.5 miles</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;Rate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;Quantity"/>
		<rdfs:label>rate</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.icoachmath.com/math_dictionary/rate.html</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.thefreedictionary.com/rate</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a quantity measured with respect to some other quantity</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;SystemOfQuantities">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-cls;ClassificationScheme"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-qt-qtu;BaseQuantity"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;defines"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-qt-qtu;DerivedQuantity"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;involves"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>system of quantities</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<skos:definition
			rdf:datatype="&xsd;string">set of quantities together with a set of non-contradictory equations relating those quantities</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-qt-qtu;SystemOfUnits">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-cls;ClassificationScheme"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;BaseUnit"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;defines"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;DerivedUnit"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;defines"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;defines"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>system of units</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<owl:equivalentClass>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-qt-qtu;SystemOfQuantities"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</owl:equivalentClass>
		<skos:definition
			rdf:datatype="&xsd;string">a set of measurement units associated with a system of quantities, together with a set of rules that assign one measurement unit to be the base unit for each base quantity in the system of quantities and a set of rules for the derivation of other units from the base units</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">The International System of Units (SI) is a system of units.</skos:example>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-qt-qtu;hasMeasurementUnit">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has measurement unit</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/DTV/1.1/</fibo-fnd-utl-av:definitionOrigin>
		<rdfs:domain
			rdf:resource="&fibo-fnd-qt-qtu;QuantityValue"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
		<skos:definition
