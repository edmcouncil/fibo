<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-acc-tax "https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Taxation/">
	<!ENTITY fibo-fnd-arrx-meth "https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/MethodsAndRules/">
	<!ENTITY fibo-fnd-inf-sch "https://spec.edmcouncil.org/fibo/ontology/FND/Information/Schemes/">
	<!ENTITY fibo-fnd-law-jur "https://spec.edmcouncil.org/fibo/ontology/FND/Law/Jurisdiction/">
	<!ENTITY fibo-fnd-tim-tim "https://spec.edmcouncil.org/fibo/ontology/FND/Time/Time/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-val "https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Values/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/ontology/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Taxation/"
	xmlns:fibo-fnd-acc-tax="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Taxation/"
	xmlns:fibo-fnd-arrx-meth="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/MethodsAndRules/"
	xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/ontology/FND/Information/Schemes/"
	xmlns:fibo-fnd-law-jur="https://spec.edmcouncil.org/fibo/ontology/FND/Law/Jurisdiction/"
	xmlns:fibo-fnd-tim-tim="https://spec.edmcouncil.org/fibo/ontology/FND/Time/Time/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Values/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/ontology/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Taxation/">
		<rdfs:label
			xml:lang="en">Taxation</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/MethodsAndRules/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Information/Schemes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Law/Jurisdiction/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Time/Time/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Values/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-tax;TaxJurisdiction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
		<rdfs:label
			xml:lang="en">tax jurisdiction</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-tax;TaxLaw">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-tax;applicableIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxJurisdiction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-tax;laysDown"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">tax law</rdfs:label>
		<skos:definition
			xml:lang="en">A law or body of law pertaining to the liability for and payment of tax by persons or legal entities which are subject to the jurisdiction covered by that law.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-tax;TaxRule">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;Rule"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-tax;applicableIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxJurisdiction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-tax;isLaidDownIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxLaw"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-tax;setsOut"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxTreatment"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">tax rule</rdfs:label>
		<skos:definition
			xml:lang="en">The tax rule which is applied in determing the applicable tax.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-tax;TaxRuleCode">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;RuleCode"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-meth;standsFor"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;definedIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxRuleScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">tax rule code</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-tax;TaxRuleScheme">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;RuleScheme"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;defines.1"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxRuleCode"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">tax rule scheme</rdfs:label>
		<skos:definition
			xml:lang="en">The scheme in which the tax rule is identified and defined.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-tax;TaxTreatment">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-tax;definedIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">tax treatment</rdfs:label>
		<skos:definition
			xml:lang="en">Amount of money due to the government or tax authority, applicable to some thing or activity in some jurisdiction</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;applicableIn">
		<rdfs:label
			xml:lang="en">applicable in</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxLaw"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-tax;TaxJurisdiction"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;definedIn">
		<rdfs:label
			xml:lang="en">defined in</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxTreatment"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<owl:inverseOf
			rdf:resource="&fibo-fnd-acc-tax;setsOut"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;description">
		<rdfs:label
			xml:lang="en">description</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-val;TextValue"/>
		<skos:definition
			xml:lang="en">Free text description of the tax rule.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;effectivePeriod">
		<rdfs:label
			xml:lang="en">effective period</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-tim-tim;DatePeriod"/>
		<skos:definition
			xml:lang="en">Period during which the tax rule applies within the jurisdiction.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;isLaidDownIn">
		<rdfs:label
			xml:lang="en">is laid down in</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-tax;TaxLaw"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;laysDown">
		<rdfs:label
			xml:lang="en">lays down</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxLaw"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<owl:inverseOf
			rdf:resource="&fibo-fnd-acc-tax;isLaidDownIn"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;rate">
		<rdfs:label
			xml:lang="en">rate</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-val;PercentageValue"/>
		<skos:definition
			xml:lang="en">Rate used for calculation of the tax.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;ruleName">
		<rdfs:label
			xml:lang="en">rule name</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-val;TextValue"/>
		<skos:definition
			xml:lang="en">Common name of the Tax Rule</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-tax;setsOut">
		<rdfs:label
			xml:lang="en">sets out</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-tax;TaxRule"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-tax;TaxTreatment"/>
		<skos:definition
			xml:lang="en">The treatment of the financial instrument for taxation purposes. This is the most general form of tax treatment for financial instruments, including cash.</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>