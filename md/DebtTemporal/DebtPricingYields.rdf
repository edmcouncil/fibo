			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-md-dbt-py;hasOutlookPeriod"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-md-dbt-aly;LifeAnalytic"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-md-tem-ins;Maturity"/>
										<rdf:rest>
											<rdf:Description>
												<rdf:first
													rdf:resource="&fibo-sec-dbt-dbt-cft;PrescriptiveEvent"/>
												<rdf:rest>
