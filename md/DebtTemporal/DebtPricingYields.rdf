														<rdf:rest
															rdf:resource="&rdf;nil"/>
													</rdf:Description>
												</rdf:rest>
											</rdf:Description>
										</rdf:rest>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-md-tem-ins;derivedFromOutlay"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-md-dbt-py;DebtSecurityPrice"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">debt instrument yield</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">The return on the debt instrument at the stated price.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Yield has a relationship to the price.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
