	<owl:Ontology
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/">
		<rdfs:label>Debt Ontology</rdfs:label>
		<dct:license
			rdf:resource="&sm;MITLicense"/>
		<sm:contentLanguage
			rdf:resource="http://www.omg.org/spec/ODM/"/>
		<sm:contentLanguage
			rdf:resource="http://www.w3.org/standards/techs/owl#w3c_all"/>
		<sm:copyright
			rdf:datatype="&xsd;string">Copyright (c) 2016-2017 EDM Council, Inc.
Copyright (c) 2016-2017 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn
			rdf:resource="http://spec.edmcouncil.org/fibo/BE/"/>
		<sm:dependsOn
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/"/>
		<sm:fileAbbreviation
			rdf:datatype="&xsd;string">fibo-fbc-dae-dbt</sm:fileAbbreviation>
		<sm:fileAbstract
			rdf:datatype="&xsd;string">This ontology defines concepts that are common to all debt instruments, such as debt, borrower, lender, debtor, creditor, interest, principal, and the like. It is designed to be used by various other FIBO specifications, including but not limited to SEC/Debt and LOAN.</sm:fileAbstract>
		<sm:filename
			rdf:datatype="&xsd;string">Debt.rdf</sm:filename>
		<rdfs:seeAlso
			rdf:resource="http://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"/>
		<rdfs:seeAlso
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/AboutFBC/"/>
		<rdfs:seeAlso
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/AboutDebtAndEquities/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/FinancialProductsAndServices/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
