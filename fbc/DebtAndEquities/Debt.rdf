	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Accrual">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-rl;ThingInRole"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">accrual</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">There are legal contractual terms for the accrual of interest, as distinct from the payment of interest.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">the process of accumulating interest or other income that has been earned but not paid</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Amortization">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-rl;ThingInRole"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">amortization</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">the process of reduction of debt or other costs through periodic charges to assets or liabilities, such as through principal payments on mortgages</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Borrower">
		<rdfs:subClassOf
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debtor"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractParty"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/owes"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">borrower</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">a party to a debt instrument that is obligated to repay the amount borrowed (principal) with interest and other fees according to the terms of the instrument</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/BorrowingCapacity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasAvailableAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;Date"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">borrowing capacity</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">http://www.investopedia.com/terms/c/credit.asp</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">an amount of money that an individual or organization can borrow</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Collateral">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-oac-own;Asset"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">collateral</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">an asset offered as security, pledged as an inducement to another party, to lend money, extend credit, or provision securities</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/CreditAgreement">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-agr-ctr;hasContractParty"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-agr-ctr;hasContractParty"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debtor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">credit agreement</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">http://www.investopedia.com/terms/c/credit.asp</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">a contractual agreement in which a debtor receives something of value now and agrees to repay the creditor at some date in the future, in some form (e.g., cash, securities, etc.), generally with interest</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isOwed"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">creditor</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">a party to whom an obligation, such as an amount of money, or good, or performance of some service exists</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/DayCountConvention">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-bd;BusinessRecurrenceIntervalConvention"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;Expression"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">day-count convention</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">http://www.investinginbonds.com/story.asp?id=3140</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">http://www.investopedia.com/terms/d/daycount.asp</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Day-count conventions apply to swaps, mortgages and forward rate agreements as well as bonds, each of which has its own day-count convention, which varies depending on the type of instrument, whether the interest rate is fixed or floating, and the country of issuance. Among the most common conventions are 30/360 or 365, actual/360 or 365, and actual/actual. A 30/360 convention assumes 30 days in a month and 360 days in a year. An actual/360 convention assumes the actual number of days in the given month and 360 days in the year. An actual/ actual convention uses the actual number of days in the given interest period and year.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">a business recurrence interval convention that is used to calculate the number of days in an interest payment, which applies to the amount of accrued interest or the present value for debt instruments</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-agr;Commitment"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isOwedBy"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debtor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isOwedTo"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">debt</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">In cases where the debtor and payer are the same legal person, then a debt is equivalent to a payment obligation.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">an obligation to pay something, such as an amount of money, good, service, or instrument</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/DebtTerms">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractualCommitment"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">debt terms</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">These may be terms in a loan contract (including for example a mortgage contract) or they may be the contractual terms of a debt security.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">contract terms that specify the formal rights and obligations of borrower and lender under a contract in which funds are lent from the one party to the other</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debtor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">debtor</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">http://legal-dictionary.thefreedictionary.com/Debtor+and+Creditor</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">a party that owes a debt</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/ExtensionProvision">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractualElement"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">extension provision</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">In the case of a debt instrument, an extension may include extending the time allowed for repayment of the principal, the maturity date, and so forth.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">contract terms that specify the conditions under which a contract can be extended</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/FixedInterestRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;InterestRate"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">fixed interest rate</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<owl:disjointWith
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/VariableInterestRate"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">an interest rate that does not fluctuate over the lifetime of a loan or other debt instrument</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Interest">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;InterestRate"/>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasInterestRate"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-dt-fd;DatePeriod"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-dt-fd;hasDatePeriod"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isInterestOn"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">interest</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">the cost of using credit, or another&apos;s money, expressed as a rate per period of time, payable by a debtor to a creditor in consideration of the credit extended to the debtor</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/InterestPaymentTerms">
		<rdfs:subClassOf
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/DebtTerms"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/governsPaymentOf"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Interest"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">interest payment terms</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Interest is usually payable on any outstanding principal amount, therefore Interest itself relates to the amount of debt at any given point of time, not to the principal amount advanced at the time that the loan was advanced or the debt security issued.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">contract terms for payment of interest on a debt</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Lender">
		<rdfs:subClassOf
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractParty"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">lender</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">a party that extends credit or money to a borrower with the expectation of being repaid, usually with interest</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Principal">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isPrincipalOf"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">principal</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">the value (monetary amount) of an obligation, such as a bond or loan, raised and that must be repaid at maturity</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/PrincipalRepaymentTerms">
		<rdfs:subClassOf
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/DebtTerms"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/governsPaymentOf"/>
				<owl:someValuesFrom
					rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Principal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">principal repayment terms</rdfs:label>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">contract terms that specify requirements for repayment of the principal</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/VariableInterestRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;InterestRate"/>
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">variable interest rate</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">The amount of variation is generally governed by an appropriate index.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&rdf;langString"
			xml:lang="en">floating interest rate</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">an interest rate that is allowed to vary over the maturity of a loan or other debt instrument</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/governsPaymentOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">governs payment of</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/DebtTerms"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasAccrualBasis">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has accrual basis</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/InterestPaymentTerms"/>
		<rdfs:range
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/DayCountConvention"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">identifies the convention that defines how interest accrues on the debt, that is the number of days in a month and days in a year that are counted when performing interest accrual calculations</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasAvailableAmount">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has available amount</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">indicates an amount of money available for an individual or organization to borrow</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasInterestRate">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has interest rate</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;InterestRate"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">relates something, such as an agreement, or debt instrument, to the rate of annual interest that is to be paid by the debtor to the creditor on the debt</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasOutstandingAmount">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has outstanding amount</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">indicates an amount of money representing the principal, interest, or other amount owed at a specific point in time</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasPrincipal">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has principal</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<rdfs:range
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Principal"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">indicates the face value of an obligation, such as a bond or loan, that must be repaid at maturity, i.e., the base amount raised by a mortgage or other debt instrument</skos:definition>
		<skos:editorialNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">This is not the balance of the debt at a point in time, but the amount drawn down at a specific point in time, after which (for interest bearing debts) interest becomes payable. Semantic Modeling note This term is over-ridden for specific kinds of debt (securities, loans) and is therefore not needed in any data model. It is included here to define part of the meaning of the Debt Finance term. It is a Relationship Fact and strictly speaking should be regarded as being specialized and over-ridden by the terms in the Loan and Security classes, however it is modeled here as a &quot;Referenceable Archetype&quot; meaning that it appears in diagrams as a textual entry not a relationship line, and so it is not possible to formally show this specialization.</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/hasPrincipalRepaymentDate">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">has principal repayment date</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&rdf;langString"
			xml:lang="en">Depending on the terms of the instrument (debt security, such as a bond, loan, etc.), this may be the date of a single payment of the debt principal or of the completion of scheduled partial redemption payments.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/PrincipalRepaymentTerms"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">relates an instrument to the date when the principal must be repaid in full</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isAmortizationOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is amortization of</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Amortization"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isCollateralizationOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is collateralization of</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Collateral"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">relates some collateral to a credit agreement or debt instrument for which the property has been pledged as security for the debt</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isCollateralizedBy">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is collateralized by</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&rdf;langString"
			xml:lang="en">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<rdfs:range
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Collateral"/>
		<skos:definition
			rdf:datatype="&rdf;langString"
			xml:lang="en">relates a credit agreement or debt instrument to property pledged as security for the debt</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isInterestOn">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is interest on</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Interest"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isOwed">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is owed</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
		<rdfs:range
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isOwedBy">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is owed by</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
		<rdfs:range
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debtor"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isOwedTo">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is owed to</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debt"/>
		<rdfs:range
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Creditor"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/isPrincipalOf">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">is principal of</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Principal"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/owes">
		<rdfs:label
			rdf:datatype="&rdf;langString"
			xml:lang="en">owes</rdfs:label>
		<rdfs:domain
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Debt/Debtor"/>
