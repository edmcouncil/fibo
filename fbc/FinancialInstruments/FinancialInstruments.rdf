    <!-- Ontology/File-Level Metadata for the FIBO SEC Financial Instruments Ontology -->

        <sm:filename rdf:datatype="&xsd;string">FinancialInstruments.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-fbc-fi-fi</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://spec.edmcouncil.org/fibo/FBC/20150501/FinancialInstruments/FinancialInstruments/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology defines the fundamental concepts relevant to financial instruments in general, and for those that are regulated more specificically, such as for equities, options, debt instruments, and so forth, some of which may be negotiable.</sm:fileAbstract>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>

