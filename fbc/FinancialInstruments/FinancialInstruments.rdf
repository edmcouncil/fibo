   <owl:ObjectProperty rdf:about="&fibo-fbc-fi-fi;hasIssuer">
       <rdfs:label>has issuer</rdfs:label>
       <rdfs:domain rdf:resource="&fibo-fbc-fi-fi;FinancialInstrument"/>
       <rdfs:range rdf:resource="&fibo-fbc-fi-fi;Issuer"/>
       <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
       <skos:definition rdf:datatype="&xsd;string">relates an instrument to the party that issued it</skos:definition>
   </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fi-fi;hasPrincipalExecutiveOfficeAddress">
        <rdfs:label>has principal executive office address</rdfs:label>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
       <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
       <skos:definition rdf:datatype="&xsd;string">relates an organization, specifically the issuer of a financial instrument, to its principal executive address, as required for issuance of that instrument</skos:definition>
    </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fbc-fi-fi;hasValueExpressedIn">
       <rdfs:label>has value expressed in</rdfs:label>
       <rdfs:domain rdf:resource="&fibo-fbc-fi-fi;FinancialInstrument"/>
       <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
       <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
       <skos:definition rdf:datatype="&xsd;string">relates an instrument to the currency its value is typically expressed in</skos:definition>
       <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">This should be the same currency that was declared at the time of issuance.</fibo-fnd-utl-av:explanatoryNote>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fbc-fi-fi;isLegallyRecordedIn">
       <rdfs:label>is legally recorded in</rdfs:label>
       <rdfs:domain rdf:resource="&fibo-fbc-fi-fi;FinancialInstrument"/>
       <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
       <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;hasGoverningJurisdiction"/>
       <skos:definition rdf:datatype="&xsd;string">jurisdiction (country, county, state, province, city) in which the financial instrument is legally recorded for regulatory and/or tax purposes</skos:definition>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fbc-fi-fi;mayBeTradedIn">
       <rdfs:label>may be traded in</rdfs:label>
       <rdfs:domain rdf:resource="&fibo-fbc-fi-fi;Security"/>
       <rdfs:range rdf:resource="&fibo-fbc-fct-mkt;Market"/>
       <skos:definition rdf:datatype="&xsd;string">optionally identifies one or more markets in which the security may be traded</skos:definition>
   </owl:ObjectProperty>


