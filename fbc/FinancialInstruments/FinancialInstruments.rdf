    <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/">
        <rdfs:label>Financial Instruments Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO SEC Financial Instruments Ontology -->

