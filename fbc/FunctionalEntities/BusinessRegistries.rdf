<!DOCTYPE rdf:RDF [
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY fibo-be-le-fbo "https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/">
	<!ENTITY fibo-fbc-fct-breg "https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/">
	<!ENTITY fibo-fbc-fct-ra "https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/">
	<!ENTITY fibo-fnd-dt-fd "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/">
	<!ENTITY fibo-fnd-law-jur "https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/">
	<!ENTITY fibo-fnd-plc-adr "https://spec.edmcouncil.org/fibo/FND/Places/Addresses/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY lcc-cr "http://www.omg.org/spec/LCC/Countries/CountryRepresentation/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-be-le-fbo="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"
	xmlns:fibo-fbc-fct-breg="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/"
	xmlns:fibo-fbc-fct-ra="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-law-jur="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"
	xmlns:fibo-fnd-plc-adr="https://spec.edmcouncil.org/fibo/FND/Places/Addresses/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:lcc-cr="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/">
		<rdfs:label>Business Registries Ontology</rdfs:label>
		<dct:abstract>This ontology extends the Registration Authorities ontology to define specific kinds of registries, such as business entity registries, registries for identifiers and codes of various sorts, and registries for financial institutions and intermediaries based on jurisdiction, who regulates them, and the services they provide.</dct:abstract>
		<dct:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dct:license>
		<sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
		<sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright>Copyright (c) 2015 EDM Council, Inc.
Copyright (c) 2015 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/LCC/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/BE/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/</sm:dependsOn>
		<sm:fileAbbreviation>fibo-fbc-fct-breg</sm:fileAbbreviation>
		<sm:filename>BusinessRegistries.rdf</sm:filename>
		<rdfs:seeAlso rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
		<rdfs:seeAlso rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FBC/AboutFBC/</rdfs:seeAlso>
		<rdfs:seeAlso rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/AboutFBCFunctionalEntities/</rdfs:seeAlso>
		<owl:imports rdf:resource="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Addresses/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:versionIRI rdf:resource="https://spec.edmcouncil.org/fibo/FBC/20150801/FunctionalEntities/BusinessRegistries/"/>
		<fibo-fnd-utl-av:hasMaturityLevel rdf:resource="&fibo-fnd-utl-av;Release"/>
	</owl:Ontology>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistrationAuthority">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;manages"/>
				<owl:onClass rdf:resource="&fibo-fbc-fct-breg;BusinessRegistry"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
				<owl:onClass rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>business registration authority</rdfs:label>
		<skos:definition>a governmental registration authority that is responsible for maintaining a registry of business entities for a particular jurisdiction</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistry">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;Registry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
				<owl:onClass rdf:resource="&fibo-fbc-fct-breg;BusinessRegistrationAuthority"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
				<owl:onClass rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>business registry</rdfs:label>
		<skos:definition>a registry for registering and maintaining information about business entities for a particular jurisdiction</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistryEntry">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;RegistryEntry"/>
		<rdfs:label>business registry entry</rdfs:label>
		<skos:definition>an entry in a business registry</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-fct-breg;RegistrationAddress">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-plc-adr;Address"/>
		<rdfs:label>registration address</rdfs:label>
		<skos:definition>an address for a business, such as its legal address, headquarters address, address registered for obtaining a license, and so forth</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine1">
		<rdfs:label>has address line 1</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the first line of the street address</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine2">
		<rdfs:label>has address line 2</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the second line of the street address</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine3">
		<rdfs:label>has address line 3</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the third line of the street address</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote>This element SHALL be omitted if address line 2 is omitted.</fibo-fnd-utl-av:explanatoryNote>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine4">
		<rdfs:label>has address line 4</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the fourth line of the street address</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote>This element SHALL be omitted if address line 3 is omitted.</fibo-fnd-utl-av:explanatoryNote>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasCity">
		<rdfs:label>has city</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the name of the city</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasCountry">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has country</rdfs:label>
		<rdfs:range rdf:resource="&lcc-cr;Country"/>
		<skos:definition>identifies the country according to ISO 3166-1</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasHeadquartersAddress">
		<rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasPrimaryAddress"/>
		<rdfs:label>has headquarters address</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<skos:definition>identifies the headquarters address for the registered entity</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasPostalCode">
		<rdfs:label>has postal code</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the postal code of this address as specified by the local postal service</skos:definition>
		<fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatus">
		<rdfs:label>has registration status</rdfs:label>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>the status of a specific registration, such as a registered identifier or license</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatusRevisionDate">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label>has registration status revision date</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<skos:definition>the date that the status of a specific registration in the registry was revised</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasSubdivision">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has subdivision</rdfs:label>
		<rdfs:range rdf:resource="&lcc-cr;CountrySubdivision"/>
		<skos:definition>identifies the country subdivision (state, province, region, etc.) according to ISO 3166-2</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>
