    <!ENTITY fibo-fbc-fct-breg "http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/" >
]>

<rdf:RDF xml:base="http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:lcc-cr="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/"

     xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-utl-bt="http://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"
     xmlns:fibo-fnd-aap-agt="http://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
     xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
     xmlns:fibo-fnd-dt-fd="http://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
     xmlns:fibo-fnd-plc-adr="http://spec.edmcouncil.org/fibo/FND/Places/Addresses/"
     xmlns:fibo-fnd-law-jur="http://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"

     xmlns:fibo-be-le-fbo="http://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"

     xmlns:fibo-fbc-fct-ra="http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/"
     xmlns:fibo-fbc-fct-breg="http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/">


    <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/BusinessRegistries/">
        <rdfs:label>Business Registries Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO FBC Business Registries Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2015 EDM Council, Inc.
Copyright (c) 2015 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the FIBO FBC Business Registries Ontology -->

        <sm:filename rdf:datatype="&xsd;string">BusinessRegistries.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-fbc-fct-breg</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://spec.edmcouncil.org/fibo/FBC/20150801/FunctionalEntities/BusinessRegistries/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology extends the Registration Authorities ontology to define specific kinds of registries, such as business entity registries, registries for identifiers and codes of various sorts, and registries for financial institutions and intermediaries based on jurisdiction, who regulates them, and the services they provide.</sm:fileAbstract>

        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/LCC/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FND/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/BE/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/</sm:dependsOn>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>

        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FBC/AboutFBC/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/AboutFBCFunctionalEntities/</rdfs:seeAlso>

        <owl:imports rdf:resource="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Places/Addresses/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"/>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/"/>

    </owl:Ontology>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasCountry">
        <rdfs:label>has country</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies the country according to ISO 3166-1</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
        <rdfs:range rdf:resource="&lcc-cr;Country"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasHeadquartersAddress">
        <rdfs:label>has headquarters address</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies the headquarters address for the registered entity</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasPrimaryAddress"/>
        <rdfs:range rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasSubdivision">
        <rdfs:label>has subdivision</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">identifies the country subdivision (state, province, region, etc.) according to ISO 3166-2</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
        <rdfs:range rdf:resource="&lcc-cr;CountrySubdivision"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatusRevisionDate">
        <rdfs:label>has registration status revision date</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the date that the status of a specific registration in the registry was revised</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
        <rdfs:range rdf:resource="&fibo-fnd-dt-fd;Date"/>
    </owl:ObjectProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine1">
        <rdfs:label>has address line 1</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the first line of the street address</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine2">
        <rdfs:label>has address line 2</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the second line of the street address</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine3">
        <rdfs:label>has address line 3</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the third line of the street address</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">This element SHALL be omitted if address line 2 is omitted.</fibo-fnd-utl-av:explanatoryNote>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasAddressLine4">
        <rdfs:label>has address line 4</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the fourth line of the street address</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">This element SHALL be omitted if address line 3 is omitted.</fibo-fnd-utl-av:explanatoryNote>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasCity">
        <rdfs:label>has city</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the name of the city</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasPostalCode">
        <rdfs:label>has postal code</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the postal code of this address as specified by the local postal service</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fbc-fct-breg;RegistrationAddress"/>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatus">
        <rdfs:label>has registration status</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the status of a specific registration, such as a registered identifier or license</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
    </owl:DatatypeProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistrationAuthority">
        <rdfs:label>business registration authority</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;manages"/>
                <owl:onClass rdf:resource="&fibo-fbc-fct-breg;BusinessRegistry"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
                <owl:onClass rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a governmental registration authority that is responsible for maintaining a registry of business entities for a particular jurisdiction</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistry">
        <rdfs:label>business registry</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;Registry"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
                <owl:onClass rdf:resource="&fibo-fbc-fct-breg;BusinessRegistrationAuthority"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
                <owl:onClass rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a registry for registering and maintaining information about business entities for a particular jurisdiction</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
        
    <owl:Class rdf:about="&fibo-fbc-fct-breg;BusinessRegistryEntry">
        <rdfs:label>business registry entry</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fbc-fct-ra;RegistryEntry"/>
        <skos:definition rdf:datatype="&xsd;string">an entry in a business registry</skos:definition>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fbc-fct-breg;RegistrationAddress">
        <rdfs:label>registration address</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-plc-adr;Address"/>
        <skos:definition rdf:datatype="&xsd;string">an address for a business, such as its legal address, headquarters address, address registered for obtaining a license, and so forth</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>

</rdf:RDF>