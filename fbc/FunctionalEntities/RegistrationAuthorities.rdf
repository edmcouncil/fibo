     xmlns:fibo-fnd-law-lcap="http://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/"
     xmlns:fibo-be-le-lp="http://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"
     xmlns:fibo-be-le-fbo="http://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"
     xmlns:fibo-be-le-lei="http://spec.edmcouncil.org/fibo/BE/LegalEntities/LEIEntities/"
     xmlns:fibo-be-fct-fct="http://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/"
     xmlns:fibo-be-oac-exec="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/Executives/"
     xmlns:fibo-fbc-pas-pas="http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/ProductsAndServices/"
     xmlns:fibo-fbc-fct-ra="http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/">


    <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/RegistrationAuthorities/">
        <rdfs:label>Registration Authorities Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO FBC Registration Authorities Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2015 EDM Council, Inc.
Copyright (c) 2015 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the FIBO FBC Registration Authorities Ontology -->

        <sm:filename rdf:datatype="&xsd;string">RegistrationAuthorities.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">fibo-fbc-fct-ra</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://spec.edmcouncil.org/fibo/FBC/20150501/FunctionalEntities/RegistrationAuthorities/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology defines concepts for representation of registration authorities, registrars, registration-specific identifiers and related identification schemes, and registration authorities specific to ISO and the financial industry.  Examples of financial industry registration authorities in the US include the Securities Exchange Commission and SWIFT.</sm:fileAbstract>

        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FND/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/BE/</sm:dependsOn>
        <sm:dependsOn rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/ProductsAndServices/</sm:dependsOn>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>

        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FBC/AboutFBC/</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/AboutFBCFunctionalEntities/</rdfs:seeAlso>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Arrangements/ClassificationSchemes/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Arrangements/IdentifiersAndIndices/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Parties/Roles/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Parties/Parties/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/"/>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/BE/LegalEntities/LEIEntities/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/BE/FunctionalEntities/FunctionalEntities/"/>
        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/Executives/"/>

        <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/ProductsAndServices/"/>
    </owl:Ontology>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-ra;hasRegistrationDate">
        <rdfs:label>has registration date</rdfs:label>
        <rdfs:range rdf:resource="&fibo-fnd-dt-fd;Date"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-ra;hasRegistryEntry">
        <rdfs:label>has registry entry</rdfs:label>
        <rdfs:domain rdf:resource="&fibo-fbc-fct-ra;Registry"/>
        <rdfs:range rdf:resource="&fibo-fbc-fct-ra;RegistryEntry"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-ra;isRegisteredBy">
        <rdfs:label>is registered by</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">indicates the registration authority that registers something</skos:definition>
        <rdfs:range rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-ra;isRegisteredIn">
        <rdfs:label>is registered in</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">indicates the registry that something is registered in</skos:definition>
        <rdfs:range rdf:resource="&fibo-fbc-fct-ra;Registry"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-ra;registers">
        <rdfs:label>registers</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">links a registration authority to something it registers</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&fibo-fbc-fct-ra;specifies">
        <rdfs:label>specifies</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">states a fact about something</skos:definition>
    </owl:ObjectProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-fbc-fct-ra;Registrar">
        <rdfs:label>registrar</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a party that has the capacity to act as a representative of a registration authority to provide registration services, including official record keeping</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">Barron's Dictionary of Business and Economic Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-exec;ResponsibleParty"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
                <owl:onClass rdf:resource="&fibo-be-le-lp;NaturalPerson"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
                <owl:someValuesFrom>              
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
                                <owl:onClass rdf:resource="&fibo-fbc-fct-ra;RegistrationCapacity"/>
                                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;represents"/>
                                <owl:onClass rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
                                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>              
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fbc-fct-ra;RegistrationAuthority">
        <rdfs:label>registration authority</rdfs:label>
        <fibo-fnd-utl-av:abbreviation rdf:datatype="&xsd;string">RA</fibo-fnd-utl-av:abbreviation>
        <rdfs:subClassOf rdf:resource="&fibo-fbc-pas-pas;ServiceProvider"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;provides"/>
                <owl:someValuesFrom rdf:resource="&fibo-fbc-fct-ra;RegistrationService"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
                <owl:onClass>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
                            <rdf:Description rdf:about="&fibo-be-le-lp;LegalEntity"/>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:onClass>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;manages"/>
                <owl:onClass rdf:resource="&fibo-fbc-fct-ra;Registry"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a formally constituted organization and legal entity that is responsible for maintaining a registry and provides registration services</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fbc-fct-ra;RegistrationCapacity">
        <rdfs:label>registration capacity</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
        <skos:definition rdf:datatype="&xsd;string">the capacity of some natural person to formally register information in a registry on behalf of some registration authority</skos:definition>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fbc-fct-ra;RegistrationScheme">
        <rdfs:label>registration scheme</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-arr-id;IdentificationScheme"/>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-arr-cls;ClassificationScheme"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;characterizes"/>
                <owl:someValuesFrom rdf:resource="&fibo-fbc-fct-ra;Registry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;defines"/>
                <owl:someValuesFrom rdf:resource="&fibo-fbc-fct-ra;RegistryIdentifier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a scheme for organizing information and allocating identifiers to items in a registry</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    
    <owl:Class rdf:about="&fibo-fbc-fct-ra;RegistrationService">
        <rdfs:label>registration service</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fbc-pas-pas;Service"/>
        <skos:definition rdf:datatype="&xsd;string">service provided to individuals and/or organizations to register items in a registry</skos:definition>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isProvidedBy"/>
                <owl:onClass rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fbc-fct-ra;Registry">
        <rdfs:label>registry</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
                <owl:onClass rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-fct-fct;isCharacterizedBy"/>
                <owl:onClass rdf:resource="&fibo-fbc-fct-ra;RegistrationScheme"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fbc-fct-ra;hasRegistryEntry"/>
                <owl:allValuesFrom rdf:resource="&fibo-fbc-fct-ra;RegistryEntry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">a system, typically an information system, that records the registration of items</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
        
    <owl:Class rdf:about="&fibo-fbc-fct-ra;RegistryEntry">
        <rdfs:label>registry entry</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-rel-rel;Reference"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isPartOf"/>
                <owl:allValuesFrom rdf:resource="&fibo-fbc-fct-ra;Registry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasDefinition"/>
                <owl:onClass rdf:resource="&fibo-fbc-fct-ra;RegistrationScheme"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
                <owl:someValuesFrom rdf:resource="&fibo-fbc-fct-ra;RegistryIdentifier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">an entry in a registry that specifies something about the thing that is registered</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
        
    <owl:Class rdf:about="&fibo-fbc-fct-ra;RegistryIdentifier">
        <rdfs:label>registry identifier</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-arr-id;Identifier"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-arr-id;isIndexTo"/>
                <owl:someValuesFrom rdf:resource="&fibo-fbc-fct-ra;Registry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">an identifier associated with an entry in a registry, i.e., one that provides an index to the registry for the identified item</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
        
</rdf:RDF>