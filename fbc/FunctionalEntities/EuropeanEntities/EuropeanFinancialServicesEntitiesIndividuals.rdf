	<fibo-fbc-pas-fpas:FinancialServiceProvider
		rdf:about="&fibo-fbc-fct-eufseind;Clearstream">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Euroclear</rdfs:label>
		<fibo-fnd-rel-rel:hasIdentity
			rdf:resource="&fibo-fbc-fct-eufseind;ClearstreamBankingSA"/>
		<rdfs:seeAlso
			rdf:resource="http://www.clearstream.com/clearstream-en/about-clearstream"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the Clearstream functional entity, which is an international central securities depository (ICSD) based in Luxembourg, providing post-trade infrastructure and securities services for the international market and 56 domestic markets worldwide</skos:definition>
	</fibo-fbc-pas-fpas:FinancialServiceProvider>
	
	<fibo-be-le-cb:StockCorporation
		rdf:about="&fibo-fbc-fct-eufseind;ClearstreamBankingSA">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Clearstream Banking S.A.</rdfs:label>
		<fibo-fnd-rel-rel:hasLegalName
			rdf:datatype="&xsd;string">Clearstream Banking S.A.</fibo-fnd-rel-rel:hasLegalName>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">The company offers asset services, cash and banking services, connectivity, global securities financing, investment fund services, issuance, IT solutions, settlement, and market coverage services. It also provides post-trade infrastructure services for the German securities industry; and manages, safe keeps, and administers the securities that it holds on behalf of its customers. Clearstream Banking S.A was formerly known as Cedel Bank S.A. and changed its name to Clearstream Banking S.A. in March 2002. The company was founded in 1970 and is based in Luxembourg.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:seeAlso
			rdf:resource="https://www.euroclear.com/en/about/our-structure.html"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the Euroclear legal entity that provides post-trade infrastructure and securities services in Europe and internationally</skos:definition>
	</fibo-be-le-cb:StockCorporation>
	
	<fibo-fbc-pas-fpas:FinancialServiceProvider
		rdf:about="&fibo-fbc-fct-eufseind;Euroclear">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Euroclear</rdfs:label>
		<fibo-fnd-rel-rel:hasIdentity
			rdf:resource="&fibo-fbc-fct-eufseind;EuroclearSANV"/>
		<rdfs:seeAlso
			rdf:resource="https://www.euroclear.com/en.html"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the Euroclear functional entity, which is a global clearing house, central securities depository, and custodian that specializes in the settlement of securities transactions as well as the safekeeping and asset servicing of these securities</skos:definition>
	</fibo-fbc-pas-fpas:FinancialServiceProvider>
	
	<fibo-be-le-cb:StockCorporation
		rdf:about="&fibo-fbc-fct-eufseind;EuroclearSANV">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Euroclear SA/NV</rdfs:label>
		<fibo-fnd-rel-rel:hasLegalName
			rdf:datatype="&xsd;string">Euroclear SA/NV</fibo-fnd-rel-rel:hasLegalName>
		<rdfs:seeAlso
			rdf:resource="https://www.euroclear.com/en/about/our-structure.html"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing the Euroclear legal entity that is the parent company of the international and national central securities depositories ((I)CSDs) of the Euroclear group of companies; it owns the group&apos;s shared securities processing platforms and delivers a range of services to the group&apos;s depositories</skos:definition>
	</fibo-be-le-cb:StockCorporation>
	
	<fibo-be-le-fbo:NotForProfitOrganization
		rdf:about="&fibo-fbc-fct-eufseind;EuropeanBankingFederation">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>European Banking Federation</rdfs:label>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">EBF</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.ebf.eu/about-us/</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an international non-profit association founded in 1960 that is the voice of the European banking sector, uniting 32 national banking associations in Europe that together represent some 4,500 banks - large and small, wholesale and retail, local and international - employing about 2.1 million people</skos:definition>
	</fibo-be-le-fbo:NotForProfitOrganization>
	
	<fibo-be-le-fbo:NotForProfitOrganization
		rdf:about="&fibo-fbc-fct-eufseind;EuropeanMoneyMarketsInstitute">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>European Money Markets Institute</rdfs:label>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">EMMI</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.emmi-benchmarks.eu/</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Its members are national banking associations in the Member States of the European Union which are involved in the Eurozone.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">an international non-profit association under Belgian law founded in 1999 with the launch of the Euro and based in Brussels (56, Ave des Arts, 1000 Brussels)</skos:definition>
	</fibo-be-le-fbo:NotForProfitOrganization>
	
	<fibo-fbc-pas-fpas:FinancialServiceProvider
		rdf:about="&fibo-fbc-fct-eufseind;LondonStockExchange">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>London Stock Exchange</rdfs:label>
		<fibo-fnd-utl-av:abbreviation
			rdf:datatype="&xsd;string">LSE</fibo-fnd-utl-av:abbreviation>
		<rdfs:seeAlso
			rdf:resource="http://www.londonstockexchange.com/home/homepage.htm"/>
		<skos:definition
			rdf:datatype="&xsd;string">individual representing London Stock Exchange functional entity, which is a global business and financial information services and news provider, a securities exchange, and the SEDOL code issuer and registration authority</skos:definition>
	</fibo-fbc-pas-fpas:FinancialServiceProvider>
	
	<fibo-be-le-cb:Corporation
		rdf:about="&fibo-fbc-fct-eufseind;NasdaqOMXGroup">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Nasdaq OMX Group</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.nasdaqomx.com/</fibo-fnd-utl-av:adaptedFrom>
		<rdfs:seeAlso
			rdf:resource="http://www.nasdaqomxnordic.com/"/>
		<skos:definition
			rdf:datatype="&xsd;string">a corporation that is a subsidiary of Nasdaq, Inc. that manages Nasdaq exchanges in Helsinki, Copenhagen, Stockholm, Iceland, Riga, Tallinn and Vilnius</skos:definition>
	</fibo-be-le-cb:Corporation>
	
	<fibo-fnd-org-fm:FormalOrganization
		rdf:about="&fibo-fbc-fct-eufseind;SwedishBankersAssociation">
		<rdf:type
			rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>Swedish Bankers&apos; Association</rdfs:label>
		<fibo-fnd-rel-rel:isMemberOf
			rdf:resource="&fibo-fbc-fct-eufseind;EuropeanBankingFederation"/>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.swedishbankers.se/Sidor/en/Home.aspx</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an association that represents banks and financial institutions established in Sweden that contributes to a sound and efficient regulatory framework that facilitates for banks to help create economic wealth for customers and society</skos:definition>
	</fibo-fnd-org-fm:FormalOrganization>

