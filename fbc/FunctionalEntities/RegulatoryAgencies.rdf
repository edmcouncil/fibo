	<owl:Class
		rdf:about="&fibo-fbc-fct-rga;Examiner">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-oac-exec;ResponsibleParty"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-aap-ppl;LegallyCapablePerson"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf>
							<rdf:Description>
								<rdf:first>
									<owl:Restriction>
										<owl:minQualifiedCardinality
											rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
										<owl:onClass
											rdf:resource="&fibo-fbc-fct-rga;RegulatoryCapacity"/>
										<owl:onProperty
											rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
									</owl:Restriction>
								</rdf:first>
								<rdf:rest>
									<rdf:Description>
										<rdf:first>
											<owl:Restriction>
												<owl:onClass
													rdf:resource="&fibo-fbc-fct-rga;RegulatoryAgency"/>
												<owl:onProperty
													rdf:resource="&fibo-fnd-rel-rel;represents"/>
												<owl:qualifiedCardinality
													rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
											</owl:Restriction>
										</rdf:first>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>examiner</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Black&apos;s Law Dictionary, see http://thelawdictionary.org/examiner/</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a party appointed by, and who has the capacity to act as an official representative of, a regulatory agency to provide examination, review, and other regulatory services</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-rga;GovernmentIssuedLicense">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-doc;LegalDocument"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;License"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-law-lcap;LicenseIdentifier"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fbc-fct-rga;isIssuedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fbc-fct-rga;RegulatoryAgency"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>government-issued license</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">grant of permission needed to legally perform some task, provide some service, exercise a certain privilege, or pursue some business or occupation</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-rga;Regulation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;governs"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-law-cor;Law"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasDefinition"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fbc-fct-rga;RegulationIdentifier"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fbc-fct-rga;RegulatoryAgency"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-fct-rga;isIssuedBy"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>regulation</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Many government agencices issue regulations to administer laws.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">a rule used to carry out a law</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-rga;RegulationIdentifier">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-id;Identifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fbc-fct-rga;Regulation"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;identifies"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>regulation identifier</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an identifier associated with a regulation</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-rga;RegulatoryAgency">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pas-pas;ServiceProvider"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fbc-fct-rga;GovernmentIssuedLicense"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-fct-rga;issues"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fbc-fct-rga;RegulatoryService"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;provides"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
				<owl:onProperty
					rdf:resource="&fibo-fbc-fct-rga;hasJurisdiction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass>
					<owl:Class>
						<owl:intersectionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-be-le-lp;StatutoryBody"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:intersectionOf>
					</owl:Class>
				</owl:onClass>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>regulatory agency</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://en.wikipedia.org/wiki/Regulatory_agency</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">http://www.thefreedictionary.com/regulatory+agency</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Typically, a regulatory agency is chartered to protect some constituancy, (e.g., investors in the financial industry), to ensure the fairness and integrity of some market (e.g., the securities market), and fair and safe business practices among the service providers in that market.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">regulator</fibo-fnd-utl-av:synonym>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">regulatory authority</fibo-fnd-utl-av:synonym>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">regulatory body</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a public authority or government agency responsible for exercising autonomous authority over some area of human activity in a regulatory or supervisory capacity</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">See http://www.finra.org/AboutFINRA/ for an example describing a regulatory agency.</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-rga;RegulatoryCapacity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<rdfs:label>regulatory capacity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">the capacity of some natural person to regulate some industry, organization, or product by virtue of some certification program on behalf of some regulatory agency</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-rga;RegulatoryService">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pas-pas;Service"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fbc-fct-rga;RegulatoryAgency"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isProvidedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>regulatory service</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a service provided by a regulatory agency, which may include, but not be limited to, examination, monitoring, supervision, testing, or other capabilities required to ensure the integrity, fairness, safety, or other capacity of a given industry, organization, or product</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-fct-rga;hasJurisdiction">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has jurisdiction</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-rga;RegulatoryAgency"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the jurisdiction in which the regulatory agency has authority</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-fct-rga;isIssuedBy">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;isProvidedBy"/>
		<rdfs:label>is issued by</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<owl:inverseOf
			rdf:resource="&fibo-fbc-fct-rga;issues"/>
		<skos:definition
			rdf:datatype="&xsd;string">indicates the party that issues something</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-fct-rga;isRegulatedBy">
