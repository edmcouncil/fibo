	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-pas-fpas;isOfferingOf">
		<rdfs:label>is offering of</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fbc-pas-fpas;Offering"/>
