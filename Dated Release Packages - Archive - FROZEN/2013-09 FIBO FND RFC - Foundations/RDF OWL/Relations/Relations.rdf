    <rdfs:label>comprises</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/isAppointedBy">
    <owl:inverseOf>
