  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/identifies">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
