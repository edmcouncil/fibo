    <rdfs:label>is in force in</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/comprises">
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
