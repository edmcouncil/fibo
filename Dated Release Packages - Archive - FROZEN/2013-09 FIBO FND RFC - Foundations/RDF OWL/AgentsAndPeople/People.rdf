    <rdfs:Datatype rdf:about="&fibo-fnd-aap-ppl;Gender">
        <rdfs:label>gender</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Selection of possible gender values that may be attributed to a person.</skos:definition>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">male</rdf:first>
                        <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">female</rdf:first>
                        <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">unspecified</rdf:first>
                        <rdf:rest> rdf:resource="&rdf;nil">
                                                  </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                 </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>


