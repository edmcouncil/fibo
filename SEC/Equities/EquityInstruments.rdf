			rdf:resource="&fibo-sec-eq-eq;confersOwnershipOfEquity"/>
		<rdfs:label
			xml:lang="en">confers ownership of issued equity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-eq-eq;PubliclyTradedShare"/>
		<rdfs:range
			rdf:resource="&fibo-sec-eq-eq;PubliclyIssuedEquity"/>
		<skos:definition
			xml:lang="en">A share confers on the holder the legal right of ownership of issued equity.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-eq-eq;confersOwnershipOfPrivateEquity">
