		<rdfs:range
			rdf:resource="&fibo-sec-eq-eq;ShareholderEquity"/>
		<skos:definition
			xml:lang="en">The shareholder holds some of the issued shareholder equity in the company, by way of the shares that they hold.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-eq-eq;isHeldBy">
		<rdfs:subPropertyOf
