	<owl:Class
		rdf:about="&fibo-sec-eq-eq;VotingRight">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agrx-con;ContractualRight"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:maxQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
