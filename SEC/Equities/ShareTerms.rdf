	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/">
		<rdfs:label
			xml:lang="en">ShareTerms</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
