	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/NegotiableSecurities/">
		<rdfs:label
			xml:lang="en">NegotiableSecurities</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SecuritiesTransactions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/SupplyTransactions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-neg;NegotiableSecurity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-ecr;EconomicResource"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-sec-sec-neg;negotiableSecurityTakesFormOf"/>
				<owl:someValuesFrom
					rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">negotiable security  (please review)</rdfs:label>
		<skos:definition
			xml:lang="en">A security in the context of being something that can be bought and sold, i.e. as a kind of negotiable thing. This is the context in which a trading or listing service provides for the sale or purchase of such a security.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-sec-neg;negotiableSecurityTakesFormOf">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-txn-ecr;takesMaterialForm"/>
		<rdfs:label
			xml:lang="en">negotiable security takes form of  (please review)</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-sec-sec-neg;NegotiableSecurity"/>
		<rdfs:range
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security"/>
