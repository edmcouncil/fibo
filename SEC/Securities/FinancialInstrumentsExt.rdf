<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-acc-acc "https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-sec-dbt-dbt-dbt "https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/">
	<!ENTITY fibo-sec-sec-finx "https://spec.edmcouncil.org/fibo/SEC/Securities/FinancialInstrumentsExt/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/SEC/Securities/FinancialInstrumentsExt/"
	xmlns:fibo-fnd-acc-acc="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-sec-dbt-dbt-dbt="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/"
	xmlns:fibo-sec-sec-finx="https://spec.edmcouncil.org/fibo/SEC/Securities/FinancialInstrumentsExt/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/SEC/Securities/FinancialInstrumentsExt/">
		<rdfs:label
			xml:lang="en">FinancialInstrumentsExt</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Securities/SecuritiesClassification/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/DerivativeInstrument">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-sec-finx;DerivativeInstrument"/>
	</rdf:Description>
	
	<rdf:Description
		rdf:about="https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/FinancialInstruments/Security">
		<owl:equivalentClass
			rdf:resource="&fibo-sec-sec-finx;Security"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-finx;DerivativeInstrument">
		<rdfs:label
			xml:lang="en">derivative instrument</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-finx;IsoFinancialInstrument">
		<rdfs:label
			xml:lang="en">iso financial instrument</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-fnd-acc-acc;CashCurrency"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-sec-dbt-dbt-dbt;NonTradableDebtInstrument"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-sec-sec-finx;DerivativeInstrument"/>
										<rdf:rest>
											<rdf:Description>
												<rdf:first
													rdf:resource="&fibo-sec-sec-finx;Security"/>
												<rdf:rest
													rdf:resource="&rdf;nil"/>
											</rdf:Description>
										</rdf:rest>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition
			xml:lang="en">Financial instruments are cash, evidence of an ownership interest in an entity, or a contractual right to receive, or deliver, cash or another financial instrument.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-sec-sec-finx;Security">
		<rdfs:label
			xml:lang="en">security</rdfs:label>
	</owl:Class>

</rdf:RDF>