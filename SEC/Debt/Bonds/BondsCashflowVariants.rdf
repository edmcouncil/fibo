				<owl:onProperty
					rdf:resource="&fibo-sec-dbt-bo-cfv;hasPrincipalRepaymentTerms"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-md-dbt-int;hasFactor"/>
