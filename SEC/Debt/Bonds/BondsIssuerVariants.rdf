		<rdfs:range
			rdf:resource="&fibo-sec-dbt-bo-iss;MunicipalDebtObligor"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-bo-iss;hasMunicipalTrustee">
		<rdfs:subPropertyOf
