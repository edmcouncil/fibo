		<rdfs:domain
			rdf:resource="&fibo-sec-dbt-bo-iss;MunicipalBond"/>
		<rdfs:range
			rdf:resource="&fibo-sec-dbt-bo-iss;MunicipalTrustee"/>
		<skos:definition
			xml:lang="en">The financial institution with trust powers, designated by the issuer or borrower, that acts, pursuant to a bond contract, in a fiduciary capacity for the benefit of the bondholders in enforcing the terms of the bond contract.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-sec-dbt-bo-iss;hasMunicipalUse">
		<rdfs:label
			xml:lang="en">has municipal use</rdfs:label>
		<rdfs:domain
