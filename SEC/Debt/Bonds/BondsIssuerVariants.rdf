		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-bo-bo;Bond"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-sec-dbt-bo-iss;GovernmentIssuedDebtSecurity"/>
		<rdfs:label
			xml:lang="en">government issued bond</rdfs:label>
		<skos:definition
			xml:lang="en">a bond issued by some government on behalf of some polity, including sovereign and municipal bonds</skos:definition>
	</owl:Class>
	
