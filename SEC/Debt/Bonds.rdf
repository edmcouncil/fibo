		<rdfs:range rdf:resource="&fibo-fnd-utl-val;PercentageValue"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-sec-dbt-bnd;hasPublishedSinkingFundAmortizationTermsDescription">
