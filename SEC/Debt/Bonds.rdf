	</owl:Class>
	
	<owl:Class rdf:about="&fibo-sec-dbt-bnd;IssuersOptionCall">
		<rdfs:subClassOf rdf:resource="&fibo-sec-dbt-dbti;CallEvent"/>
		<rdfs:label>issuers option call</rdfs:label>
