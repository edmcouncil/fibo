	</owl:Class>
	
	<owl:Class rdf:about="&fibo-sec-dbt-bnd;MandatoryConvertibleBond">
		<rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bnd;ConvertibleBond"/>
