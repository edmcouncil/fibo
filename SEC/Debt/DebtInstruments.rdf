	<owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbti;hasRelativePriceAtMaturity">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-cur;hasPrice"/>
		<rdfs:label>has relative price at maturity</rdfs:label>
		<rdfs:range rdf:resource="&fibo-sec-dbt-dbti;RelativePrice"/>
		<skos:definition>indicates a relative price with respect to the face value at the time an instrument matures, namely par, discount or premium</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbti;hasRelativePriceAtRedemption">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-cur;hasPrice"/>
		<rdfs:label>has relative price at redemption</rdfs:label>
