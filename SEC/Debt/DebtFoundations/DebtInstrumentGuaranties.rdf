			rdf:resource="https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/Guaranty/Guaranty"/>
		<rdfs:label
			xml:lang="en">simple guaranty</rdfs:label>
		<skos:definition
			xml:lang="en">No definition</skos:definition>
	</owl:Class>
	
	<owl:Class
