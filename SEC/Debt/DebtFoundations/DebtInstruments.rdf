					rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtInstruments/NonTradableDebtRedemptionProvision"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
