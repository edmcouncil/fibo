                <owl:allValuesFrom>
                  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/DegreeOfControl"/>
                </owl:allValuesFrom>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/hasDegreeOfControl"/>
                </owl:onProperty>
              </owl:Restriction>
