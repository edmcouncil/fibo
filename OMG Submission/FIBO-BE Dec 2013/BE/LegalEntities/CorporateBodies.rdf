        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/InstrumentOfIncorporation"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/isConstitutedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>body corporate</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Law/Jurisdiction/Jurisdiction"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/isIncorporatedIn"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/JudicialPerson"/>
    <rdfs:subClassOf>
