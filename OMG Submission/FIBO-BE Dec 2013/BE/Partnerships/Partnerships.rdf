    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/DeJureControllingInterestParty"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partner"/>
    <skos:definition>a partner in a partnership, who holds some part of the general partner equity and typically is jointly and severally liable with the other partners for the liabilities incurred by that partnership</skos:definition>
    <rdfs:label>general partner</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LegallyIncorporatedPartnershipMember">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partner"/>
    <skos:definition>A party which is a member or partner in a Legally Incorporated Partnership such as an LLC or LLP.</skos:definition>
    <rdfs:label>legally incorporated partnership member</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithLimitedPartners">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass>
          <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LimitedPartner"/>
        </owl:onClass>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasMember"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partnership"/>
    </rdfs:subClassOf>
    <skos:editorialNote>Possibly but not necessarily a Legal Person. If there are only Limited Partners then this is of necessity a Legal Person (no-one else has liability in this structure).</skos:editorialNote>
    <skos:definition>A partnership having Limited Partners, that is partners whose liabilities are limited to the extent of their equity or guarantees.</skos:definition>
    <rdfs:label>partnership with limited partners</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipIncorporatedByEquity">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/BodyIncorporatedWithEquity"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithLimitedPartners"/>
    <skos:definition>partnership which is incorporated by the issuance of equity</skos:definition>
    <rdfs:label>partnership incorporated by equity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LiabilityApportionmentTermsSet">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Agreements/Contracts/ContractTermsSet"/>
    <skos:definition>Terms setting out the apportionment of liabilities for debts or other liabilities accrued by some formal business organization.</skos:definition>
    <rdfs:label>liability apportionment terms set</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithOnlyLimitedPartners">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithLimitedPartners"/>
    <skos:definition>A Partnership having limited partners but no general partners.</skos:definition>
    <rdfs:label>partnership with only limited partners</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partnership">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipEquity"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/hasEquity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass>
          <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipAgreement"/>
        </owl:onClass>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/isGovernedBy"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partner"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/hasPartyInRole"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >2</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partner"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasMember"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
    <skos:editorialNote>If the partnership doesn't limit the liability of the partners then the party to the contract is a natural person. If it does, then it's an artificial legal person (Body Corporate) - see Legally Incorporated Partnerships. Scope Note: This term of Partnership in the most general sense is ancestral to both of those.</skos:editorialNote>
    <skos:definition>Venture in which two or more legal entities carry out some business activities under a common identity.</skos:definition>
    <rdfs:label>partnership</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LimitedPartnerEquity">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Accounting/AccountingEquity/IssuedEquity"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipEquity"/>
    <skos:definition>equity in a partnership held by limited partners; this is not accompanied by any liability capability on the part of the holders of this equity.</skos:definition>
    <rdfs:label>limited partner equity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipIncorporatedThroughAgreement">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partnership"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/BodyIncorporatedThroughAgreement"/>
    <skos:editorialNote>An LLP (in the UK) is an example of this - in this case, the LLP document is the legal document which effectively constitutes the partnership</skos:editorialNote>
    <skos:definition>a partnership which has legal personhood, but for which there is not some issuance of limited partner equity</skos:definition>
    <rdfs:label>partnership incorporated through agreement</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithGeneralPartners">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >2</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/GeneralPartner"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasMember"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partnership"/>
    <skos:editorialNote>The partnership may or may not also have Limited Partners. In a typical non incorporated partnership, it does not. General Partners of a partnership must be natural persons.</skos:editorialNote>
    <skos:definition>A partnership which has two or more General Partners.</skos:definition>
    <rdfs:label>partnership with general partners</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LimitedPartner">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LimitedPartnerEquity"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/holdsPartnershipEquity"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/hasRole"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partner"/>
    <skos:definition>A partner in a partnership whose liabilities are limited to the extent of their equity holding or guarantees</skos:definition>
    <rdfs:label>limited partner</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/GeneralPartnerEquity">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/ControlParties/ConstitutionalDeJureControl"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/confers"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/OwnershipParties/ControllingEquity"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipEquity"/>
    <skos:editorialNote>For a typical, non incorporated partnership, this is the only equity in the entity. Other typically incorporated partnerships may have additional limited equity in addition to or instead of this.</skos:editorialNote>
    <skos:definition>Equity in a partnership held by general partners; this is typically accompanied by full liability capability on the part of the holders of the equity.</skos:definition>
    <rdfs:label>general partner equity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LegallyIncorporatedPartnership">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >2</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LegallyIncorporatedPartnershipMember"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasMember"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partnership"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
    <skos:editorialNote>The precise details and definition of these may vary from one jurisdiction to another. This type of entity is defined by being a legal person in its own right, as distinct from the usual type of partnership where the partners remain jointly and severally liable for debts.</skos:editorialNote>
    <skos:definition>Any partnership which is defined as a legal person within a given jurisdiction, for example a limited liability partnership (if that is a legal person).</skos:definition>
    <rdfs:label>legally incorporated partnership</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipAgreement">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partner"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/hasPartyInRole"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/OrganizationCoveringAgreement"/>
    <fibo-fnd-utl-av:definitionOrigin>http://www.investorguide.com/definition/partnership-agreement.html</fibo-fnd-utl-av:definitionOrigin>
    <skos:definition>a document setting up a partnership, giving the details of the business and the amount each partner is contributing to it.</skos:definition>
    <fibo-fnd-utl-av:synonym>articles of partnership</fibo-fnd-utl-av:synonym>
    <rdfs:label>partnership agreement</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/NonIncorporatedPartnership">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithGeneralPartners"/>
    <skos:definition>A partnership in the usual sense of the term, that is a partnership in which the partners are jointly and severally liable for liabilities incurred by the entity. </skos:definition>
    <rdfs:label>Non Incorporated Partnership</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/NaturalPersonLimitedPartner">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/LegalPersons/NaturalPerson"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasIdentity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LimitedPartner"/>
    <skos:definition>A limited partner in a partnership, who is and may only be a Natural Person i.e. a human being.</skos:definition>
    <rdfs:label>natural person limited partner</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithGeneralAndLimitedPartners">
    <owl:disjointWith rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithOnlyLimitedPartners"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithLimitedPartners"/>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipWithGeneralPartners"/>
    <skos:definition>A partnership having both limited partners and general partners.</skos:definition>
    <rdfs:label>partnership with general And limited partners</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/CorporateLimitedPartner">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/hasIdentity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LimitedPartner"/>
    <skos:definition>A limited partner in a partnership, who is and may only be a Corporate Legal Person (i.e., not a natural person)</skos:definition>
    <rdfs:label>corporate limited partner</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/setsOutLiabilityTermsIn">
    <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/LiabilityApportionmentTermsSet"/>
    <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/OrganizationCoveringAgreement"/>
    <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Agreements/Contracts/hasTerms"/>
    <skos:definition>has terms setting out the apportionment of liabilities for debts or other liabilities accrued by some formal business organization</skos:definition>
    <rdfs:label>sets out liability terms in</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/holdsPartnershipEquity">
    <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/PartnershipEquity"/>
    <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/Partner"/>
    <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/owns"/>
    <skos:definition>the equity held by the partner in the partnership</skos:definition>
    <rdfs:label>holds partnership equity</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/setsOutEquityTermsIn">
    <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/EquityApportionmentTermsSet"/>
    <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/OrganizationCoveringAgreement"/>
    <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Agreements/Contracts/hasTerms"/>
    <skos:definition>has terms setting out the apportionment of equity in some formal business organization</skos:definition>
    <rdfs:label>sets out equity terms in</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/BE/Partnerships/Partnerships/dateTradingFrom">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <skos:definition>date on which the partnership started trading</skos:definition>
    <rdfs:label>date trading from</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

