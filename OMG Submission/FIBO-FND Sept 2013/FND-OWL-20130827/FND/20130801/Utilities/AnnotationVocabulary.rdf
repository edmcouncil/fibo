    <rdfs:label>explanatory note</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/termOrigin">
