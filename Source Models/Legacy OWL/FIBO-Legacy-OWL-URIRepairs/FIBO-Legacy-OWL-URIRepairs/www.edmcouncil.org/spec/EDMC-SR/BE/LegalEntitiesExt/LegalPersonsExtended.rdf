   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/LegalPersonsExtended/signatoryCapacityIsCapacityOf">
      <skos:definition>That entity of which the Signatory Capacity is a capacity, i.e. that entity upon which that capacity is incurred. This is always an adult human being (human being which is also a Legal Person).</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCapacities/isCapacityOf"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCapacities/SignatoryCapacity"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LegalPersons/NaturalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/LegalPersonsExtended/accruesTo">
      <skos:definition>That to which the liability ultimately unwinds, or to which it accrues.</skos:definition>
      <skos:editorialNote>This is by definition a Legal Entity in the sense used here, that is to say a legal entity is defined by the fact that it is the entity to which any and all liability ultimately unwinds, regardless of whether agreements have been entered into by some other entity or on behalf of some other person.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCapacities/isCapacityOf"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCapacities/LiabilityCapacity"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LegalPersons/LegalPerson"/>
   </owl:ObjectProperty>

