<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/contractualDeJureControlConferredBy">
      <skos:definition>Contractual Control is conferred by some contract in which one party agrees to confer some degree and type of control upon the other party to that contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCapacities/isConferredBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/ContractualControl"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/ControlRelatedContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/deJureControllingInterestPartyHasIdentity">
      <skos:definition>That which performs the role of De Jure Controlling Party.</skos:definition>
      <skos:editorialNote>This may be any form of potential owning party, that is a person, legal person, legitimate organization or formal organization (anything which is able to hold and own formal ownership instruments of some sort).</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyInRoleHasIdentity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/DeJureControllingInterestParty"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/IndependentParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/entityControllingPartyIdentity">
      <skos:definition>That which is able to be the Entity Controlling Party. Scope Note: It is assumed that since control follows from some form of ownership or contractual instrument, that the range of entities which may fulfil this party role is the same as that for entity ownership, namely "Involved Party"; that is, a logical union of natural persons, legal persons and formal organizations.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyInRoleHasIdentity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/EntityControllingParty"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/IndependentParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/generalPartnerExercises">
      <skos:definition>The General Partner exercises control as a result of holding General Partner equity, which is therefore a form of constitutional control of the entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/holdsSomeInstrumentOfDeJureControl"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Partnerships/Partnerships/GeneralPartner"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/ConstitutionalDeJureControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/exercisesMajorityOf">
      <skos:definition>Exercises, either alone or in equal degree with other such parties, majority De Facto Control over the entity in respect of which this party is defined.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/hasDegreeOfControl"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/MajorityControllingParty"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/DegreeOfControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/effectivelyExercises">
      <skos:definition>The Investment Owning Party effectively exercises some degree of de facto control as a result of their interest in the entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/holdsSomeMeansOfDeFactoControl"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/OwnershipParties/Investor"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/InvestmentBasedDeFactoControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/exercisesSomeDegreeOfControlOver">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/EntityControllingParty"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
      <owl:inverseOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/isControlledToSomeDegreeBy"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/hasDeFactoControllingInterestParty">
      <skos:definition>Some party which exercises some de facto control over the Formal Organization.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/hasControllingInterestParty"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/DeFactoControllingInterestParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/hasDeJureControllingInterestParty">
      <skos:definition>The Formal Organization is legally controlled in some way by some de jure controlling party, that is some entity which exercises legally based control.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/hasControllingInterestParty"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/ControlParties/DeJureControllingInterestParty"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/ControlPartiesExtras/ControlRelatedContract">
      <rdfs:label>Control Related Contract</rdfs:label>
      <skos:definition>A contract in which one party agrees to confer some degree and type of control upon the other party to that contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/Contract"/>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>