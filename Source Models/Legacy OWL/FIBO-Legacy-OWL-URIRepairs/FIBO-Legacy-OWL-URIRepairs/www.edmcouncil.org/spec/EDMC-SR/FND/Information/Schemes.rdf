   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/defines.2">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/defines.4"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/BusinessCalendarScheme"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/BusinessCalendarCode"/>
