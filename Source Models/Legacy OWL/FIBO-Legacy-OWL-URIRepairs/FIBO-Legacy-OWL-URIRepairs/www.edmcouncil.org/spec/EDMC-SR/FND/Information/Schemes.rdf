      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/classificationSchemeDefines"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/SectorClassificationScheme"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/OrganizationIndustrySectorClassification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/defines.1">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/classificationSchemeDefines"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/SectorClassificationScheme"/>
