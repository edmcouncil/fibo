   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/Class1">
      <rdfs:label>Class1</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/CountryCode">
      <rdfs:label>Country Code</rdfs:label>
      <skos:definition>A recognized, standardized code by which a country can be uniquely identified.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/SchematicCode"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/CountryCodeScheme">
      <rdfs:label>Country Code Scheme</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/Scheme"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/SectorClassificationScheme">
      <rdfs:label>Sector Classification Scheme</rdfs:label>
      <skos:definition>Scheme defining the structure and semantics of a set of classification codes which identify different industry sectors and (optionally) sub-sectors.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/ClassificationScheme"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/BusinessCalendarCode">
      <rdfs:label>Business Calendar Code</rdfs:label>
      <skos:definition>The Calendar for a particular business center, which defines what are the working days in that center.</skos:definition>
      <skos:editorialNote>This is the identification code for the calendar, not the calendar of dates itself. This code uniquely identifies the business calendar in which business days are defined. This is usually published year on year, but formal terms only need to identify the Calendar so that events are understood to be specified in terms of that calendar.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/BusinessCalendarScheme">
      <rdfs:label>Business Calendar Scheme</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/Classification">
      <rdfs:label>Classification</rdfs:label>
      <skos:definition>Some Classification of some Thing to identify what type of Thing it is (usually within a particular class of Things such as financial securities or organisations).</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/ClassificationScheme">
      <rdfs:label>Classification Scheme</rdfs:label>
      <skos:definition>The Scheme defining how the Classification is determined. This may be a URI or some other unique identification of the Scheme.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/CurrencyCode">
      <rdfs:label>Currency Code</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/CurrencyCodeScheme">
      <rdfs:label>Currency Code Scheme</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/RegistrationCode">
      <rdfs:label>Registration Code</rdfs:label>
      <skos:definition>Any code by which something in formally registered, for example for the purposes of legal identification.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/ContinuantThing"/>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Information"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/SchematicCode">
      <rdfs:label>Schematic Code</rdfs:label>
      <skos:definition>A code which is defined in some scheme as having some meaning.</skos:definition>
      <skos:editorialNote>It is assumed that, even though this may sometimes be defined in the relevant scheme as a numerical value, this can be treated universally as a text code. Otherwise each individual kind of code would have to have some definition of whether it is necessarily code or numeric, which makes no sense. Where schemes are numeric, this is treated as a text because its meaning is defined in a scheme and so is not in any sense a number.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/IndependentThing"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/Scheme">
      <rdfs:label>Scheme</rdfs:label>
      <skos:definition>The scheme by which the schematic code is identified.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>