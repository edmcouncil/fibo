<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/ProcessActorContactInformation">
      <rdfs:label>Process Actor Contact Information</rdfs:label>
      <skos:definition>Contact details as required for a contact person within the Legal Entity that is the Actor in a Business Process.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Information"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/BusinessProcess">
      <rdfs:label>Business Process</rdfs:label>
      <skos:definition>A set of activities carried out within some Business Context and in pursuance of some Business Goal as indicated by that context. A Business Activity consists of a number of Events and Activities, which have dependencies and activity flow between them.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Activities/Activity"/>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/MediatingThing"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/Decision">
      <rdfs:label>Decision</rdfs:label>
      <skos:definition>A Decision as understood in business process flow diagrams and models. This has a "Decision Point" leading into it, and two or more outcomes.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/ProcessEvent"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/ProcessActivity">
      <rdfs:label>Process Activity</rdfs:label>
      <skos:definition>An occurrence or activity defined in a formal Process, for example Process Step (Activity), Start and End.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/ProcessActor">
      <rdfs:label>Process Actor</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Activities/Actor"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/ProcessEnd">
      <rdfs:label>Process End</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Processes/ProcessEvent">
      <rdfs:label>Process Event</rdfs:label>
      <skos:definition>An occurrence or activity defined in a formal Process, for example Process Step (Activity), Start and End.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Events/Event"/>
   </owl:Class>

