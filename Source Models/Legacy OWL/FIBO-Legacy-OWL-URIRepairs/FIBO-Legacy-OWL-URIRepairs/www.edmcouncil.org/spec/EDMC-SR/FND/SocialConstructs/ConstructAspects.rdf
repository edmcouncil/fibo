<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/SocialConstructs/ConstructAspects/Anon-4839">
      <skos:definition>Any human being (adult or minor) or any legal entity.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/People/Person"/>
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LegalPersons/LegalPerson"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/SocialConstructs/ConstructAspects/CommitmentAspect">
      <rdfs:label>Commitment Aspect</rdfs:label>
