      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyInRoleHasIdentity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustFundManager"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LegalPersons/LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/fundUnitHolderIdentity">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyInRoleHasIdentity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/FundUnitHolder"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LegalPersons/LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/definesBeneficiary">
      <skos:definition>The party formally identified in the Trust Agreement as the Beneficiary.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgreementsExt/ContractsExtensions/hasParty"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustAgreement"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustBeneficiary"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/definesTrustorParty">
      <skos:definition>The party formally identified in the Trust Agreement as the Sponsor.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgreementsExt/ContractsExtensions/hasParty"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustAgreement"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/definesTrusteeParty">
      <skos:definition>The party formally identified in the Trust Agreement as the Trustee.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgreementsExt/ContractsExtensions/hasParty"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustAgreement"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustee"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/trustFundManagerManages">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/manages"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustFundManager"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/hasBeneficiary">
      <skos:definition>Some unit holder in the Trust Fund</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/hasPartyBeneficiary"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustFundTrust"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/FundUnitHolder"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/trusteeIdentity">
      <skos:definition>The entity which fulfills the role of the Trustee.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyInRoleHasIdentity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustee"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LegalPersons/LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/trusteeManages">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/manages"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustee"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/governedByTrustAgreement">
      <skos:definition>The agreement which governs the relationships among the principals in the Trust.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/formallyConstitutedOrganizationIsGovernedBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/hasFundManager">
      <skos:definition>The party which is the manager of the Trust Fund.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/isManagedBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustFundTrust"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustFundManager"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/hasPartyBeneficiary">
      <skos:definition>Has a Beneficiary of the Trust</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/hasPartyInRole"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustBeneficiary"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/hasPartySponsor">
      <skos:definition>Has a sponsor of the Trust.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/hasPartyInRole"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/hasPartyTrustee">
      <skos:definition>Has a Trustee of the Trust</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/hasPartyInRole"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustee"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/FundUnitHolder">
      <rdfs:label>Fund Unit holder</rdfs:label>
      <skos:definition>A party which holds some unit in the Trust Fund.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustBeneficiary"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustAgreement">
      <rdfs:label>Trust Agreement</rdfs:label>
      <skos:definition>The formal contract by which the Trust exists.</skos:definition>
      <skos:editorialNote>See also Deed. These are distinct from Contracts in that they impose obligations but without necessarily reciprocating rights.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/OrganizationCoveringAgreement"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustBeneficiary">
      <rdfs:label>Trust Beneficiary</rdfs:label>
      <skos:definition>The Beneficiary of the Trust</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormalBusinessOrganizationMember"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustFundManager">
      <rdfs:label>Trust Fund Manager</rdfs:label>
      <skos:definition>Trust Fund Manager acts on behalf of the Trustee to manage the assets of the Trust.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/TrustFundTrust">
      <rdfs:label>Trust Fund Trust</rdfs:label>
      <skos:definition>A trust which forms the basis for a fund.</skos:definition>
      <skos:editorialNote>The fund is identified as being a kind of Trust Fund.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustee">
      <rdfs:label>Trustee</rdfs:label>
      <skos:definition>The Trustee of the Trust</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgreementsExt/ContractsExtensions/ContractParty"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trustor">
      <rdfs:label>Trustor</rdfs:label>
      <skos:definition>The sponsor of the Trust.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/BE/Trusts/Trusts/Trust">
      <rdfs:label>Trust</rdfs:label>
      <skos:definition>A fiduciary relationship in which one party, known as a trustor, gives another party, the trustee, the right to hold title to property or assets for the benefit of a third party, the beneficiary.</skos:definition>
      <skos:editorialNote>This is a legal agreement between parties that someone owns, and is thereby an asset that they own. They can be taxed on this as any other asset. There are generally accepted things such as the source of funds that will determine who the revenue agency will go after. Definition reference URL: &lt;a href="http://www.investopedia.com/terms/t/trust.asp \l ixzz2MtESDTlP"&gt;&lt;font color="#0000ff"&gt;&lt;u&gt;http://www.investopedia.com/terms/t/trust.asp#ixzz2MtESDTlP&lt;/u&gt;&lt;/font&gt;&lt;/a&gt;</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
   </owl:Class>

   <!--Individuals-->

