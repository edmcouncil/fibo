<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/consistsOf">
      <skos:definition>The or a security which makes up the Security Holding.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/identity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/SecurityHolding"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/heldBy">
      <skos:definition>The holder of the security, that is the party of which this is the Security holding.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Ownership/isAssetOf"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/SecurityHolding"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/SecurityHolder"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/holder">
      <skos:definition>The party which holds the security and is entitled to the rights contractually defined in the security terms.</skos:definition>
      <skos:editorialNote>The holder is anonymous to the issuer and may transfer their holding in the open marketplace without reference to the issuer (subject to any holding restrictions set out for the Security either in the security contractual terms or in the laws of the applicable jurisdiction).</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/heldBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/Security"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/SecurityHolder"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/SecurityHolding">
      <rdfs:label>Security Holding</rdfs:label>
      <skos:definition>The holding of a (tradable) security in a portfolio.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Ownership/Asset"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/SecurityHolder">
      <rdfs:label>Security Holder</rdfs:label>
      <skos:definition>A party which holds a transferable contract such as a license or a security.</skos:definition>
      <skos:editorialNote>Known as Bearer. This party has the rights described in the security contract.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/TransferableContractHolder"/>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>