<rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/LOAN/Loans/LoansGeneral/IncomeVerificationMethodSelection">
      <skos:definition>Selection of possible methods by which borrower's income was determined.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self Certified</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self Certified Confirmation</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verified</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Non Verified</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description>
                                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Other</rdf:first>
                                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                       </rdf:Description>
                                    </rdf:rest>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/LOAN/Loans/LoansGeneral/LoanBorrowerRoleSelection">
