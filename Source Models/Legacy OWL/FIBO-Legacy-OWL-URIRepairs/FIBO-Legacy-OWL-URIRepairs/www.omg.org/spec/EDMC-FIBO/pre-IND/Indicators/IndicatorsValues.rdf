   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsValues/forYear">
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Time/Time/Year"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsValues/hasGDPAmount">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/TemporalConcepts/DatedMonetaryAmount"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsValues/hasPercentageValue">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/percentage"/>
   </owl:DatatypeProperty>

