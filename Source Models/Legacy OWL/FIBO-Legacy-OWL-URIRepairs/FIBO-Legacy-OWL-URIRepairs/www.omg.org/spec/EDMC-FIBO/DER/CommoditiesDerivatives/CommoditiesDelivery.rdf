<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/">
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/"/>
   </owl:Ontology>

   <!--Datatypes-->

<rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/BullionTypeSelection">
      <skos:definition>A type of bullion i.e. a metal which will be defined as a bullion commodity.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gold</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Silver</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Platinum</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/deliverTo">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/CommodityPhysicalDelivery"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/BusinessSite"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/bullionType">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/BullionTypeSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/totalPhysicalQuantity">
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/AgriculturalProductsDelivery">
      <rdfs:label>Agricultural Products Delivery</rdfs:label>
      <skos:definition>Delivery of any agricultural produce.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/CommodityPhysicalDelivery"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/BaseMetalsDelivery">
      <rdfs:label>Base Metals Delivery</rdfs:label>
      <skos:definition>Delivery of base metals.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/BullionPhysicalDelivery">
      <rdfs:label>Bullion Physical Delivery</rdfs:label>
      <skos:definition>Delivery of any precious metal.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/CommodityContractCashCloseout">
      <rdfs:label>Commodity Contract Cash Closeout</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/Forwards/CashCloseout"/>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/CommodityForwardDelivery"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/CommodityForwardDelivery">
      <rdfs:label>Commodity Forward Delivery</rdfs:label>
      <skos:definition>The agreed forward delivery of a commodity or cash equivalent.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/Forwards/ForwardDeliveryTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/CommodityPhysicalDelivery">
      <rdfs:label>Commodity Physical Delivery</rdfs:label>
      <skos:definition>The physical delivery of some commodity at some agreed point in the future.</skos:definition>
      <skos:editorialNote>Would be entered into by bullion houses and the like - people who specialze in commodities. They would have facilities for physical delivery of their primary commodities. THere are very different terms for delivery of these different kinds of commodity.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/EnergyFlowsDelivery">
      <rdfs:label>Energy Flows Delivery</rdfs:label>
      <skos:definition>Financial Transmission Rights.</skos:definition>
      <skos:editorialNote>This may or may not fit into the definition of a kid of delivery of the commodity.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/EnergyPhysicalDelivery"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/EnergyPhysicalDelivery">
      <rdfs:label>Energy Physical Delivery</rdfs:label>
      <skos:definition>Delivery of oil, gas and electricity.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/GasDelivery">
      <rdfs:label>Gas Delivery</rdfs:label>
      <skos:definition>Delivery of fuel gas.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/CommoditiesDerivatives/CommoditiesDelivery/OilDelivery">
      <rdfs:label>Oil Delivery</rdfs:label>
      <skos:definition>Delivery of oil.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>