      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCInstrumentContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/governs">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCDerivativeMasterAgreements/governs"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCInstrumentConfirmation"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCDerivatives/OTCDerivativeTransaction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/oTCInstrumentContractEmbodies">
      <skos:definition>The economic agreement (transaction related agreement) which the OTC Instrument Contract describes as obtaining between the parties to the Contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/economicContractEmbodies"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCInstrumentContract"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCTransactionAgreement"/>
   </owl:ObjectProperty>

