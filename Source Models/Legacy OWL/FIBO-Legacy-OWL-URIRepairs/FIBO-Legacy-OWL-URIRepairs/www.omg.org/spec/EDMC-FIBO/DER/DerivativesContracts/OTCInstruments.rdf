      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/DeliverCash"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/derivativeContractEmbodies">
      <skos:definition>The agreement which the contract describes as obtaining between the parties to the Derivatives Contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/economicContractEmbodies"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/DerivativesBasics/DerivativeContract"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/DerivativesBasics/DerivativeAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/oTCDerivativeContractEmbodies">
      <skos:definition>The agreement which the contract describes as obtaining between the parties to the OTC Derivatives Contract and transaction.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/oTCInstrumentContractEmbodies"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCDerivatives/OTCDerivativeContract"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCDerivativeAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/supersedes">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/supersedes"/>
