      <rdfs:label>Derivative Contract</rdfs:label>
      <skos:definition>An instrument which is derived in some way from some other instrument or other underlying financial asset or measure.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/EconomicContract"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/DerivativesBasics/DerivativeContractParty">
      <rdfs:label>Derivative Contract Party</rdfs:label>
      <skos:definition>A party to a Derivative contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/ContractualTransactionParty"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/DerivativesBasics/DerivativeDeliveryTermsSet">
      <rdfs:label>Derivative Delivery Terms Set</rdfs:label>
