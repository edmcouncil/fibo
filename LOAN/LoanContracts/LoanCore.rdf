		<rdfs:label>manual</rdfs:label>
		<skos:definition>Underwriting was performed using an automated underwriting system.</skos:definition>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_UnderwritingAutomation_manual">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;UnderwritingAutomation"/>
		<rdfs:label>manual</rdfs:label>
		<skos:definition>Underwriting was performed manually</skos:definition>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_UnderwritingDecision_approve">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;UnderwritingDecision"/>
