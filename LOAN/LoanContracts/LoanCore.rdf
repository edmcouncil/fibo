	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_PublicRecordCategory_marital">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;PublicRecordCategory"/>
		<rdfs:label>marital</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_PublicRecordCategory_personalProperty">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;PublicRecordCategory"/>
		<rdfs:label>personal property</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_PublicRecordCategory_realEstate">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;PublicRecordCategory"/>
		<rdfs:label>real estate</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_UnderwritingAutomation_automatic">
