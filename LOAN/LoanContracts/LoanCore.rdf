	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_LoanMarketCategory_Commercial">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;LoanMarketCategory"/>
		<rdfs:label>commercial loan</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_LoanMarketCategory_Construction">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;LoanMarketCategory"/>
		<rdfs:label>construction loan</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_LoanMarketCategory_Education">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;LoanMarketCategory"/>
		<rdfs:label>education loan</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_LoanMarketCategory_Mortgage">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;LoanMarketCategory"/>
		<rdfs:label>mortgage loan</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_LoanMarketCategory_Personal">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;LoanMarketCategory"/>
		<rdfs:label>personal loan</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_LoanMarketCategory_SmallBusiness">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;LoanMarketCategory"/>
		<rdfs:label>small business loan</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote>This is related to the mortgage loan purpose: &apos;business or commercial&apos;.</fibo-fnd-utl-av:explanatoryNote>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_ManufacturedHomeLegalClassification_personalProperty">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;ManufacturedHomeLegalClassification"/>
		<rdfs:label>personal property</rdfs:label>
		<skos:definition>the covered loan is secured by a manufactured home but not land</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>the 2015 Revised HMDA regulation.</fibo-fnd-utl-av:adaptedFrom>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_ManufacturedHomeLegalClassification_realProperty">
