		<rdf:type rdf:resource="&fibo-loan-ln-ln;UnderwritingDecision"/>
		<rdfs:label>deny</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-ln;_UnderwritingDecision_eligible">
		<rdf:type rdf:resource="&fibo-loan-ln-ln;UnderwritingDecision"/>
