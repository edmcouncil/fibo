	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-hmda;_HMDA_Disposition_LoanOriginated">
		<rdf:type rdf:resource="&fibo-loan-ln-hmda;HMDA_Disposition"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-hmda;_HMDA_Disposition_LoanPurchasedByYourInstitution">
		<rdf:type rdf:resource="&fibo-loan-ln-hmda;HMDA_Disposition"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-hmda;_HMDA_Disposition_PreapprovalRequestApprovedButNotAccepted">
		<rdf:type rdf:resource="&fibo-loan-ln-hmda;HMDA_Disposition"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-hmda;_HMDA_Disposition_PreapprovalRequestDenied">
		<rdf:type rdf:resource="&fibo-loan-ln-hmda;HMDA_Disposition"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-hmda;_HowSubmitted_directly">
		<rdf:type rdf:resource="&fibo-loan-ln-hmda;HowSubmitted"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-loan-ln-hmda;_HowSubmitted_notDirectly">
		<rdf:type rdf:resource="&fibo-loan-ln-hmda;HowSubmitted"/>
	</owl:NamedIndividual>
	
	<owl:ObjectProperty rdf:about="&fibo-loan-ln-hmda;hasHMDA_DispositionDate">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label>HMDA disposition date</rdfs:label>
		<skos:definition>the date associated with the HMDA Disposition for a HMDA Covered Loan</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>