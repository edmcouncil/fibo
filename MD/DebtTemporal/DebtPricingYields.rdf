			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-md-dbt-py;priceDeterminationMethod"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-utl-val;TextValue"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-md-dbt-py;priceIsTraded"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-utl-val;TrueFalseValue"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-md-dbt-py;takesAccountOfQuoteSize"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-utl-val;IntegerValue"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">bond exchange high price</rdfs:label>
		<skos:definition
			xml:lang="en">The highest valuation over the period specified.</skos:definition>
		<skos:editorialNote
			xml:lang="en">This can be regarded as a derived price. See also note in LowPrice Difference: different rules used. May be mased on trade OR quote price Analytic or Price?? this is a derived type but with a much simpler rule.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-md-dbt-py;BondExchangeLastPrice">
		<rdfs:subClassOf
			rdf:resource="&fibo-md-dbt-py;ExchangeTradedBondPrice"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-md-dbt-py;priceIsTraded"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-utl-val;TrueFalseValue"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">bond exchange last price</rdfs:label>
		<skos:definition
			xml:lang="en">The last (or most recent) valuation.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">This is an actual trade price not a derived price</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-md-dbt-py;BondExchangeLowPrice">
		<rdfs:subClassOf
			rdf:resource="&fibo-md-dbt-py;ExchangeTradedBondPrice"/>
		<rdfs:subClassOf>
			<owl:Restriction>
