		rdf:about="&fibo-md-tem-ins;PriceSpread">
		<rdfs:subClassOf
			rdf:resource="&fibo-md-tem-ins;Spread"/>
		<rdfs:label
			xml:lang="en">price spread</rdfs:label>
		<skos:definition
			xml:lang="en">The difference between two prices.</skos:definition>
	</owl:Class>
	
	<owl:Class
