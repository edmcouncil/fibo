		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-md-tem-ins;Yield"/>
				<owl:onProperty
					rdf:resource="&fibo-md-tem-tem;isBetween"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">yield spread</rdfs:label>
		<skos:definition
			xml:lang="en">The spread between the yields of two items.</skos:definition>
	</owl:Class>
	
