	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/MD/TemporalCore/AnalysisCurves/">
		<rdfs:label
			xml:lang="en">AnalysisCurves</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Math/Math/"/>
