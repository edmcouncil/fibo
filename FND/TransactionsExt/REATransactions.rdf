		<owl:inverseOf rdf:resource="&fibo-fnd-rel-rel;governs"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;transactionEventFollowsBusinessProcess">
