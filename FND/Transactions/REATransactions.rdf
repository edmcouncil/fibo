	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Transactions/REATransactions/">
		<rdfs:label
			xml:lang="en">REATransactions</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/LEIEntities/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Agreements/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
