		<rdfs:subClassOf rdf:resource="&fibo-fnd-gao-obj;BusinessStrategy"/>
		<rdfs:label>sales strategy</rdfs:label>
		<skos:definition>strategy for selling something, including allocating resources to the sales process, reducing costs, and optimizing revenues</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fnd-gao-obj;Strategy">
