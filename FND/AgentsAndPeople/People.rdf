		<rdfs:domain rdf:resource="&fibo-fnd-aap-ppl;IdentityDocument"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>verifies the person&apos;s date of birth</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&fibo-fnd-aap-ppl;verifiesPlaceOfBirth">
		<rdfs:label>verifies place of birth</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fnd-aap-ppl;IdentityDocument"/>
		<rdfs:range rdf:resource="&xsd;string"/>
		<skos:definition>verifies the person&apos;s place of birth</skos:definition>
	</owl:DatatypeProperty>

</rdf:RDF>