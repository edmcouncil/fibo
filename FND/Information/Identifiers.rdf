<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-aap-agt "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/">
	<!ENTITY fibo-fnd-inf-idx "https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/">
	<!ENTITY fibo-fnd-inf-sch "https://spec.edmcouncil.org/fibo/FND/Information/Schemes/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-val "https://spec.edmcouncil.org/fibo/FND/Utilities/Values/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/"
	xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-inf-idx="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/"
	xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/">
		<rdfs:label
			xml:lang="en">Identifiers</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idx;IdentificationScheme">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;Scheme"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;defines.1"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-idx;Identifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">identification scheme</rdfs:label>
		<skos:definition
			xml:lang="en">A formal scheme defining identification codes for some purpose.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idx;Identifier">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;SchematicCode"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idx;uniquelyIdentifies"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;definedIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-idx;IdentificationScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">identifier</rdfs:label>
		<skos:definition
			xml:lang="en">A textual code which uniquely identifies something.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idx;IndependentThing">
		<rdfs:label
			xml:lang="en">independent thing</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idx;hasIdentifier">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
		<rdfs:label
			xml:lang="en">has identifier</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-idx;Identifier"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idx;hasIdentifierCode">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-inf-sch;code"/>
		<rdfs:label
			xml:lang="en">has identifier code</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-idx;Identifier"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-val;TextValue"/>
		<skos:definition
			xml:lang="en">The textual value of the unique identification code. Editorial Note: Renamed to match the property in FIBO Foundations Relations ontology.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idx;uniquelyIdentifies">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-aap-agt;identifies"/>
		<rdfs:label
			xml:lang="en">uniquely identifies</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-idx;Identifier"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>

</rdf:RDF>