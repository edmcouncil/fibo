<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-aap-agt "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/">
	<!ENTITY fibo-fnd-inf-idx "https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/">
	<!ENTITY fibo-fnd-inf-iid "https://spec.edmcouncil.org/fibo/FND/Information/ISO3166Identifiers/">
	<!ENTITY fibo-fnd-inf-sch "https://spec.edmcouncil.org/fibo/FND/Information/Schemes/">
	<!ENTITY fibo-fnd-plc-cty "https://spec.edmcouncil.org/fibo/FND/Places/Countries/">
	<!ENTITY fibo-fnd-pub-con "https://spec.edmcouncil.org/fibo/FND/Publications/Conventions/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Information/ISO3166Identifiers/"
	xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-inf-idx="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/"
	xmlns:fibo-fnd-inf-iid="https://spec.edmcouncil.org/fibo/FND/Information/ISO3166Identifiers/"
	xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"
	xmlns:fibo-fnd-plc-cty="https://spec.edmcouncil.org/fibo/FND/Places/Countries/"
	xmlns:fibo-fnd-pub-con="https://spec.edmcouncil.org/fibo/FND/Publications/Conventions/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/ISO3166Identifiers/">
		<rdfs:label
			xml:lang="en">ISO3166Identifiers</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Countries/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Publications/Conventions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-iid;Country">
		<rdfs:label
			xml:lang="en">country</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-iid;ISO3LetterCountryCodeScheme">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;CountryCodeScheme"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pub-con;ISOStandard"/>
		<rdfs:label
			xml:lang="en">i s o 3 letter country code scheme</rdfs:label>
		<skos:definition
			xml:lang="en">[add reference of scheme as ISO Standard plus sub-set]</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-iid;ISOCountryCode">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;CountryCode"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-iid;definedIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-iid;ISO3LetterCountryCodeScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idx;uniquelyIdentifies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-plc-cty;Country"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">i s o country code</rdfs:label>
		<skos:definition
			xml:lang="en">The country code standard defined by ISO as ISO 3166. Note that this comes in several defined formats. For dta management purposes the precise format would need to be identified.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-iid;definedIn">
		<rdfs:label
			xml:lang="en">defined in</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-iid;ISOCountryCode"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-iid;ISO3LetterCountryCodeScheme"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-iid;identifier">
		<rdfs:label
			xml:lang="en">identifier</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-iid;Country"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-iid;ISOCountryCode"/>
	</owl:ObjectProperty>
	
	<rdf:Description
		rdf:about="&fibo-fnd-plc-cty;Country">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-inf-iid;Country"/>
	</rdf:Description>

</rdf:RDF>