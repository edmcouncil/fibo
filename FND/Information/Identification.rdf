<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-inf-idn "https://spec.edmcouncil.org/fibo/FND/Information/Identification/">
	<!ENTITY fibo-fnd-inf-idx "https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/">
	<!ENTITY fibo-fnd-inf-sch "https://spec.edmcouncil.org/fibo/FND/Information/Schemes/">
	<!ENTITY fibo-fnd-pty-pty "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Information/Identification/"
	xmlns:fibo-fnd-inf-idn="https://spec.edmcouncil.org/fibo/FND/Information/Identification/"
	xmlns:fibo-fnd-inf-idx="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/"
	xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"
	xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Identification/">
		<rdfs:label
			xml:lang="en">Identification</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Identifiers/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idn;Identification">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idn;isIdentificationOf"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">identification</rdfs:label>
		<skos:definition
			xml:lang="en">The context of identification.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">This the context whereby some things are identified by some means. As a context it brings together the means of identification and the things so identified, as well as possibly other factors such as the reasons for such identification. This is not a term that is expected to be used in applications.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idn;IdentifierIdentification">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-idn;SchematicCodeIdentification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idn;appliesCode"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-idx;Identifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idn;appliesToScheme"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-idx;IdentificationScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">identifier identification</rdfs:label>
		<skos:definition
			xml:lang="en">Identification of the scheme under which an Identifier is defined.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idn;PartyAndEntityIdentification">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-idn;Identification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idn;identifiesUsing"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-idx;Identifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">party and entity identification</rdfs:label>
		<skos:definition
			xml:lang="en">The identification of some thing by some means of identity.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">The word &apos;entity&apos; here does not refer to business entities but to any kind of thing at all - to be distinguished only from the identification of schematic codes themselves.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idn;PartyIdentification">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-idn;PartyAndEntityIdentification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idn;isPartyIdentificationOf"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">party identification</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-idn;SchematicCodeIdentification">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-idn;Identification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idn;appliesCode"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-sch;SchematicCode"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-idn;appliesToScheme"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-sch;Scheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">schematic code identification</rdfs:label>
		<skos:definition
			xml:lang="en">The identification of some type of schematic code (such as an identifier) b some means.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idn;appliesCode">
		<rdfs:label
			xml:lang="en">applies code</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-idn;SchematicCodeIdentification"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-sch;SchematicCode"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idn;appliesToScheme">
		<rdfs:label
			xml:lang="en">applies to scheme</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-idn;SchematicCodeIdentification"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-sch;Scheme"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idn;identifiesUsing">
		<rdfs:label
			xml:lang="en">identifies using</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-idn;PartyAndEntityIdentification"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-idx;Identifier"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idn;isIdentificationOf">
		<rdfs:label
			xml:lang="en">is identification of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-idn;Identification"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
		<skos:definition
			xml:lang="en">That to which the identification applies - that is, whatever it is that is so identified.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-idn;isPartyIdentificationOf">
		<rdfs:label
			xml:lang="en">is identification of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-idn;PartyIdentification"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<skos:definition
			xml:lang="en">Some Party as identified in the Party Identification.</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>