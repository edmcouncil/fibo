<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-inf-inf "https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/">
	<!ENTITY fibo-fnd-inf-lan "https://spec.edmcouncil.org/fibo/FND/Information/Languages/">
	<!ENTITY fibo-fnd-inf-sch "https://spec.edmcouncil.org/fibo/FND/Information/Schemes/">
	<!ENTITY fibo-fnd-plcx-gph "https://spec.edmcouncil.org/fibo/FND/PlacesExt/Geophysical/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Information/Languages/"
	xmlns:fibo-fnd-inf-inf="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"
	xmlns:fibo-fnd-inf-lan="https://spec.edmcouncil.org/fibo/FND/Information/Languages/"
	xmlns:fibo-fnd-inf-sch="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"
	xmlns:fibo-fnd-plcx-gph="https://spec.edmcouncil.org/fibo/FND/PlacesExt/Geophysical/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Information/Languages/">
		<rdfs:label
			xml:lang="en">Languages</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/InfoCore/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Information/Schemes/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/PlacesExt/Geophysical/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-lan;Language">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-lan;spokenIn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-plcx-gph;GeographicalArea"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">language</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-lan;LanguageCode">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;SchematicCode"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-lan;identifies"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-lan;Language"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">language code</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-lan;LanguageCodeScheme">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-sch;Scheme"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-inf-sch;defines.1"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-inf-lan;LanguageCode"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">language code scheme</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-inf-lan;LanguageName">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-inf-inf;Name"/>
		<rdfs:label
			xml:lang="en">language name</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-lan;hasLanguageName">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-inf-inf;hasName"/>
		<rdfs:label
			xml:lang="en">has language name</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-lan;Language"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-lan;LanguageName"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-lan;identifies">
		<rdfs:label
			xml:lang="en">identifies</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-lan;LanguageCode"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-inf-lan;Language"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-inf-lan;spokenIn">
		<rdfs:label
			xml:lang="en">spoken in</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-inf-lan;Language"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-plcx-gph;GeographicalArea"/>
	</owl:ObjectProperty>

</rdf:RDF>