<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-acc-cur "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/">
	<!ENTITY fibo-fnd-arrx-qua "https://spec.edmcouncil.org/fibo/FND/Arrangements/Quantities/">
	<!ENTITY fibo-fnd-occ-occ "https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/">
	<!ENTITY fibo-fnd-qt-qtu "https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-val "https://spec.edmcouncil.org/fibo/FND/Utilities/Values/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY fibo-uo-plh "https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Arrangements/Quantities/"
	xmlns:fibo-fnd-acc-cur="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
	xmlns:fibo-fnd-arrx-qua="https://spec.edmcouncil.org/fibo/FND/Arrangements/Quantities/"
	xmlns:fibo-fnd-occ-occ="https://spec.edmcouncil.org/fibo/FND/Occurrent/Occurrents/"
	xmlns:fibo-fnd-qt-qtu="https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:fibo-uo-plh="https://spec.edmcouncil.org/fibo/UO/PlatonicAndHomogeneity/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Arrangements/Quantities/">
		<rdfs:label
			xml:lang="en">Quantities</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;Barrel">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-qua;ReferenceMaterial"/>
		<rdfs:label
			xml:lang="en">Barrel</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;DiscountedValue">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-qua;MeasuredValue"/>
		<rdfs:label
			xml:lang="en">Discounted Value</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;MeasurableQuantity">
		<rdfs:label
			xml:lang="en">Measurable Quantity</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;MeasuredValue">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-qt-qtu;QuantityValue"/>
		<rdfs:label
			xml:lang="en">Measured Value</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;MeasurementProcedure">
		<rdfs:label
			xml:lang="en">Measurement Procedure</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;MonetaryReference">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-qua;QuantityReference"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;Currency"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-arrx-qua;takesReferenceForm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">Monetary Reference</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;MonetaryValuationProcedure">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-qua;MeasurementProcedure"/>
		<rdfs:label
			xml:lang="en">Monetary Valuation Procedure</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;QuantityReference">
		<rdfs:label
			xml:lang="en">Quantity Reference</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;QuantityReferent">
		<rdfs:label
			xml:lang="en">Quantity Referent</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-fnd-arrx-qua;ReferenceMaterial"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-arrx-qua;Unit"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-fnd-arrx-qua;UnitaryValue"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;ReferenceMaterial">
		<rdfs:label
			xml:lang="en">Reference Material</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-arrx-qua;Unit"/>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-arrx-qua;UnitaryValue"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;Unit">
		<rdfs:label
			xml:lang="en">Unit</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-arrx-qua;UnitaryValue"/>
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-arrx-qua;UnitaryValue">
		<rdfs:label
			xml:lang="en">Unitary Value</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-qua;mayBeTreatedAs">
		<rdfs:label
			xml:lang="en">may be treated as</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-qua;ReferenceMaterial"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-qua;Unit"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-qua;resultsIn">
		<rdfs:label
			xml:lang="en">results in</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-qua;MeasurementProcedure"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-qua;UnitaryValue"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-qua;resultsInMeasuredValue">
		<rdfs:label
			xml:lang="en">results in measured value</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-qua;MonetaryValuationProcedure"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-qua;MeasuredValue"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-arrx-qua;takesReferenceForm">
		<rdfs:label
			xml:lang="en">takes reference form</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-arrx-qua;QuantityReference"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-arrx-qua;QuantityReferent"/>
	</owl:ObjectProperty>

</rdf:RDF>