	xml:base="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/PaymentsAndSchedules/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-fnd-acc-cur="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
	xmlns:fibo-fnd-agr-agr="https://spec.edmcouncil.org/fibo/FND/Agreements/Agreements/"
	xmlns:fibo-fnd-agr-ctr="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-dt-oc="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/"
	xmlns:fibo-fnd-law-lcap="https://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/"
	xmlns:fibo-fnd-pas-pas="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/"
	xmlns:fibo-fnd-pas-psch="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/PaymentsAndSchedules/"
	xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"
	xmlns:fibo-fnd-pty-rl="https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/PaymentsAndSchedules/">
		<rdfs:label>Payments and Schedules Ontology</rdfs:label>
		<dct:license
			rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright>Copyright (c) 2015-2016 EDM Council, Inc.
Copyright (c) 2015-2016 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Agreements/Agreements/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Parties/Parties/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Parties/Roles/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Relations/Relations/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/</sm:dependsOn>
		<sm:fileAbbreviation>fibo-fnd-pas-psch</sm:fileAbbreviation>
		<sm:fileAbstract>This ontology defines basic concepts such as payment, payee, payer, and payment schedule, extending the scheduling concepts from the Dates and Times module, among others.</sm:fileAbstract>
		<sm:filename>PaymentsAndSchedules.rdf</sm:filename>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/AboutFND/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/AboutProductsAndServices/</rdfs:seeAlso>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Agreements/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:versionIRI
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/20160201/ProductsAndServices/PaymentsAndSchedules/"/>
		<skos:changeNote>The https://spec.edmcouncil.org/fibo/FND/20150801/ProductsAndServices/PaymentsAndSchedules.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.1 RTF report to replace MoneyAmount with MonetaryAmount.</skos:changeNote>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-psch;Payee">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:minQualifiedCardinality
							rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
						<owl:onClass
							rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
						<owl:onProperty
							rdf:resource="&fibo-fnd-pty-pty;isAPartyTo"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payee</rdfs:label>
		<skos:definition>a party to whom a debt should be paid, or to whose order a bill of exchange, note, or check is made payable</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-psch;Payer">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-psch;hasObligation"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payer</rdfs:label>
		<skos:definition>a party who pays a bill or fees</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-psch;Payment">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-oc;Occurrence"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-psch;hasPaymentAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-psch;fulfillsObligation"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-pas-psch;Payee"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-pas-psch;Payer"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payment</rdfs:label>
		<skos:definition>delivery of money in fulfillment of an obligation, such as to satisfy a claim or debt</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-psch;PaymentEvent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pas-pas;TransactionEvent"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-psch;Payment"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;involves"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payment event</rdfs:label>
		<skos:definition>an event that involves delivery of money in fulfillment of an obligation</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-psch;PaymentObligation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-agr;Commitment"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;Duty"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;isConferredBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:allValuesFrom
					rdf:resource="&fibo-fnd-pas-psch;Payer"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pas-psch;isObligationOf"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payment obligation</rdfs:label>
		<skos:definition>a legally enforceable duty to pay a sum of money, or agree to do something (or not to do something), according to the terms stated in a contract</skos:definition>
		<skos:example>the duty of a borrower to repay a loan, and the legal right of a lender to enforce payment</skos:example>
		<fibo-fnd-utl-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-pas-psch;PaymentSchedule">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-fd;Schedule"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-pas-psch;PaymentEvent"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payment schedule</rdfs:label>
		<skos:definition>schedule for delivery of money in fulfillment of an obligation, such as a coupon payment schedule, loan payment schedule, interest payment schedule</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-pas-psch;fulfillsObligation">
		<rdfs:label>fulfills obligation</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-pas-psch;Payment"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
		<skos:definition>satisfies a requirement for payment of some claim, debt, or other obligation</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-pas-psch;hasObligation">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has obligation</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-pas-psch;Payer"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
		<owl:inverseOf
			rdf:resource="&fibo-fnd-pas-psch;isObligationOf"/>
		<skos:definition>identifies a duty or obligation that a given party has taken on</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-pas-psch;hasPaymentAmount">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has payment amount</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-pas-psch;Payment"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<skos:definition>specifies the amount of money involved in a payment</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-pas-psch;isObligationOf">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;isConferredOn"/>
		<rdfs:label>is obligation of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-pas-psch;Payer"/>
		<skos:definition>identifies a party that has incurred a given obligation</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>