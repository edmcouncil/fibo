<!DOCTYPE rdf:RDF [
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY fibo-fnd-aap-agt "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/">
	<!ENTITY fibo-fnd-plc-adr "https://spec.edmcouncil.org/fibo/FND/Places/Addresses/">
	<!ENTITY fibo-fnd-plc-loc "https://spec.edmcouncil.org/fibo/FND/Places/Locations/">
	<!ENTITY fibo-fnd-plc-vrt "https://spec.edmcouncil.org/fibo/FND/Places/VirtualPlaces/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-bt "https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Places/VirtualPlaces/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-fnd-aap-agt="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"
	xmlns:fibo-fnd-plc-adr="https://spec.edmcouncil.org/fibo/FND/Places/Addresses/"
	xmlns:fibo-fnd-plc-loc="https://spec.edmcouncil.org/fibo/FND/Places/Locations/"
	xmlns:fibo-fnd-plc-vrt="https://spec.edmcouncil.org/fibo/FND/Places/VirtualPlaces/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-bt="https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Places/VirtualPlaces/">
		<rdfs:label>Virtual Places Ontology</rdfs:label>
		<dct:license
			rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright>Copyright (c) 2014 EDM Council, Inc.
Copyright (c) 2014 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Places/Addresses/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Places/Locations/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Relations/Relations/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/</sm:dependsOn>
		<sm:fileAbbreviation>fibo-fnd-plc-vrt</sm:fileAbbreviation>
		<sm:fileAbstract>This ontology provides scaffolding for use in describing virtual location-oriented concepts.</sm:fileAbstract>
		<sm:filename>VirtualPlaces.rdf</sm:filename>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/AboutFND/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/FND/Places/AboutPlaces/</rdfs:seeAlso>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Addresses/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Locations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"/>
		<owl:versionIRI
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/20141101/Places/VirtualPlaces/"/>
		<skos:changeNote>This ontology was added to Foundations in advance of the December 2014 Long Beach meeting in support of the SEC specification.</skos:changeNote>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-plc-vrt;NetworkLocation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-plc-vrt;VirtualLocation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onClass
					rdf:resource="&fibo-fnd-plc-adr;Address"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
				<owl:qualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>network location</rdfs:label>
		<skos:definition>A network address is location in a telecommunications network that may be identified by a network address (an identifier for a node or interface)</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-plc-vrt;NotionalPlace">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-plc-loc;Location"/>
		<rdfs:label>notional place</rdfs:label>
		<skos:definition>A place described in terms of some abstract description or as a list of commonly understood concepts such as domestic, Eurozone etc.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-plc-vrt;VirtualLocation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-plc-loc;Location"/>
		<rdfs:label>virtual location</rdfs:label>
		<skos:definition>A place which has no physical location.</skos:definition>
	</owl:Class>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fnd-plc-vrt;hasURL">
		<rdfs:label>has url</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;URI"/>
		<skos:altLabel>has uniform resource locator</skos:altLabel>
		<skos:prefLabel>has URL</skos:prefLabel>
	</owl:DatatypeProperty>

</rdf:RDF>