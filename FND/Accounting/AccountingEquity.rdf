    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-acc-aeq;representsAnInterestIn">
        <rdfs:label>represents an interest in</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Equity always represents an interest in some business organization. This is the organization, company or venture in which the holder of the equity has a stake in by virtue of holding that equity</skos:definition>
    </owl:ObjectProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;Capital">
        <rdfs:label>capital</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-oac-own;takesForm"/>
                <owl:allValuesFrom rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">financial capital, which represents obligations, and is liquidated as money for trade, and owned by legal entities</skos:definition>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Capital_(economics)</fibo-fnd-utl-av:definitionOrigin>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">Financial capital is in the form of capital assets, traded in financial markets. Its market value is not based on the historical accumulation of money invested but on the perception by the market of its expected revenues and of the risk entailed.</fibo-fnd-utl-av:explanatoryNote>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;CapitalSurplus">
        <rdfs:label>capital surplus</rdfs:label>
        <skos:prefLabel rdf:datatype="&xsd;string">capital surplus</skos:prefLabel>
        <skos:altLabel rdf:datatype="&xsd;string">additional paid in capital</skos:altLabel>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-aeq;Equity"/>
        <skos:definition rdf:datatype="&xsd;string">Capital surplus is that amount which a firm raises in excess of the par value (nominal value) of the shares (common stock).</skos:definition>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Additional_paid_in_capital</fibo-fnd-utl-av:definitionOrigin>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">Capital surplus is a term that frequently appears as a balance sheet item as a component of shareholders equity.</fibo-fnd-utl-av:explanatoryNote>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;Equity">
        <rdfs:label>equity</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-acc-aeq;representsAnInterestIn"/>
                <owl:onClass rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-oac-own;takesForm"/>
                <owl:allValuesFrom rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">the value of an ownership interest in property, including shareholders equity in a business</skos:definition>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Equity</fibo-fnd-utl-av:definitionOrigin>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;FinancialAsset">
        <rdfs:label>financial asset</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">An asset consisting of one or more financial instruments, treated as an asset</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-own;Asset"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;IssuedEquity">
        <rdfs:label>issued equity</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">externally-held stockholders equity that may be transferred from one party to another</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-aeq;StockholdersEquity"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;OwnersEquity">
        <rdfs:label>owners equity</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-aeq;Equity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-acc-aeq;CapitalSurplus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
                <owl:someValuesFrom rdf:resource="&fibo-fnd-acc-aeq;StockholdersEquity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">Equity owned in the entity as recorded on the books of that entity.</skos:definition>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;RetainedEarnings">
        <rdfs:label>retained earnings</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the portion of net income which is retained by the corporation rather than distributed to its owners as dividends</skos:definition>
        <fibo-fnd-utl-av:definitionOrigin rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Retained_earnings</fibo-fnd-utl-av:definitionOrigin>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">If the corporation takes a loss, then that loss is retained and called variously retained losses, accumulated losses or accumulated deficit. Retained earnings and losses are cumulative from year to year with losses offsetting earnings.</fibo-fnd-utl-av:explanatoryNote>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-aeq;Equity"/>
    </owl:Class>

    <owl:Class rdf:about="&fibo-fnd-acc-aeq;StockholdersEquity">
        <rdfs:label>stockholders' equity</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">equity held in an entity by stockholders</skos:definition>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-aeq;Equity"/>
        <skos:editorialNote rdf:datatype="&xsd;string">When total assets are greater than total liabilities, stockholders have a positive equity (positive book value). Conversely, when total liabilities are greater than total assets, stockholders have a negative stockholders equity (negative book value, also sometimes called stockholders deficit.</skos:editorialNote>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Retained_earnings</fibo-fnd-utl-av:adaptedFrom>
        <skos:editorialNote rdf:datatype="&xsd;string">paid in capital, donated capital, and retained earnings less the liabilities of a corporation (Barron's)</skos:editorialNote>
    </owl:Class>

</rdf:RDF>