   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/FND/Accounting/AccountingEquity/">
        <rdfs:label>Accounting Equity Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the FIBO Accounting Equity Ontology -->

