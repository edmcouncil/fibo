		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/MD/TemporalCore/TemporalConcepts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtInstruments/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Accreting">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;AmortizationType"/>
		<rdfs:label
			xml:lang="en">accreting</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Accrual">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;appliesTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;Interest"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">accrual</rdfs:label>
		<skos:definition
			xml:lang="en">The build-up of interest over a period of time.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">There are legal contractual terms for the accrual of interest, as distinct from the payment of interest. Action: Add this.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Amortization">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;isAmortizationOf"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;DebtFinance"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">amortization</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;AmortizationType">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arrx-meth;Method"/>
		<rdfs:label
			xml:lang="en">amortization type</rdfs:label>
		<skos:definition
			xml:lang="en">The different types of principal payments applicable to a debt security.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Borrower">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;Debtor"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;partyTo.1"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;DebtContractTermsSet"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-pty;hasPartyIdentity"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">borrower</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Collateral">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-acc;takesForm"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-oac-own;Asset"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">collateral</rdfs:label>
		<skos:definition
			xml:lang="en">Assets pledged in the provision of debt or loan products or securities.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">See also &quot;Loan collateral&quot; which is defined specifically in the context of an individual loan or mortage. These terms may be duplicates, depending on whether there is some more abstract meaning for collateralization of securitized debt.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Creditor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;isOwed"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;MoniesOwing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">creditor</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;DebtContractTermsSet">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
		<rdfs:label
			xml:lang="en">debt terms</rdfs:label>
		<skos:definition
			xml:lang="en">Terms setting out the formal rights and obligations of borrower and lender under a contract in which funds are lent from the one party to the other.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">These may be terms in a Loan Contract (incliding for example Mportage ContracT) or they may be the contractual terms of a Debt security.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;DebtFinance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;MoniesOwing"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-ptyx-prc;RelationshipContext"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:maxQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-fnd-acc-dtt;Collateral"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;collateralizedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;hasBorrowingParty"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;Borrower"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;hasLendingParty"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;Lender"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;hasfibo-fnd-acc-dtt"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;DebtContractTermsSet"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">debt finance</rdfs:label>
		<skos:definition
			xml:lang="en">Funds loaned or extended from one party to another for some purpose. Further notes: This is a loan or a debt security principal amount. This term is distinguished from &quot;Debt&quot; or &quot;Credit&quot; in that the latter are balances at a point in time; however it is also referred to as debt for example in debt securities, since a loaned or securitized amount constitutes a debt when it is advanced (and ceases to constitute a debt when it is paid off). As such, this usually has interest payable and may have collateral. In Islamic products there is no interest. This may be a loan (realized as a draw-down on a credit facillity) or it may be a debt set up as part of the issuance of a debt security.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;DebtInterest">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;Interest"/>
		<rdfs:label
			xml:lang="en">debt interest</rdfs:label>
		<skos:definition
			xml:lang="en">An amount payable by a debtor to a creditor in consideration of a current debt amount at a given point in time.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;DebtInterestPaymentTermsSet">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;DebtContractTermsSet"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;governsPaymentOf"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;Interest"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">debt interest payment terms set</rdfs:label>
		<skos:definition
			xml:lang="en">Terms for payment of Interest on any Debt.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">Interest is usually payable on any outstanding principal amount, therefore Interest itself relates to the amount of debt at any given point of time, not to the principal amount advanced at the time that the loan was advanced or the debt security issued.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;DebtPrincipalRepaymentTermsSet">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;DebtContractTermsSet"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:maxQualifiedCardinality
					rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
				<owl:onClass
					rdf:resource="&fibo-sec-dbt-dbt-dbt;DayCountBasis"/>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;amortizationCalculationsDayCountConvention"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">debt principal repayment terms set</rdfs:label>
		<skos:definition
			xml:lang="en">Terms for repayment on the principal of any debt.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Debtor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;owes"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;MoniesOwing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">debtor</rdfs:label>
		<skos:definition
			xml:lang="en">The party which owes the debt.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Interest">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;isInterestOn"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;MoniesOwing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">interest</rdfs:label>
		<skos:definition
			xml:lang="en">An amount payable by a debtor to a creditor in consideration of the credit extended to the debtor.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;InterestOnly">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;AmortizationType"/>
		<rdfs:label
			xml:lang="en">interest only</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;Lender">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;Creditor"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;partyTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;DebtContractTermsSet"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-pty-pty;hasPartyIdentity"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-be-le-lp;LegalPerson"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">lender</rdfs:label>
		<skos:editorialNote
			xml:lang="en">lender: The lender of a loan is some form of Legal Entity.</skos:editorialNote>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">For formal loans, the entity will be an Artificial Person. For loans more generally, it may be any legal entity.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;MoniesOwing">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-agr;Commitment"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-acc;isMeasuredAs"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-acc;DatedBalance"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;owedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;Debtor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-dtt;owedTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-dtt;Creditor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">debt</rdfs:label>
		<skos:definition
			xml:lang="en">A sum of money owed by one party to another party.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">This is an amount of money regarded as a debt by one party, or as credit by another party. Note that the sub terms Credit and Debt are also balance sheet entries, that is these can be anything in which monies are owed by one party to another. Monies Owing may therefore also be goods payable/receivable. This term is therefore independent of the existence of any formal credit facility provided by a credit provider. Change note Reviewed 18 Oct 2012 and determined that Monies Owing is a mediating thing (context0 in which Credit and Debt are defined, and not a common parent of those two things. Credit and Debt changed to be sub classes of Relative Thing and not cub classes of Monies Owing as before. New relationship fact &apos;viewpoint&apos; added to Credit and similarly to Debt, with Monies Owing as the target of that fact, in place of the previous superclass relationship.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;PrincipalOnly">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;AmortizationType"/>
		<rdfs:label
			xml:lang="en">principal only</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-dtt;PrincipalPlusInterest">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-dtt;AmortizationType"/>
		<rdfs:label
			xml:lang="en">principal plus interest</rdfs:label>
	</owl:Class>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fnd-acc-dtt;accrualBasis">
		<rdfs:label
			xml:lang="en">accrual basis</rdfs:label>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-dtt;amortizationCalculationsDayCountConvention">
		<rdfs:label
			xml:lang="en">amortization calculations day count convention</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-dtt;DebtPrincipalRepaymentTermsSet"/>
		<rdfs:range
			rdf:resource="&fibo-sec-dbt-dbt-dbt;DayCountBasis"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-dtt;appliesTo">
		<rdfs:label
			xml:lang="en">applies to1</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-dtt;Accrual"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-dtt;Interest"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-dtt;collateralizedBy">
		<rdfs:label
			xml:lang="en">collateralized by</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-dtt;DebtFinance"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-dtt;Collateral"/>
