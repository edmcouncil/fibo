<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-be-le-fbo "https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/">
	<!ENTITY fibo-ext-snap-par-wt "https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/">
	<!ENTITY fibo-fnd-acc-acc "https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/">
	<!ENTITY fibo-fnd-acc-aeq "https://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/">
	<!ENTITY fibo-fnd-acc-assx "https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/">
	<!ENTITY fibo-fnd-acc-bal "https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/">
	<!ENTITY fibo-fnd-acc-cur "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/">
	<!ENTITY fibo-fnd-acc-dbpx "https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-val "https://spec.edmcouncil.org/fibo/FND/Utilities/Values/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/"
	xmlns:fibo-be-le-fbo="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"
	xmlns:fibo-ext-snap-par-wt="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"
	xmlns:fibo-fnd-acc-acc="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"
	xmlns:fibo-fnd-acc-aeq="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/"
	xmlns:fibo-fnd-acc-assx="https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/"
	xmlns:fibo-fnd-acc-bal="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/"
	xmlns:fibo-fnd-acc-cur="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
	xmlns:fibo-fnd-acc-dbpx="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Accounting/BalanceSheetBalances/">
		<rdfs:label
			xml:lang="en">BalanceSheetBalances</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountingEquity/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AccountsMain/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/AssetExtensions/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Accounting/DebtPerspectives/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/ext/snap/partial/W3CTime/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="&fibo-fnd-acc-aeq;OwnersEquity">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-acc-bal;OwnersEquity"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;AccountPayableBalance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-acc;LedgerBalance"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-bal;isBalanceOf"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-acc;AccountPayable"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">account payable balance</rdfs:label>
		<skos:definition
			xml:lang="en">Balance of accounts receivable</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;AccountReceivableBalance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-acc;LedgerBalance"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-bal;isBalanceOf"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-acc;AccountReceivable"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">account receivable balance</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-bal;CashBalance"/>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-bal;StockBalance"/>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-dbpx;CreditSide"/>
		<skos:definition
			xml:lang="en">Balance of accounts receivable</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;Balance">
		<rdfs:label
			xml:lang="en">balance</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;BalanceSheetAssetBalance">
		<rdfs:label
			xml:lang="en">balance sheet asset balance</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-bal;BalanceSheetLiabilityBalance"/>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-bal;OwnersEquity"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-fnd-acc-bal;AccountReceivableBalance"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-acc-bal;CashBalance"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-fnd-acc-bal;StockBalance"/>
										<rdf:rest>
											<rdf:Description>
												<rdf:first
													rdf:resource="&fibo-fnd-acc-dbpx;CreditSide"/>
												<rdf:rest
													rdf:resource="&rdf;nil"/>
											</rdf:Description>
										</rdf:rest>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition
			xml:lang="en">A balance sheet entry representing some asset of the entity.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;BalanceSheetBalance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-bal;BalanceSheetFeature"/>
		<rdfs:label
			xml:lang="en">balance sheet balance</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;BalanceSheetEntry">
		<rdfs:label
			xml:lang="en">balance sheet entry</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-fnd-acc-bal;BalanceSheetAssetBalance"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-acc-bal;BalanceSheetLiabilityBalance"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-fnd-acc-bal;OwnersEquity"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition
			xml:lang="en">Some entry on the balance sheet of some business entity.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;BalanceSheetFeature">
		<rdfs:label
			xml:lang="en">balance sheet feature</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;BalanceSheetLiabilityBalance">
		<rdfs:label
			xml:lang="en">balance sheet liability balance</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-bal;OwnersEquity"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf>
					<rdf:Description>
						<rdf:first
							rdf:resource="&fibo-fnd-acc-acc;AccruedLiability"/>
						<rdf:rest>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-fnd-acc-bal;AccountPayableBalance"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-fnd-acc-dbpx;DebtSide"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition
			xml:lang="en">A balance sheet entry representing some asset of the entity.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;BalanceSheetTotal">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-bal;BalanceSheetFeature"/>
		<rdfs:label
			xml:lang="en">balance sheet total</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;CashBalance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-acc;LedgerBalance"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-bal;representsAmountOf"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-acc;CashCurrency"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-bal;representsMonetaryAmount"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-cur;AmountOfMoney"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">cash balance</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-bal;StockBalance"/>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-dbpx;CreditSide"/>
		<skos:definition
			xml:lang="en">A balance of cash. This is identified as an amount of cash which is denominated with a currency.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;NostroBalance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-bal;Balance"/>
		<rdfs:label
			xml:lang="en">nostro balance</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-bal;VostroBalance"/>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;OwnersEquity">
		<rdfs:label
			xml:lang="en">owners equity</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;StockBalance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-assx;PhysicalAsset"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-bal;isQuantityOf"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-acc;StockInTrade"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-acc-bal;represents"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-acc-acc;StockAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">stock balance</rdfs:label>
		<owl:disjointWith
			rdf:resource="&fibo-fnd-acc-dbpx;CreditSide"/>
		<skos:definition
			xml:lang="en">Balance of stock in trade.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-acc-bal;VostroBalance">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-bal;Balance"/>
		<rdfs:label
			xml:lang="en">vostro balance</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;appliesTo">
		<rdfs:label
			xml:lang="en">applies to</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;BalanceSheetEntry"/>
		<rdfs:range
			rdf:resource="&fibo-ext-snap-par-wt;Interval"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;balance">
		<rdfs:label
			xml:lang="en">balance</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;BalanceSheetBalance"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-val;DecimalValue"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;isBalanceOf">
		<rdfs:label
			xml:lang="en">is balance of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;Balance"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-acc;Account"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;isFor">
		<rdfs:label
			xml:lang="en">is for</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;BalanceSheetEntry"/>
		<rdfs:range
			rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;isQuantityOf">
		<rdfs:label
			xml:lang="en">is quantity of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;StockBalance"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-acc;StockInTrade"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;represents">
		<rdfs:label
			xml:lang="en">represents</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;StockBalance"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-acc;StockAmount"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;representsAmountOf">
		<rdfs:label
			xml:lang="en">represents amount of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;CashBalance"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-acc;CashCurrency"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-acc-bal;representsMonetaryAmount">
		<rdfs:label
			xml:lang="en">represents monetary amount</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-acc-bal;CashBalance"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;AmountOfMoney"/>
	</owl:ObjectProperty>

</rdf:RDF>