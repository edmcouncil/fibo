	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Control/">
		<rdfs:label>Control Ontology</rdfs:label>
		<dct:license
			rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
