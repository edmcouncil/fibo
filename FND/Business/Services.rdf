<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-fnd-agr-ctr "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/">
	<!ENTITY fibo-fnd-bus-agc "https://spec.edmcouncil.org/fibo/FND/Business/AgentsContexts/">
	<!ENTITY fibo-fnd-bus-srv "https://spec.edmcouncil.org/fibo/FND/Business/Services/">
	<!ENTITY fibo-fnd-occ-act "https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/">
	<!ENTITY fibo-fnd-plc-fac "https://spec.edmcouncil.org/fibo/FND/Places/Facilities/">
	<!ENTITY fibo-fnd-txn-ecr "https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY terms "http://purl.org/dc/terms/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Business/Services/"
	xmlns:fibo-fnd-agr-ctr="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"
	xmlns:fibo-fnd-bus-agc="https://spec.edmcouncil.org/fibo/FND/Business/AgentsContexts/"
	xmlns:fibo-fnd-bus-srv="https://spec.edmcouncil.org/fibo/FND/Business/Services/"
	xmlns:fibo-fnd-occ-act="https://spec.edmcouncil.org/fibo/FND/Occurrent/Activities/"
	xmlns:fibo-fnd-plc-fac="https://spec.edmcouncil.org/fibo/FND/Places/Facilities/"
	xmlns:fibo-fnd-txn-ecr="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:terms="http://purl.org/dc/terms/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Business/Services/">
		<rdfs:label
			xml:lang="en">Services</rdfs:label>
		<owl:imports
			rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Business/AgentsContexts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Places/Facilities/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
	</owl:Ontology>
	
	<rdf:Description
		rdf:about="&fibo-fnd-agr-ctr;Contract">
		<owl:equivalentClass
			rdf:resource="&fibo-fnd-bus-srv;Contract"/>
	</rdf:Description>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;Contract">
		<rdfs:label
			xml:lang="en">contract</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;Facility">
		<rdfs:label
			xml:lang="en">facility</rdfs:label>
		<skos:definition
			xml:lang="en">The means by which some service is provided; something made available to the user of a service by the provider of that service.</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote
			xml:lang="en">A facility may be concrete (as in a manufacturing facility) or abstract. It is a Relative Thing, since that which is made available as a facility may be an independent thing such as a factory. In the case of an abstract facility (such as a credit facility), there is no corresponding independent thing, except perhaps a set of standard terms that describe the available facility.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;Market">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-plc-fac;Venue"/>
		<rdfs:label
			xml:lang="en">market</rdfs:label>
		<skos:definition
			xml:lang="en">A venue where business may be done. This includes the market where securities may be issued. A market may be described in terms of a country, a region, or an abstract place construct such as the domestic market.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;MarketServicesProvision">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-srv;ServiceProvision"/>
		<rdfs:label
			xml:lang="en">market services provision</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;MarketplaceProvider">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-srv;ServiceProvider"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-srv;provides"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-srv;MarketplaceService"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">marketplace provider</rdfs:label>
		<skos:definition
			xml:lang="en">A provider of some service in which things may be sold by third parties to other third parties, such as a department store concessoin or a securities exchange or trading venue.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;MarketplaceService">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-srv;Service"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-srv;providedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-srv;MarketplaceProvider"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-srv;serviceProvisionSubject"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-txn-ecr;EconomicResource"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">marketplace service</rdfs:label>
		<terms:description
			xml:lang="en">In financial markets, these may often differ only in terms of the conventions used, for example the pricing convention (per 100, by yield and so on). so a &quot;Market&quot; can also be seen in the abstract as one or other price.</terms:description>
		<skos:definition
			xml:lang="en">Some service in which things may be sold by third parties to other third parties, such as a department store concessoin or a securities exchange or trading venue.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;RetailConcession">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-srv;MarketplaceService"/>
		<rdfs:label
			xml:lang="en">retail concession</rdfs:label>
		<skos:definition
			xml:lang="en">The provision of retail space for the sale of goods by a party other than the owner of the retail space. Note: this is included with this Financial services notology architecture in order to put financial market listings in a context.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;Service">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-srv;providedBy"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-srv;ServiceProvider"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">service</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;ServiceProvider">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-agc;BusinessAgent"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-srv;provides"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-srv;Service"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">service provider</rdfs:label>
		<skos:definition
			xml:lang="en">A provider of some business service.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;ServiceProvision">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-agc;involves"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-srv;ServiceProvider"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">service provision</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-srv;TradingVenue">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-plc-fac;Venue"/>
		<rdfs:label
			xml:lang="en">trading venue</rdfs:label>
		<skos:definition
			xml:lang="en">A venue where something may be bought, sold or exchanged.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-srv;entailsProvisionOf">
		<rdfs:label
			xml:lang="en">entails provision of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-srv;Service"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-bus-srv;Facility"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-srv;providedBy">
		<rdfs:label
			xml:lang="en">provided by</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-srv;Service"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-bus-srv;ServiceProvider"/>
		<owl:inverseOf
			rdf:resource="&fibo-fnd-bus-srv;provides"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-srv;provides">
		<rdfs:label
			xml:lang="en">provides</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-srv;ServiceProvider"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-bus-srv;Service"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-srv;serviceProvisionSubject">
		<rdfs:label
			xml:lang="en">service provision subject</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-srv;MarketplaceService"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-txn-ecr;EconomicResource"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-srv;takesFormOf">
		<rdfs:label
			xml:lang="en">takes form of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-srv;Facility"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-srv;takesFormOf.1">
		<rdfs:label
			xml:lang="en">takes form of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-srv;Service"/>
	</owl:ObjectProperty>

</rdf:RDF>