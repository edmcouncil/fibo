<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY fibo-der-der-dbx "https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasicsExt/">
	<!ENTITY fibo-fnd-agr-ctr "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/">
	<!ENTITY fibo-fnd-bus-pasx "https://spec.edmcouncil.org/fibo/FND/Business/ProductsAndServices/">
	<!ENTITY fibo-fnd-bus-srv "https://spec.edmcouncil.org/fibo/FND/Business/Services/">
	<!ENTITY fibo-fnd-phy-phy "https://spec.edmcouncil.org/fibo/FND/Physical/Physical/">
	<!ENTITY fibo-fnd-txn-ecr "https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-val "https://spec.edmcouncil.org/fibo/FND/Utilities/Values/">
	<!ENTITY fibo-uo-lat "https://spec.edmcouncil.org/fibo/UO/Lattice/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xml:base="https://spec.edmcouncil.org/fibo/FND/Business/ProductsAndServices/"
	xmlns:fibo-der-der-dbx="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasicsExt/"
	xmlns:fibo-fnd-agr-ctr="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"
	xmlns:fibo-fnd-bus-pasx="https://spec.edmcouncil.org/fibo/FND/Business/ProductsAndServices/"
	xmlns:fibo-fnd-bus-srv="https://spec.edmcouncil.org/fibo/FND/Business/Services/"
	xmlns:fibo-fnd-phy-phy="https://spec.edmcouncil.org/fibo/FND/Physical/Physical/"
	xmlns:fibo-fnd-txn-ecr="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-val="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"
	xmlns:fibo-uo-lat="https://spec.edmcouncil.org/fibo/UO/Lattice/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology
		rdf:about="https://spec.edmcouncil.org/fibo/FND/Business/ProductsAndServices/">
		<rdfs:label
			xml:lang="en">ProductsAndServices</rdfs:label>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/DerivativesBasicsExt/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Business/Services/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Physical/Physical/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Transactions/EconomicResources/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="https://spec.edmcouncil.org/fibo/FND/Utilities/Values/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-pasx;BundleOfResources">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-ecr;EconomicResource"/>
		<rdfs:label
			xml:lang="en">bundle of resources</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-pasx;ContractualProduct">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-pasx;Product"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-pasx;isTemplateFor"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-pasx;realizedAs"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">contractual product</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-pasx;Good">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-pasx;BundleOfResources"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-txn-ecr;takesMaterialForm"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-phy-phy;PhysicalItem"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">good</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-pasx;MarketableService">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-bus-pasx;BundleOfResources"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-txn-ecr;takesMaterialForm"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-bus-srv;Service"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">marketable service</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-pasx;NegotiableCommodity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-txn-ecr;EconomicResource"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-txn-ecr;takesMaterialForm"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fnd-phy-phy;PhysicalItem"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">negotiable commodity</rdfs:label>
		<skos:definition
			xml:lang="en">Goods or raw materials which may be bought or sold in some marketplace.</skos:definition>
		<skos:editorialNote
			xml:lang="en">3 attributes for each commoditiy (to be added as subclasses of this: 1. The commodity 2. the Grade 3 quantity (number and unit) 4. Unit of measure Grade can get very complex e.g. wheat in Aus includes physical attributes like moisture and protein content. This will vary from one market to another. Review notes: Q: Does that vary according to the market of the derivative or the market in which the physical thing is provided? A: the latter - it&apos;s more the way the underlying is traded. So this will be facts relating to the spot market in those things. Modeling note: The above question allows us to identify whether these facts are facts about &quot;Underlying&quot; specifically, or facts about noeogitable commodities. These are all facts about negotiable comodities since that&apos;s what the spot market is. The concept of &quot;Underlying&quot; will therefore be modeled as a separate concept to this one.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-bus-pasx;Product">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&fibo-fnd-bus-pasx;commoditizes"/>
				<owl:someValuesFrom
					rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label
			xml:lang="en">product</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-pasx;commoditizes">
		<rdfs:label
			xml:lang="en">commoditizes</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-pasx;Product"/>
		<rdfs:range
			rdf:resource="&owl;Thing"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-pasx;isTemplateFor">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-bus-pasx;commoditizes"/>
		<rdfs:label
			xml:lang="en">is template for</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-pasx;ContractualProduct"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-pasx;realizedAs">
		<rdfs:label
			xml:lang="en">realized as</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-pasx;ContractualProduct"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-bus-pasx;unitOfMeasure">
		<rdfs:label
			xml:lang="en">unit of measure</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-bus-pasx;NegotiableCommodity"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-val;TextValue"/>
		<skos:definition
			xml:lang="en">The unit in which the underlying commodity is measured. Further Notes This is modeled as text at this level in the model. It may be possible to model individual types of unit in relation to individual types of commodity, e.g. the various units of measurement for oil, grain and so on, though these are usually one default worldwide one type of commodity.</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>