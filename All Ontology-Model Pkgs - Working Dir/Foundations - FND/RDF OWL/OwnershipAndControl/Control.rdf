    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/Control -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;Control">
        <rdfs:label>control</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;involves"/>
                <owl:onClass rdf:resource="&fibo-fnd-oac-ctl;ControllingParty"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;involves"/>
                <owl:onClass rdf:resource="&fibo-fnd-oac-ctl;ControlledParty"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>The term control (including the terms controlling, controlled by and under common control with) means the possession, direct or indirect, of the power to direct or cause the direction of the management and policies of a person, whether through the ownership of voting shares, by contract, or otherwise.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/ControlledParty -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;ControlledParty">
        <rdfs:label rdf:datatype="&xsd;string">Controlled party</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;hasRole"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isControlledBy"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/ControllingParty -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;ControllingParty">
        <rdfs:label>controlling party</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;hasRole"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;controls"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>Party which exercises some form of control in some context.</skos:definition>
        <skos:editorialNote>At this level of abstraction it is not defined whether the control is some degree of controlling interest, or some level of actual control (asserted or calculated) in some entity.</skos:editorialNote>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/DeFactoControl -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;DeFactoControl">
        <rdfs:label>de facto control</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-ctl;Control"/>
        <owl:disjointWith rdf:resource="&fibo-fnd-oac-ctl;DeJureControl"/>
        <fibo-fnd-utl-av:adaptedFrom>http://www.investorwords.com/1349/de_facto.html</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition>control that is understood, due to condition or situation treated as standard or official, even if not explicitly stated (or actually standardized)</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Control/DeJureControl -->

    <owl:Class rdf:about="&fibo-fnd-oac-ctl;DeJureControl">
        <rdfs:label>de jure control</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-ctl;Control"/>
        <fibo-fnd-utl-av:adaptedFrom>http://www.businessdictionary.com/definition/de-jure.html</fibo-fnd-utl-av:adaptedFrom>
        <skos:definition>control that is formalized in law, or codified in some legal instrument</skos:definition>
    </owl:Class>
</rdf:RDF>



