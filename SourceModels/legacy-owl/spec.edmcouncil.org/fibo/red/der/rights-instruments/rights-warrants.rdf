<owl:Class rdf:about="&fibo-der-rgt-rw;AllotmentRight">
      <rdfs:label>allotment right</rdfs:label>
      <skos:definition>Privileges allotted to existing security holders, entitling them to receive new securities free of charge.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-rgt-rw;StockholdersRightsInstrument"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;AllotmentRightFormula">
      <rdfs:label>allotment right formula</rdfs:label>
      <skos:definition>A formula used to calculate the number of securities for an Allotment Right, based on the number of these instruments that the holder holds. Conesnsus:Review</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;Formula"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;AllotmentRightHolder">
      <rdfs:label>allotment right holder</rdfs:label>
      <skos:definition>The party which is the holder of an allotment right. This is identified as being a party which is also an existing holder of the security identified as the underlying security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-rgt-rw;StockholdersRightsInstrumentHolder"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;CommodityWarrant">
      <rdfs:label>commodity warrant</rdfs:label>
      <skos:definition>A derivative based on commodities contracts traded on exchanges.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-rgt-rw;Warrant"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;CommodityWarrantExerciseTerms">
      <rdfs:label>commodity warrant exercise terms</rdfs:label>
      <skos:definition>Terms setting out how a commodity warrant may be exercised, including the form which delivery takes.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-rgt-rw;WarrantExerciseTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;CompanyWarrant">
      <rdfs:label>company warrant</rdfs:label>
      <skos:definition>Warrants which are issued by the issuer of the underlying.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;CompanyWarrantExerciseTermsSet">
      <rdfs:label>company warrant exercise terms set</rdfs:label>
      <skos:definition>Terms setting out how a company warrant or naked warrant may be exercised, including the form which delivery takes.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;CoveredWarrant">
      <rdfs:label>covered warrant</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;CoveredWarrantExerciseTerms">
      <rdfs:label>covered warrant exercise terms</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;EntitlementRightsInstrument">
      <rdfs:label>entitlement rights instrument</rdfs:label>
      <skos:definition>A security giving the holder of that security the entitlement to purchase or sell back to the issuer new securities or some other commodity or instrument at a predetermined price.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;ExchangeTradedWarrant">
      <rdfs:label>exchange traded warrant</rdfs:label>
      <skos:definition>A warrant which is traded on a securities exchange.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sec;ExchangeTradedSecurity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;PurchaseRight">
      <rdfs:label>purchase right</rdfs:label>
      <skos:definition>Anti-takeover device that gives a prospective acquireeâ€™s shareholders the right to buy usually shares of the firm or shares of anyone who acquires the firm at a deep discount to their fair market value.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;StockholdersRightsInstrument">
      <rdfs:label>stockholders rights instrument</rdfs:label>
      <skos:definition>A security giving stockholders entitlement to purchase new securities issued by the corporation at a predetermined price (normally less than the current market price) in proportion to the number of securities already owned.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-rgt-rw;EntitlementRightsInstrument"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;StockholdersRightsInstrumentHolder">
      <rdfs:label>stockholders rights instrument holder</rdfs:label>
      <skos:definition>A party which is the holder of a Stockholders Rights instrument.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-ast;SecurityHolder"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;StockholdersRightsInstrumentIssuer">
      <rdfs:label>stockholders rights instrument issuer</rdfs:label>
      <skos:definition>The party which is the issuer of a Stockholders Rights instrument.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-iss;SecurityIssuer"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;SubscriptionRight">
      <rdfs:label>subscription right</rdfs:label>
      <skos:definition>Privileges allotted to existing security holders, entitling them to subscribe to new securities at a price normally lower than the prevailing market price.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;SubscriptionRightHolder">
      <rdfs:label>subscription right holder</rdfs:label>
      <skos:definition>The party which is the holder of a subscription right. This is identified as being a party which is also an existing holder of the security identified as the underlying security.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;ThirdPartyWarrantIssuer">
      <rdfs:label>third party warrant issuer</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-rgt-rw;WarrantIssuer"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;TraditionalWarrant">
      <rdfs:label>traditional warrant</rdfs:label>
      <skos:definition>Financial instruments which permit the holder to purchase or sell back to the issuer a specified amount of a share.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-rgt-rw;CompanyWarrant"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;UnderlyingIssuer">
      <rdfs:label>underlying issuer</rdfs:label>
      <skos:definition>A party which is the issuer of a Covered Warrant. This party is by definition the issuer of the underlying securities.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;Warrant">
      <rdfs:label>warrant</rdfs:label>
      <skos:definition>Financial instruments which permit the holder to purchase or sell back to the issuer a specified amount of a financial instrument, commodity, currency or other during a specified period at a specified price.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;WarrantExerciseTermsSet">
      <rdfs:label>warrant exercise terms set</rdfs:label>
      <skos:definition>Terms setting out how a warrant instrument may be exercised. Further Notes Exercise terms as defined here also cover the automatic exercise of the warrant in the event of its expiry. These terms include the exercise style (FIBIM = OptionStyle type OptionStyleCode) and the delivery.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;WarrantIssuer">
      <rdfs:label>warrant issuer</rdfs:label>
      <skos:definition>The issuer of a Warrant. This is defined in terms of what individual entity is the issuer and also what other roles they may or may not play for a specific type of warranty, specifically whether or not they are also the issuer of the underlying, if the underlying is a security.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rgt-rw;WarrantListing">
      <rdfs:label>warrant listing</rdfs:label>
      <skos:definition>The listing of a warrant on a securities exchange. Action: confirm whether all the facts shown for listing of tradable securities generally, apply to the listing of a warrant. Also determine whether any of the terms in Listing need to be specialized or added to for Warrant Listing.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-lst;SecurityListing"/>
   </owl:Class>


