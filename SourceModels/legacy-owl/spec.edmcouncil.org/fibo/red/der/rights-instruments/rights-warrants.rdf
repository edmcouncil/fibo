<owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;numberOfSecurities">
      <rdfs:label>number of securities</rdfs:label>
      <skos:definition>The number of securities based on the number of these instruments that the holder holds.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;formulaText">
      <rdfs:label>formula text</rdfs:label>
      <skos:definition>The formula stated as text.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;ratio">
      <rdfs:label>ratio</rdfs:label>
      <skos:definition>The ratio of warrant units to underlying security units for delivery when the warrant is exercised.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;ratio.1">
      <rdfs:label>ratio</rdfs:label>
      <skos:definition>The ratio of warrant units to underlying security units for delivery when the warrant is exercised.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;expiryDate">
      <rdfs:label>expiry date</rdfs:label>
      <skos:definition>Legal expiry of the Contract.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;purchasePrice">
      <rdfs:label>purchase price</rdfs:label>
      <skos:definition>The price at which the holder of the Purchase Right may purchase the underlying security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-acc-acc;Price"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;oversubscribeOption">
      <rdfs:label>oversubscribe option</rdfs:label>
      <skos:definition>Option whereby the holders of the rights instrument may get securities in the event that other right holders choose not to subscribe to theirs.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;purchasePrice.1">
      <rdfs:label>purchase price</rdfs:label>
      <skos:definition>The price at which the holder of the Subscription Right may purchase the underlying security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-acc-acc;Price"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;expiryDate.1">
      <rdfs:label>expiry date</rdfs:label>
      <skos:definition>Expiry date of the warrant. This is the last date on which the holder may exercise the right to buy or sell back the underlying security.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;perpetualMaturity">
      <rdfs:label>perpetual maturity</rdfs:label>
      <skos:definition>Whether this is a perpetual warrant, in other words it has no expiry date.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-rgt-rw;putWarrant">
      <rdfs:label>put warrant</rdfs:label>
      <skos:definition>If Yes, this warrant gives the holder the right to sell the underlying security back to the issuer at a predetermined price; if No, the warrant gives the holder the right to purchase the instrument at the predetermined price (a call warrant).</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


