<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-fx-spot "http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-spots/">
    <!ENTITY fibo-der-ctr-otc "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/">
    <!ENTITY fibo-der-ctr-spot "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/spots/">
    <!ENTITY fibo-der-fx-fxc "http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-contracts/">
    <!ENTITY fibo-fnd-txn-sup "http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/">
    <!ENTITY fibo-der-ctr-der "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-ind-fx-fx "http://spec.edmcouncil.org/fibo/red/ind/foreign-exchange/foreign-exchange/">
    <!ENTITY fibo-fnd-txn-rea "http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-spots/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-spots/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/spots/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/foreign-exchange/foreign-exchange/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-fx-spot;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotContract"/>
      <rdfs:range rdf:resource="&fibo-ind-fx-fx;CurrencySpotRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;buyerCurrency">
      <rdfs:label>buyer currency</rdfs:label>
      <skos:definition>The buyer's currency as specified in the contract.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;sellerCurrency">
      <rdfs:label>seller currency</rdfs:label>
      <skos:definition>The seller's currency as specified in the contract.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;establishes">
      <rdfs:label>establishes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-otc;CashDeliveryCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;establishes.1">
      <rdfs:label>establishes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotContract"/>
      <rdfs:range rdf:resource="&fibo-der-fx-spot;FxSpotSettlementCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;commitsTo">
      <rdfs:label>commits to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotSettlementCommitment"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-spot;SpotCashSettlementTermsSet"/>
      <owl:inverseOf rdf:resource="&fibo-der-ctr-spot;embodies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;benefitSubject">
      <rdfs:label>benefit subject</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;subject"/>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-sup;NegotiableCash"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-spot;embodies.2"/>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-fx-spot;FxSpotContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;side">
      <rdfs:label>side</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-otc;DeliverCash"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;side.1">
      <rdfs:label>side</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpotTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-spot;SpotCashSettlementTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-spot;embodies.1">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-spot;FxSpot"/>
      <rdfs:range rdf:resource="&fibo-der-fx-spot;FxSpotTransaction"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-fx-spot;fxSpotRate">
      <rdfs:label>fx spot rate</rdfs:label>
      <skos:definition>'The rate of exchange between the two currencies.'</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-fx-spot;FxSpot">
      <rdfs:label>fx spot</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-fx-fxc;OTCFxProduct"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-fx-spot;FxSpotContract">
      <rdfs:label>fx spot contract</rdfs:label>
      <skos:definition>The rate of a foreign-exchange contract for immediate delivery. Also known as "benchmark rates", "straightforward rates" or "outright rates", spot rates represent the price that a buyer expects to pay for a foreign currency in another currency.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-spot;SpotContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-fx-spot;FxSpotSettlementCommitment">
      <rdfs:label>fx spot settlement commitment</rdfs:label>
      <skos:definition>FpML exchangedCurrency2 referred to as a "Currency Stream"</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-spot;SpotSettlementCommitment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-fx-spot;FxSpotTransaction">
      <rdfs:label>fx spot transaction</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-spot;SpotTransaction"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>