   </owl:Class>

   <owl:Class rdf:about="&fibo-der-fx-fxc;ForeignExchangeDerivativeContract">
      <rdfs:label>foreign exchange derivative contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
