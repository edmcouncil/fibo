    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-etd-fex;becomeFeaturesOf">
      <rdfs:label>become features of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-etd-fex;StandardizedFuturesListingTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-etd-fex;FuturesListing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-etd-fex;definedBy">
      <rdfs:label>defined by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publishedBy"/>
      <rdfs:domain rdf:resource="&fibo-der-etd-fex;StandardizedFuturesListingTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-etd-dst;DerivativesExchange"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-etd-fex;marginCall">
      <rdfs:label>margin call</rdfs:label>
      <skos:definition>A broker's demand on an investor using margin to deposit additional money or securities so that the margin account is brought up to the minimum maintenance margin. This is sometimes called a 'fed call' or 'maintenance call'.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fex;marginProvision">
      <rdfs:label>margin provision</rdfs:label>
      <skos:definition>An amount necessary to cover the purchase of the underlying security since delivery is mandatory, be it physical or cash. Failing to provide a sufficient provision will result in a margin call.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fex;marginCallApplicable">
      <rdfs:label>margin call applicable</rdfs:label>
      <skos:definition>Whether or not a Margin Call may be applicable to the listing.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
