   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/exchange-traded-derivatives/futures-exchanges/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/exchange-traded-derivatives/derivatives-standardized-terms/"/>
   </owl:Ontology>


