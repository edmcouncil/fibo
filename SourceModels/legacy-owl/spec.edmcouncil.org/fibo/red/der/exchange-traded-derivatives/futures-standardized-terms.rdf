<owl:ObjectProperty rdf:about="&fibo-der-etd-fst;becomesTermsOf">
      <rdfs:label>becomes terms of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-etd-fut;becomesTermsOf.5"/>
      <rdfs:domain rdf:resource="&fibo-der-etd-fst;EquityFutureStandardizedTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-etd-fut;EquityFuture"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-etd-fst;pricingToBeDeterminedBy">
      <rdfs:label>pricing to be determined by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-etd-fst;StandardizedFuturesTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-etd-fst;FuturesPriceDeterminationMethod"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-etd-fst;definedBy">
      <rdfs:label>defined by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publishedBy"/>
      <rdfs:domain rdf:resource="&fibo-der-etd-fst;StandardizedFuturesTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-etd-dst;DerivativesExchange"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-etd-fst;becomesTermsOf.1">
      <rdfs:label>becomes terms of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-etd-fst;StandardizedFuturesTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-etd-fut;FutureInstrument"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;conversionFactor">
      <rdfs:label>conversion factor</rdfs:label>
      <skos:definition>The conversion factor is the price of the delivered bond/note ($1 par value) to yield a fixed rate. The conversion factor is used to calculate a final delivery price.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;contractSize">
      <rdfs:label>contract size</rdfs:label>
      <skos:definition>The number of share units underlying the futures contract.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;unitOfTrading">
      <rdfs:label>unit of trading</rdfs:label>
      <skos:definition>The pre-determined unit in which financial futures will be traded on this exchange.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;priceDeterminationMethod">
      <rdfs:label>price determination method</rdfs:label>
      <skos:definition>The method by which the price is determined, for example open outcry.</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-etd-dst;DerivativesPriceDeterminationMethodSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;multiple">
      <rdfs:label>multiple</rdfs:label>
      <skos:definition>The multiple for determining the price of the futures contract in relation to the underlying index rate.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;lotSize">
      <rdfs:label>lot size</rdfs:label>
      <skos:definition>How many of the underlying commodity can be sold. The groupings of certain units of measure in the underlying commodity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;unitOfMeasure">
      <rdfs:label>unit of measure</rdfs:label>
      <skos:definition>The unit in which the underlying commodity is to be measured.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;contractSize.1">
      <rdfs:label>contract size</rdfs:label>
      <skos:definition>The deliverable quantity of goods or commodities underlying the futures contract. This is denominated as a multiple of the lot size in the case of a commodity future. For financial futures this is the number of units of the underlying security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;delivery">
      <rdfs:label>delivery</rdfs:label>
      <skos:definition>Type of delivery for contracts. This determines what is delivered to the holder when the future is exercised.</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-etd-dst;DerivativesDeliveryTypeSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;tickSize">
      <rdfs:label>tick size</rdfs:label>
      <skos:definition>The minimum value of the upward or downward movement of the price of the contract.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-etd-fst;tickValue">
      <rdfs:label>tick value</rdfs:label>
      <skos:definition>The minimum value of the upward or downward movement of the contract. This is the contract size multiplied by the tick size.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:DatatypeProperty>


