<owl:Class rdf:about="&fibo-der-ctr-oof;FuturesContractUnderlying">
      <rdfs:label>futures contract underlying</rdfs:label>
      <skos:definition>An underlying which is a futures contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeUnderlying"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-oof;OTCContractOnFuture">
      <rdfs:label>o t c contract on future</rdfs:label>
      <skos:definition>An option contract with an underlying futures contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-opt;OTCOptionContract"/>
   </owl:Class>


