<owl:ObjectProperty rdf:about="&fibo-der-ctr-oof;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-opt;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-oof;OTCContractOnFuture"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-oof;FuturesContractUnderlying"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-oof;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;identity.1"/>
