   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-for;MasterConfirmationValuationTimeTermsSet">
      <rdfs:label>master confirmation valuation time terms set</rdfs:label>
      <skos:definition>Formal terms specifying the time for Valuation of Equity Underlying.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-for;UnderlyingValuation">
      <rdfs:label>underlying valuation</rdfs:label>
