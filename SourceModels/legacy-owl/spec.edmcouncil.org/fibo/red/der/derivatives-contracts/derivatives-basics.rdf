    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-ctr-der;settlementDate">
      <rdfs:label>settlement date</rdfs:label>
      <skos:definition>The date that the transaction will settle.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Date"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-ctr-der;.UnionClass.3">
      <skos:definition>Either a date determined by some means, or a relative date which is specified in relation to some other date.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-tim-tim;RelativeDate"/>
               <owl:Class rdf:about="&fibo-fnd-tim-tim;DeterminedDate"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;.UnionClass.4">
      <skos:definition>Any number or combination of financial instruments and / or market referential parameters (Indices and / or interest rates).</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-bus-ecr;NegotiableCommodity"/>
               <owl:Class rdf:about="&fibo-ind-ind-ind;MarketReferentialParameter"/>
               <owl:Class rdf:about="&fibo-sec-sec-bsk;Basket"/>
               <owl:Class rdf:about="&fibo-sec-sec-fi;FinancialInstrument"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DeliverableUnderlying">
      <rdfs:label>deliverable underlying</rdfs:label>
      <skos:definition>The Underlying of a Derivatives Contract, which may also be delivered on exercise or expiry of that contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeUnderlying"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DeliveryCommitment">
      <rdfs:label>delivery commitment</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeAgreement">
      <rdfs:label>derivative agreement</rdfs:label>
      <skos:definition>The agreement which corresponds to some derivative transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeContract">
      <rdfs:label>derivative contract</rdfs:label>
      <skos:definition>An instrument which is derived in some way from some other instrument or other underlying financial asset or measure.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeContractParty">
      <rdfs:label>derivative contract party</rdfs:label>
      <skos:definition>A party to a Derivative contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;ContractualTransactionParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeDeliveryTermsSet">
      <rdfs:label>derivative delivery terms set</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeTransactionTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeTransaction">
      <rdfs:label>derivative transaction</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;ContractualTransaction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeTransactionParty">
      <rdfs:label>derivative transaction party</rdfs:label>
      <skos:definition>The party to a derivatives transaction (forward, option or swap).</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeTransactionTermsSet">
      <rdfs:label>derivative transaction terms set</rdfs:label>
      <skos:definition>Terms for a derivative transaction, covering payment or delivery by either party to the other.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeUnderlying">
      <rdfs:label>derivative underlying</rdfs:label>
      <skos:definition>The instrument or parameter, or combinations thereof, that are used as the basis for a Derivative Instrument.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;ReferenceEntity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;ObservableUnderlying">
      <rdfs:label>observable underlying</rdfs:label>
      <skos:definition>An underlying observable which is in numeric form and does not represent a deliverable asset.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;ObservableValue">
      <rdfs:label>observable value</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;FormulaArgument"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;SettlementCommitment">
      <rdfs:label>settlement commitment</rdfs:label>
      <skos:definition>A commitment to settle one side of a transaction.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-der;SettlementTermsSet">
      <rdfs:label>settlement terms set</rdfs:label>
      <skos:definition>Settlement of cash as one side of a transaction.</skos:definition>
   </owl:Class>


  <!--
