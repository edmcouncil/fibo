    <!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
    <!ENTITY fibo-fnd-txn-sup "http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/">
    <!ENTITY fibo-be-le-cb "http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/">
    <!ENTITY fibo-sec-sec-fi "http://spec.edmcouncil.org/fibo/red/sec/securities/financial-instruments/">
