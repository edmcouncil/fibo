<owl:ObjectProperty rdf:about="&fibo-der-ctr-der;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;setsOutContractualEconomicCommitment"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;SettlementTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;SettlementCommitment"/>
      <owl:inverseOf rdf:resource="&fibo-der-ctr-der;commitsTo.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;transactsAs">
      <rdfs:label>transacts as</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeContractParty"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DerivativeTransactionParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;commitsTo">
      <rdfs:label>commits to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;contractualEconomicCommitmentDescribedIn"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DeliveryCommitment"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DerivativeDeliveryTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;embodies.1">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;embodiesEconomicCommitment"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeDeliveryTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DeliveryCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;contractPartyIdentity"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeContractParty"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;derivativeContractHasTerms">
      <rdfs:label>derivative contract has terms</rdfs:label>
      <skos:definition>A set of terms that form part of the Derivative Contract. These are terms governing aspects of the derivatives transaction between the parties.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;economicContractHasTerms"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DerivativeTransactionTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DerivativeUnderlying"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;identity"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeUnderlying"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;.UnionClass.4"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;hasParty">
      <rdfs:label>has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DerivativeContractParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;subject">
      <rdfs:label>subject</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;SettlementTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-sup;NegotiableCash"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-der;commitsTo.1">
      <rdfs:label>commits to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;contractualEconomicCommitmentDescribedIn"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;SettlementCommitment"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;SettlementTermsSet"/>
   </owl:ObjectProperty>


