  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-ctr-rsw;AssetDerivativeContract">
      <rdfs:label>asset derivative contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-rsw;AssetReturnCommitment">
      <rdfs:label>asset return commitment</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapLegCommitment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-rsw;AssetReturnStream">
      <rdfs:label>asset return stream</rdfs:label>
      <skos:definition>A leg in which the returns on an asset are paid.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;Swapstream"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-rsw;ReturnLeg">
      <rdfs:label>return leg</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapLeg"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-rsw;ReturnSwapContract">
      <rdfs:label>return swap contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-rsw;AssetDerivativeContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-rsw;ReturnSwapTransaction">
      <rdfs:label>return swap transaction</rdfs:label>
      <skos:definition>The transaction of a total Return Swap. This is the transaction in which the return on some asset is exchanged for some agreed payment or series of payments.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapTransaction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-rsw;SimpleReturnStream">
      <rdfs:label>simple return stream</rdfs:label>
      <skos:definition>A leg defining the returns on an underlying instrument, namely the interest payments on a debt instrument or the dividends on an equity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-rsw;AssetReturnStream"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-rsw;TotalReturnSwapContract">
      <rdfs:label>total return swap contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-rsw;ReturnSwapContract"/>
   </owl:Class>


