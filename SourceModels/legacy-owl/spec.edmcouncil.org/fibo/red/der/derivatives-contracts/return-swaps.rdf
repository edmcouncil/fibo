<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-ctr-rsw "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/return-swaps/">
    <!ENTITY fibo-der-ctr-swp "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/">
    <!ENTITY fibo-der-ctr-der "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/return-swaps/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/return-swaps/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-ctr-rsw;hasReferenceAsset">
      <rdfs:label>has reference asset</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;AssetReturnStream"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DerivativeUnderlying"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-rsw;rateOfReturn">
      <rdfs:label>rate of return</rdfs:label>
      <skos:definition>The terms of the initial price of the return type swap and of the subsequent valuations of the underlyer.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;AssetReturnStream"/>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-rsw;definedAs">
      <rdfs:label>defined as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;swapLegDefinedAs"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;AssetReturnCommitment"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-rsw;AssetReturnStream"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-rsw;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;embodies"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;ReturnSwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-rsw;ReturnSwapContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-rsw;hasFundingLeg">
      <rdfs:label>has funding leg</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;hasLeg"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;ReturnSwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-swp;PaymentLeg"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-rsw;hasReturnLeg">
      <rdfs:label>has return leg</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;hasLeg"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;ReturnSwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-rsw;ReturnLeg"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-rsw;embodiesAssetReturnCommitment">
      <rdfs:label>embodies asset return commitment</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;embodiesSwapLegCommitment"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;ReturnLeg"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-rsw;AssetReturnCommitment"/>
   </owl:ObjectProperty>


