<owl:Class rdf:about="&fibo-der-ctr-otcd;OTCDerivativeContract">
      <rdfs:label>o t c derivative contract</rdfs:label>
      <skos:definition>A derivative contract which is struck between two parties "Over the Counter".</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otcd;OTCDerivativeTransaction">
      <rdfs:label>o t c derivative transaction</rdfs:label>
      <skos:definition>A derivative transaction which is struck over the counter between two parties.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeTransaction"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-ext;CoveredTransaction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otcd;OTCProduct">
      <rdfs:label>o t c product</rdfs:label>
      <skos:definition>An Over the Counter Derivative. This is made up of one or more transactions, with a corresponding contract or contracts.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-occ-prod;Production"/>
   </owl:Class>


