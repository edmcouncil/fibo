<owl:ObjectProperty rdf:about="&fibo-der-ctr-spot;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-spot;SpotCashSettlementTermsSet"/>
