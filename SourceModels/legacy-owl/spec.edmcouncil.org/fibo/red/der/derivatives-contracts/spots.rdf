<owl:DatatypeProperty rdf:about="&fibo-der-ctr-spot;settlementDate">
      <rdfs:label>settlement date</rdfs:label>
      <skos:definition>'The date on which both currencies traded will settle.'</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-ctr-spot;settlementInformation">
      <rdfs:label>settlement information</rdfs:label>
      <skos:definition>FpML settlementInformation (in payment) 'The information required to settle a currency payment that results from a trade.'</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-ctr-spot;spotPrice">
