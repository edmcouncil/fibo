<owl:Class rdf:about="&fibo-der-ctr-cf;AssetTypeFacet">
      <skos:definition>Types od Derivatives contract classlfied by their underlying asset type.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-der-ctr-rsw;AssetDerivativeContract"/>
               <owl:Class rdf:about="&fibo-der-fx-fxc;ForeignExchangeDerivativeContract"/>
               <owl:Class rdf:about="&fibo-der-cr-cds;CreditDerivativeContract"/>
               <owl:Class rdf:about="&fibo-der-com-cmc;CommodityDerivativeContract"/>
               <owl:Class rdf:about="&fibo-der-rd-rd;RateBasedDerivativeContract"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-cf;ContractStructureFacet">
