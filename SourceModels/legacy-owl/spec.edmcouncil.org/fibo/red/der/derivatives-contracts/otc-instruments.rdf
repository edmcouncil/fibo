<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-ctr-otc "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/">
    <!ENTITY fibo-fnd-txn-rea "http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/">
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
    <!ENTITY fibo-der-ctr-ma "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-derivative-master-agreements/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-der-ctr-otcd "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-derivatives/">
    <!ENTITY fibo-der-ctr-der "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-derivative-master-agreements/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-derivatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;commitsTo">
      <rdfs:label>commits to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-otc;CashDeliveryCommitment"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-otc;DeliverCash"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;deliveryAmount">
      <rdfs:label>delivery amount</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-otc;DeliverCash"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;paymentType">
      <rdfs:label>payment type</rdfs:label>
      <skos:definition>FpML = paymentType 'A classification of the type of fee or additional payment, e.g. brokerage, upfront fee etc. FpML does not define domain values for this element.'</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-ctr-otc;DeliverCash"/>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;derivativeContractEmbodies">
      <rdfs:label>derivative contract embodies</rdfs:label>
      <skos:definition>The agreement which the contract describes as obtaining between the parties to the Derivatives Contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;economicContractEmbodies"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-der;DerivativeAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;oTCDerivativeContractEmbodies">
      <rdfs:label>o t c derivative contract embodies</rdfs:label>
      <skos:definition>The agreement which the contract describes as obtaining between the parties to the OTC Derivatives Contract and transaction.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-otc;oTCInstrumentContractEmbodies"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-otcd;OTCDerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-otc;OTCDerivativeAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;supersedes">
      <rdfs:label>supersedes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;supersedes"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-otc;OTCInstrumentConfirmation"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;governs">
      <rdfs:label>governs</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-ma;governs"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-otc;OTCInstrumentConfirmation"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-otcd;OTCDerivativeTransaction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-otc;oTCInstrumentContractEmbodies">
      <rdfs:label>o t c instrument contract embodies</rdfs:label>
      <skos:definition>The economic agreement (transaction related agreement) which the OTC Instrument Contract describes as obtaining between the parties to the Contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;economicContractEmbodies"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-otc;OTCTransactionAgreement"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-ctr-otc;deliveryDate">
      <rdfs:label>delivery date</rdfs:label>
      <skos:definition>'The date on which both currencies traded will settle.'</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-ctr-otc;CashDeliveryCommitment">
      <rdfs:label>cash delivery commitment</rdfs:label>
      <skos:definition>Commitment to deliver cash at the earliest available date as per settlement convention. This is the same date as a settlement date when the corresponding cash is made available in the other currency, in settlement for this amount. FpML exchangedCurrency1 referred to as a "Currency Stream"</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DeliveryCommitment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otc;DeliverCash">
      <rdfs:label>deliver cash</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeDeliveryTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otc;OTCContractSettlementTerms">
      <rdfs:label>o t c contract settlement terms</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otc;OTCDerivativeAgreement">
      <rdfs:label>o t c derivative agreement</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCTransactionAgreement"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeAgreement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otc;OTCInstrumentConfirmation">
      <rdfs:label>o t c instrument confirmation</rdfs:label>
      <skos:definition>A formal confirmation document which constitutes a written contract between the parties to the transaction, setting out the terms and conditions of the transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;WrittenContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otc;OTCInstrumentContract">
      <rdfs:label>o t c instrument contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-otc;OTCTransactionAgreement">
      <rdfs:label>o t c transaction agreement</rdfs:label>
      <skos:definition>The agreement which forms the basis of any Over the Counter (OTC) transaction, including OTC derivatives and bilateral debt instruments.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>