         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-derivatives/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-derivatives/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/return-swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/debt-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/market-indices/basket-indices/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-baskets/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/baskets/"/>
