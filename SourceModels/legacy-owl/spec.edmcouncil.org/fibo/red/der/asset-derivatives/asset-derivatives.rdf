<owl:ObjectProperty rdf:about="&fibo-der-ass-ass;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-rsw;AssetDerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;AssetUnderlyer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;identity.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;AssetUnderlyer"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;.UnionClass.4"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;hasUnderlying.1">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;DebtDerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;DebtObservable"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;identity"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;DebtObservable"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;hasUnderlying.2">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;EquityDerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;EquityObservable"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;identity.2">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;identity.3"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;EquityIndexObservable"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;EquityReferenceIndex"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;identity.3">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;identity"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;EquityObservable"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;.UnionClass.3"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;definedAs">
      <rdfs:label>defined as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;swapLegDefinedAs"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;RealizedVariableLeg"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;RealizedVariableSwapSideTerms"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;forwardPrice">
      <rdfs:label>forward price</rdfs:label>
      <skos:definition>The forward price per share, index or basket.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;SecurityForwardDelivery"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;identity.4">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;identity.3"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;SetOfEquityIndexConstituentsObservable"/>
      <rdfs:range rdf:resource="&fibo-der-ass-bas;SetOfEquityIndexConstituents"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;drawnFrom">
      <rdfs:label>drawn from</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;SetOfEquityIndexConstituents"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;EquityReferenceIndex"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;hasLeg">
      <rdfs:label>has leg</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;hasLeg"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;SingleSettlementSwap"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;RealizedVariableLeg"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;hasStrikeLeg">
      <rdfs:label>has strike leg</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;hasLeg"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;SingleSettlementSwap"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-swp;StrikeLeg"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-ass;identity.5">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;identity.3"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;SingleShareObservable"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-eqi;PubliclyTradedShare"/>
   </owl:ObjectProperty>


