<owl:DatatypeProperty rdf:about="&fibo-der-ass-ass;cashDelivery">
      <rdfs:label>cash delivery</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-ass-ass;cashDelivery.1">
      <rdfs:label>cash delivery</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


