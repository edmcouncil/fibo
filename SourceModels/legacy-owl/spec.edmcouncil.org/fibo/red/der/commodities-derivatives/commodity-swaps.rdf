<owl:ObjectProperty rdf:about="&fibo-der-com-swp;hasReferenceAsset">
      <rdfs:label>has reference asset</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-rsw;hasReferenceAsset"/>
      <rdfs:domain rdf:resource="&fibo-der-com-swp;CommodityReturnLeg"/>
      <rdfs:range rdf:resource="&fibo-der-com-cmc;CommodityUnderlying"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-swp;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;embodies"/>
      <rdfs:domain rdf:resource="&fibo-der-com-swp;CommoditySwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-com-swp;CommoditySwapContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-swp;hasReturnLeg">
      <rdfs:label>has return leg</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-rsw;hasReturnLeg"/>
      <rdfs:domain rdf:resource="&fibo-der-com-swp;CommoditySwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-com-swp;CommodityReturnLeg"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-swp;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-com-cmc;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-com-swp;CommoditySwapContract"/>
      <rdfs:range rdf:resource="&fibo-der-com-cmc;CommodityUnderlying"/>
   </owl:ObjectProperty>


