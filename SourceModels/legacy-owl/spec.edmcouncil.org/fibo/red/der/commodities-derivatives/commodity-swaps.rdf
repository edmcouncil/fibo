<owl:Class rdf:about="&fibo-der-com-swp;CommodityReturnLeg">
      <rdfs:label>commodity return leg</rdfs:label>
      <skos:definition>Return on the value of an underlying commodity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-rsw;AssetReturnStream"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-swp;CommoditySwapContract">
      <rdfs:label>commodity swap contract</rdfs:label>
      <skos:definition>Definition needed.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-cmc;CommodityDerivativeContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-swp;CommoditySwapTransaction">
      <rdfs:label>commodity swap transaction</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-rsw;ReturnSwapTransaction"/>
   </owl:Class>


