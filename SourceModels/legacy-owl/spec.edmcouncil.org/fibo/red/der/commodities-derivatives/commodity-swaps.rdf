   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodity-swaps/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/return-swaps/"/>
   </owl:Ontology>


