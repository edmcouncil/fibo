<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-com-del "http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-delivery/">
    <!ENTITY fibo-fnd-plcx-ads "http://spec.edmcouncil.org/fibo/red/fnd/places-ext/address-sites/">
    <!ENTITY fibo-der-ctr-for "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-delivery/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-delivery/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/address-sites/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-der-com-del;BullionTypeSelection">
      <skos:definition>A type of bullion i.e. a metal which will be defined as a bullion commodity.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gold</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Silver</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Platinum</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-com-del;totalPhysicalQuantity">
      <rdfs:label>total physical quantity</rdfs:label>
      <skos:definition>The amount of the bullion to be delivered, expressed in the specified units.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-com-del;BullionPhysicalDelivery"/>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-del;deliverTo">
      <rdfs:label>deliver to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-com-del;CommodityPhysicalDelivery"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-ads;BusinessSite"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-com-del;bullionType">
      <rdfs:label>bullion type</rdfs:label>
      <skos:definition>The type of bullion i.e. the metal which this bullion commodity is (Gold, Silver etc.).</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-com-del;BullionTypeSelection"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-com-del;AgriculturalProductsDelivery">
      <rdfs:label>agricultural products delivery</rdfs:label>
      <skos:definition>Delivery of any agricultural produce.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-del;CommodityPhysicalDelivery"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;BaseMetalsDelivery">
      <rdfs:label>base metals delivery</rdfs:label>
      <skos:definition>Delivery of base metals.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;BullionPhysicalDelivery">
      <rdfs:label>bullion physical delivery</rdfs:label>
      <skos:definition>Delivery of any precious metal.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;CommodityContractCashCloseout">
      <rdfs:label>commodity contract cash closeout</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-del;CommodityForwardDelivery"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;CashCloseout"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;CommodityForwardDelivery">
      <rdfs:label>commodity forward delivery</rdfs:label>
      <skos:definition>The agreed forward delivery of a commodity or cash equivalent.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardDeliveryTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;CommodityPhysicalDelivery">
      <rdfs:label>commodity physical delivery</rdfs:label>
      <skos:definition>The physical delivery of some commodity at some agreed point in the future.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;EnergyFlowsDelivery">
      <rdfs:label>energy flows delivery</rdfs:label>
      <skos:definition>Financial Transmission Rights.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-del;EnergyPhysicalDelivery"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;EnergyPhysicalDelivery">
      <rdfs:label>energy physical delivery</rdfs:label>
      <skos:definition>Delivery of oil, gas and electricity.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;GasDelivery">
      <rdfs:label>gas delivery</rdfs:label>
      <skos:definition>Delivery of fuel gas.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-del;OilDelivery">
      <rdfs:label>oil delivery</rdfs:label>
      <skos:definition>Delivery of oil.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>