<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-com-com "http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities/">
    <!ENTITY fibo-der-com-del "http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-delivery/">
    <!ENTITY fibo-fnd-bus-ecr "http://spec.edmcouncil.org/fibo/red/fnd/business/marketable-economic-resource/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-delivery/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/business/marketable-economic-resource/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-der-com-com;BullionMeasurementUnit">
      <skos:definition>Units in which bullion may be measured.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Imperial</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Troy</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Metric</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-der-com-com;OilGradeSelection">
      <skos:definition>The grade of oil which is defined as this oil commodity.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Brent Crude</rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-com-com;bullionType">
      <rdfs:label>bullion type</rdfs:label>
      <skos:definition>The type of bullion i.e. the metal which this bullion commodity is (Gold, Silver etc.).</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-com-del;BullionTypeSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-com-com;measurementUnit">
      <rdfs:label>measurement unit</rdfs:label>
      <skos:definition>The unit of measure which is used to measure this metal in the context of it being defined as a negotiable, bullion commodity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-com-com;BullionMeasurementUnit"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-com-com;grade">
      <rdfs:label>grade</rdfs:label>
      <skos:definition>The grade of oil e.g. Brent Crude.</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-com-com;OilGradeSelection"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-com-com;AgriculturalProduct">
      <rdfs:label>agricultural product</rdfs:label>
      <skos:definition>A commodity which is some agrigultural product, including grain, fruit and meat.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-bus-ecr;NegotiableCommodity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;BaseMetal">
      <rdfs:label>base metal</rdfs:label>
      <skos:definition>Any metal which is not precious.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;Bullion">
      <rdfs:label>bullion</rdfs:label>
      <skos:definition>Any precious metal.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;ElectricityCommodity">
      <rdfs:label>electricity commodity</rdfs:label>
      <skos:definition>Electricity as a commodity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-com;EnergyCommodity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;EnergyCommodity">
      <rdfs:label>energy commodity</rdfs:label>
      <skos:definition>A commodity which relates to the provision or distribution of energy.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;EnergyTransmissionRights">
      <rdfs:label>energy transmission rights</rdfs:label>
      <skos:definition>Rights to the transmission of power across an electricity distribution network.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;GasCommodity">
      <rdfs:label>gas commodity</rdfs:label>
      <skos:definition>Combustible hydrocarbon in gasous phase.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;GrainCommodity">
      <rdfs:label>grain commodity</rdfs:label>
      <skos:definition>A commodity which is a kind of grain, such as wheat.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-com;AgriculturalProduct"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;MeatCommodity">
      <rdfs:label>meat commodity</rdfs:label>
      <skos:definition>A commodity which is a kind of meat, such as pork bellies.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-com;OilCommodity">
      <rdfs:label>oil commodity</rdfs:label>
      <skos:definition>Oil as a commodity.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
