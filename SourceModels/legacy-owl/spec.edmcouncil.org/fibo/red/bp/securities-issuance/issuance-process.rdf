    <!ENTITY fibo-sec-sec-iss "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/">
    <!ENTITY fibo-bp-iss-muni "http://spec.edmcouncil.org/fibo/red/bp/securities-issuance/muni-issuance/">
    <!ENTITY fibo-fnd-occ-act "http://spec.edmcouncil.org/fibo/red/fnd/occurrent/activities/">
