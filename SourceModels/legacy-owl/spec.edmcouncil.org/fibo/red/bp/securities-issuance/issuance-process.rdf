<owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;isGuarantor">
      <rdfs:label>is guarantor</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;processRole"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;SecurityIssuanceGuarantor"/>
      <rdfs:range rdf:resource="&fibo-bp-iss-iss;SecuritiesIssuanceProcess"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;distributionType">
      <rdfs:label>distribution type</rdfs:label>
      <skos:definition>Describes whether this underwriting is a Primary Offering or Secondary Offering (where Secondary includes Add-ons or Remarketings using the same CUSIP). These are: Primary Offering - The original sale of a company's securities, in which the proceeds from the sale are received directly by the company; Secondary Offering - An Offering of a security which has been previously issued.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;Offering"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;SecurityOfferingDistributionType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;offeringType">
      <rdfs:label>offering type</rdfs:label>
      <skos:definition>The type of Offering.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;Offering"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;SecurityOfferingType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;saleMethod">
      <rdfs:label>sale method</rdfs:label>
      <skos:definition>Sale Method of the security.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;Offering"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;SecurityOfferingSaleMethod"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;pertainsTo">
      <rdfs:label>pertains to</rdfs:label>
      <skos:definition>The Offering Document that this information pertains to.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;OfferingDocumentInformation"/>
      <rdfs:range rdf:resource="&fibo-bp-iss-iss;OfferingDocument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-bp-iss-iss;defines.1"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;Offering"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;OfferIssue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;requiredToMakeEligible">
      <rdfs:label>required to make eligible</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;refers"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;Offering"/>
      <rdfs:range rdf:resource="&fibo-bp-iss-iss;OfferingDocument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;becomes">
      <rdfs:label>becomes</rdfs:label>
      <skos:definition>The Potential Issuer (Actor in the Issuance Proces) becomes the Issuer (Party to the Security contract).</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-act;becomes"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;PotentialIssuer"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;SecurityIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;becomes.1">
      <rdfs:label>becomes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-act;becomes"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;PotentialUnderwriter"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;SecurityUnderwriter"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;defines.1">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-prod;defines"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;SecuritiesIssuanceProcess"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;OfferIssue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;givesRiseTo">
      <rdfs:label>gives rise to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-bp-iss-muni;SecuritiesUnderwritingIssuance"/>
      <rdfs:range rdf:resource="&fibo-bp-iss-muni;IssueSecurities"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;produces">
      <rdfs:label>produces</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;givesRiseTo"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;SecuritiesIssuanceProcess"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;TradedInstrumentIssuanceProcessInformation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;refersToIssuanceOf">
      <rdfs:label>refers to issuance of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-bp-iss-muni;SecuritiesIssuanceContext"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;context">
      <rdfs:label>context</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-prod;context"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-iss;OfferIssue"/>
      <rdfs:range rdf:resource="&fibo-bp-iss-muni;SecuritiesIssuanceContext"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;isIssueOf">
      <rdfs:label>is issue of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-bp-iss-muni;IssueSecurities"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-iss-iss;isPotentialIssuer">
      <rdfs:label>is potential issuer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;processRole"/>
      <rdfs:domain rdf:resource="&fibo-bp-iss-iss;PotentialIssuer"/>
      <rdfs:range rdf:resource="&fibo-bp-iss-iss;SecuritiesIssuanceProcess"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;expectedClosingDate">
      <rdfs:label>expected closing date</rdfs:label>
      <skos:definition>The date on which the transfer of positions to underwriters is expected to take place. This date is provided by underwriters as part of an announcement.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;expectedClosingDate.1">
      <rdfs:label>expected closing date</rdfs:label>
      <skos:definition>The date on which the transfer of positions to underwriters is expected to take place. This date is provided by underwriters as part of an announcement.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;firstTradeDateAndTime">
      <rdfs:label>first trade date and time</rdfs:label>
      <skos:definition>First Date and Time a trade may be executed for this security. All times in Eastern Time Zone only. NOTE: This is a date in the future tense at the time of the Offering.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;dateAndTime"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;formalAwardDateAndTime">
      <rdfs:label>formal award date and time</rdfs:label>
      <skos:definition>Date and time the issuer formally accepts a bid for Competitive Issues or, the Date and Time the Bond Purchase Agreement is executed for Negotiated Issues. Time Zone: Include in date/time data or add a term for it?</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;dateAndTime"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;legalDescription">
      <rdfs:label>legal description</rdfs:label>
      <skos:definition>The legal description of the Offering, as existing in the Prospectus or Offering statement or another applicable document.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;documentDate">
      <rdfs:label>document date</rdfs:label>
      <skos:definition>The date stated in the document, from which the document is considered 'Official' or Released.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;documentType">
      <rdfs:label>document type</rdfs:label>
      <skos:definition>The type of document that is required to be sent in an Offering to make the securities in the Offering eligible at DTC.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-bp-iss-iss;version">
      <rdfs:label>version</rdfs:label>
      <skos:definition>The version of the document, i.e. whether it is a Draft or a Final version. Note: This is not the revision number, if that exists.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-info-doc;DocumentVersionType"/>
   </owl:DatatypeProperty>


  <!--
