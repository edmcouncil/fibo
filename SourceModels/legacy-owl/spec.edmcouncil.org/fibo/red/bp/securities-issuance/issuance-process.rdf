<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/bp/securities-issuance/issuance-process/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/bp/securities-issuance/issuance-process/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/documentation/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/processes/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/bp/securities-issuance/debt-issuance/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/production/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/bp/securities-issuance/muni-issuance/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/activities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
   </owl:Ontology>


