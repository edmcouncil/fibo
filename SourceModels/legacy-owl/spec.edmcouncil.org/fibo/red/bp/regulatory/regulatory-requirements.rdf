<owl:ObjectProperty rdf:about="&fibo-bp-reg-reg;mayDrive">
      <rdfs:label>may drive</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-bp-reg-reg;RegulatoryRule"/>
      <rdfs:range rdf:resource="&fibo-bp-reg-reg;OperationalRule"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-reg-reg;mandates">
      <rdfs:label>mandates</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-bp-reg-reg;RegulatoryRule"/>
      <rdfs:range rdf:resource="&fibo-bp-reg-reg;RegulatoryRequirement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-reg-reg;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
      <rdfs:range rdf:resource="&fibo-bp-reg-reg;LegalRequirement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-bp-reg-reg;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-bp-reg-reg;LegalRequirement"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
   </owl:ObjectProperty>


