<owl:Class rdf:about="&fibo-bp-reg-reg;BusinessRequirement">
      <rdfs:label>business requirement</rdfs:label>
      <skos:definition>A requirement driven by business imperatives.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-bp-reg-reg;Requirement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;BusinessRule">
      <rdfs:label>business rule</rdfs:label>
      <skos:definition>A rule which applies in some business context. Includes operations rules, regulatory rules and so on.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-bus-pol;Rule"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;LegalRequirement">
      <rdfs:label>legal requirement</rdfs:label>
      <skos:definition>A requirement driven by legal instruments, that is a requirement which is defined in and mandated by some body of statute law.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;OperationalRequirement">
      <rdfs:label>operational requirement</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-bp-reg-reg;BusinessRequirement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;OperationalRule">
      <rdfs:label>operational rule</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-bp-reg-reg;BusinessRule"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;ProductRequirement">
      <rdfs:label>product requirement</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;RegulatoryOperationalRequirement">
      <rdfs:label>regulatory operational requirement</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-lawx-rst;RegulatoryRequirement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;RegulatoryRegistrationRequirement">
      <rdfs:label>regulatory registration requirement</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;RegulatoryReportingRequirement">
      <rdfs:label>regulatory reporting requirement</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;RegulatoryRequirement">
      <rdfs:label>regulatory requirement</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-bp-reg-reg;LegalRequirement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;RegulatoryRule">
      <rdfs:label>regulatory rule</rdfs:label>
      <skos:definition>A rule which has compliance mandates backed by some Government, with associated lenalties.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;Requirement">
      <rdfs:label>requirement</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;RiskMitigationRequirement">
      <rdfs:label>risk mitigation requirement</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-bp-reg-reg;SystemRequirement">
      <rdfs:label>system requirement</rdfs:label>
      <skos:definition>A requirement to be met in some System.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-bp-reg-reg;ProductRequirement"/>
   </owl:Class>


