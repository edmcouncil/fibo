   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/bp/regulatory/regulatory-requirements/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/business/policy/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law-ext/restrictions/"/>
   </owl:Ontology>


