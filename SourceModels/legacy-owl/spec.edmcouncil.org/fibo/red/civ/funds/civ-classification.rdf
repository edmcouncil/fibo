<owl:Class rdf:about="&fibo-civ-fun-cls;FundClassification">
      <rdfs:label>fund classification</rdfs:label>
      <skos:definition>The category of the fund according to the asset class. This is a published code by which the Fund is classified. Further notes: Could be EFCF codes. This is the way in which "Type of fund" is articulated.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;Classification"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-civ-fun-cls;FundClassificationScheme">
      <rdfs:label>fund classification scheme</rdfs:label>
      <skos:definition>A published scheme for the category of a fund according to the asset class.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;ClassificationScheme"/>
   </owl:Class>


