<owl:ObjectProperty rdf:about="&fibo-civ-fun-cls;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;classificationSchemeDefines"/>
      <rdfs:domain rdf:resource="&fibo-civ-fun-cls;FundClassificationScheme"/>
      <rdfs:range rdf:resource="&fibo-civ-fun-cls;FundClassification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-civ-fun-cls;classifies">
      <rdfs:label>classifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;classifies"/>
      <rdfs:domain rdf:resource="&fibo-civ-fun-cls;FundClassification"/>
      <rdfs:range rdf:resource="&fibo-civ-fun-civ;Fund"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-civ-fun-cls;typeOfFund">
      <rdfs:label>type of fund</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-civ-fun-civ;Fund"/>
      <rdfs:range rdf:resource="&fibo-civ-fun-cls;FundClassification"/>
      <owl:inverseOf rdf:resource="&fibo-civ-fun-cls;classifies"/>
   </owl:ObjectProperty>


