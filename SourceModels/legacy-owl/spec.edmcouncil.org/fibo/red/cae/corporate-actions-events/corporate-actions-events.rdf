   <owl:Class rdf:about="&fibo-cae-cae-cae;ConsentSolicitationStart">
      <rdfs:label>consent solicitation start</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-cae-cae-cae;ConversionAction">
