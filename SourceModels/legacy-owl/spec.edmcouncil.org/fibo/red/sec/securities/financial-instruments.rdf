<owl:Class rdf:about="&fibo-sec-sec-fi;FinancialInstrument">
      <skos:definition>Financial instruments are cash, evidence of an ownership interest in an entity, or a contractual right to receive, or deliver, cash or another financial instrument.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-acc-acc;CashCurrency"/>
               <owl:Class rdf:about="&fibo-sec-sec-sec;Security"/>
               <owl:Class rdf:about="&fibo-der-ctr-otc;OTCInstrumentContract"/>
               <owl:Class rdf:about="&fibo-der-ctr-der;DerivativeContract"/>
               <owl:Class rdf:about="&fibo-sec-dbt-dbt-ins;NonTradableDebtInstrument"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>


