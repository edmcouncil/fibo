<owl:ObjectProperty rdf:about="&fibo-sec-sec-prt;denomination">
      <rdfs:label>denomination</rdfs:label>
      <skos:definition>The currency in which the portfolio holding is denominated.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-prt;PortfolioHolding"/>
