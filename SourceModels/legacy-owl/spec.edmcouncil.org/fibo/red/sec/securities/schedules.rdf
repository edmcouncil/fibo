    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-tim-tim;ContractuallyDeterminedDate"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;BusinessCalendarCode"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;regularPeriodDuration">
      <rdfs:label>regular period duration</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-tim-tim;hasPeriodDuration"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfl;CouponPeriodicDateSeries"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfl;CouponRegularPeriod"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;modifiedBy">
      <rdfs:label>modified by</rdfs:label>
      <skos:definition>This is the top Relationship Fact in this hierarchy.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-tim-tim;DateDeterminationReferenceRule"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;DateDeterminationReferenceRule"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;modifiedBy.1">
      <rdfs:label>modified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-sch;modifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-tim-tim;DateRollRule"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;ModifyConventionRollRule"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;specifies">
      <rdfs:label>specifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-tim-tim;scheduledEvent"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfl;ExplicitCouponScheduleListing"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfl;ScheduledCouponPayment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;eventDate">
      <rdfs:label>event date</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-evt;when"/>
      <rdfs:domain rdf:resource="&fibo-fnd-tim-tim;ExplicitlyScheduledEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;ScheduledEventDate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;expressedAs">
      <rdfs:label>expressed as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-tim-tim;expressedAs"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-cfl;PutSchedule"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-cfl;PutScheduleListing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;occursOn">
      <rdfs:label>occurs on</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-sch;eventDate"/>
      <rdfs:domain rdf:resource="&fibo-md-dbt-py;PutOfIssue"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sch;PutDate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;expressedAs.1">
      <rdfs:label>expressed as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-bond-cfl;expressedAs"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sch;ShareCouponSchedule"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-sh;ShareCouponScheduleSpecification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sch;occursOn.1">
      <rdfs:label>occurs on</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-evt;when"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sch;ScheduledEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Schedule"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-tim-tim;schedules"/>
   </owl:ObjectProperty>


