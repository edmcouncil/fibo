<owl:DatatypeProperty rdf:about="&fibo-sec-sec-sch;description">
      <rdfs:label>description</rdfs:label>
      <skos:definition>Textual description of the FRN Date Rule.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sch;reference">
      <rdfs:label>reference</rdfs:label>
      <skos:definition>Textual reference to the document and section that contains the rule.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


