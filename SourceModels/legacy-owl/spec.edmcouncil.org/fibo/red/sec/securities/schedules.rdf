<rdfs:Datatype rdf:about="&fibo-sec-sec-sch;DateRollRuleDirection">
      <skos:definition>The selection of possible directions in which to roll dates in a Date Roll Rule or in a Modify Convention Roll Rule.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nearest</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Roll Forward</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Roll Back</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


