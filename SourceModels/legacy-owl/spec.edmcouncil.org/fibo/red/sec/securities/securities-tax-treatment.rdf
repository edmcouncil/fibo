<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-sec-tax "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-tax-treatment/">
    <!ENTITY fibo-fnd-acc-tax "http://spec.edmcouncil.org/fibo/red/fnd/accounting/taxation/">
    <!ENTITY fibo-sec-sec-fi "http://spec.edmcouncil.org/fibo/red/sec/securities/financial-instruments/">
    <!ENTITY fibo-sec-sec-sec "http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-tax-treatment/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-tax-treatment/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/taxation/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/financial-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-sec-tax;hasTaxTreatment">
      <rdfs:label>has tax treatment</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-fi;FinancialInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-tax;FinancialInstrumentTaxTreatment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-tax;hasTaxTreatment.1">
      <rdfs:label>has tax treatment</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-tax;hasTaxTreatment"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-tax;SecurityTaxTreatment"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-sec-tax;exempt">
      <rdfs:label>exempt</rdfs:label>
      <skos:definition>Whether the tax rule applies within the jurisdiction as a condition of this instrument.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-tax;frankedRate">
      <rdfs:label>franked rate</rdfs:label>
      <skos:definition>Percentage of dividend for which tax is already paid.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-tax;securityTaxCode">
      <rdfs:label>security tax code</rdfs:label>
      <skos:definition>The tax code of the security in the stated tax jurisdiction.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-sec-tax;FinancialInstrumentTaxTreatment">
      <rdfs:label>financial instrument tax treatment</rdfs:label>
      <skos:definition>The treatment of the financial instrument for taxation purposes. This is the most general form of tax treatment for financial instruments, including cash.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-tax;TaxTreatment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-tax;SecurityTaxTreatment">
      <rdfs:label>security tax treatment</rdfs:label>
      <skos:definition>Amount of money due to the government or tax authority, according to various pre-defined parameters such as thresholds or income.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-tax;FinancialInstrumentTaxTreatment"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>