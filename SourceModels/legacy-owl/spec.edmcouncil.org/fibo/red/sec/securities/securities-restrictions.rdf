   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-sec-sec-rst;InvestorDomicileType">
      <skos:definition>The type of an Investor, for example for use in defining a Restriction on Holders.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Citizen</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Legal Resident</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Individual</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-sec-sec-rst;InvestorType">
      <skos:definition>Type of Investor, in terms of their role and standing (as distinct from residency or citizenship status).</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Physical Person</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Staff</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Retail</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Professional</rdf:first>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;notClearedIn">
      <rdfs:label>not cleared in</rdfs:label>
      <skos:definition>List of the states where issue is NOT cleared under Blue Sky Restrictions (an exception list).</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;BlueSkyRegulation"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-gpli;USState"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;hasRestrictions">
      <rdfs:label>has restrictions</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-rst;hasRestrictions.1"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;ExchangeTradedSecurity"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestmentRestriction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;hasRestrictions.1">
      <rdfs:label>has restrictions</rdfs:label>
      <skos:definition>Restrictions applicable to the Security</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-fi;FinancialInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestmentRestriction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;restricts">
      <rdfs:label>restricts</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lawx-rst;restricts.1"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;InvestmentRestriction"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-fi;FinancialInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;investorDomicileRestrictionType">
      <rdfs:label>investor domicile restriction type</rdfs:label>
      <skos:definition>Specifies whether the restriction to be applied is relevant for citizen, resident, country. This property must be applied for this type of Restriction (it is optional in the parent class).</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;InvestorsDomicileRestriction"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestorDomicileType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;holdingPeriod">
      <rdfs:label>holding period</rdfs:label>
      <skos:definition>Period during which only Accredited Investors may hold the Security.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;QualifiedInvestorsRestriction"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;investorType">
      <rdfs:label>investor type</rdfs:label>
      <skos:definition>Type of investor that is allowed to hold the security. This property must be applied for this type of Restriction (it is optional in the parent class).</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;QualifiedInvestorsRestriction"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestorType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;appliesTo">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;SecurityRegistrationRequirement"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
      <owl:inverseOf rdf:resource="&fibo-sec-sec-rst;hasRegistrationRequirement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;holdingPeriod.1">
      <rdfs:label>holding period</rdfs:label>
      <skos:definition>Two-year holding period requirement on privately placed securities to permit qualified institutional buyers to trade these positions among themselves.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;Restriction144A"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;investorType.1">
      <rdfs:label>investor type</rdfs:label>
      <skos:definition>Investor type is restricted to Professional Investors for the stipulated time period.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;Restriction144A"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestorType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;contractuallyDefinesRestrictions">
      <rdfs:label>contractually defines restrictions</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lawx-rst;defines"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;SecurityContractualRestriction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;hasRestriction">
      <rdfs:label>has restriction</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-rst;hasRestrictions.1"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestmentRestriction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;hasRegistrationRequirement">
      <rdfs:label>has registration requirement</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;SecurityRegistrationRequirement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;holderRestrictions">
      <rdfs:label>holder restrictions</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-rst;hasRestrictions.1"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;LegalHoldingRestriction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;investorDomicileRestrictionType.1">
      <rdfs:label>investor domicile restriction type</rdfs:label>
      <skos:definition>Specifies whether the restriction to be applied is relevant for citizen, resident, country.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;LegalHoldingRestriction"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestorDomicileType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;investorType.2">
      <rdfs:label>investor type</rdfs:label>
      <skos:definition>Type of investor that is allowed to hold the security.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;LegalHoldingRestriction"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestorType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;appliesIn">
      <rdfs:label>applies in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lawx-rst;appliesIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;InvestmentRestriction"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-bp-reg-reg;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;InvestmentRegulatoryRequirement"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestmentRegulatoryLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;effectivePeriod">
      <rdfs:label>effective period</rdfs:label>
      <skos:definition>Period during which the restriction applies.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;InvestmentRestriction"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;DatePeriod"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rst;definedIn.1">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lawx-rst;definedIn.1"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-rst;InvestmentRestriction"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestmentRegulatoryLaw"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-sec-rst;inclusive">
      <rdfs:label>inclusive</rdfs:label>
      <skos:definition>The Blue Sky Regulatory Requirement is framed in terms of the States NOT covered by the Regulation.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-rst;inclusive.1">
      <rdfs:label>inclusive</rdfs:label>
      <skos:definition>Note: Regulation S is an exclusive restriction for Investor Domicile Type (Type = Resident, Inclusive = no), but Exclusive for Investor Type (Type = Professional, Inclusive = yes). Remodel this during review.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-rst;inclusive.2">
      <rdfs:label>inclusive</rdfs:label>
      <skos:definition>The restriction includes the party mentioned in the investor restriction type.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-rst;inclusive.3">
      <rdfs:label>inclusive</rdfs:label>
      <skos:definition>Whether the Restriction is defined inclusively (the stated terms put something within the Restriction), or Exclusively (the stated terms put something outside the reach or remit of the Restriction). Yes = Inclusive.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-rst;inclusive.4">
      <rdfs:label>inclusive</rdfs:label>
      <skos:definition>Whether the restriction includes or excludes the party mentioned in the investor restriction type.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-rst;description">
      <rdfs:label>description</rdfs:label>
      <skos:definition>Free text description of the restriction.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-rst;rate">
      <rdfs:label>rate</rdfs:label>
      <skos:definition>Rate used for the calculation of the restriction.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>


