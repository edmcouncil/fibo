    <!ENTITY fibo-bp-reg-reg "http://spec.edmcouncil.org/fibo/red/bp/regulatory/regulatory-requirements/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-sec-sec-sec "http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
    <!ENTITY fibo-sec-sec-fi "http://spec.edmcouncil.org/fibo/red/sec/securities/financial-instruments/">
