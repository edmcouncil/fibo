<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-sec-ast "http://spec.edmcouncil.org/fibo/red/sec/securities/security-assets/">
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-sec-sec-sec "http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
    <!ENTITY fibo-fnd-oac-own "http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/securities/security-assets/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/securities/security-assets/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-sec-ast;consistsOf">
      <rdfs:label>consists of</rdfs:label>
      <skos:definition>The or a security which makes up the Security Holding.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-ast;SecurityHolding"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-ast;heldBy">
      <rdfs:label>held by</rdfs:label>
      <skos:definition>The holder of the security, that is the party of which this is the Security holding.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;isAssetOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-ast;SecurityHolding"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-ast;SecurityHolder"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-ast;holder">
      <rdfs:label>holder</rdfs:label>
      <skos:definition>The party which holds the security and is entitled to the rights contractually defined in the security terms.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;heldBy"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-ast;SecurityHolder"/>
   </owl:ObjectProperty>


  <!--
