<owl:Class rdf:about="&fibo-sec-sec-iss;AllotmentInformation">
      <rdfs:label>allotment information</rdfs:label>
      <skos:definition>Information about the allotment of quantities of the issue to different subscribers. This relates a single instrument allotment against the subscription amounts allotted to each Subscriber.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-iss;TradedInstrumentIssuanceProcessInformation"/>
   </owl:Class>

