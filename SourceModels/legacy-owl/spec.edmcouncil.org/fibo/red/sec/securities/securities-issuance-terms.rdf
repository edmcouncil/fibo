<owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;actualClosingDate">
      <rdfs:label>actual closing date</rdfs:label>
      <skos:definition>The Closing Date for the Issue, recorded after the event.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;announcementDate">
      <rdfs:label>announcement date</rdfs:label>
      <skos:definition>Date/time, as announced by the issuer, at which the securities were to be issued and subsequently were issued.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;definedAsSecurity">
      <rdfs:label>defined as security</rdfs:label>
      <skos:definition>Whether the Security is a Security as defined in relevant Act for the appropriate Jurisdiction.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;firstTradeDate">
      <rdfs:label>first trade date</rdfs:label>
      <skos:definition>The date of the first trade of the security in the secondary market.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;firstTradeSettlementDate">
      <rdfs:label>first trade settlement date</rdfs:label>
      <skos:definition>The date the Issuer and Underwriter exchange money for bonds.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;issueNominalNumberOfUnits">
      <rdfs:label>issue nominal number of units</rdfs:label>
      <skos:definition>Total original quantity of securities issued.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;registeredWithRegulator">
      <rdfs:label>registered with regulator</rdfs:label>
      <skos:definition>Indicates if the security of the issue has been registered with the Regulator for the appropriate Jurisdiction, pursuant to the relevant Act.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;series">
      <rdfs:label>series</rdfs:label>
      <skos:definition>Series identification for the individual Traded Financial Security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;issueOfferingUnits">
      <rdfs:label>issue offering units</rdfs:label>
      <skos:definition>Quantity of all securities that is offered in an equity or Corporate Bond issue.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;series.1">
      <rdfs:label>series</rdfs:label>
      <skos:definition>Uniquely identified collection of securities within an Issue with same Expected Closing Date. The text gives the Series Identifier within the Issuance process. There may be one or more Series within one Issue.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;registeredWithRegulator.1">
      <rdfs:label>registered with regulator</rdfs:label>
      <skos:definition>Whether the security of the issue has been registered with the appropriate regulator in its jurisdiction.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;overAlloted">
      <rdfs:label>over alloted</rdfs:label>
      <skos:definition>Whether the subscriber has been over-alloted the requested number of units. REVIEW: Not seen in data terms but implied elsewhere.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;subscriptionAmount">
      <rdfs:label>subscription amount</rdfs:label>
      <skos:definition>Number of units of the issue that an individual subscriber is allocated.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;definedAsSecurity.1">
      <rdfs:label>defined as security</rdfs:label>
      <skos:definition>Whether the Security is A Security as defined in Article 8 of the New York Uniform Commercial Code or the equivalent for other Jurisdictions. Known as Article 8 in the NY Jurisdiction.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-iss;underwriterTakedownShares">
      <rdfs:label>underwriter takedown shares</rdfs:label>
      <skos:definition>Takedown quantity of the security handled by the underwriter (that will be brought into DTC).</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>


