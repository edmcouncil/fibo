<owl:ObjectProperty rdf:about="&fibo-sec-sec-id;definedInScheme">
      <rdfs:label>defined in scheme</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-id;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-id;ListedSecurityIdentifier"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;ExchangeSecurityIdentificationScheme"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;hasNationalIdentifier">
      <rdfs:label>has national identifier</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-id;hasSecurityIdentifier"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;ExchangeTradedSecurity"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;NationalNumberingAgencyIdentificationCode"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;hasIdentifier">
      <rdfs:label>has identifier</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-id;hasSecurityIdentifier"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-iss;OfferIssue"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;ListedSecurityIdentifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;canBecomePartOf">
      <rdfs:label>can become part of</rdfs:label>
      <skos:definition>A national identification code may become part of a more general identification code (for example, ISIN).</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-id;NationalNumberingAgencyIdentificationCode"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;SecurityIdentifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;becomesPartOf">
      <rdfs:label>becomes part of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-id;canBecomePartOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-id;NationalNumberingAgencyIdentificationCode"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-ind;ISINIdentifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;definedInScheme.1">
      <rdfs:label>defined in scheme</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-id;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-id;NationalNumberingAgencyIdentificationCode"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;NationalSecurityIdentificationScheme"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;definedInScheme.2">
      <rdfs:label>defined in scheme</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-id;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-id;ProprietarySecurityIdentificationCode"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;ProprietarySecurityIdentificationScheme"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-ids;defines"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-id;SecurityIdentificationScheme"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;SecurityIdentifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-ids;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-id;SecurityIdentifier"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;SecurityIdentificationScheme"/>
      <owl:inverseOf rdf:resource="&fibo-sec-sec-id;defines"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-id;hasSecurityIdentifier">
      <rdfs:label>has security identifier</rdfs:label>
      <skos:definition>The primary identification code of the security. This may be any publicly defined code that is used to identify the Security.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-ids;hasIdentifier"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-id;SecurityIdentifier"/>
   </owl:ObjectProperty>


