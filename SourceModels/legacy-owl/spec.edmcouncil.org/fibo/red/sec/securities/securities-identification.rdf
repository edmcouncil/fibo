<owl:Class rdf:about="&fibo-sec-sec-id;ExchangeSecurityIdentificationScheme">
      <rdfs:label>exchange security identification scheme</rdfs:label>
      <skos:definition>An identification scheme used by a securities exchange in issuing security identifiers.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-id;SecurityIdentificationScheme"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-id;ListedSecurityIdentifier">
      <rdfs:label>listed security identifier</rdfs:label>
      <skos:definition>A Security Identifier issued in the public domain and referred to in listings etc.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-id;SecurityIdentifier"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-id;NationalNumberingAgencyIdentificationCode">
      <rdfs:label>national numbering agency identification code</rdfs:label>
      <skos:definition>An identifier assigned by a national numbering agency under the ISO 6166 standard.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-id;NationalSecurityIdentificationScheme">
      <rdfs:label>national security identification scheme</rdfs:label>
      <skos:definition>A security identification scheme published nationally on behalf of a whole country. Generally incorporated into the ISIN scheme as well.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-id;ProprietarySecurityIdentificationCode">
      <rdfs:label>proprietary security identification code</rdfs:label>
      <skos:definition>An identifier supplied by a commercial data providers or governments, for example a RIC code.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-id;ProprietarySecurityIdentificationScheme">
      <rdfs:label>proprietary security identification scheme</rdfs:label>
      <skos:definition>A security identification format published by a data provider or other commercial entity.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-id;SecurityIdentificationScheme">
      <rdfs:label>security identification scheme</rdfs:label>
      <skos:definition>A formal definition of the structure and application of a particular set of securities identifiers.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-ids;IdentificationScheme"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-id;SecurityIdentifier">
      <rdfs:label>security identifier</rdfs:label>
      <skos:definition>Any publicly defined code that is used to identify a Security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-ids;Identifier"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
