    <!ENTITY fibo-fnd-info-ids "http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/">
    <!ENTITY fibo-sec-sec-iss "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/">
    <!ENTITY fibo-sec-sec-sec "http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
    <!ENTITY fibo-sec-sec-ind "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-identification-individuals/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-identification/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-identification/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-identification-individuals/"/>
   </owl:Ontology>


