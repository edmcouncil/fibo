      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;PreferenceShare"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-sh;.UnionClass.9"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;hasListing">
