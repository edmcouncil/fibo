      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;ParticipatingPreferenceShare"/>
      <rdfs:range rdf:resource="&fibo-md-eq-pri;OrdinaryShareDividend"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;participatingPreferenceShareSeniorToPreferenceShare">
