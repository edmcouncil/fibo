      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;PreferredCouponPaymentTermSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;dividendPaymentFrequency.1">
      <rdfs:label>dividend payment frequency</rdfs:label>
      <skos:definition>The frequency of intended regular dividend payments</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;PreferredCouponPaymentTermSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Frequency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;gracePeriod">
