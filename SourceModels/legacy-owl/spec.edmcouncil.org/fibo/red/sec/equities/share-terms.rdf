<owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;setsOut">
      <rdfs:label>sets out</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;ArticlesOfIncorporation"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-sh;CompanyEquityTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;convertibleInto">
