      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;PreferredCouponPaymentTermSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;hasCoupon">
