      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-cnv;specifiesConversionInto"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;EquityConversionTermsSet"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;dividendPaymentFrequency">
      <rdfs:label>dividend payment frequency</rdfs:label>
      <skos:definition>The frequency with which dividends are expected to be paid out.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;OrdinaryDividendTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Frequency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;pays">
