      <rdfs:domain rdf:resource="&fibo-sec-eq-eqi;PubliclyTradedShare"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-sh;EquityListing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;isSeniorToAClassOfOrdinaryShare">
      <rdfs:label>is senior to a class of ordinary share</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-eq-eqi;isSeniorTo"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;OrdinaryShare"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-sh;OrdinaryShare"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;isSeniorToAClassOfPreferenceShare">
