   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;convertibleInto.1">
      <rdfs:label>convertible into</rdfs:label>
      <skos:definition>The security into which the convertible share can be converted. It is assumed this can be any tradable security.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-eq-sh;ConvertibleShare"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-sh;hasTerms">
