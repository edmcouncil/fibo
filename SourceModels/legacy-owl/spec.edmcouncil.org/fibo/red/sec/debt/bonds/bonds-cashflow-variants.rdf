      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfv;InflationFactorUnderlying"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;identity">
