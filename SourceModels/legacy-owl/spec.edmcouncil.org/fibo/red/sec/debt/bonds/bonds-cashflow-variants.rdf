   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;hasSubject">
      <rdfs:label>has subject</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;subject"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;InflationFactorFormula"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfv;InflationFactorSubject"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;operandIs">
