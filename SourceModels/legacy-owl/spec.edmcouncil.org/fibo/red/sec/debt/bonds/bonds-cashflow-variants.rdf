   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;underlyingParameter">
      <rdfs:label>underlying parameter</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;hasArgument"/>
