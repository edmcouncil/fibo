      <rdfs:label>has zero coupon call terms</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-bond-cfl;hasCallTerms.1"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;ZeroCouponBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfl;ZeroCouponAndOIDBondCallTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;hasPrincipalAmortizationTerms">
      <rdfs:label>has principal amortization terms</rdfs:label>
