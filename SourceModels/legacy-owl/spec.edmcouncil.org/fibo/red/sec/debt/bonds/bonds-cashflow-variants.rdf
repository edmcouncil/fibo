      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;InflationBondPrincipalRepaymentTermsSet"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;InflationBondFactor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;refersTo.1">
