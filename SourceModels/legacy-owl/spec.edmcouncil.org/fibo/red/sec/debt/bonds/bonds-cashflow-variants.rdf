      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;SinkingFundBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfl;SinkingFundAmortizationTermsSet"/>
   </owl:ObjectProperty>

