      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;VariableCouponBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfl;DebtVariableCouponTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;issuedDebtAmount">
      <rdfs:label>issued debt amount</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;VariableDebtPrincipal"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-tmp;DatedMonetaryAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfv;setsOutTermsFor">
      <rdfs:label>sets out terms for</rdfs:label>
