<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-dbt-mm-td "http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/term-deposits/">
    <!ENTITY fibo-der-ctr-otc "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/term-deposits/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/term-deposits/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-td;amount">
      <rdfs:label>amount</rdfs:label>
      <skos:definition>The principal amount deposited.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-td;TermDeposit"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-td;termDuration">
      <rdfs:label>term duration</rdfs:label>
      <skos:definition>The duration of the term specified in days, months, quarters or years.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-td;TermDeposit"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-td;settlementTerms">
      <rdfs:label>settlement terms</rdfs:label>
      <skos:definition>The contractual terms specifying the obligations of the certificate holder for the specified duration of that deposit.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;contractHasTerms"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-td;TermDeposit"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-mm-td;TermDepositSettlementTerms"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-mm-td;termDepositRate">
      <rdfs:label>term deposit rate</rdfs:label>
      <skos:definition>The rate of return on the deposit.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-dbt-mm-td;TermDeposit">
      <rdfs:label>term deposit</rdfs:label>
      <skos:definition>A money deposit at a bank that cannot be withdrawn for a certain "term" or period of time. When the term is over it can be withdrawn or it can be held for another term. The longer the term the better the yield on the money. A certificate of deposit is a time deposit product.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-mm-td;TermDepositSettlementTerms">
      <rdfs:label>term deposit settlement terms</rdfs:label>
      <skos:definition>Contractual terms for the settlement of a Term Deposit.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCContractSettlementTerms"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>