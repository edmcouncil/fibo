         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/participation-notes/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/participation-notes/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/functional-entities-ext/spv/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/debt-issuance-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/debt-instruments/"/>
   </owl:Ontology>


