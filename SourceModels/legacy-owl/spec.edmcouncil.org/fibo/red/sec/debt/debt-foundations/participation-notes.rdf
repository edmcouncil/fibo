<owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-pn;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-fct-spv;constitutedAs"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-pn;ParticipationNoteIssuer"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-iss;SecuritiesIssuanceSPV"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-pn;issuer">
      <rdfs:label>issuer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;issuer"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-pn;ParticipationNote"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-pn;ParticipationNoteIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-pn;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <skos:definition>THe underlying for the Participation Note.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-pn;ParticipationNote"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
   </owl:ObjectProperty>


