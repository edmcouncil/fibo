<owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;hasCallTerms">
      <rdfs:label>has call terms</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;.UnionClass.7"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-cfl;DebtCallTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;relatesTo">
      <rdfs:label>relates to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;.UnionClass.7"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;IssuedDebt"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;debtAmount">
      <rdfs:label>debt amount</rdfs:label>
      <skos:definition>The monetary amount of the debt that is securitized.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;IssuedDebt"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;hasRedemptionTerms">
      <rdfs:label>has redemption terms</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;contractHasTerms"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;NonTradableDebtInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-mm-tr;NonTradableDebtRedemptionTerms"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;definesTermsFor">
      <rdfs:label>defines terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-dbt-ins;relatesTo"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-bt;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;NonTradableDebtInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;IssuedDebt"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;mayBeSubordinatedTo">
      <rdfs:label>may be subordinated to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;denomination">
      <rdfs:label>denomination</rdfs:label>
      <skos:definition>The face value of an individual note of the security.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;setsOutTermsFor">
      <rdfs:label>sets out terms for</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;PubliclyIssuedDebt"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-ins;issuedDebtAmount">
      <rdfs:label>issued debt amount</rdfs:label>
      <skos:definition>The amount of Debt issued.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-ins;PubliclyIssuedDebt"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>


