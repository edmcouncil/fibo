<rdfs:Datatype rdf:about="&fibo-sec-dbt-dbt-ins;DayCountBasis">
      <skos:definition>The Interest Accrual Basis which is used to calculate interest on a debt or Debt security.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">30/360</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual IM</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual/360</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual/365</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description>
                                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual/365L</rdf:first>
                                          <rdf:rest>
                                             <rdf:Description>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IC 30/Actual</rdf:first>
                                                <rdf:rest>
                                                   <rdf:Description>
                                                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual/Actual AFB</rdf:first>
                                                      <rdf:rest>
                                                         <rdf:Description>
                                                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eurobond</rdf:first>
                                                            <rdf:rest>
                                                               <rdf:Description>
                                                                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IC 30/360</rdf:first>
                                                                  <rdf:rest>
                                                                     <rdf:Description>
                                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IC 30/360 ICMA</rdf:first>
                                                                        <rdf:rest>
                                                                           <rdf:Description>
                                                                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IC 30/365</rdf:first>
                                                                              <rdf:rest>
                                                                                 <rdf:Description>
                                                                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actual/Actual</rdf:first>
                                                                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                                                 </rdf:Description>
                                                                              </rdf:rest>
                                                                           </rdf:Description>
                                                                        </rdf:rest>
                                                                     </rdf:Description>
                                                                  </rdf:rest>
                                                               </rdf:Description>
                                                            </rdf:rest>
                                                         </rdf:Description>
                                                      </rdf:rest>
                                                   </rdf:Description>
                                                </rdf:rest>
                                             </rdf:Description>
                                          </rdf:rest>
                                       </rdf:Description>
                                    </rdf:rest>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


