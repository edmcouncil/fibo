<owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;hasConstituent">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-pbs;hasConstituent.1"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;CashStructuredFinanceInstrumentPool"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;CashStructuredFinanceInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;isTrancheOf">
      <rdfs:label>is tranche of</rdfs:label>
      <skos:definition>The asset pool which is the underlying of a traditional or cash structured finance instrument. This is a real pool of real assets, as distinct from synthetic pools.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-pbs;hasUnderlying.1"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;takesForm"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;CashStructuredFinanceInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;DebtPool"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;collateralizedBy">
      <rdfs:label>collateralized by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-dbt;collateralizedBy"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;CollateralizedDebt"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;DebtCollateral"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;dealAmount">
      <rdfs:label>deal amount</rdfs:label>
      <skos:definition>The amount of the overall deal.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecuritiesDeal"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;describedIn">
      <rdfs:label>described in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-dbt-iss;describedIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecuritiesDeal"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedDealProspectus"/>
      <owl:inverseOf rdf:resource="&fibo-sec-dbt-ab-pbs;describes"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;hasIssuer">
      <rdfs:label>has issuer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-pbs;issuedBy"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecuritiesDeal"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecurityIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;describes">
      <rdfs:label>describes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-dbt-iss;describes"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedDealProspectus"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecuritiesDeal"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;isIssueOf">
      <rdfs:label>is issue of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-dbt-iss;isIssueOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecuritiesDeal"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecurityInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;poolBackedSecurityIssuerIdentity">
      <rdfs:label>pool backed security issuer identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecurityIssuer"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;isTrancheOf.1">
      <rdfs:label>is tranche of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-pbs;isTrancheOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecurityInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolOfIndividualDebts"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;issuedBy">
      <rdfs:label>issued by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;issuer"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecurityInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecurityIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;becomes">
      <rdfs:label>becomes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolOfIndividualDebts"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolOfIndividualDebts"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-prod;produces"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecuritiesDeal"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-mbs;IssuedAndSecuritizedPoolOfIndividualDebts"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;backs">
      <rdfs:label>backs</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;DebtPool"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;CashStructuredFinanceInstrument"/>
      <owl:inverseOf rdf:resource="&fibo-sec-dbt-ab-pbs;isTrancheOf"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;backs.1">
      <rdfs:label>backs</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-pbs;backs"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;PoolOfIndividualDebts"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;PoolBackedSecurityInstrument"/>
      <owl:inverseOf rdf:resource="&fibo-sec-dbt-ab-pbs;isTrancheOf.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;hasConstituent.1">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;hasConstituent"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;StructuredFinanceInstrumentPool"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-pbs;StructuredFinanceInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;hasUnderlying.1">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;StructuredFinanceInstrument"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-own;Asset"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-pbs;mayBe">
      <rdfs:label>may be</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-pbs;StructuredFinanceInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfv;AmortizingSecurity"/>
   </owl:ObjectProperty>


