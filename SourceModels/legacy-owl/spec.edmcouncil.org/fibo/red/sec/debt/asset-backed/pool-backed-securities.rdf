<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-ab-pbs;passThrough">
      <rdfs:label>pass through</rdfs:label>
      <skos:definition>Whether the cash flows from the underlying asset pool are passed through to the investor by way of redemption payments.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-ab-pbs;lifecycleState">
      <rdfs:label>lifecycle state</rdfs:label>
      <skos:definition>The state of the pool in terms of the lifecycle of that pool from not yet issued, through the issuance process, to securitized pool.</skos:definition>
      <rdfs:range rdf:resource="&fibo-bp-iss-pl;PoolLifecycleStateSelection"/>
   </owl:DatatypeProperty>


