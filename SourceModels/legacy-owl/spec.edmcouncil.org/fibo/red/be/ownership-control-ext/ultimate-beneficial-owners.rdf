<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-be-oacx-ubo "http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/ultimate-beneficial-owners/">
    <!ENTITY fibo-be-oac-cown "http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/corporate-ownership/">
    <!ENTITY fibo-fnd-aap-ppl "http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/people/">
    <!ENTITY fibo-be-oacx-cownx "http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/corporate-ownership-extras/">
    <!ENTITY fibo-be-oacx-optyx "http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/ownership-parties-extras/">
    <!ENTITY fibo-be-corp-corp "http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/ultimate-beneficial-owners/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/ultimate-beneficial-owners/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/corporate-ownership/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/people/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/corporate-ownership-extras/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-control-ext/ownership-parties-extras/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-be-oacx-ubo;identity">
      <rdfs:label>identity</rdfs:label>
      <skos:definition>ultimate Beneficial Owner is any kind of Human Being.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oacx-optyx;entityOwningPartyIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oacx-ubo;UltimateBeneficialOwner"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-ppl;Person"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oacx-ubo;hasUltimateBeneficialOwner">
      <rdfs:label>has ultimate beneficial owner</rdfs:label>
      <skos:definition>The ultimate holders of the assets of a hierarchy of companies. These may be people or other legal entities.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oacx-cownx;partHeldByShareholder"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-be-oacx-ubo;UltimateBeneficialOwner"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oacx-ubo;isUltimateBeneficialOwnerOf">
      <rdfs:label>is ultimate beneficial owner of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-oacx-ubo;UltimateBeneficialOwner"/>
      <rdfs:range rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <owl:inverseOf rdf:resource="&fibo-be-oacx-ubo;hasUltimateBeneficialOwner"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-be-oacx-ubo;UltimateBeneficialOwner">
      <rdfs:label>ultimate beneficial owner</rdfs:label>
      <skos:definition>The ultimate holders of the assets of a hierarchy of companies. These may be people or other legal entities.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-oac-cown;BeneficialOwner"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
