<owl:ObjectProperty rdf:about="&fibo-be-fct-proc;isSeenAs">
      <rdfs:label>is seen as</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-fct-proc;FinancialCommitmentCounterparty"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ext;ContractParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-proc;definedInContextOf">
      <rdfs:label>defined in context of</rdfs:label>
      <skos:definition>The business context in which the entity has its definition.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-rbe;relativeBusinessEntityDefinedInContextOf"/>
      <rdfs:domain rdf:resource="&fibo-be-fct-proc;ProcessRoleBusinessEntity"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-proc;definedInContextOf.1">
      <rdfs:label>defined in context of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-fct-proc;definedInContextOf"/>
      <rdfs:domain rdf:resource="&fibo-be-fct-proc;SettlementEntity"/>
      <rdfs:range rdf:resource="&fibo-bp-bp-con;Settlement"/>
   </owl:ObjectProperty>


