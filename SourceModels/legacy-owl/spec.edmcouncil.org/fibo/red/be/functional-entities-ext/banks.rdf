<owl:Class rdf:about="&fibo-be-fct-ban;Bank">
      <rdfs:label>bank</rdfs:label>
      <skos:definition>A business entity which takes deposits and deals in monetary instruments. Legal definition sought.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-fct-ban;CommercialBank">
      <rdfs:label>commercial bank</rdfs:label>
      <skos:definition>A bank which is a company with shares, providing banking services. This may be a retail or wholesale bank.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-fct-ban;Bank"/>
   </owl:Class>


