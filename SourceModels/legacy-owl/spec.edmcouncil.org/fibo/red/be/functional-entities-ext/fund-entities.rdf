    <!ENTITY fibo-be-fct-fct "http://spec.edmcouncil.org/fibo/red/be/functional-entities/functional-entities/">
    <!ENTITY fibo-sec-sec-pls "http://spec.edmcouncil.org/fibo/red/sec/securities/pools/">
    <!ENTITY fibo-civ-fun-civ "http://spec.edmcouncil.org/fibo/red/civ/funds/civ/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
