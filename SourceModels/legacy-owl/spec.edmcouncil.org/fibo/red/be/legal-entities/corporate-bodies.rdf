<owl:DatatypeProperty rdf:about="&fibo-be-le-cb;hasLegalName">
      <rdfs:label>has legal name</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-be-le-cb;instrumentURI">
      <rdfs:label>instrument u r i</rdfs:label>
      <skos:definition>The URI of the Instrument of Incorporation.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;URI"/>
   </owl:DatatypeProperty>


