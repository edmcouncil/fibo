<owl:ObjectProperty rdf:about="&fibo-be-le-cb;bodyCorporateIsConstitutedBy">
      <rdfs:label>body corporate is constituted by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-cb;isConstitutedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;InstrumentOfIncorporation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;bodyIncorporatedWithGuaranteeIsGovernedBy">
      <rdfs:label>body incorporated with guarantee is governed by</rdfs:label>
      <skos:definition>The company is governed by some formal agreement between the principals.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;formallyConstitutedOrganizationIsGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyIncorporatedWithGuarantee"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyLimitedByGuaranteePrincipalsAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;guaranteeIsProvidedBy">
      <rdfs:label>guarantee is provided by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isProvidedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;IncorporationGuarantee"/>
      <rdfs:range rdf:resource="&fibo-fnd-rel-rel;Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;incorporationGuaranteeHasNotionalAmount">
      <rdfs:label>incorporation guarantee has notional amount</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-cur;hasNotionalAmount"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;IncorporationGuarantee"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;instrumentOfIncorporationIsGovernedBy">
      <rdfs:label>instrument of incorporation is governed by</rdfs:label>
      <skos:definition>The legal jurisdiction under which the Instrument of Incorporation has standing.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isGoverneBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;InstrumentOfIncorporation"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;isConstitutedBy">
      <rdfs:label>is constituted by</rdfs:label>
      <skos:definition>The instrument by which the entity is incorporated.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-cor;Constitution"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;isDomiciledIn">
      <rdfs:label>is domiciled in</rdfs:label>
      <skos:definition>The country in which the Body Corporate is officially domiciled.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;isIncorporatedIn">
      <rdfs:label>is incorporated in</rdfs:label>
      <skos:definition>The legal jurisdiction under which the legal entity is incorporated.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-cb;issuesEquity">
      <rdfs:label>issues equity</rdfs:label>
      <skos:definition>The Body Incorporated by Equity is incorporated through the issuance of some Issued Equity, which is a form of Stockholder Equity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasEquity"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyIncorporatedWithEquity"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-aeq;IssuedEquity"/>
   </owl:ObjectProperty>


