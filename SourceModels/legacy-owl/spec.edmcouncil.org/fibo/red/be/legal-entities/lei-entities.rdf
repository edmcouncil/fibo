<owl:ObjectProperty rdf:about="&fibo-be-le-lei;contractuallyCapableEntityIdentity">
      <rdfs:label>contractually capable entity identity</rdfs:label>
      <skos:definition>The Contractually Capable Entity is some Formal Organization.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-rl;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-le-lei;ContractuallyCapableEntity"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-lei;isRecognizedIn">
      <rdfs:label>is recognized in</rdfs:label>
      <skos:definition>The concept of what is considered as legal is a function of the jurisdiction in which it is recognized as being a Legal Entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;inContext"/>
      <rdfs:domain rdf:resource="&fibo-be-le-lei;ContractuallyCapableEntity"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-lei;legalEntityIdentifierSchemeDefines">
      <rdfs:label>legal entity identifier scheme defines</rdfs:label>
      <skos:definition>An Legal Entity Identification code defined in the LEI scheme.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;defines"/>
      <rdfs:domain rdf:resource="&fibo-be-le-lei;LegalEntityIdentifierScheme"/>
      <rdfs:range rdf:resource="&fibo-be-le-lei;LegalEntityIdentifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-le-lei;identifies">
      <rdfs:label>identifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;identifies"/>
      <rdfs:domain rdf:resource="&fibo-be-le-lei;LegalEntityIdentifier"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <owl:inverseOf rdf:resource="&fibo-be-lex-lec;identifiedBy"/>
   </owl:ObjectProperty>


