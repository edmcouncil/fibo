<owl:ObjectProperty rdf:about="&fibo-be-ptrx-ptrx;incorportatedPartnershipMemberIdentity">
      <rdfs:label>incorportated partnership member identity</rdfs:label>
      <skos:definition>The legal entity which is the Legally Incorporated Partnership Member.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-ptr-ptr;partnerIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-ptr-ptr;LegallyIncorporatedPartnershipMember"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-ptrx-ptrx;hasPartnershipEquity">
      <rdfs:label>has partnership equity</rdfs:label>
      <skos:definition>The Partnership has some Partnership Equity in it.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasEquity"/>
      <rdfs:domain rdf:resource="&fibo-be-ptr-ptr;Partnership"/>
      <rdfs:range rdf:resource="&fibo-be-ptr-ptr;PartnershipEquity"/>
   </owl:ObjectProperty>


