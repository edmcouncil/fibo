  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-be-tr-tr;fundManagerIdentity">
      <rdfs:label>fund manager identity</rdfs:label>
      <skos:definition>The entity which fulfills the role of the Trust Fund Manager</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;TrustFundManager"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;fundUnitHolderIdentity">
      <rdfs:label>fund unit holder identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;FundUnitHolder"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;definesBeneficiary">
      <rdfs:label>defines beneficiary</rdfs:label>
      <skos:definition>The party formally identified in the Trust Agreement as the Beneficiary.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;TrustAgreement"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;TrustBeneficiary"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;definesTrustorParty">
      <rdfs:label>defines trustor party</rdfs:label>
      <skos:definition>The party formally identified in the Trust Agreement as the Sponsor.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;TrustAgreement"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;Trustor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;definesTrusteeParty">
      <rdfs:label>defines trustee party</rdfs:label>
      <skos:definition>The party formally identified in the Trust Agreement as the Trustee.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;TrustAgreement"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;Trustee"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;trustFundManagerManages">
      <rdfs:label>trust fund manager manages</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;manages"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;TrustFundManager"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;Trust"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;hasBeneficiary">
      <rdfs:label>has beneficiary</rdfs:label>
      <skos:definition>Some unit holder in the Trust Fund</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-tr-tr;hasPartyBeneficiary"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;TrustFundTrust"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;FundUnitHolder"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;trusteeIdentity">
      <rdfs:label>trustee identity</rdfs:label>
      <skos:definition>The entity which fulfills the role of the Trustee.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;Trustee"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;trusteeManages">
      <rdfs:label>trustee manages</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;manages"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;Trustee"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;Trust"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;governedByTrustAgreement">
      <rdfs:label>governed by trust agreement</rdfs:label>
      <skos:definition>The agreement which governs the relationships among the principals in the Trust.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;formallyConstitutedOrganizationIsGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;Trust"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;TrustAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;hasFundManager">
      <rdfs:label>has fund manager</rdfs:label>
      <skos:definition>The party which is the manager of the Trust Fund.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;TrustFundTrust"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;TrustFundManager"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;hasPartyBeneficiary">
      <rdfs:label>has party beneficiary</rdfs:label>
      <skos:definition>Has a Beneficiary of the Trust</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;Trust"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;TrustBeneficiary"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;hasPartySponsor">
      <rdfs:label>has party sponsor</rdfs:label>
      <skos:definition>Has a sponsor of the Trust.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;Trust"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;Trustor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-tr-tr;hasPartyTrustee">
      <rdfs:label>has party trustee</rdfs:label>
      <skos:definition>Has a Trustee of the Trust</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-be-tr-tr;Trust"/>
      <rdfs:range rdf:resource="&fibo-be-tr-tr;Trustee"/>
   </owl:ObjectProperty>


