   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-ins;FinancialInstrumentAnalyticalParameter">
      <rdfs:label>financial instrument analytical parameter</rdfs:label>
      <skos:definition>Some measure of a financial instrument, that may be referred to in analysing or understanding that instrument for investment or risk management purposes.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-tem;AnalyticalInformation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-ins;FinancialMarketInformation">
      <rdfs:label>financial market information</rdfs:label>
