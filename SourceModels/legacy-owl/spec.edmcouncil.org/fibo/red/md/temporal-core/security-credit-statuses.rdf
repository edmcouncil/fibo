<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-md-tmp-scr "http://spec.edmcouncil.org/fibo/red/md/temporal-core/security-credit-statuses/">
    <!ENTITY fibo-fnd-info-tem "http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/">
    <!ENTITY fibo-sec-sec-sec "http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-acc-dbt "http://spec.edmcouncil.org/fibo/red/fnd/accounting/debt-terms/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/md/temporal-core/security-credit-statuses/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/md/temporal-core/security-credit-statuses/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/debt-terms/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-md-tmp-scr;isAbout">
      <rdfs:label>is about</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-tem;isOf"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-scr;SecurityCreditStatus"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-scr;inRespectOf">
      <rdfs:label>in respect of</rdfs:label>
      <skos:definition>Basis of the Credit Rating, i.e. the Credit of the Creditor assuming this is taken into account in determining the Credit Rating. This model may be adjusted to put more detail into the basis, including payments history, credit history and the like.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-md-tmp-scr;SecurityCreditStatus"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Credit"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-md-tmp-scr;acceleratedPrincipalPaymentsExpected">
      <rdfs:label>accelerated principal payments expected</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-scr;interestPaymentsExpected">
      <rdfs:label>interest payments expected</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-scr;principalPaymentsExpected">
      <rdfs:label>principal payments expected</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-md-tmp-scr;SecurityCashflowStatus">
      <rdfs:label>security cashflow status</rdfs:label>
      <skos:definition>The status of the cashflow due to the holder from the security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-tem;Status"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-scr;CreditOK">
      <rdfs:label>credit o k</rdfs:label>
      <skos:definition>Not defaulted.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-md-tmp-scr;SecurityCreditStatus"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-scr;SecurityCreditStatus">
      <rdfs:label>security credit status</rdfs:label>
      <skos:definition>This term might not exist.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-scr;InDefault">
      <rdfs:label>in default</rdfs:label>
      <skos:definition>The issuer has failed to pay somthing that they are contractually obliged to pay.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>