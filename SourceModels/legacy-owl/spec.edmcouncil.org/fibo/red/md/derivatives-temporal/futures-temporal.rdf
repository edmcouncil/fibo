      <rdfs:domain rdf:resource="&fibo-md-der-fut;FuturesTradingMargin"/>
      <rdfs:range rdf:resource="&fibo-md-der-fut;FuturesTradingAccountHolder"/>
   </owl:ObjectProperty>


