  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-md-dbt-int;couponAmount">
      <rdfs:label>coupon amount</rdfs:label>
      <skos:definition>The current pr past amount of the bond coupon.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-md-dbt-int;BondVariableCoupon"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-tmp;DatedPercentage"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;bondHasCoupon">
      <rdfs:label>bond has coupon</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-bond;Bond"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;BondCoupon"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;hasCoupon">
      <rdfs:label>has coupon</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-dbt-int;bondHasCoupon"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;FixedCouponBond"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;BondFixedCoupon"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;hasFactor">
      <rdfs:label>has factor</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;InflationBond"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;InflationBondFactor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;hasInterestRate">
      <rdfs:label>has interest rate</rdfs:label>
      <skos:definition>The rate of interest payable on the Treasury Bill.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-dbt-py;hasInterestRate"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-tr;TreasuryBill"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;TreasuryBillReferenceRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;hasInterestRateValue">
      <rdfs:label>has interest rate value</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;takesValuesOverTime"/>
      <rdfs:domain rdf:resource="&fibo-md-dbt-int;TreasuryBillReferenceRate"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;TreasuryBillInterestRateValue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;projectedValue">
      <rdfs:label>projected value</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-md-dbt-int;BondVariableCoupon"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;ProjectedCouponPaymentValue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;hasCoupon.1">
      <rdfs:label>has coupon</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-dbt-int;hasCoupon.3"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;FloatingRateNote"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;FRNCoupon"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;hasCoupon.2">
      <rdfs:label>has coupon</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-dbt-int;hasCoupon.3"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;IndexBasedCouponBond"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;IndexLinkedCoupon"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-dbt-int;hasCoupon.3">
      <rdfs:label>has coupon</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-dbt-int;bondHasCoupon"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfv;VariableCouponBond"/>
      <rdfs:range rdf:resource="&fibo-md-dbt-int;BondVariableCoupon"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-md-dbt-int;projectedCoupon">
      <rdfs:label>projected coupon</rdfs:label>
      <skos:definition>Anticipated amount of a future (variable) coupon amount.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-dbt-int;interestRate">
      <rdfs:label>interest rate</rdfs:label>
      <skos:definition>The rate of interest payable by the Treasury Bill at a stated time.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-md-dbt-int;BondCoupon">
      <rdfs:label>bond coupon</rdfs:label>
      <skos:definition>Mechanism whereby an investor receives income from interest payments, which are usually made periodically.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;ContractuallyDefinedNumericalParameter"/>
      <rdfs:subClassOf rdf:resource="&fibo-md-dbt-int;DebtSecurityInterestAmount"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;BondFixedCoupon">
      <rdfs:label>bond fixed coupon</rdfs:label>
      <skos:definition>Mechanism whereby an investor receives income from interest payments, which are usually made periodically. These are for a fixed amount.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-md-dbt-int;BondCoupon"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;BondVariableCoupon">
      <rdfs:label>bond variable coupon</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;DebtInstrumentReferenceRate">
      <rdfs:label>debt instrument reference rate</rdfs:label>
      <skos:definition>The interest rate of some debt instrument which is referred to in calculations of some sort.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ind-ind;MarketReferentialParameter"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;DebtSecurityInterestAmount">
      <rdfs:label>debt security interest amount</rdfs:label>
      <skos:definition>The amount of interest paid out on a debt security at a given point in time.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;TimedParameter"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;FRNCoupon">
      <rdfs:label>f r n coupon</rdfs:label>
      <skos:definition>Mechanism whereby an investor receives income from interest payments, which are usually made periodically. These are for a variable amount based on a specified Interest rate.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-md-dbt-int;BondVariableCoupon"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;IndexLinkedCoupon">
      <rdfs:label>index linked coupon</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;InflationBondFactor">
      <rdfs:label>inflation bond factor</rdfs:label>
      <skos:definition>The factor which is used in determining the principal on an inflation bond. This is used either in calculating the interest payments, in calculating the principal payback or more typically both. Further notes: This is a dated parameter, that is it varies over time.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-md-tmp-ins;ReferenceFactor"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;InflationBondVariableCoupon">
      <rdfs:label>inflation bond variable coupon</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;ProjectedCouponPaymentValue">
      <rdfs:label>projected coupon payment value</rdfs:label>
      <skos:definition>Projected Coupon amount, along with parameters defining when and how this is projected.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;ProjectedValue"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;TreasuryBillInterestRateValue">
      <rdfs:label>treasury bill interest rate value</rdfs:label>
      <skos:definition>The value of a government short term debt security, or treasury bill, at a given point in time.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;TimedValue"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;TreasuryBillReferenceRate">
      <rdfs:label>treasury bill reference rate</rdfs:label>
      <skos:definition>The parameter which is the interest rate payable by the T Bill at any point in time. This therefore takes a value at any given time and has historical values in the past.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-md-dbt-int;DebtInstrumentReferenceRate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-dbt-int;WACBondCoupon">
      <rdfs:label>w a c bond coupon</rdfs:label>
   </owl:Class>


  <!--
