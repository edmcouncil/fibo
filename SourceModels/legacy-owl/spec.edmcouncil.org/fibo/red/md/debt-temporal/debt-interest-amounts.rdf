    <!ENTITY fibo-md-dbt-int "http://spec.edmcouncil.org/fibo/red/md/debt-temporal/debt-interest-amounts/">
    <!ENTITY fibo-fnd-math-math "http://spec.edmcouncil.org/fibo/red/fnd/math/math/">
    <!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
    <!ENTITY fibo-md-tmp-ins "http://spec.edmcouncil.org/fibo/red/md/temporal-core/instrument-temporal-terms/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-md-dbt-py "http://spec.edmcouncil.org/fibo/red/md/debt-temporal/debt-pricing-yields/">
    <!ENTITY fibo-sec-dbt-bond-bond "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-common/">
    <!ENTITY fibo-sec-dbt-bond-cfv "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-cashflow-variants/">
    <!ENTITY fibo-sec-dbt-mm-tr "http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/traded-short-term-debt/">
    <!ENTITY fibo-md-tmp-tmp "http://spec.edmcouncil.org/fibo/red/md/temporal-core/temporal-concepts/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/md/debt-temporal/debt-interest-amounts/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/md/debt-temporal/debt-interest-amounts/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/math/math/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/md/temporal-core/instrument-temporal-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/md/debt-temporal/debt-pricing-yields/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-common/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-cashflow-variants/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/traded-short-term-debt/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/md/temporal-core/temporal-concepts/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
