<owl:ObjectProperty rdf:about="&fibo-ind-ei-pub;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ind-ind;financialInformationPublisherHasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-ind-ei-pub;InflationRatePublisher"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-pub;.UnionClass.4"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-pub;inflationRatePublishedBy">
      <rdfs:label>inflation rate published by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ind-ind;isPublishedBy"/>
