      <rdfs:range rdf:resource="&fibo-ind-ei-pub;InflationRatePublisher"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-pub;identifiedAs.1">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ind-ind;financialInformationPublisherHasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-ind-ei-pub;UnemploymentRatePublisher"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-pub;NationalStatisticsOffice"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-pub;unemploymentRatePublishedBy">
      <rdfs:label>unemployment rate published by</rdfs:label>
      <skos:definition>The publisher of the unemployment rate for a given economic area.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ind-ind;isPublishedBy"/>
