<owl:Class rdf:about="&fibo-ind-ei-pub;.UnionClass.4">
      <skos:definition>This can be any of an international agency, a market data vendor or a government body. Can also be central bank. Further Notes; This is a logical combination of types of entity, referred to by the relative term for the publisher of an inflation rate.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-ind-ir-pub;MonetaryAuthority"/>
               <owl:Class rdf:about="&fibo-ind-ei-pub;MarketDataVendor"/>
               <owl:Class rdf:about="&fibo-ind-ei-pub;InternationalAgency"/>
               <owl:Class rdf:about="&fibo-ind-ei-pub;GovernmentBody"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-pub;GovernmentBody">
      <rdfs:label>government body</rdfs:label>
      <skos:definition>An individual organization which forms part of the mechanisms of government, and through which a government (which is a relative entity) discharges its functions in governing.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-pub;InflationRatePublisher">
      <rdfs:label>inflation rate publisher</rdfs:label>
      <skos:definition>The body that initially determines and publishes the inflation rate for a given economy.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ind-ind;FinancialInformationPublisher"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-pub;InternationalAgency">
      <rdfs:label>international agency</rdfs:label>
      <skos:definition>A body set up internationally which operates internationally.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-pub;MarketDataVendor">
      <rdfs:label>market data vendor</rdfs:label>
      <skos:definition>A business entity which specializes in the sourcing and distribution of financial market data.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pub-pub;PublishingHouse"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-pub;NationalStatisticsOffice">
      <rdfs:label>national statistics office</rdfs:label>
      <skos:definition>The government body responsible for publishing statistics for a country or economic area.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ei-pub;GovernmentBody"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-pub;UnemploymentRatePublisher">
      <rdfs:label>unemployment rate publisher</rdfs:label>
      <skos:definition>The body which initially determines and publishes the unemployment rate for a given economic area.</skos:definition>
   </owl:Class>


