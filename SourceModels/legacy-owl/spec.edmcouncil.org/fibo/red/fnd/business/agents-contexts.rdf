  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-bus-agc;BusinessAgent">
      <rdfs:label>business agent</rdfs:label>
      <skos:definition>An agent which is capable of causing some business event or providing some service in a business context.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-ena;EnablingAgent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-bus-agc;BusinessAgentContext">
      <rdfs:label>business agent context</rdfs:label>
      <skos:definition>A business process, work stream or other business area, in which various processes, actors, activities and so on are combined to achieve some business goal.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-rbe;BusinessContext"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>