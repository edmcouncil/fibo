<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-bus-agc "http://spec.edmcouncil.org/fibo/red/fnd/business/agents-contexts/">
    <!ENTITY fibo-rl-ena "http://spec.edmcouncil.org/fibo/red/fnd/relative/enabling-agents/">
    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
    <!ENTITY fibo-fnd-bus-bg "http://spec.edmcouncil.org/fibo/red/fnd/business/business-goals/">
    <!ENTITY fibo-rl-rbe "http://spec.edmcouncil.org/fibo/red/fnd/relative/relative-business-entities/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/business/agents-contexts/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/business/agents-contexts/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/enabling-agents/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/business/business-goals/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/relative-business-entities/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-bus-agc;motivation">
      <rdfs:label>motivation</rdfs:label>
      <skos:definition>The goal in furtherance of which the agent acts.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-bus-agc;BusinessAgent"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-bg;BusinessGoal"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-bus-agc;businessAgentHasIdentity">
      <rdfs:label>business agent has identity</rdfs:label>
      <skos:definition>That which performs the role of the Business Agent in the specified context.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-ena;enablingAgentHasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-bus-agc;BusinessAgent"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-bus-agc;goal">
      <rdfs:label>goal</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-bg;BusinessGoal"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-bus-agc;involves">
      <rdfs:label>involves</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-agc;BusinessAgent"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
