<owl:Class rdf:about="&fibo-fnd-soc-asp;.UnionClass.5">
      <skos:definition>Any human being (adult or minor) or any legal entity.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-be-le-lp;LegalPerson"/>
               <owl:Class rdf:about="&fibo-fnd-aap-ppl;Person"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;CommitmentAspect">
      <rdfs:label>commitment aspect</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-rl-asp;Aspect"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;ConditionalObligation">
      <rdfs:label>conditional obligation</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-soc-asp;Obligation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;ConditionalRight">
      <rdfs:label>conditional right</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-soc-asp;Right"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;ConstitutionalRight">
      <rdfs:label>constitutional right</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;LegalObligation">
      <rdfs:label>legal obligation</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;LegalRight">
      <rdfs:label>legal right</rdfs:label>
      <skos:definition>A right conferred by some law.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;ObligationConditionalOnSomeCircumstance">
      <rdfs:label>obligation conditional on some circumstance</rdfs:label>
      <skos:definition>An Obligation which is conditional on the existence or continuing existence of some circumstance.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-soc-asp;ConditionalObligation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;ObligationConditionalOnSomeEvent">
      <rdfs:label>obligation conditional on some event</rdfs:label>
      <skos:definition>Obligation conditional on the occurrence of some event.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;Right">
      <rdfs:label>right</rdfs:label>
      <skos:definition>Some right conferred by some contract or by law.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-soc-asp;CommitmentAspect"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;UnconditionalObligation">
      <rdfs:label>unconditional obligation</rdfs:label>
      <skos:definition>An obligation which is not conditional on the occurrence of any event or the existence or continuation of any condition.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;UnconditionalRight">
      <rdfs:label>unconditional right</rdfs:label>
      <skos:definition>A right to which no conditions are applied. Further Notes; these may be moral, consititutional. Contractual and so on.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-soc-asp;Obligation">
      <rdfs:label>obligation</rdfs:label>
      <skos:definition>Some obligation set out by some contract or by law, mandated upon some party.</skos:definition>
   </owl:Class>


