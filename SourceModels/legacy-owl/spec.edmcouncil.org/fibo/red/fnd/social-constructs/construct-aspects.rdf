   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/social-constructs/construct-aspects/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/aspects/"/>
