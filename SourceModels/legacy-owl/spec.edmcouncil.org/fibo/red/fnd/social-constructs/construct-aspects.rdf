<owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;isAspectOf">
      <rdfs:label>is aspect of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-asp;isAspectOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;CommitmentAspect"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-agr;Commitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;conferredBy">
      <rdfs:label>conferred by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;conferredBy.2"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;ConstitutionalRight"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-cor;Constitution"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;contractTermsSetDescribes">
      <rdfs:label>contract terms set describes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-soc-asp;CommitmentAspect"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;mandatedBy">
      <rdfs:label>mandated by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;LegalObligation"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;conferredBy.1">
      <rdfs:label>conferred by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;conferredBy.2"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;LegalRight"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;impliedBy">
      <rdfs:label>implied by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;Obligation"/>
      <rdfs:range rdf:resource="&fibo-fnd-soc-asp;Right"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-soc-asp;impliesSome"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;incurredBy">
      <rdfs:label>incurred by</rdfs:label>
      <skos:definition>An obligation may be incurred by any Legal Entity, either through duty at law or by agreement in a contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;legalConstructIsConferredOn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;Obligation"/>
      <rdfs:range rdf:resource="&fibo-be-le-lei;ContractuallyCapableEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;isAspectOf.1">
      <rdfs:label>is aspect of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;isAspectOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;Obligation"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-agr;Commitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;conferredBy.2">
      <rdfs:label>conferred by</rdfs:label>
      <skos:definition>That which confers the right. This may be a body of law or it may be explicitly conferred by one party upon another by virtue of a contract between those parties.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;Right"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;.UnionClass.5"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;enjoyedBy">
      <rdfs:label>enjoyed by</rdfs:label>
      <skos:definition>That which enjoys the right.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;legalConstructIsConferredOn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;Right"/>
      <rdfs:range rdf:resource="&fibo-fnd-soc-asp;.UnionClass.5"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;impliesSome">
      <rdfs:label>implies some</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;Right"/>
      <rdfs:range rdf:resource="&fibo-fnd-soc-asp;Obligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-soc-asp;isAspectOf.2">
      <rdfs:label>is aspect of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;isAspectOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;Right"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-agr;Commitment"/>
   </owl:ObjectProperty>


