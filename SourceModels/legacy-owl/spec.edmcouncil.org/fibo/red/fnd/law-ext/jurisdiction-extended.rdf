<owl:Class rdf:about="&fibo-fnd-lawx-jur;BijuridicialJurisdiction">
      <rdfs:label>bijuridicial jurisdiction</rdfs:label>
      <skos:definition>A Jurisdiction in which two or more systems of law are in force.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;BodyOfLaw">
      <rdfs:label>body of law</rdfs:label>
      <skos:definition>Some body of law, that is some set of laws, statutes, ordinances and the like.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;CanonLawSystem">
      <rdfs:label>canon law system</rdfs:label>
      <skos:definition>The body of laws & regulations made or adopted by ecclesiastical authority, for the government of the Christian organization and its members.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-lawx-jur;ReligiousLawSystem"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;CivilLaw">
      <rdfs:label>civil law</rdfs:label>
      <skos:definition>The branch of the law dealing with disputes between individuals or organizations, in which compensation may be awarded to the victim.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-lawx-jur;CommonLaw"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;CommonLaw">
      <rdfs:label>common law</rdfs:label>
      <skos:definition>Law developed by judges through decisions of courts and similar tribunals rather than through legislative statutes or executive branch action.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-lawx-jur;BodyOfLaw"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;ConstitutionalLaw">
      <rdfs:label>constitutional law</rdfs:label>
      <skos:definition>The body of law which defines the relationship of different entities within a state, namely the executive, legislature and the judiciary. Further notes: (from Wikipedia): Constitutional laws may be considered second order rulemaking or rules about making rules to exercise power.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;CriminalLaw">
      <rdfs:label>criminal law</rdfs:label>
      <skos:definition>The body of law that defines conduct that is not allowed because it is held to threaten, harm or endanger the safety and welfare of people, and that sets out the punishments to be imposed on people who do not obey these laws.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;InternationalLaw">
      <rdfs:label>international law</rdfs:label>
      <skos:definition>The set of rules generally regarded and accepted as binding in relations between states and nations.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;ReligiousLaw">
      <rdfs:label>religious law</rdfs:label>
      <skos:definition>Some set of laws derived from and practised in the name of some religion.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;ReligiousLawJurisdiction">
      <rdfs:label>religious law jurisdiction</rdfs:label>
      <skos:definition>A Jurisdiction in which some system of religious law is in force.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;ReligiousLawSystem">
      <rdfs:label>religious law system</rdfs:label>
      <skos:definition>Some system of law based on the ordering principle of reality as being knowledge as revealed by God and governing all human affairs.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-law-jur;LegalSystem"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;ShariaLawJurisdiction">
      <rdfs:label>sharia law jurisdiction</rdfs:label>
      <skos:definition>A Jurisdiction in which a Shari'a system of law is in force.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-lawx-jur;ReligiousLawJurisdiction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;ShariaLawSystem">
      <rdfs:label>sharia law system</rdfs:label>
      <skos:definition>A system of Shari'a Law.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-lawx-jur;StatutoryLaw">
      <rdfs:label>statutory law</rdfs:label>
      <skos:definition>Written law set down by a legislature or by a legislator.</skos:definition>
   </owl:Class>


