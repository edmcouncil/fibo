<owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;bijuridicialJurisdictionPartiallyGovernedUnderCivilLawSystem">
      <rdfs:label>bijuridicial jurisdiction partially governed under civil law system</rdfs:label>
      <skos:definition>One system of law under which a Bijuridicial Jurisdiction is governed is the system of Civil Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;jurisdictionGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-lawx-jur;BijuridicialJurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;CivilLawSystem"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;bijuridicialJurisdictionPartiallyGovernedUnderCommonLawSystem">
      <rdfs:label>bijuridicial jurisdiction partially governed under common law system</rdfs:label>
      <skos:definition>One system of law under which a Bijuridicial Jurisdiction is governed is the system of Common Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;jurisdictionGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-lawx-jur;BijuridicialJurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;CommonLawSystem"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;incorporatesSome">
      <rdfs:label>incorporates some</rdfs:label>
      <skos:definition>The laws included in this Body of Law.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-lawx-jur;BodyOfLaw"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-cor;Law"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;hasInForce">
      <rdfs:label>has in force</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-law-jur;isInForceIn"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;definesTheApplicationOf">
      <rdfs:label>defines the application of</rdfs:label>
      <skos:definition>The Body of Law, the application of which is defined in the Legal System.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;LegalSystem"/>
      <rdfs:range rdf:resource="&fibo-fnd-lawx-jur;BodyOfLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;religiousLawJurisdictionGovernedUnder">
      <rdfs:label>religious law jurisdiction governed under</rdfs:label>
      <skos:definition>The system of law under which a Religious Law Jurisdiction is governed. This is some system of Religious Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;jurisdictionGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-lawx-jur;ReligiousLawJurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-lawx-jur;ReligiousLawSystem"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;shariaLawJurisdictionGovernedUnder">
      <rdfs:label>sharia law jurisdiction governed under</rdfs:label>
      <skos:definition>The system of law under which a Shari'a Law Jurisdiction is governed. This is the system of Shari'a Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lawx-jur;religiousLawJurisdictionGovernedUnder"/>
      <rdfs:domain rdf:resource="&fibo-fnd-lawx-jur;ShariaLawJurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-lawx-jur;ShariaLawSystem"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;constrainsStatuteLaw">
      <rdfs:label>constrains statute law</rdfs:label>
      <skos:definition>The Constitution constrains what can or cannot be legislated as Law for the Jurisdiction in which that Constitution applies.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-cor;governs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-cor;GovernmentalConstitution"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-jur;governsLegalSystem">
      <rdfs:label>governs legal system</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-law-cor;GovernmentalConstitution"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;LegalSystem"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-law-jur;legalSystemIsGovernedBy"/>
   </owl:ObjectProperty>


