<owl:ObjectProperty rdf:about="&fibo-rl-inc;investmentCapitalIsApplicationOf">
      <rdfs:label>investment capital is application of</rdfs:label>
      <skos:definition>The Capital (as an economic resource) which is identified as the Investment Capital.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-ins;purposiveAmountIsApplicationOf"/>
      <rdfs:domain rdf:resource="&fibo-rl-inc;InvestmentCapital"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-aeq;Capital"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-rl-inc;investmentCapitalHasPurpose">
      <rdfs:label>investment capital has purpose</rdfs:label>
      <skos:definition>The investment purpose to which the capital is to be put.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-ins;purposiveAmountHasPurpose"/>
      <rdfs:domain rdf:resource="&fibo-rl-inc;InvestmentCapital"/>
      <rdfs:range rdf:resource="&fibo-rl-inc;InvestmentPurpose"/>
   </owl:ObjectProperty>


