<owl:Class rdf:about="&fibo-fnd-occ-prod;EventProgram">
      <rdfs:label>event program</rdfs:label>
      <skos:definition>A planned sequence of events.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-occ-prod;Program"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-occ-prod;Production">
      <rdfs:label>production</rdfs:label>
      <skos:definition>A combination of an event and a context, in which something or some range of things are produced, created or issued.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;MediatingThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;OccurentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-occ-prod;ProductionProgram">
      <rdfs:label>production program</rdfs:label>
      <skos:definition>A planned set of Productions.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-occ-prod;Program">
      <rdfs:label>program</rdfs:label>
      <skos:definition>A set of activities and / or events set up at one time by some agency with the intent to have those activities and / or events take place at some point in the future.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-set;Set"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-occ-act;Activity"/>
   </owl:Class>


