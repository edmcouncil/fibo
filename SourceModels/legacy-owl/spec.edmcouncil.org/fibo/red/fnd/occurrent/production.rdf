<owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;productionRole">
      <rdfs:label>production role</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-act;Actor"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-prod;Production"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-prod;Production"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-occ-prod;follows"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;hasProgrammedElement">
      <rdfs:label>has programmed element</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-prod;hasConstituent.1"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-prod;EventProgram"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-evt;Event"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;isAbout">
      <rdfs:label>is about</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-info;Information"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-prod;Production"/>
      <owl:inverseOf rdf:resource="&fibo-bp-iss-doc;hasDetails.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;completes">
      <rdfs:label>completes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-lcy;signalsCompletion"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-lcy;Milestone"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-prod;Production"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;hasConstituent">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-prod;hasConstituent.1"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-prod;ProductionProgram"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-prod;Production"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;follows">
      <rdfs:label>follows</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-prod;Production"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;produces">
      <rdfs:label>produces</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-prod;Production"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;context">
      <rdfs:label>context</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-prod;Production"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-prod;hasConstituent.1">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-prod;Program"/>
      <rdfs:range rdf:resource="&fibo-uo-lat;OccurentThing"/>
   </owl:ObjectProperty>


