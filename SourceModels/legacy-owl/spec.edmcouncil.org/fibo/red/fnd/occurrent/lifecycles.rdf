<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-occ-lcy "http://spec.edmcouncil.org/fibo/red/fnd/occurrent/lifecycles/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-gao-gl "http://spec.edmcouncil.org/fibo/red/fnd/goals-and-objectives/goals/">
    <!ENTITY fibo-fnd-occ-proc "http://spec.edmcouncil.org/fibo/red/fnd/occurrent/processes/">
    <!ENTITY fibo-fnd-occ-evt "http://spec.edmcouncil.org/fibo/red/fnd/occurrent/events/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/lifecycles/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/lifecycles/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/goals-and-objectives/goals/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/processes/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/events/"/>
   </owl:Ontology>


