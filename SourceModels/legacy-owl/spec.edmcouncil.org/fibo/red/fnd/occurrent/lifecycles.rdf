<owl:Class rdf:about="&fibo-fnd-occ-lcy;Lifecycle">
      <rdfs:label>lifecycle</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Abstract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-occ-lcy;Milestone">
      <rdfs:label>milestone</rdfs:label>
      <skos:definition>An event with a date and some achievement of some stated Goal.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-occ-evt;Event"/>
   </owl:Class>


