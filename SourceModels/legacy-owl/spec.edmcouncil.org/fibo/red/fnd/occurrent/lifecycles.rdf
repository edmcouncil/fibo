<owl:ObjectProperty rdf:about="&fibo-fnd-occ-lcy;includes">
      <rdfs:label>includes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-lcy;Lifecycle"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-evt;Event"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-lcy;isLifecycleOf">
      <rdfs:label>is lifecycle of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-lcy;Lifecycle"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-lcy;realizes">
      <rdfs:label>realizes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-lcy;Milestone"/>
      <rdfs:range rdf:resource="&fibo-fnd-gao-gl;Goal"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-lcy;signalsCompletion">
      <rdfs:label>signals completion</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-lcy;Milestone"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
   </owl:ObjectProperty>


