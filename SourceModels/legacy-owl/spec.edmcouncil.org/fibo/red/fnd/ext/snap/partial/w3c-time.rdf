<owl:Class rdf:about="&fibo-ext-snap-w3c-tim;Instant">
      <rdfs:label>instant</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-ext-snap-w3c-tim;TemporalEntity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ext-snap-w3c-tim;Interval">
      <rdfs:label>interval</rdfs:label>
      <skos:definition>A Proper Interval, i.e. an Interval whose beginnign and end are different Note: we have ignored the distinction in the W3C suggested OWL ontology for Time, which has Interval and Proper Interval as separate types of Thing.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ext-snap-w3c-tim;TemporalEntity">
      <rdfs:label>temporal entity</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;TemporalThing"/>
   </owl:Class>


