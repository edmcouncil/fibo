<owl:ObjectProperty rdf:about="&fibo-fnd-org-lg;illegalCartelHasMember">
      <rdfs:label>illegal cartel has member</rdfs:label>
      <skos:definition>Some organization which is a member of the cartel.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-org-org;organizationHasMember"/>
      <rdfs:domain rdf:resource="&fibo-fnd-org-lg;IllegalCartel"/>
      <rdfs:range rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
   </owl:ObjectProperty>


