         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/organizations/legitimate-organizations/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/organizations/legitimate-organizations/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/organizations/formal-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/organizations/organizations/"/>
   </owl:Ontology>


