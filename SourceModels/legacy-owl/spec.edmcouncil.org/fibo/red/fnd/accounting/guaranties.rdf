<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-acc-gty "http://spec.edmcouncil.org/fibo/red/fnd/accounting/guaranties/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-be-le-lei "http://spec.edmcouncil.org/fibo/red/be/legal-entities/lei-entities/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/accounting/guaranties/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/accounting/guaranties/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/lei-entities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-fnd-acc-gty;PriorityLevel">
      <skos:definition>Level of Priority for example for a Credit Enhancement (Guaranty) priority.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secondary</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Primary</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-acc-gty;guarantorIdentity">
      <rdfs:label>guarantor identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-gty;Guarantor"/>
      <rdfs:range rdf:resource="&fibo-be-le-lei;ContractuallyCapableEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-gty;amount">
      <rdfs:label>amount</rdfs:label>
      <skos:definition>Amount of the guaranty.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-gty;Guaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-gty;effectivePeriod">
      <rdfs:label>effective period</rdfs:label>
      <skos:definition>Calendar Period during which the guaranty is valid.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-gty;Guaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;DatePeriod"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-gty;priorityLevel">
      <rdfs:label>priority level</rdfs:label>
      <skos:definition>Credit Enhancer Level of Priority.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-gty;Guaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-gty;PriorityLevel"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-gty;hasGuarantor">
      <rdfs:label>has guarantor</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-gty;Guaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-gty;Guarantor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-gty;takesFormOf">
      <rdfs:label>takes form of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;identity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-gty;Guaranty"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-acc-gty;expirationDate">
      <rdfs:label>expiration date</rdfs:label>
      <skos:definition>Expiration Date for credit enhancement on the security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Date"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-acc-gty;Guarantor">
      <rdfs:label>guarantor</rdfs:label>
      <skos:definition>A Party which guarantees to underwrite some cash flow obligation on behalf of some other party.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;ContractThirdParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-gty;Guaranty">
      <rdfs:label>guaranty</rdfs:label>
      <skos:definition>Arrangement whereby cash flows on the debt security, eg, interest payment, are guaranteed by a firm other than the issuer.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>