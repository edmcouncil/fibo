<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-fnd-math-amt "http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-plc-cty "http://spec.edmcouncil.org/fibo/red/fnd/places/countries/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-acc-cur;monetaryAmountHasCurrency">
      <rdfs:label>monetary amount has currency</rdfs:label>
      <skos:definition>The currency in which the monetary amount is defined.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-cur;hasCurrency"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-amt;isMeasuredIn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-cur;moneyAmountHasBaseMoneyUnit">
      <rdfs:label>money amount has base money unit</rdfs:label>
      <skos:definition>The currency in which the money amount is denominated.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-cur;hasBaseMoneyUnit"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-amt;hasBaseUnit"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-cur;hasBaseMoneyUnit">
      <rdfs:label>has base money unit</rdfs:label>
      <skos:definition>the currency in which the money amount is denominated</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-cur;hasCurrency">
      <rdfs:label>has currency</rdfs:label>
      <skos:definition>the currency in which the monetary amount is defined</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-cur;hasNotionalAmount">
      <rdfs:label>has notional amount</rdfs:label>
      <skos:definition>has a notional value expressed as some monetary amount, that is a number and a currency in which that number is denominated</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-cur;isTenderIn">
      <rdfs:label>is tender in</rdfs:label>
      <skos:definition>A country in which the currency is exchangeable for goods and services. Commonly referred to also as legal tender, however this definition does not hold literally in some countries e.g. Scotland.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-cur;Currency"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-acc-cur;hasAmount">
      <rdfs:label>has amount</rdfs:label>
      <skos:definition>a total number or quantity</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-acc-cur;hasPercentageAmount">
      <rdfs:label>has percentage amount</rdfs:label>
      <skos:definition>a number or quantity represented as a percentage</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-acc-cur;Currency">
      <rdfs:label>currency</rdfs:label>
      <skos:definition>The unit of measure of cash.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-amt;Unit"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-cur;MonetaryAmount">
      <rdfs:label>monetary amount</rdfs:label>
      <skos:definition>The measure which is an amount of money specified in monetary units.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-cur;MonetaryMeasure"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-cur;MonetaryMeasure">
      <rdfs:label>monetary measure</rdfs:label>
      <skos:definition>Some measure of some sum of money.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-amt;Measure"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-cur;MoneyAmount">
      <rdfs:label>money amount</rdfs:label>
      <skos:definition>A sum of money.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-amt;MaterialAmount"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-cur;PercentageMonetaryAmount">
      <rdfs:label>percentage monetary amount</rdfs:label>
      <skos:definition>A measure of some amount of money expressed as a percentage of some other amount, some notional amount or some concrete Money Amount.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
