<owl:ObjectProperty rdf:about="&fibo-fnd-pty-rl;agentInRoleHasRole">
      <rdfs:label>agent in role has role</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-rl;hasRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-rl;AgentInRole"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-rl;Role"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pty-rl;hasIdentity">
      <rdfs:label>has identity</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-rl;AgentInRole"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pty-rl;hasRole">
      <rdfs:label>has role</rdfs:label>
      <skos:definition>provides a means for relating a person, organization, group, or other entity to a role that entity plays in some relationship and context</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-rl;Role"/>
   </owl:ObjectProperty>


