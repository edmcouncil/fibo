    <!ENTITY fibo-fnd-phy-phy "http://spec.edmcouncil.org/fibo/red/fnd/physical/physical/">
    <!ENTITY fibo-fnd-acc-acc "http://spec.edmcouncil.org/fibo/red/fnd/accounting/accounting-main/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/math/amounts/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/physical/physical/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/accounting-main/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-math-amt;mayBeExpressedAs">
      <rdfs:label>may be expressed as</rdfs:label>
      <skos:definition>A measure in which the material amount may be expressed.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-math-amt;MaterialAmount"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-amt;Measure"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-math-amt;reciprocal">
      <rdfs:label>reciprocal</rdfs:label>
      <skos:definition>A measure stated in terms of the recpirocal of another measure.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-math-amt;Measure"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-amt;Measure"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-math-amt;hasBaseUnit">
      <rdfs:label>has base unit</rdfs:label>
      <skos:definition>That in which the material amount is measured.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-math-amt;MaterialAmount"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-amt;Unit"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-math-amt;isMeasuredIn">
      <rdfs:label>is measured in</rdfs:label>
      <skos:definition>The unit of measure in which the Measure is formally expressed.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-math-amt;Measure"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-amt;Unit"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-math-amt;isAmountOf">
      <rdfs:label>is amount of</rdfs:label>
      <skos:definition>That which the Material Amount is an amount of.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-amt;isQuantityOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-math-amt;MaterialAmount"/>
      <rdfs:range rdf:resource="&fibo-fnd-phy-phy;ContinuousPhysicalStuff"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-math-amt;isQuantityOf">
      <rdfs:label>is quantity of</rdfs:label>
      <skos:definition>That which the amount is an amount of.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-math-amt;NumericAmount"/>
      <rdfs:range rdf:resource="&fibo-fnd-phy-phy;DiscretePhysicalThing"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-math-amt;amount">
      <rdfs:label>amount</rdfs:label>
      <skos:definition>The amount of whatever this is a quantity of.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-math-amt;.UnionClass.6">
      <skos:definition>Something that is widely accepted in payment for goods and services and in settling debts.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-acc-acc;Coinage"/>
               <owl:Class rdf:about="&fibo-fnd-acc-acc;CashCurrency"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-math-amt;Unit">
      <rdfs:label>unit</rdfs:label>
      <skos:definition>A unit of measure ofa quantity, time or anything else which is measurable. NB don't confuse this with units in a Trust Fund!!</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-amt;ParametricThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-math-amt;MaterialAmount">
      <rdfs:label>material amount</rdfs:label>
      <skos:definition>A qualified amount of some continuous substance or some thing measured in units. Material Amount is always qualified by some unit in which the substance is measured.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-amt;NumericAmount"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-math-amt;MathematicalThing">
      <rdfs:label>mathematical thing</rdfs:label>
      <skos:definition>A Mathematical Thing is something in the universe of mathematics. That is, something that is a parameter (mathematical quantity) or something that is used to express that parameter, such as a value or components of a formula.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Abstract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-math-amt;Measure">
      <rdfs:label>measure</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-amt;MathematicalThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-math-amt;NumericAmount">
      <rdfs:label>numeric amount</rdfs:label>
      <skos:definition>An amount of some discrete thing Note: does not require units as the individual things are counted. Tech note: in other (data) standards this stands alongside (material) Amount rather than above it see e.g. CCTS, ISO 20022 v1.5</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-amt;QuantitativeAmount"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-math-amt;ParametricThing">
      <rdfs:label>parametric thing</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-math-amt;QuantitativeAmount">
      <rdfs:label>quantitative amount</rdfs:label>
      <skos:definition>A measured amount of anything (material or time). This is an actual quantity of something rather than the abstract concept of the amount that something is of. Hence it is concrete not abstract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Concrete"/>
   </owl:Class>


  <!--
