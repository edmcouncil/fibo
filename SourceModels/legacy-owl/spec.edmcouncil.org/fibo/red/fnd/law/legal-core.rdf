<owl:ObjectProperty rdf:about="&fibo-fnd-law-cor;governs">
      <rdfs:label>governs</rdfs:label>
      <skos:definition>The constitution governs and constrains the application of the or each body of law in the jurisdiction other framework to which it applies.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;governs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-cor;Constitution"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-cor;Law"/>
   </owl:ObjectProperty>


