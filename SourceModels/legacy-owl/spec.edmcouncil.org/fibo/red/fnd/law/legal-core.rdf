<owl:Class rdf:about="&fibo-fnd-law-cor;Constitution">
      <rdfs:label>constitution</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Concrete"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-cor;CourtOfLaw">
      <rdfs:label>court of law</rdfs:label>
      <skos:definition>Some formal body with the mandate to hear disputes or cases.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-cor;GovernmentalConstitution">
      <rdfs:label>governmental constitution</rdfs:label>
      <skos:definition>A body of rules, commitments and statements that set out how a country, state or territory is to be run.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-law-cor;Constitution"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-cor;Law">
      <rdfs:label>law</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-cor;Ordinance">
      <rdfs:label>ordinance</rdfs:label>
      <skos:definition>An authoritative rule or law; a decree or command; a public injunction or regulation, such as a city ordinance against excessive horn blowing. (Source: Dictionary.com)</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-law-cor;Law"/>
   </owl:Class>


