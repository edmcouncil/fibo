<owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;contractEmbodies">
      <rdfs:label>contract embodies</rdfs:label>
      <skos:definition>The agreement which the contract describes as obtaining between the parties to the Contract.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;contractualEconomicAgreementConferredBy">
      <rdfs:label>contractual economic agreement conferred by</rdfs:label>
      <skos:definition>That which confers the contractual economic commitment (transaction related commitment), namely some Economic Agreement with contractual standing, and in which the contract between the parties sets out the details of the Commitment.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-cst;contractualCommitmentConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;contractualEconomicAgreementFormalizedBy">
      <rdfs:label>contractual economic agreement formalized by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-cst;agreementFormalizedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-txn-rea;economicContractEmbodies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;contractualEconomicAgreementHasParty">
      <rdfs:label>contractual economic agreement has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-cst;contractualAgreementHasParty"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;ContractualTransactionParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;contractualEconomicCommitmentDescribedIn">
      <rdfs:label>contractual economic commitment described in</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-txn-rea;setsOutContractualEconomicCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;contractualTransactionPartyIdentity">
      <rdfs:label>contractual transaction party identity</rdfs:label>
      <skos:definition>Transaction Party is identified as some form of Legal Entity in all cases.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;contractPartyIdentity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;ContractualTransactionParty"/>
      <rdfs:range rdf:resource="&fibo-be-le-lei;ContractuallyCapableEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;economicAgreementGovernsTransaction">
      <rdfs:label>economic agreement governs transaction</rdfs:label>
      <skos:definition>The transaction which is set out by this agreement.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;economicAgreementHasParty">
      <rdfs:label>economic agreement has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-inv;agreementHasParty"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;economicCommitmentIsConferredBy">
      <rdfs:label>economic commitment is conferred by</rdfs:label>
      <skos:definition>The Economic Commitment, that is some commitment which forms part of a transaction, is conferred by the bilateral Economic Agreement between the parties to the transaction.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-inv;commitmentConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicCommitment"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;economicContractEmbodies">
      <rdfs:label>economic contract embodies</rdfs:label>
      <skos:definition>The economic agreement (transaction related agreement) which the Economic Contract describes as obtaining between the parties to the Contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;contractEmbodies"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;economicContractHasTerms">
      <rdfs:label>economic contract has terms</rdfs:label>
      <skos:definition>A set of terms that form part of the Economic Contract. These are terms governing aspects of the transaction between the parties.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;contractHasTerms"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;definedInContextOf">
      <rdfs:label>defined in context of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;inContext"/>
      <rdfs:domain rdf:resource="&fibo-rl-ecr;EconomicResource"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;mayTakeForm">
      <rdfs:label>may take form</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-ecr;takesMaterialForm"/>
      <rdfs:domain rdf:resource="&fibo-rl-ecr;EconomicResource"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;isAspectOf">
      <rdfs:label>is aspect of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-asp;isAspectOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEventAspect"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;seenFromPerspectiveOf">
      <rdfs:label>seen from perspective of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-asp;seenFromPerspectiveOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEventAspect"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;hasCorresponding">
      <rdfs:label>has corresponding</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;hasCorrespondingAlternativeAspect">
      <rdfs:label>has corresponding alternative aspect</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-asp;hasOtherSide"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEventAspect"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionEventAspect"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;transactionEmbodiesEconomicAgreement">
      <rdfs:label>transaction embodies economic agreement</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicAgreement"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-txn-rea;economicAgreementGovernsTransaction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;hasComponent">
      <rdfs:label>has component</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;confersContractualEconomicAgreement">
      <rdfs:label>confers contractual economic agreement</rdfs:label>
      <skos:definition>A Contractual Commitment forming part of a (Transaction related) Contractual Economic Agreement and which is documented in the Economic Contract which evidences the agreement between the parties in respect of that transaction.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-cst;confersContractualCommitment"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;confersEconomicCommitment">
      <rdfs:label>confers economic commitment</rdfs:label>
      <skos:definition>Some Economic Commitment, that is a commitment in respect of an economic transaction, which arises from the Economic Agreement and exists as a result of that Agreement between the parties to the transaction.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;embodiesEconomicCommitment">
      <rdfs:label>embodies economic commitment</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;establishesContractualEconomicCommitment">
      <rdfs:label>establishes contractual economic commitment</rdfs:label>
      <skos:definition>An economic contract sets up some commitment on the part of one or other party to the contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-cst;mandates"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;setsOutContractualEconomicCommitment">
      <rdfs:label>sets out contractual economic commitment</rdfs:label>
      <skos:definition>The economic commitment set out in the terms to the contract which formalizes the agreement to the transaction.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;transactsWith">
      <rdfs:label>transacts with</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;transactionEventFollowsBusinessProcess">
      <rdfs:label>transaction event follows business process</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;transactionParty">
      <rdfs:label>transaction party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;subject">
      <rdfs:label>subject</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
      <rdfs:range rdf:resource="&fibo-rl-ecr;EconomicResource"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;hasPerspective">
      <rdfs:label>has perspective</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;TransactionEventAspect"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-txn-rea;isAspectOf"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-rea;transactedUnder">
      <rdfs:label>transacted under</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
   </owl:ObjectProperty>


