<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-txn-sup "http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/">
    <!ENTITY fibo-rl-ecr "http://spec.edmcouncil.org/fibo/red/fnd/relative/economic-resources/">
    <!ENTITY fibo-fnd-txn-rea "http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-fnd-bus-ecr "http://spec.edmcouncil.org/fibo/red/fnd/business/marketable-economic-resource/">
    <!ENTITY fibo-fnd-txn-mkt "http://spec.edmcouncil.org/fibo/red/fnd/transactions/market-transactions/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/economic-resources/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/business/marketable-economic-resource/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/market-transactions/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-txn-sup;denomination">
      <rdfs:label>denomination</rdfs:label>
      <skos:definition>The currency in which the negotiable cash is denominated. This is the currency seen in the "Monetary Amount".</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-sup;NegotiableCash"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-sup;customer">
      <rdfs:label>customer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-mkt;counterparty"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-sup;SupplyTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-sup;Customer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-sup;vendor">
      <rdfs:label>vendor</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-mkt;principal"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-sup;SupplyTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-sup;Vendor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-sup;supply">
      <rdfs:label>supply</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;subject"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-sup;SupplyTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-ecr;BundleOfResources"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-sup;supplyTransactionConsideration">
      <rdfs:label>supply transaction consideration</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-mkt;consideration"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-sup;SupplyTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-sup;NegotiableCash"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-sup;governedByTerms">
      <rdfs:label>governed by terms</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;transactedUnder"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-sup;SupplyTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-sup;SupplyTransactionTerms"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-txn-sup;scheduledDeliveryDate">
      <rdfs:label>scheduled delivery date</rdfs:label>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-txn-sup;NegotiableCash">
      <rdfs:label>negotiable cash</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-rl-ecr;EconomicResource"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-sup;Customer">
      <rdfs:label>customer</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-mkt;TransactionCounterparty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-sup;DeliveryTerms">
      <rdfs:label>delivery terms</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-sup;SupplyTransactionTerms"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-sup;SupplyTransaction">
      <rdfs:label>supply transaction</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-mkt;MarketTransaction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-sup;SupplyTransactionTerms">
      <rdfs:label>supply transaction terms</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-sup;Vendor">
      <rdfs:label>vendor</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-mkt;TransactionPrincipal"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>