<owl:ObjectProperty rdf:about="&fibo-fnd-txn-ext;governs">
      <rdfs:label>governs</rdfs:label>
      <skos:definition>The contractual relationship defined by the master agreement.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-ext;MasterAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;ContractualRelationship"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-ext;governedBy">
      <rdfs:label>governed by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-ext;CoveredTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-ext;MasterAgreement"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
