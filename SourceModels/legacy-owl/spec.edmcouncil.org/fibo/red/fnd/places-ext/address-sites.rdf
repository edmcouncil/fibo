<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-plcx-ads "http://spec.edmcouncil.org/fibo/red/fnd/places-ext/address-sites/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-be-corp-corp "http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/">
    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
    <!ENTITY fibo-be-le-cb "http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/">
    <!ENTITY fibo-fnd-plc-adr "http://spec.edmcouncil.org/fibo/red/fnd/places/addresses/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/address-sites/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/address-sites/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/addresses/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-plcx-ads;headquarteredAt">
      <rdfs:label>headquartered at</rdfs:label>
      <skos:definition>The address of the company's head office.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plcx-ads;hasPrimaryAddress"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-ads;PrimaryBusinessAddressRelation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-ads;siteIdentity">
      <rdfs:label>site identity</rdfs:label>
      <skos:definition>That which is a type of address, is a Postal Address.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;identity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-ads;Site"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-ads;hasOperatingAddress">
      <rdfs:label>has operating address</rdfs:label>
      <skos:definition>The address at which the organization carries out its business and receives correspondence relating to its business activities.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plcx-ads;tradingAtSite"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-ads;OperatingBusinessAddressRelation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-ads;hasPrimaryAddress">
      <rdfs:label>has primary address</rdfs:label>
      <skos:definition>The primary address at which this organization does business and received correspondence.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plcx-ads;tradingAtSite"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-ads;PrimaryBusinessAddressRelation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-ads;tradingAtSite">
      <rdfs:label>trading at site</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-ads;BusinessSite"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-ads;tradingAt">
      <rdfs:label>trading at</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plcx-ads;hasOperatingAddress"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-ads;OperatingBusinessAddressRelation"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-plcx-ads;isPrimaryAddress">
      <rdfs:label>is primary address</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-plcx-ads;isPrimaryAddress.1">
      <rdfs:label>is primary address</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-plcx-ads;effectiveDate">
      <rdfs:label>effective date</rdfs:label>
      <skos:definition>The date from which the address fulfils this role.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-plcx-ads;terminationDate">
      <rdfs:label>termination date</rdfs:label>
      <skos:definition>The date if known, when the address is no longer to fulfil this role.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-plcx-ads;isPrimaryAddress.2">
      <rdfs:label>is primary address</rdfs:label>
      <skos:definition>Whether the address is the primary or headquarters address of the entity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-plcx-ads;OperatingBusinessAddressRelation">
      <rdfs:label>operating business address relation</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-plcx-ads;BusinessSite"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-ads;PrimaryBusinessAddressRelation">
      <rdfs:label>primary business address relation</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-ads;Site">
      <rdfs:label>site</rdfs:label>
      <skos:definition>A site where an entity or organization has some presence.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-ads;RegisteredAddressSite">
      <rdfs:label>registered address site</rdfs:label>
      <skos:definition>An official Registered Address of a company, as registered with the appropriate Companies Registry for the jurisdiction in which the company is registered, and at which legal proceedings may be served upon the entity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-plcx-ads;Site"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-ads;BusinessSite">
      <rdfs:label>business site</rdfs:label>
      <skos:definition>An address from which a business entity carries out its business, such as a shop or offices. This is the address which is generally shown on advertising by that company.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>