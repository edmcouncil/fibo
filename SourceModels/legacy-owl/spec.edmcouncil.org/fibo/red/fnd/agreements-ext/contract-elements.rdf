<owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;IdentifiesContractPartyAs">
      <rdfs:label>identifies contract party as</rdfs:label>
      <skos:definition>The business entities identified in the contact party identification.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractPartyIdentification"/>
      <rdfs:range rdf:resource="&fibo-be-le-lei;ContractuallyCapableEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;clauseContainsTerm">
      <rdfs:label>clause contains term</rdfs:label>
      <skos:definition>An individual term contained within the clause.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;writtenContractHasPart"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractClause"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractTerm"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;identifies">
      <rdfs:label>identifies</rdfs:label>
      <skos:definition>A party identified in the contract party identification.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-idn;identifies"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractPartyIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ext;ContractParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;definesPrincipals">
      <rdfs:label>defines principals</rdfs:label>
      <skos:definition>The identification of parties to the contract as embodied in the preamble.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractPreamble"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractPrincipalsIdentification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;identifiesPrincipal">
      <rdfs:label>identifies principal</rdfs:label>
      <skos:definition>The principal to the contract, identified in the contract principals identification.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;identifies"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractPrincipalsIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;ContractPrincipal"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;identifiesCounterparty">
      <rdfs:label>identifies counterparty</rdfs:label>
      <skos:definition>The counterparty to the contract, identified in the contract principals identification.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;identifies"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractPrincipalsIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;ContractCounterparty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;hasTerm">
      <rdfs:label>has term</rdfs:label>
      <skos:definition>An individual term within the contractual terms set.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractTerm"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;identifiesThirdParty">
      <rdfs:label>identifies third party</rdfs:label>
      <skos:definition>A third party identified in the third party identification.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;identifies"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractThirdPartyIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;ContractThirdParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;identifiesAs">
      <rdfs:label>identifies as</rdfs:label>
      <skos:definition>The business entity which is identified as a third party.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;IdentifiesContractPartyAs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractThirdPartyIdentification"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;principalsIdentification">
      <rdfs:label>principals identification</rdfs:label>
      <skos:definition>The identification of the principals of the contract.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractPartyIdentification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;sectionContainsClause">
      <rdfs:label>section contains clause</rdfs:label>
      <skos:definition>A clause contained in the contract section.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;writtenContractHasPart"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractSection"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractClause"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;defines">
      <rdfs:label>defines</rdfs:label>
      <skos:definition>The definition of some matter as agreed by the parties to the contract and written in some part of that contract.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;WrittenContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;ContractualDefinition"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;definedInClause">
      <rdfs:label>defined in clause</rdfs:label>
      <skos:definition>The individual clause of the contract in which the set of terms is defined, if applicable.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;definedInSection"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractClause"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;definedInSection">
      <rdfs:label>defined in section</rdfs:label>
      <skos:definition>The section of the contract in which the contract terms set is defined.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractSection"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;identifiesContractPrincipalAs">
      <rdfs:label>identifies contract principal as</rdfs:label>
      <skos:definition>The business entities which are identified as the principals to the contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ele;IdentifiesContractPartyAs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-ele;ContractPrincipalsIdentification"/>
      <rdfs:range rdf:resource="&fibo-be-le-lei;ContractuallyCapableEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-ele;writtenContractHasPart">
      <rdfs:label>written contract has part</rdfs:label>
      <skos:definition>A formal part of the contract such as a clause or term.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;hasPart"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;WrittenContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ele;ContractPart"/>
   </owl:ObjectProperty>


