      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/lei-entities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contracts-extensions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
   </owl:Ontology>


