<owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractClause">
      <rdfs:label>contract clause</rdfs:label>
      <skos:definition>A set of contractual terms, grouped according to subject, intent or the type of rights and / or obligations to which it refers.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agrx-ele;ContractPart"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractPart">
      <rdfs:label>contract part</rdfs:label>
      <skos:definition>A formal part of a contract such as a clause or term.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;ContractualElement"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;WrittenInformation"/>
      <rdfs:subClassOf rdf:resource="&fibo-rl-mer;Part"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractPartyIdentification">
      <rdfs:label>contract party identification</rdfs:label>
      <skos:definition>The identification of parties to a contract. This is usually but not necessarily identified in the preamble to a contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-idn;PartyIdentification"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractPreamble">
      <rdfs:label>contract preamble</rdfs:label>
      <skos:definition>The part of a contract which defines the parties and gives sufficient information for them to be unambiguously identified, along with other pertinent information such as the subject of the contract.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractPrincipalsIdentification">
      <rdfs:label>contract principals identification</rdfs:label>
      <skos:definition>The identification of the principals to a contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agrx-ele;ContractPartyIdentification"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractSection">
      <rdfs:label>contract section</rdfs:label>
      <skos:definition>A formally identified Section of a Contract, comtaining terms dealing with a specific type of subject matter.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractTerm">
      <rdfs:label>contract term</rdfs:label>
      <skos:definition>An individual term in a Contract. Forms part of a set of Contractual Terms. Also exists within a Clause of a Contract A Clause exists within a Section of a Contract</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;ContractThirdPartyIdentification">
      <rdfs:label>contract third party identification</rdfs:label>
      <skos:definition>The identification of any third parties referred to in a contract but not being signatories to that contract. This would for example be the identification of calculation agents in a derivatives contract.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;RepresentationsSection">
      <rdfs:label>representations section</rdfs:label>
      <skos:definition>Section containing statements held out by one party to the other as being true and correct at the time of the Agreement. A representation, as contained in this section of a contract, is a statement by one other party asserting that some given state of the world exists.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;WarrantiesSection">
      <rdfs:label>warranties section</rdfs:label>
      <skos:definition>Section defining what is warranted by either party to the other. A warranty, as contained in this section of a contract, is a statement by one other party asserting that some given state of the world exists.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-ele;DRIP">
      <rdfs:label>d r i p</rdfs:label>
      <skos:definition>A kind of financial contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;BilateralContract"/>
   </owl:Class>


