  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-agrx-lic;License">
      <rdfs:label>license</rdfs:label>
      <skos:definition>A contract conferring certain rights and obligations on the holder of the contract (the licensee) who is generally unknown to the license issuer (the licensor).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;TransferableContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-lic;Licensee">
      <rdfs:label>licensee</rdfs:label>
      <skos:definition>The holder of a license such as a software license.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;TransferableContractHolder"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-lic;Licensor">
      <rdfs:label>licensor</rdfs:label>
      <skos:definition>The issuer of a license such as a software llicense.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;ContractOriginator"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-agrx-lic;SoftwareLicense">
      <rdfs:label>software license</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agrx-lic;License"/>
   </owl:Class>


