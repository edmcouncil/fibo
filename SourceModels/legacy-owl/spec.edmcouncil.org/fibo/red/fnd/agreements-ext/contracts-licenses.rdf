<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-agrx-lic "http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contracts-licenses/">
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contracts-licenses/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contracts-licenses/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-agrx-lic;heldBy">
      <rdfs:label>held by</rdfs:label>
      <skos:definition>The party which holds the license and enjoys the rights and benefits described in the license agreement.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;heldBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-lic;License"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-lic;Licensee"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-lic;issuedBy">
      <rdfs:label>issued by</rdfs:label>
      <skos:definition>The party which issues the license and extends the rights defined in the license agreement.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;writtenBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-lic;License"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-lic;Licensor"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
