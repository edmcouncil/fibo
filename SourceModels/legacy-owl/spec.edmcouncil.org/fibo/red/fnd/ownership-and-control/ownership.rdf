<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-oac-own "http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-rl-prc "http://spec.edmcouncil.org/fibo/red/fnd/relative/party-role-context/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/party-role-context/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;ownershipInvolves">
      <rdfs:label>ownership involves</rdfs:label>
      <skos:definition>The context of Ownership involves some party which is defined as an Owner.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;involves"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Ownership"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-own;Owner"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isAssetOf">
      <rdfs:label>is asset of</rdfs:label>
      <skos:definition>The party which is the Owner of the Asset.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Asset"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-own;Owner"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isOwnedBy">
      <rdfs:label>is owned by</rdfs:label>
      <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-own;Owner"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-oac-own;owns"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;owns">
      <rdfs:label>owns</rdfs:label>
      <skos:definition>A thing owned by the party.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Owner"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;takesForm">
      <rdfs:label>takes form</rdfs:label>
      <skos:definition>identifies the form the entity takes</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;identity"/>
      <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;asssetTakesForm">
      <rdfs:label>assset takes form</rdfs:label>
      <skos:definition>The form of the thing which is held as an asset.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;takesForm"/>
      <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Asset"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-oac-own;hasAcquisitionDate">
      <rdfs:label>has acquisition date</rdfs:label>
      <skos:definition>links an asset or owner/controller/controllee to the date of acquisition Editorial Note: Term introduces in OMG OWL migration, and is actually in Relations.rdf. In this Ownership ontology there is a cardinality restriction on Asset with cardinality = 1 which is what is represented here.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-oac-own;Owner">
      <rdfs:label>owner</rdfs:label>
      <skos:definition>A party which owns something. The thing owned is an Asset to that Party.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-oac-own;Ownership">
      <rdfs:label>ownership</rdfs:label>
      <skos:definition>Ownership is the context in which some Party is said to own some Independent Thing. The Party is defined as such due to its being the owning party to that Thing.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-rl-prc;RelationshipContext"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-oac-own;Asset">
      <rdfs:label>asset</rdfs:label>
      <skos:definition>A Thing held by some party and having some value.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Concrete"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>