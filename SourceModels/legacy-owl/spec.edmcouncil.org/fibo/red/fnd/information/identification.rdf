<owl:Class rdf:about="&fibo-fnd-info-idn;IdentifierIdentification">
      <rdfs:label>identifier identification</rdfs:label>
      <skos:definition>Identification of the scheme under which an Identifier is defined.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-idn;SchematicCodeIdentification"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-idn;IdentifyingThing">
      <rdfs:label>identifying thing</rdfs:label>
      <skos:definition>The context of identification.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;MediatingThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-idn;EntityIdentification">
      <rdfs:label>entity identification</rdfs:label>
      <skos:definition>The identification of some thing by some means of identity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-idn;IdentifyingThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-idn;PartyIdentification">
      <rdfs:label>party identification</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-idn;EntityIdentification"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-idn;SchematicCodeIdentification">
      <rdfs:label>schematic code identification</rdfs:label>
      <skos:definition>The identification of some type of schematic code (such as an identifier) b some means.</skos:definition>
   </owl:Class>


