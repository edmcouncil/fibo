<owl:ObjectProperty rdf:about="&fibo-fnd-info-idn;appliesTo">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idn;IdentifierIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-ids;IdentificationScheme"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idn;appliesTo.1">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idn;IdentifierIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-ids;Identifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idn;identifies">
      <rdfs:label>identifies</rdfs:label>
      <skos:definition>Some Party as identified in the Party Identification.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idn;PartyIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idn;applies">
      <rdfs:label>applies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idn;EntityIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-ids;Identifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idn;appliesTo.2">
      <rdfs:label>applies to</rdfs:label>
      <skos:definition>That to which the identification applies - that is, whatever it is that is so identified.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idn;EntityIdentification"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idn;appliesTo.3">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idn;SchematicCodeIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;Scheme"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idn;appliesTo.4">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idn;SchematicCodeIdentification"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;SchematicCode"/>
   </owl:ObjectProperty>


