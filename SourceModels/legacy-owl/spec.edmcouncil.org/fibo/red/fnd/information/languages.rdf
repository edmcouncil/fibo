<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-info-lang "http://spec.edmcouncil.org/fibo/red/fnd/information/languages/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-fnd-plcx-gph "http://spec.edmcouncil.org/fibo/red/fnd/places-ext/geophysical/">
    <!ENTITY fibo-fnd-info-sch "http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/languages/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/languages/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/geophysical/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-fnd-info-lang;LanguageSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Deutsch</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Francais</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-lang;spokenIn">
      <rdfs:label>spoken in</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-lang;Language"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-gph;GeographicalArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-lang;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-lang;LanguageCodeScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-lang;LanguageCode"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-lang;identifies">
      <rdfs:label>identifies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-lang;LanguageCode"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-lang;Language"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-info-lang;languageName">
      <rdfs:label>language name</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-info-lang;LanguageSelection"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-info-lang;Language">
      <rdfs:label>language</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Abstract"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-lang;LanguageCode">
      <rdfs:label>language code</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;SchematicCode"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-lang;LanguageCodeScheme">
      <rdfs:label>language code scheme</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;Scheme"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>