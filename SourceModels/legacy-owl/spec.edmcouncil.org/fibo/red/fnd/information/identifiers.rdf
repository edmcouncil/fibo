<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-info-ids "http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/">
    <!ENTITY fibo-fnd-info-sch "http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-ids;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-ids;Identifier"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-ids;IdentificationScheme"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-ids;defines"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-ids;identifies">
      <rdfs:label>identifies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-ids;Identifier"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-ids;hasIdentifier">
      <rdfs:label>has identifier</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-uo-rl;IndependentThing"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-ids;Identifier"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-ids;identifies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-ids;defines">
      <rdfs:label>defines</rdfs:label>
      <skos:definition>The Identification Scheme sets out the structure and nature of some set of codes which are Identifiers.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-ids;IdentificationScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-ids;Identifier"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-info-ids;hasUniqueIdentifier">
      <rdfs:label>has unique identifier</rdfs:label>
      <skos:definition>The textual value of the unique identification code. Editorial Note: Renamed to match the property in FIBO Foundations Relations ontology.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-info-ids;Identifier">
      <rdfs:label>identifier</rdfs:label>
      <skos:definition>A textual code which uniquely identifies something.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;SchematicCode"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-ids;IdentificationScheme">
      <rdfs:label>identification scheme</rdfs:label>
      <skos:definition>A formal scheme defining identification codes for some purpose.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;Scheme"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>