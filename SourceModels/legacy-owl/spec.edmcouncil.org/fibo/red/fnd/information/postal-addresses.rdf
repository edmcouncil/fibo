  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-info-pos;.UnionClass.8">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-plcx-gph;GeophysicalLocation"/>
               <owl:Class rdf:about="&fibo-fnd-plc-cty;Country"/>
               <owl:Class rdf:about="&fibo-fnd-info-pos;PostBox"/>
               <owl:Class rdf:about="&fibo-fnd-plcx-rea;BuiltProperty"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;BuildingSubUnitAddressFragment">
      <rdfs:label>building sub unit address fragment</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-pos;PostalAddressElement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;CountryName">
      <rdfs:label>country name</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;NonSpecificAddressText">
      <rdfs:label>non specific address text</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;POBoxNumber">
      <rdfs:label>p o box number</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;PostBox">
      <rdfs:label>post box</rdfs:label>
      <skos:definition>A PO Box.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-plc-loc;PhysicalLocation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;PostalAddressElement">
      <rdfs:label>postal address element</rdfs:label>
      <skos:definition>Text which is part of some Postal Address. Editorial Note: This is labeled as Address Element but is specific to Postal Addresses. The more general term for elements of any type of address is Address Component. This also covers e.g. network addresses, URIs.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-plc-adr;AddressComponent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;PostalCode">
      <rdfs:label>postal code</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-pos;StreetAddressText">
      <rdfs:label>street address text</rdfs:label>
   </owl:Class>


