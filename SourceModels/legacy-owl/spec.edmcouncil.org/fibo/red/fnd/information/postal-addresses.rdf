<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-info-pos "http://spec.edmcouncil.org/fibo/red/fnd/information/postal-addresses/">
    <!ENTITY fibo-fnd-plc-loc "http://spec.edmcouncil.org/fibo/red/fnd/places/locations/">
    <!ENTITY fibo-fnd-plc-adr "http://spec.edmcouncil.org/fibo/red/fnd/places/addresses/">
    <!ENTITY fibo-fnd-plcx-gph "http://spec.edmcouncil.org/fibo/red/fnd/places-ext/geophysical/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-plc-cty "http://spec.edmcouncil.org/fibo/red/fnd/places/countries/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-plcx-rea "http://spec.edmcouncil.org/fibo/red/fnd/places-ext/real-estate/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/postal-addresses/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/postal-addresses/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/locations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/addresses/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/geophysical/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/real-estate/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;addressIsContainedIn">
      <rdfs:label>address is contained in</rdfs:label>
      <skos:definition>The location which the Postal Address corresponds to in the world.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-loc;PhysicalLocation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;isLocatedIn">
      <rdfs:label>is located in</rdfs:label>
      <skos:definition>The postcode area which contains the premises identified by the Postal Address, Explanatory Note: This is an actual physical area, identified by the postal code (at whatever level of generality the corresponding postal code is defined), and is referred to in risk management, for example to identify common mortgage risk factors such as environmental risks or employment statistics.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;addressIsContainedIn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostcodeArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;isOf">
      <rdfs:label>is of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;apartmentIdentifiedBy">
      <rdfs:label>apartment identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-rea;Apartment"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;BuildingSubUnitAddressFragment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;buildingIdentifiedBy">
      <rdfs:label>building identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-rea;Building"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;StreetAddressText"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-pos;identifiesBuilding"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;countryHasName">
      <rdfs:label>country has name</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-cty;Country"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;CountryName"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-pos;identifiesCountry"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;floorIdentifiedBy">
      <rdfs:label>floor identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-rea;Floor"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;BuildingSubUnitAddressFragment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;hasPart">
      <rdfs:label>has part</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-cty;UrbanCenter"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostcodeArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy">
      <rdfs:label>physical address component identified by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-pos;.UnionClass.8"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;PostalAddressElement"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-pos;identifiesPhysicalAddressComponent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;postCodeAreaIdentifiedBy">
      <rdfs:label>post code area identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostcodeArea"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;PostalCode"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-pos;identifiesPostCodeArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;isSetOf">
      <rdfs:label>is set of</rdfs:label>
      <skos:definition>An element of the postal address, as a textual fragment. Explanatory Note: One or more of these makes up the written postal address.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;PostalAddressElement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;contains">
      <rdfs:label>contains</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostcodeArea"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-pos;isLocatedIn"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;isPartOf">
      <rdfs:label>is part of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isPartOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostcodeArea"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;UrbanCenter"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-pos;hasPart"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;streetIdentifiedBy">
      <rdfs:label>street identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-gph;Street"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;StreetAddressText"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;suiteIdentifiedBy">
      <rdfs:label>suite identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;physicalAddressComponentIdentifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-rea;Suite"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;BuildingSubUnitAddressFragment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;identifiesBuilding">
      <rdfs:label>identifies building</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;identifiesPhysicalAddressComponent"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-pos;StreetAddressText"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-rea;Building"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;identifiesBuildingSubUnit">
      <rdfs:label>identifies building sub unit</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;identifiesPhysicalAddressComponent"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-pos;BuildingSubUnitAddressFragment"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-rea;BuildingSubUnit"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;identifiesCountry">
      <rdfs:label>identifies country</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;identifiesPhysicalAddressComponent"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-pos;CountryName"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;identifiesPhysicalAddressComponent">
      <rdfs:label>identifies physical address component</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;isIndexTo"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-pos;PostalAddressElement"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;.UnionClass.8"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;identifiesPostBox">
      <rdfs:label>identifies post box</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;identifiesPhysicalAddressComponent"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-pos;POBoxNumber"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-pos;PostBox"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-pos;identifiesPostCodeArea">
      <rdfs:label>identifies post code area</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-pos;identifiesPhysicalAddressComponent"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-pos;PostalCode"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostcodeArea"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-info-pos;addressFragment">
      <rdfs:label>address fragment</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-pos;countryName">
      <rdfs:label>country name</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-pos;nonSpecificAddressText">
      <rdfs:label>non specific address text</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-pos;boxNumber">
      <rdfs:label>box number</rdfs:label>
      <skos:definition>The PO Box number expressed as text.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-pos;postalCode">
      <rdfs:label>postal code</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-pos;streetAddress">
      <rdfs:label>street address</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


