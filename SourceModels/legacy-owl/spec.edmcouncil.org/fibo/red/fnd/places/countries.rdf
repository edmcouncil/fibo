<owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cty;abbreviation">
      <rdfs:label>abbreviation</rdfs:label>
      <skos:definition>The abbreviation by which the state or province is known in its country, e.g. CA, BC.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cty;designation">
      <rdfs:label>designation</rdfs:label>
      <skos:definition>What the federal unit is called in this country, e.g. State, Province, Canton.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cty;officialName">
      <rdfs:label>official name</rdfs:label>
      <skos:definition>The official name of the country, e.g. "People's Republic of ..." Note that the common name for the country is the "Name" term inherited from the Geographical Entity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


