         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/social-constructs/social-constructs/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
