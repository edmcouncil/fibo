<owl:Class rdf:about="&fibo-loan-typ-per;PersonalLoan">
      <rdfs:label>personal loan</rdfs:label>
      <skos:definition>Consumer Loan: The recipient (borrower) is an end consumer. Types: Home Mortgage</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;Loan"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-per;PersonalLoanBorrower">
      <rdfs:label>personal loan borrower</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-per;PersonalLoanContract">
      <rdfs:label>personal loan contract</rdfs:label>
      <skos:definition>The contract for a loan which is advanced to an individual human beings or two more individual human beings in their personal capacities.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;LoanContract"/>
   </owl:Class>


