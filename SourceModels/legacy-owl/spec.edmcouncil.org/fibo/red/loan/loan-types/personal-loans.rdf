   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loan-types/personal-loans/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/legal-persons/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-parties/"/>
