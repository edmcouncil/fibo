<owl:ObjectProperty rdf:about="&fibo-loan-typ-per;setsOutTermsFor">
      <rdfs:label>sets out terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-bt;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-per;PersonalLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-per;PersonalLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-per;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;identity"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-per;PersonalLoanBorrower"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;NaturalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-per;hasBorrower">
      <rdfs:label>has borrower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;hasBorrower.1"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-per;PersonalLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-per;PersonalLoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-per;hasBorrower.1">
      <rdfs:label>has borrower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;hasBorrower"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-per;PersonalLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-per;PersonalLoanBorrower"/>
   </owl:ObjectProperty>


