<rdfs:Datatype rdf:about="&fibo-loan-typ-mtg;MortgagePurposeSelection">
      <skos:definition>A purpose for which a Mortgage Loan may be taken out.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Combination Mortgage</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Construction</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Debt Consolidation</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Equity Release</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description>
                                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Government Sponsored Loan</rdf:first>
                                          <rdf:rest>
                                             <rdf:Description>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Right To Buy</rdf:first>
                                                <rdf:rest>
                                                   <rdf:Description>
                                                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Purchase</rdf:first>
                                                      <rdf:rest>
                                                         <rdf:Description>
                                                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remortgage</rdf:first>
                                                            <rdf:rest>
                                                               <rdf:Description>
                                                                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remortgage On Different Terms</rdf:first>
                                                                  <rdf:rest>
                                                                     <rdf:Description>
                                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remortgage With Equity Release</rdf:first>
                                                                        <rdf:rest>
                                                                           <rdf:Description>
                                                                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Renovation</rdf:first>
                                                                              <rdf:rest>
                                                                                 <rdf:Description>
                                                                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Investment Mortgage</rdf:first>
                                                                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                                                 </rdf:Description>
                                                                              </rdf:rest>
                                                                           </rdf:Description>
                                                                        </rdf:rest>
                                                                     </rdf:Description>
                                                                  </rdf:rest>
                                                               </rdf:Description>
                                                            </rdf:rest>
                                                         </rdf:Description>
                                                      </rdf:rest>
                                                   </rdf:Description>
                                                </rdf:rest>
                                             </rdf:Description>
                                          </rdf:rest>
                                       </rdf:Description>
                                    </rdf:rest>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


