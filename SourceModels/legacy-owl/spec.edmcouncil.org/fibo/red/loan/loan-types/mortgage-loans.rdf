<owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;investmentHomeLoanContractSetsOutTermsFor">
      <rdfs:label>investment home loan contract sets out terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-typ-mtg;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;InvestmentHomeLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;InvestmentHomeLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;maturity">
      <rdfs:label>maturity</rdfs:label>
      <skos:definition>The length of time from the present until the maturity of the mortgage.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-tmp;TimedDurationSpecification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;loanMortgageMandateAmount">
      <rdfs:label>loan mortgage mandate amount</rdfs:label>
      <skos:definition>Mortgage Mandate - Amount of mortgage mandate that can be converted into a proper mortgage at a later stage.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoanContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;setsOutTermsFor">
      <rdfs:label>sets out terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-bt;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;collateralizedBy">
      <rdfs:label>collateralized by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-col;collateralizedBy"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-col;LoanPropertyCollateral"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;hasMIGProvider">
      <rdfs:label>has m i g provider</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-gty;guaranteedBy"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-gty;LoanMIGProvider"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;hasParty">
      <rdfs:label>has party</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanServicer.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;hasBorrowingParty">
      <rdfs:label>has borrowing party</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;MortgageBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;hasLendingParty">
      <rdfs:label>has lending party</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;MortgageLender"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;isLienOn">
      <rdfs:label>is lien on</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-col;LoanPropertyCollateral"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
      <owl:inverseOf rdf:resource="&fibo-loan-typ-mtg;collateralizedBy"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-mtg;setsOutTermsFor.1">
      <rdfs:label>sets out terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-typ-mtg;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-mtg;SecondHomeLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;SecondHomeLoan"/>
   </owl:ObjectProperty>


