<owl:ObjectProperty rdf:about="&fibo-loan-typ-com;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;identity"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-com;CommercialLoanBorrower"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-com;.UnionClass.4"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-com;hasBorrower">
      <rdfs:label>has borrower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;hasBorrower.1"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-com;CommercialLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-com;CommercialLoanBorrower"/>
   </owl:ObjectProperty>


