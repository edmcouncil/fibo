<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-loan-typ-gen "http://spec.edmcouncil.org/fibo/red/loan/loan-types/general-loans/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-loan-loan-bt "http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loan-types/general-loans/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loan-types/general-loans/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-loan-typ-gen;autoLoanContractSetsOutTermsFor">
      <rdfs:label>auto loan contract sets out terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-bt;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-gen;AutoLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-gen;AutoLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-gen;setsOutTermsFor">
      <rdfs:label>sets out terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-bt;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-gen;MarineFinanceContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-gen;MarineFinance"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-loan-typ-gen;loanType">
      <rdfs:label>loan type</rdfs:label>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;LoanTypeSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-gen;boatHeldAsChattel">
      <rdfs:label>boat held as chattel</rdfs:label>
      <skos:definition>Whether the vessel is to be held in ownership as a form of chattel by the lender during the period of the loan.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-gen;mooredAt">
      <rdfs:label>moored at</rdfs:label>
      <skos:definition>Textual description (name etc.) of where the vessel is to be moored if it has a permanent mooring place.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-gen;percentageAdvanced">
      <rdfs:label>percentage advanced</rdfs:label>
      <skos:definition>The percentage of the purchase price advanced as the loan.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-gen;residential">
      <rdfs:label>residential</rdfs:label>
      <skos:definition>Whether the boat is intended to be used and is legally able to be used as a place of residence.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-loan-typ-gen;AutoLoan">
      <rdfs:label>auto loan</rdfs:label>
      <skos:definition>A loan specifically for the purpose of automobile purchase.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;Loan"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-gen;AutoLoanContract">
      <rdfs:label>auto loan contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;LoanContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-gen;MarineFinance">
      <rdfs:label>marine finance</rdfs:label>
      <skos:definition>A loan for the purchase of a boat or other vessel.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-gen;MarineFinanceContract">
      <rdfs:label>marine finance contract</rdfs:label>
      <skos:definition>Contract embodying the terms under which a loan is advanced for marine finance.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>