<owl:ObjectProperty rdf:about="&fibo-loan-loan-gty;guarantyAmount">
      <rdfs:label>guaranty amount</rdfs:label>
      <skos:definition>The amount to which the guaranty is limited.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-loan-gty;LimitedGuaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-gty;givenBy">
      <rdfs:label>given by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;hasGuarantor"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-gty;LoanGuaranty"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-gty;LoanGuarantor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-gty;guaranteedBy">
      <rdfs:label>guaranteed by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-gty;LoanGuarantor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-gty;takesFormOf">
      <rdfs:label>takes form of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;takesFormOf"/>
