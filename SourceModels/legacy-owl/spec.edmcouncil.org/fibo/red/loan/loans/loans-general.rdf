<owl:Class rdf:about="&fibo-loan-loan-loan;CountryIdentificationScheme">
      <rdfs:label>country identification scheme</rdfs:label>
      <skos:definition>The scheme in which the identification code for a country is identified.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-ids;IdentificationScheme"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-loan-loan;CountryIdentifier">
      <rdfs:label>country identifier</rdfs:label>
      <skos:definition>A code for identifying a country</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-ids;Identifier"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-loan-loan;UnsubscribedDebt">
      <rdfs:label>unsubscribed debt</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-dbt-ins;PubliclyIssuedDebt"/>
   </owl:Class>


