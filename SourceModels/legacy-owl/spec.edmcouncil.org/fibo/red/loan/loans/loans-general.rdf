    <!ENTITY fibo-fnd-law-jur "http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/">
    <!ENTITY fibo-sec-dbt-dbt-ins "http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/debt-instruments/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-general/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-general/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-borrowers-temporal/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/debt-instruments/"/>
   </owl:Ontology>


