<owl:ObjectProperty rdf:about="&fibo-loan-loan-loan;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-ids;defines"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-loan;CountryIdentificationScheme"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-loan;CountryIdentifier"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-loan;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-ids;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-loan;CountryIdentifier"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-loan;CountryIdentificationScheme"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-loan;domiciled">
      <rdfs:label>domiciled</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;PersonalAssessmentFactsAtApproval"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>


