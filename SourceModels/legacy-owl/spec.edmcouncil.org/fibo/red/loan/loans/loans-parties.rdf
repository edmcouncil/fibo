<owl:ObjectProperty rdf:about="&fibo-loan-loan-par;accepts">
      <rdfs:label>accepts</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-bt;accepts"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;LoanBorrowerCommitment"/>
      <owl:inverseOf rdf:resource="&fibo-loan-loan-bt;acceptedBy.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;borrows">
      <rdfs:label>borrows</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <owl:inverseOf rdf:resource="&fibo-loan-loan-par;hasBorrower.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;identity">
      <rdfs:label>identity</rdfs:label>
      <skos:definition>The borrower of the loan is a legal entity of some type.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;contractPartyIdentity"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;borrows.1">
      <rdfs:label>borrows</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;Cosigner"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;LoanContract"/>
      <owl:inverseOf rdf:resource="&fibo-loan-loan-par;hasCoBorower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;mayBe">
      <rdfs:label>may be</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;Cosigner"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-gty;LoanGuarantor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;hasBorrower">
      <rdfs:label>has borrower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;hasCounterparty"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;LoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;hasCoBorower">
      <rdfs:label>has co borower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;hasBorrower"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;LoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;Cosigner"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;hasBorrower.1">
      <rdfs:label>has borrower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-dbt;hasBorrowingParty"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;hasLender">
      <rdfs:label>has lender</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-dbt;hasLendingParty"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanLender"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;hasParty">
      <rdfs:label>has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;hasThirdParty"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanThirdParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;originatedBy">
      <rdfs:label>originated by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;LoanOriginator"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-loan-par;servicedBy">
      <rdfs:label>serviced by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanServicer"/>
   </owl:ObjectProperty>


