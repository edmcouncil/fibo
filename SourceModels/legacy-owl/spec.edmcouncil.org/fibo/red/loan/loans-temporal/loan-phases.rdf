<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-loan-tmp-ph "http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-phases/">
    <!ENTITY fibo-loan-loan-bt "http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/">
    <!ENTITY fibo-loan-tmp-sts "http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loans-status/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-phases/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-phases/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loans-status/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-loan-tmp-ph;LoanPhaseSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Repayment</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paid In Full</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-loan-tmp-ph;hasStatus">
      <rdfs:label>has status</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-ph;DisbursedLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-sts;LoanPaymentStatus"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;deferred">
      <rdfs:label>deferred</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;deferred.1">
      <rdfs:label>deferred</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;disbursementDate">
      <rdfs:label>disbursement date</rdfs:label>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;inDefault">
      <rdfs:label>in default</rdfs:label>
      <skos:definition>Whether the loan is in default.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;loanPhase">
      <rdfs:label>loan phase</rdfs:label>
      <rdfs:range rdf:resource="&fibo-loan-tmp-ph;LoanPhaseSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;loanStatus">
      <rdfs:label>loan status</rdfs:label>
      <rdfs:range rdf:resource="&fibo-loan-tmp-sts;LoanStatus"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;loanPhase.1">
      <rdfs:label>loan phase</rdfs:label>
      <rdfs:range rdf:resource="&fibo-loan-tmp-ph;LoanPhaseSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ph;repaymentDate">
      <rdfs:label>repayment date</rdfs:label>
      <skos:definition>The date the loan was deemed to be repaid.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-loan-tmp-ph;DeferredLoan">
      <rdfs:label>deferred loan</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-tmp-ph;DisbursedLoan"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-ph;DelinquentLoan">
      <rdfs:label>delinquent loan</rdfs:label>
      <skos:definition>A loan which is an active loan (in Repayment phase), which is in default.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-ph;DisbursedLoan">
      <rdfs:label>disbursed loan</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;Loan"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-ph;InDefaultLoan">
      <rdfs:label>in default loan</rdfs:label>
      <skos:definition>Formal notice served.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-tmp-ph;DelinquentLoan"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-ph;RepaidLoan">
      <rdfs:label>repaid loan</rdfs:label>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>