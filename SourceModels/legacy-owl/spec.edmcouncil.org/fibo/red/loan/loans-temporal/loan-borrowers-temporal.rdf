      <skos:definition>Facts about the borroewr in the event of some delinquency.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-bor;IndividualPersonCreditRating">
      <rdfs:label>individual person credit rating</rdfs:label>
      <skos:definition>was Borrower Credit Rating</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-md-tmp-crr;IndividualCreditRating"/>
   </owl:Class>

