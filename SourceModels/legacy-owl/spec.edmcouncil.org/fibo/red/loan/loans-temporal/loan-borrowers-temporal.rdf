   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-bor;CorporateDatedFacts">
      <rdfs:label>corporate dated facts</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-tmp-bor;BorrowerOngoingDatedFacts"/>
