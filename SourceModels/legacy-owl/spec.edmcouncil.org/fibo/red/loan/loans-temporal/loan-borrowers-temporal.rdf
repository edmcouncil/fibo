<owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;isAbout">
      <rdfs:label>is about</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;BorrowerOngoingDatedFacts"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-ph;DisbursedLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;isAbout.1">
      <rdfs:label>is about</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-bor;isAbout.4"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;CorporateLoanBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-com;CorporateBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-par;identity"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-com;CorporateBorrower"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;annualProfitAndLoss">
      <rdfs:label>annual profit and loss</rdfs:label>
      <skos:definition>Primary borrower underwritten gross income (not rent), as per most recent Profit and Loss posting.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;CorporateDatedFacts"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;isAbout.2">
      <rdfs:label>is about</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-bor;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;DelinquencyBorrowerFacts"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-ph;DelinquentLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;borrowerRatingCreditAmount">
      <rdfs:label>borrower rating credit amount</rdfs:label>
      <skos:definition>Undefined PoC SDM Notes: Credit agency rating of Amount&nbsp;of&nbsp;the&nbsp;credit&nbsp;(Holland&nbsp;only) &lt;/p&gt; &lt;p&gt; (AO 35 , RR 31 ) &lt;/p&gt;</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;IndividualPersonCreditRating"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;appraisedBy">
      <rdfs:label>appraised by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;LoanAppraiser"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;isAbout.3">
      <rdfs:label>is about</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;LoanApplication"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;isAbout.4">
      <rdfs:label>is about</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;includes">
      <rdfs:label>includes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;PersonalDatedFacts"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-bor;ArrearsPaymentHistory"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;appraisedBy.1">
      <rdfs:label>appraised by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;BorrowerOngoingDatedFacts"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanServicer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;grossIncome">
      <rdfs:label>gross income</rdfs:label>
      <skos:definition>Primary borrower underwritten personal gross income (not rent)</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;PersonalDatedFacts"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;residencyPeriod">
      <rdfs:label>residency period</rdfs:label>
      <skos:definition>Period in which borrower is resident of the country - where the loan property is located.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;PersonalLoanBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;issuedBy">
      <rdfs:label>issued by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-crr;issuedBy"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;IndividualPersonCreditRating"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;CreditRatingIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-bor;rates">
      <rdfs:label>rates</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-crr;rates.3"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;IndividualPersonCreditRating"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;NaturalPerson"/>
   </owl:ObjectProperty>


