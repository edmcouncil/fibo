<rdfs:Datatype rdf:about="&fibo-loan-tmp-sts;LegalProceedingsStatusSelection">
      <skos:definition>The status of any legal proceedings.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Discharged</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In Process</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pending</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On Appeal By Initiator</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description>
                                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On Appeal By Respondent</rdf:first>
                                          <rdf:rest>
                                             <rdf:Description>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">None Pending</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                             </rdf:Description>
                                          </rdf:rest>
                                       </rdf:Description>
                                    </rdf:rest>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-loan-tmp-sts;LoanPerformanceStatusSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Default</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Foreclosure</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Repurchased</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Performing</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description>
                                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Redeemed</rdf:first>
                                          <rdf:rest>
                                             <rdf:Description>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In Arrears</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                             </rdf:Description>
                                          </rdf:rest>
                                       </rdf:Description>
                                    </rdf:rest>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-loan-tmp-sts;StudentLoanActiveStatusSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grace</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Repayment</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rapaid In Full</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In School</rdf:first>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


