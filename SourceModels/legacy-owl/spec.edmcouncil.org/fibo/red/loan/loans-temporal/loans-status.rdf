<owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;hasStatus">
      <rdfs:label>has status</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-ph;hasStatus"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-ph;DelinquentLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-sts;LoanArrearsStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;loanStatusArrearsBalance">
      <rdfs:label>loan status arrears balance</rdfs:label>
      <skos:definition>Arrears Balance as at the Status Date.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanArrearsStatus"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;loanStatusArrearsDuration">
      <rdfs:label>loan status arrears duration</rdfs:label>
      <skos:definition>The length of time that the loan has been in arrears.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanArrearsStatus"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;isStatusOf">
      <rdfs:label>is status of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-evt;isStatusOf"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanDefaultProceedingStatus"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-evt;LoanDefaultProceeding"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;allocationToFees">
      <rdfs:label>allocation to fees</rdfs:label>
      <skos:definition>The amount of the payment which is allocated to other fees.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanPayment"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;allocationToInterest">
      <rdfs:label>allocation to interest</rdfs:label>
      <skos:definition>The amount allocated to Interest</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanPayment"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;allocationToPrincipal">
      <rdfs:label>allocation to principal</rdfs:label>
      <skos:definition>The amount of the information allocated to Principal</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanPayment"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;paymentAmount">
      <rdfs:label>payment amount</rdfs:label>
      <skos:definition>The amount paid by the borrower at the information date.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanPayment"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;cumulativePrepaidAmount">
      <rdfs:label>cumulative prepaid amount</rdfs:label>
      <skos:definition>The cumulatie amount that has been pre paid.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanPrepaymentStatus"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;loanLastPrepaymentAmount">
      <rdfs:label>loan last prepayment amount</rdfs:label>
      <skos:definition>Last pre-payment amount. This is the amount pre-paid most recently as at the Information Date.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanPrepaymentStatus"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;loanPrepaymentPenaltiesCumulativePaidAmount">
      <rdfs:label>loan prepayment penalties cumulative paid amount</rdfs:label>
      <skos:definition>Pre-Payment Penalties - Cumulative amount of pre-payment penalties paid to date</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-sts;LoanPrepaymentStatus"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;hasSnapshot">
      <rdfs:label>has snapshot</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-sts;LoanSnapshotOrStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;hasActiveStatus">
      <rdfs:label>has active status</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-sts;hasLifecycleStatus"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-sts;ActiveLoanStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;hasLifecycleStatus">
      <rdfs:label>has lifecycle status</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-bt;Loan"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-sts;LoanLifecycleStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-sts;hasActiveStatus.1">
      <rdfs:label>has active status</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-sts;hasActiveStatus"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-stu;StudentLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-sts;StudentLoanActiveStatus"/>
   </owl:ObjectProperty>


