<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-loan-tmp-tmp "http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loans-temporal/">
    <!ENTITY fibo-fnd-acc-dbt "http://spec.edmcouncil.org/fibo/red/fnd/accounting/debt-terms/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-loan-tmp-sts "http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loans-status/">
    <!ENTITY fibo-loan-loan-bt "http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loans-temporal/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loans-temporal/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/debt-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loans-status/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
   </owl:Ontology>


