<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-loan-tmp-app "http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-applications-temporal/">
    <!ENTITY fibo-fnd-info-info "http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/">
    <!ENTITY fibo-fnd-info-msg "http://spec.edmcouncil.org/fibo/red/fnd/information/messaging/">
    <!ENTITY fibo-loan-loan-par "http://spec.edmcouncil.org/fibo/red/loan/loans/loans-parties/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-loan-loan-loan "http://spec.edmcouncil.org/fibo/red/loan/loans/loans-general/">
    <!ENTITY fibo-loan-loan-bt "http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/">
    <!ENTITY fibo-loan-tmp-bor "http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-borrowers-temporal/">
    <!ENTITY fibo-md-tmp-crr "http://spec.edmcouncil.org/fibo/red/md/temporal-core/credit-ratings/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-applications-temporal/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-applications-temporal/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/messaging/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-general/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans-temporal/loan-borrowers-temporal/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/md/temporal-core/credit-ratings/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-loan-tmp-app;LoanApplicationPhaseSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Approved</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rejected</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In Process</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-loan-tmp-app;LoanApplicationStatusSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Agreement</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disbursement</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Application</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;hasAssessmentInformation">
      <rdfs:label>has assessment information</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;details"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;hasAssessmentInformation.1">
      <rdfs:label>has assessment information</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-app;hasAssessmentInformation"/>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;Cosigner"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;madeBy">
      <rdfs:label>made by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-msg;madeBy"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplication"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;LoanApplicant"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;mayBecome">
      <rdfs:label>may become</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicant"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;grossIncome">
      <rdfs:label>gross income</rdfs:label>
      <skos:definition>Primary borrower underwritten gross income (not rent)</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;appraisedBy">
      <rdfs:label>appraised by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplication"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;LoanAppraiser"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;mayBecome.1">
      <rdfs:label>may become</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplication"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;Loan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;refersToValueOf">
      <rdfs:label>refers to value of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;IndividualCreditRating"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;includes">
      <rdfs:label>includes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-bor;PersonalAssessmentFactsAtApproval"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-app;appraisedBy.1">
      <rdfs:label>appraised by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-app;appraisedBy"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;MortgageLoanApplication"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;MortgageLoanAppraiser"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;applicationDate">
      <rdfs:label>application date</rdfs:label>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;applicationPhase">
      <rdfs:label>application phase</rdfs:label>
      <skos:definition>The phase within the application lifecycle, that this Loan Application is at,</skos:definition>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;LoanApplicationPhaseSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;intendedLoanPurpose">
      <rdfs:label>intended loan purpose</rdfs:label>
      <skos:definition>The purpose to which the loaned funds are to be put.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;assessmentDate">
      <rdfs:label>assessment date</rdfs:label>
      <skos:definition>Date at which Borrower income, credit quality, verification, first time status etc were assessed.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;borrowerCreditRatingAtApproval">
      <rdfs:label>borrower credit rating at approval</rdfs:label>
      <skos:definition>Borrower Credit Quality at the time of the loan approval.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;domicile">
      <rdfs:label>domicile</rdfs:label>
      <skos:definition>this will really be a relationship fact. This is synonymous with Residence. synonym:Residency</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;domiciled[delete]">
      <rdfs:label>domiciled [delete]</rdfs:label>
      <skos:definition>Further notes:</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;incomeVerificationMethod">
      <rdfs:label>income verification method</rdfs:label>
      <skos:definition>REVIEW Is there something like this for corporates? Credit Review Check would apply to corporates as well as persons. This is not so miuch income but their credit record. There could be both.</skos:definition>
      <rdfs:range rdf:resource="&fibo-loan-loan-loan;IncomeVerificationMethodSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;originalLoanDTI">
      <rdfs:label>original loan d t i</rdfs:label>
      <skos:definition>Original Debt to Income (DTI) ratio when the loan was advanced. For combined income.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-app;repossessionHistory">
      <rdfs:label>repossession history</rdfs:label>
      <skos:definition>Whether the potential borrower has a history of repossession.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-loan-tmp-app;LoanApplicant">
      <rdfs:label>loan applicant</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-msg;Applicant"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-app;LoanApplication">
      <rdfs:label>loan application</rdfs:label>
      <skos:definition>A loan which is being applied for and has not yet been disbursed.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-msg;Application"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation">
      <rdfs:label>loan application borrower assessment information</rdfs:label>
      <skos:definition>Information about the borrower in relation to the loan. Specifically this is a bundle of information whose purpose is to identify the borrower and to assess their ability to pay (credit worthiness etc.)</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;Information"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-app;LoanAppraiser">
      <rdfs:label>loan appraiser</rdfs:label>
      <skos:definition>That party which appraises the borrower for the purposes of the loan.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-par;LoanThirdParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-app;MortgageLoanApplication">
      <rdfs:label>mortgage loan application</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-tmp-app;LoanApplication"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-app;MortgageLoanAppraiser">
      <rdfs:label>mortgage loan appraiser</rdfs:label>
      <skos:definition>That party which appraises the borrower for the purposes of the loan.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-tmp-app;LoanAppraiser"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>