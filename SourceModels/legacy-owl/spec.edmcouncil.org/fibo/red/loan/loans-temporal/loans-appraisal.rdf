<owl:Class rdf:about="&fibo-loan-tmp-apr;MortgageLoanBorrowerAssessmentInformation">
      <rdfs:label>mortgage loan borrower assessment information</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-tmp-apr;RepossessionHistory">
      <rdfs:label>repossession history</rdfs:label>
      <skos:definition>Information about an entity which has a history of repossessions, about those repossessions.</skos:definition>
   </owl:Class>


