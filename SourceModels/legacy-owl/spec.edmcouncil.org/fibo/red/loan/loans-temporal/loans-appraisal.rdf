<owl:DatatypeProperty rdf:about="&fibo-loan-tmp-apr;firstTimeBuyer">
      <rdfs:label>first time buyer</rdfs:label>
      <skos:definition>Whether the borrower is a first time buyer.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


