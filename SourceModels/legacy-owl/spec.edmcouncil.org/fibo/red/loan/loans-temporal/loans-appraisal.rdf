<owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;mayBecome">
      <rdfs:label>may become</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-par;Cosigner"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;employerIs">
      <rdfs:label>employer is</rdfs:label>
      <skos:definition>The employer of the person.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;LoanApplicationBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;appraisedBy">
      <rdfs:label>appraised by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-bor;appraisedBy"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-apr;MortgageLoanBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;MortgageLoanAppraiser"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;mayBecome.1">
      <rdfs:label>may become</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-app;mayBecome.1"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-app;MortgageLoanApplication"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;isAbout">
      <rdfs:label>is about</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-bor;isAbout.3"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-apr;MortgageLoanBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-app;MortgageLoanApplication"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;isAbout.1">
      <rdfs:label>is about</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-bor;isAbout.4"/>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;PersonalLoanBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-per;PersonalLoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;obtainedFrom">
      <rdfs:label>obtained from</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-bor;PersonalLoanBorrowerAssessmentInformation"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-ppl;Person"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-apr;appraisedBy.1">
      <rdfs:label>appraised by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-col;SecurityInterestDatedFacts"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-par;LoanServicer"/>
   </owl:ObjectProperty>


