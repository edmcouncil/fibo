   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/jurisdictionGovernedBy">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/isGoverneBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/Jurisdiction"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/LegalSystem"/>
      <owl:inverseOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/appliesIn"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/appliesIn">
      <skos:definition>The jurisdiction which is governed under the system of law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/appliesIn.1"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/LegalSystem"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/legalSystemIsGovernedBy">
      <skos:definition>The legal system is defined in and described by some National Constitution. Scope Note: This is rendered in FIBO Foundations as a restriction on the property isGovernedBy</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/isGoverneBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/LegalSystem"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCore/GovernmentalConstitution"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/isInForceIn">
      <skos:definition>The Jurisdiction in which the Statute Law has effect.</skos:definition>
      <skos:editorialNote>This is not the same thing as a Country, but frequently corresponds to one. The Jurisdiction may also correspond to part of a country (England-and-Wales) or to a State or Province within a Federal country, or it may correspond to the federal country itself if the statute is a federal law.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/isInForceIn"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/StatuteLaw"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/appliesIn.1">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/governs"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/Thing"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/hasReach">
      <skos:definition>The geographical territory, country, federal province or city in which the jurisdiction operates.</skos:definition>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/Jurisdiction"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/PhysicalLocation"/>
   </owl:ObjectProperty>

