      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/CommonLawJurisdiction"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/CommonLawSystem"/>
      <owl:inverseOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/commonLawSystemAppliesIn"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/commonLawSystemAppliesIn">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/appliesIn"/>
