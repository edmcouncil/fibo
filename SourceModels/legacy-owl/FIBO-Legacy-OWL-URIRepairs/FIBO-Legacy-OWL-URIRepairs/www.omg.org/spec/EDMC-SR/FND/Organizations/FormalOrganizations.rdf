<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Collections/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/SetMembership/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/groupHasMember">
      <skos:definition>A member of the Entities Group. This is some Autonomous Agent.</skos:definition>
      <skos:editorialNote>At this level of abstraction this is any kind of self-actualizing entity, forming a group of some sort (not an organization, so not defined by the properties of organizations such as pursuing some common goal over a period of time). .</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/hasMember"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/Group"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/FormalOrganization">
      <rdfs:label>Formal Organization</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/Organization"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/Group">
      <rdfs:label>Group</rdfs:label>
      <skos:definition>A group of autonomous entities.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Collections/Collection"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/InformalOrganization">
      <rdfs:label>Informal Organization</rdfs:label>
      <skos:definition>An organization which is not formally constituted in some way.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>