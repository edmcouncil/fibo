<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/BlueSkyRegulation">
      <rdfs:label>Blue Sky Regulation</rdfs:label>
      <skos:definition>State regulations designed to protect investors against securities fraud by requiring sellers of new issues to register their offerings and provide financial details. This allows investors to base their judgments on trustworthy data.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/SecurityRegistrationRequirement"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/InvestorsDomicileRestriction">
      <rdfs:label>Investors Domicile Restriction</rdfs:label>
      <skos:definition>Restriction defining where holders of a security may be domiciled.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/QualifiedInvestorsRestriction">
      <rdfs:label>Qualified Investors Restriction</rdfs:label>
      <skos:definition>A Restriction in which only qualified investors may hold the Security, optionally for a definede Holding Period (if this is not defined then the period is indefinite).</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/RegulationS">
      <rdfs:label>Regulation S</rdfs:label>
      <skos:definition>Regulation S is the exemption through which corporations can issue unregistered securities to qualified foreign investors and foreign institutions</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/InvestorsDomicileRestriction"/>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/QualifiedInvestorsRestriction"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/Restriction144A">
      <rdfs:label>Restriction 144A</rdfs:label>
      <skos:definition>A Securities &amp; Exchange Commission rule modifying a two-year holding period requirement on privately placed securities to permit qualified institutional buyers to trade these positions among themselves.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/SecurityContractualRestriction">
      <rdfs:label>Security Contractual Restriction</rdfs:label>
      <skos:definition>Contractual terms setting out restrictions on either the holder or the issuer of the security.</skos:definition>
      <skos:editorialNote>These are restrictions defined in the contractual terms of the security itself and form part of the terms of the security, as distinct from restrictions defined in some body of law and being applicable to the security, which are defined separately.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/LawExt/Restrictions/ContractualRestriction"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/SecurityRegistrationRequirement">
      <rdfs:label>Security Registration Requirement</rdfs:label>
      <skos:definition>A Requirement for Registraton of a Security within some Country, Jurisdiction or Federal Province.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/InvestmentRegulatoryRequirement"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/LegalHoldingRestriction">
      <rdfs:label>Legal Holding Restriction</rdfs:label>
      <skos:definition>Restrictions applicable to holding of the security.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/InvestmentRestriction"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/TradingRestriction">
      <rdfs:label>Trading Restriction</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/InvestmentRegulatoryLaw">
      <rdfs:label>Investment Regulatory Law</rdfs:label>
      <skos:definition>A body of law dealing with investment regulation.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/StatuteLaw"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/InvestmentRegulatoryRequirement">
      <rdfs:label>Investment Regulatory Requirement</rdfs:label>
      <skos:definition>A requirement laid down in law for investment regulation.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/LawExt/Restrictions/RegulatoryRequirement"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesRestrictions/InvestmentRestriction">
      <rdfs:label>Investment Restriction</rdfs:label>
      <skos:definition>Restrictions applicable to a financial instrument as provided for in a body of Law.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/LawExt/Restrictions/LegalRestriction"/>
   </owl:Class>

