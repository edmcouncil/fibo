<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/DebtFoundations/DebtIssuanceTerms/Anon-23333">
      <skos:definition>One or more series of Bonds and a Warrant which is issued with the bond.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/Bonds/BondsCommon/Bond"/>
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RightsInstruments/RightsAndWarrants/TraditionalWarrant"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/DebtFoundations/DebtIssuanceTerms/BondIssuanceProgram">
      <rdfs:label>Bond Issuance Program</rdfs:label>
