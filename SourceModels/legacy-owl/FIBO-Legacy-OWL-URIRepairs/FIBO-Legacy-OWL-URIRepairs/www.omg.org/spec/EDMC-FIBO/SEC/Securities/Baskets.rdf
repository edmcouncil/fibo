<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/Anon-20909">
      <skos:definition>Either a publicly issued index or a security.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/BasketOfIndicesConstituent"/>
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/SecuritiesBasketConstituent"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/Basket">
      <rdfs:label>Basket</rdfs:label>
      <skos:definition>A collection of securities or other items which are combined for some purpose or measurement.</skos:definition>
      <skos:editorialNote>A Basket can be of almost anything, since sub types of this includes baskets of securities and baskets of indices. In the sense intended here, a Basket is simply a special kind of collection, with properties such as the percentage or amount of each constituent as part of the basket, which may not apply to Collections generally, i.e. it is a more formal type of collection. To model this greater formality, Basket is modeled as having Constituents which are a kind of Relative Thing, being the constituent in the context of being part of the basket, and with facts such as percentages or amounts.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Collections/Collection"/>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/Abstract"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/BasketConstituent">
      <rdfs:label>Basket Constituent</rdfs:label>
      <skos:definition>Something which makes up one of the components of a basket.</skos:definition>
      <skos:editorialNote>The basket may be a basket of almost anything. It differs from a less formal collection in that there are facts defined about the constituents and the proportions of these.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Collections/Constituent"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/BasketOfIndices">
      <rdfs:label>Basket Of Indices</rdfs:label>
      <skos:definition>A basket made up of indices.</skos:definition>
      <skos:editorialNote>For example certain equities derivatives have an underlying basket which is a basket of more than one index, not a basket of securities.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/Basket"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/BasketOfIndicesConstituent">
      <rdfs:label>Basket Of Indices Constituent</rdfs:label>
      <skos:definition>One constituent of a basket of indices. This is a published index, defined as a part of the basket. Further notes: Not to be confused with the constituent of an index of securities.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/BasketConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/BasketOfSecurities">
      <rdfs:label>Basket Of Securities</rdfs:label>
      <skos:definition>A basket of traded securities.</skos:definition>
      <skos:editorialNote>This does not include baskets of indices, which are modeled separately. also does not include Basket of Baskets. Also need to break down by Country, by Region (e.g. non Japan Asia sub-indices). See MSCI World Index.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/MixedBasket">
      <rdfs:label>Mixed Basket</rdfs:label>
      <skos:definition>A basket that has a mix of constituents which include securities and indices.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/SecuritiesBasketConstituent">
      <rdfs:label>Securities Basket Constituent</rdfs:label>
      <skos:definition>A Security defined as one of the components of a basket of Securities.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>