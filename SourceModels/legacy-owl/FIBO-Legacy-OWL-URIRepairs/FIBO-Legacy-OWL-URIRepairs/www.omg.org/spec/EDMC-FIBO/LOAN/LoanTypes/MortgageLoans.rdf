                                                         <rdf:Description>
                                                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remortgage On Different Terms</rdf:first>
                                                            <rdf:rest>
                                                               <rdf:Description>
                                                                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Combination Mortgage</rdf:first>
                                                                  <rdf:rest>
                                                                     <rdf:Description>
                                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Investment Mortgage</rdf:first>
                                                                        <rdf:rest>
                                                                           <rdf:Description>
