<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/TemporalConcepts/"/>
   </owl:Ontology>

   <!--Datatypes-->

<rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityStatusSelection">
      <skos:definition>The types of status that a security can have on a trading exchange.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">halted</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">traded</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatusSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When Issued</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When Distributed</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Issued</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityStatusSelection">
      <skos:definition>The kinds of trading status that any tradable security may have.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Active</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Inactive</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/hasStatus">
      <skos:definition>The trading status of this listing i.e. this security on this exchange.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/hasTradingStatus"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesListings/SecurityListing"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityTradingStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/hasStatus.1">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/TemporalInfo/hasStatus"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/Security"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/describes">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/TemporalInfo/isOf"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/Worthless"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/hasTradingStatus">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/TemporalInfo/hasStatus"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/Security"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityTradingStatus"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/tense">
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Time/Time/Tense"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/securityTradingStatus">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityStatusSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/delisted">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/status">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatusSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/securityStatus">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityStatusSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/worthless">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/tense.1">
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Time/Time/Tense"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/statusDate">
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/CurrentStatus">
      <rdfs:label>Current Status</rdfs:label>
      <skos:definition>The status at the present time.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/TemporalInfo/Status"/>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/TemporalConcepts/StatusCodeValue"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/ExchangeSecurityTradingStatus">
      <rdfs:label>Exchange Security Trading Status</rdfs:label>
      <skos:definition>The trading status of a security on a given trading exchange.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityLifecycleStatus">
      <rdfs:label>Security Lifecycle Status</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/SecurityTradingStatus">
      <rdfs:label>Security Trading Status</rdfs:label>
      <skos:definition>The status of the security in terms of whether it is trading or not, and any special considerations relating to trading.</skos:definition>
      <skos:editorialNote>Exchange Traded Security trading status is now a separate term, covering trading suspension on an exchange, so that does not form part of this term.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/Worthless">
      <rdfs:label>Worthless</rdfs:label>
      <skos:definition>Announcement by the regulator that the security has become worthless.</skos:definition>
      <skos:editorialNote>e.g. bankruptcy hearings - might result in this being said.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/TemporalCore/SecurityTradingStatuses/PastStatus">
      <rdfs:label>Past Status</rdfs:label>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>