   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/Options/"/>
   </owl:Ontology>

   <!--Datatypes-->

<rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/AddOrSubtractSelection">
      <skos:definition>Simple binary selection of "Add" or "Subtract" to specify this fact about the calculation of the relationship between two numeric parameters.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Add</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subtract</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/CapOrFloorSelection">
      <skos:definition>Whether a given thing is treated as a Cap (something goes no higher than this amount) or as a Floor (something goes no lower than this).</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cap</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Floor</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/hasStrikeTerms">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/Options/hasStrikeTerms"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/OTCInterestRateOption"/>
