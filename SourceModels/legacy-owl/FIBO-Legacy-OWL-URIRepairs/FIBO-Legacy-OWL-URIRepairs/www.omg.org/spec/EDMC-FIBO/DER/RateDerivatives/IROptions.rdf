   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/includesSpecificationTerm">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/Options/includesSpecificationTerm"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/InterestRateOptionStrikeTermsSet"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/Options/PercentageStrikePriceSpecification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/hasUnderlying">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/RateDerivatives/hasUnderlying.1"/>
