      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/RateDerivatives/InterestRateObservable"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/hasResetSchedule">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/OptionStripContract"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/OptionStripResetSchedule"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/strikeEffect">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/CapOrFloorSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/strikeEffect.1">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/CapOrFloorSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/maturityDate">
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/RateDerivatives/IROptions/settlementDateOffset">
