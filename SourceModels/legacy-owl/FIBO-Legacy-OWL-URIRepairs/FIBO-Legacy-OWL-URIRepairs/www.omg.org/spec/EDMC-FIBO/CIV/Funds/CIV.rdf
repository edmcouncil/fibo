   </owl:Ontology>

   <!--Datatypes-->

<rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/AccountsReportingFrequencySelection">
      <skos:definition>Frequency with which financial reports will be presented.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Annual</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Semi Annual</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/FundAdditionalRedemptionRestrictionsSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">None</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Refer To Fund Order Desk</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/FundInvestorTypeSelection">
      <skos:definition>The type of party that is eligible to invest in a fund.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Retail</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Institutional</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Retail And Institutional</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gross Distribution Eligible</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description>
                                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Qualified Investor</rdf:first>
                                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                       </rdf:Description>
                                    </rdf:rest>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/FundStructureSelection">
      <skos:definition>One of the different ways in which a Fund may be structured.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Single Fund</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Multi Class</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/FundUnitDistributionPolicySelection">
      <skos:definition>The normal distribution policy for funds.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cash Distribution</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Income Accumulation</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/OpenOrClosedEnded">
      <skos:definition>Whether a Unit in a fund or trust is open ended or closed ended.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Open</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Closed</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/PerformanceDeterminationMethodSelection">
      <skos:definition>The definition of the performance method (e.g. Time weighted and money weighted, annualized, gross of fee).</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Time Weighted Rate Of Return</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Money Weighted Rate Of Return</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Annualized</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gross of Fee</rdf:first>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/CIV/Funds/CIV/RiskLevelSelection">
      <skos:definition>Selection list identifiying levels of risk in natural language terms.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Low</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Medium</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">High</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

