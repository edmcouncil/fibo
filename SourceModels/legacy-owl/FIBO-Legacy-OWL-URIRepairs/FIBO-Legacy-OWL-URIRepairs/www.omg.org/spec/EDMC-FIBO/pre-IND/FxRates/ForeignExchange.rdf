<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsParameters/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/fXSpotVolatilityIsVolatilityOf">
      <skos:definition>The currency spot rate to which the volatility measure applies and of which it is a measure.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsParameters/isVolatilityOf"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/FXSpotVolatility"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/CurrencySpotRate"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/hasSettlementDate">
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Time/Time/Date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/hasNumericValue">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsValues/ExchangeRateTimedValue"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/hasBaseCurrency">
      <rdfs:range rdf:resource="http://www.omg.org/spec/FIBO/Foundation/Accounting/CurrencyAmount/Currency"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/hasDealtCurrency">
      <rdfs:range rdf:resource="http://www.omg.org/spec/FIBO/Foundation/Accounting/CurrencyAmount/Currency"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/CurrencyForwardRate">
      <rdfs:label>Currency Forward Rate</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/ExchangeRate"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/CurrencySpotRate">
      <rdfs:label>Currency Spot Rate</rdfs:label>
      <skos:definition>The exchange rate at a point in time, with settlement at that same point in time.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/ExchangeRate">
      <rdfs:label>Exchange Rate</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsParameters/MarketBaseRate"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/FxRates/ForeignExchange/FXSpotVolatility">
      <rdfs:label>FX Spot Volatility</rdfs:label>
      <skos:definition>A measure of exchange rate fluctuation. Mathematically the volatility is the annualized standard deviation of the daily changes in the exchange rate. Definition Adapted From: OptionsEducation.org adapted (replaced "stock" with "Exchange Rate" and re-phrased)</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsParameters/Volatility"/>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>