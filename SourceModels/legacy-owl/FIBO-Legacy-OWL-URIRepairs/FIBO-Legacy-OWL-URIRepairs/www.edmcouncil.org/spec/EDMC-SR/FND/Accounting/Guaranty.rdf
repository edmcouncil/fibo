<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/">
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/DebtTerms/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Documentation/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/"/>
   </owl:Ontology>

   <!--Datatypes-->

<rdf:Description rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/PriorityLevel">
      <skos:definition>Level of Priority for example for a Credit Enhancement (Guaranty) priority.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Primary</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secondary</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/guarantorIdentity">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyInRoleHasIdentity"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/Guarantor"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LEIEntities/ContractuallyCapableEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/hasGuarantor">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/hasPartyInRole"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/Guaranty"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/Guarantor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/takesFormOf">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/identity"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/Guaranty"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/IndependentThing"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/amount">
      <rdfs:range rdf:resource="http://www.omg.org/spec/FIBO/Foundation/Accounting/CurrencyAmount/MoneyAmount"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/effectivePeriod">
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Time/Time/DatePeriod"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/priorityLevel">
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/PriorityLevel"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/expirationDate">
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Time/Time/Date"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/Guarantor">
      <rdfs:label>Guarantor</rdfs:label>
      <skos:definition>A Party which guarantees to underwrite some cash flow obligation on behalf of some other party.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/ContractThirdParty"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Guaranty/Guaranty">
      <rdfs:label>Guaranty</rdfs:label>
      <skos:definition>Arrangement whereby cash flows on the debt security, eg, interest payment, are guaranteed by a firm other than the issuer.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/RelativeThing"/>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>