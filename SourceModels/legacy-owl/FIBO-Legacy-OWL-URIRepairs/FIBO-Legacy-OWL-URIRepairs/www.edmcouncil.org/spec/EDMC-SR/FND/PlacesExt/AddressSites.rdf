<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/headquarteredAt">
      <skos:definition>The address of the company's head office.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/hasPrimaryAddress"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Corporations/Corporations/IncorporatedCompany"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/PrimaryBusinessAddressRelation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/siteIdentity">
      <skos:definition>That which is a type of address, is a Postal Address.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/identity"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/Site"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Addresses/PostalAddress"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/hasOperatingAddress">
      <skos:definition>The address at which the organization carries out its business and receives correspondence relating to its business activities.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/tradingAtSite"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/OperatingBusinessAddressRelation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/hasPrimaryAddress">
      <skos:definition>The primary address at which this organization does business and received correspondence.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/tradingAtSite"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/PrimaryBusinessAddressRelation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/tradingAtSite">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/BusinessSite"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/tradingAt">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/hasOperatingAddress"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Corporations/Corporations/IncorporatedCompany"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/OperatingBusinessAddressRelation"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/isPrimaryAddress">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/isPrimaryAddress.1">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/effectiveDate">
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/terminationDate">
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/isPrimaryAddress.2">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/yesOrNo"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/OperatingBusinessAddressRelation">
      <rdfs:label>Operating Business Address Relation</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/BusinessSite"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/PrimaryBusinessAddressRelation">
      <rdfs:label>Primary Business Address Relation</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/Site">
      <rdfs:label>Site</rdfs:label>
      <skos:definition>A site where an entity or organization has some presence.</skos:definition>
      <skos:editorialNote>Term originally identified in Proof of Concept activities, as a relative entity (for temporal reasons) and was called Address Relation. On further inspection, this is synonymous with the W3C Organizzation Ontology concept of "Site" so it has been renamed to that. It is still a relative concept, being a place associated with some entity and having some address. Scope Note: Note that for Registered Address, this is a simple legal requirement linking and entity to some address where papers may be served, and therefore does not relate to this "Site" concept at all.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/RelativeThing"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/RegisteredAddressSite">
      <rdfs:label>Registered Address Site</rdfs:label>
      <skos:definition>An official Registered Address of a company, as registered with the appropriate Companies Registry for the jurisdiction in which the company is registered, and at which legal proceedings may be served upon the entity.</skos:definition>
      <skos:editorialNote>It is usually a requirement to display this on stationery and other correspondence. This is defined as the address at which legal papers may be served upon the entity. In many jurisdictions this therefore cannot be a Post Box number or Poste Restante, however some jurisdictions (e.g. South Africa) do allow this.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/Site"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/AddressSites/BusinessSite">
      <rdfs:label>Business Site</rdfs:label>
      <skos:definition>An address from which a business entity carries out its business, such as a shop or offices. This is the address which is generally shown on advertising by that company.</skos:definition>
      <skos:editorialNote>Term agreed in reviews but written definition not re-reviewed. Defined as a relative thing called "Business Address Relation" in Proof of Concept works. Renamed to Business Site in line with W3C Site semantics .</skos:editorialNote>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>