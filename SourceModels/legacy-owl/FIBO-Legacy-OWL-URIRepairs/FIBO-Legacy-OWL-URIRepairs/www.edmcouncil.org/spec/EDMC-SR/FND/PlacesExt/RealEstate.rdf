<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/Geophysical/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Physical/Physical/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/buildingHasPart">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/hasPart"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/Building"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/BuildingSubUnit"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/hasLocation">
      <skos:definition>The physical location of the built item or property.</skos:definition>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/BuiltProperty"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/Geophysical/GeophysicalLocation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/identifiedBy">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/details"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/RealEstate"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Addresses/PostalAddress"/>
      <owl:inverseOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/isOf"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/realEstateCoextensiveWith">
      <skos:definition>Real Estate corresponds to either some built item or some parcel of land.</skos:definition>
      <skos:editorialNote>This is represented as a logical union of Built Item and Land Parcel. Scope Note: Future changes in Real Estate ontology alignment may well change or elaborate on this.</skos:editorialNote>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/RealEstate"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/Anon-5306"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/hasPart">
      <skos:definition>A subdivision of the Real Estate, which is itself defined as Real Estate.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/hasPart"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/RealEstate"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/RealEstate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/realEstateOwnedBy">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Ownership/isOwnedBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/RealEstate"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/PropertyOwner"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/Anon-5306">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/Geophysical/LandParcel"/>
               <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/BuiltProperty"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/Apartment">
      <rdfs:label>Apartment</rdfs:label>
      <skos:definition>A building subbdivision intended for habitation.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/BuildingSubUnit"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/Building">
      <rdfs:label>Building</rdfs:label>
      <skos:definition>A discrete construction on its own foundations or pilings.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/BuiltProperty"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/BuildingSubUnit">
      <rdfs:label>Building Sub Unit</rdfs:label>
      <skos:definition>A discrete subdivision of some Building.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/BuiltProperty">
      <rdfs:label>Built Property</rdfs:label>
      <skos:definition>Something which is constructed via construction methods; bricks and mortar.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Physical/Physical/DiscretePhysicalThing"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/Floor">
      <rdfs:label>Floor</rdfs:label>
      <skos:definition>A building subdivision whose extent is the horizontal extent of the building at that height.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/PropertyOwner">
      <rdfs:label>Property Owner</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Ownership/Owner"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/RealEstate/Suite">
      <rdfs:label>Suite</rdfs:label>
      <skos:definition>A subdivision of a building for which the purposes are not clearly defined or may be variable, generally consisting of a number of contiguous rooms on one or more floors.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>