<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/addressIsContainedIn">
      <skos:definition>The location which the Postal Address corresponds to in the world.</skos:definition>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Addresses/PostalAddress"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/PhysicalLocation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/isLocatedIn">
      <skos:definition>The postcode area which contains the premises identified by the Postal Address, Explanatory Note: This is an actual physical area, identified by the postal code (at whatever level of generality the corresponding postal code is defined), and is referred to in risk management, for example to identify common mortgage risk factors such as environmental risks or employment statistics.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/addressIsContainedIn"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Addresses/PostalAddress"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Addresses/PostcodeArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/isOf">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Addresses/PostalAddress"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/RealEstate"/>
   </owl:ObjectProperty>

