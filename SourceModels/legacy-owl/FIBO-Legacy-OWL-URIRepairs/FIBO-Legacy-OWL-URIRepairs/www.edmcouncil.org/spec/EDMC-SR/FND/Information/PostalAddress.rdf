<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/Anon-8518">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
