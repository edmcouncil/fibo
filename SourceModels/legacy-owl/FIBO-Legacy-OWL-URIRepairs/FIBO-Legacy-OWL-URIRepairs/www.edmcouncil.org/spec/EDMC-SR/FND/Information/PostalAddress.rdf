   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/PostBox">
      <rdfs:label>Post Box</rdfs:label>
      <skos:definition>A PO Box.</skos:definition>
      <skos:editorialNote>This is physical and essentially geographical in that it has a location.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/PhysicalLocation"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/PostalAddressElement">
