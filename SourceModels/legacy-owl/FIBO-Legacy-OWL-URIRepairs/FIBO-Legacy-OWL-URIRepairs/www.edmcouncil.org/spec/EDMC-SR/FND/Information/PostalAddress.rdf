      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Addresses/PostalAddress"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/PostalAddress/PostalAddressElement"/>
