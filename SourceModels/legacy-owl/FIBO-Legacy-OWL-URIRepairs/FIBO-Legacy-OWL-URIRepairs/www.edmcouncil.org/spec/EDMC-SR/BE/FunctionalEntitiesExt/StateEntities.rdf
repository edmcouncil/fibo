<owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/legalName">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
