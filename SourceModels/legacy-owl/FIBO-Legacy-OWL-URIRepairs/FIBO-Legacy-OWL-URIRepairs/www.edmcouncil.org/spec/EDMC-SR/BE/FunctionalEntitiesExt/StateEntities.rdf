<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/governmentMortgageAgencyTakesFormOf">
      <skos:definition>The form which the Government Mortgage Agency takes. This is a Legal Entity and not a Natural Person, that is, it is an Artificial Person.</skos:definition>
      <skos:editorialNote>Review session Note: A government body is not necessarily a Legal Entity, but the sort of Government Body that is a government Mortgage Agency, necessarily is a Legal entity, as it issues securities and loan products and is a legal party to those contracts. This is subject to confirming that it is not in fact taking on those liabilities direclty on behalf of the Sovereign, in which case it is an agent working purely on the Sovereig</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/FunctionalEntities/FunctionalEntities/functionalBusinessEntityTakesForm"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/GovernmentMortgageAgency"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/dischargedBy">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/Government"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/IndicatorPublishers/GovernmentBody"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/municipalityCorrespondsTo">
      <skos:definition>The city or other geopolitical area that the municipality corresponds to.</skos:definition>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/SovereignEntities/Municipality"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/Geopolitical/MunicipalArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/hasSovereigntyOver">
      <skos:definition>The geopolitican entity (country, federal province or city) which the state embodies.</skos:definition>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/SovereignEntities/Sovereign"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Countries/Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/governmentGovernsOnBehalfOfSovereign">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/NationalGovernment"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/SovereignEntities/Sovereign"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/municipalGovernmentGoverns">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/MunicipalGovernment"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/Geopolitical/MunicipalArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/sovereignRepresentedByNationalGovernment">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/SovereignEntities/Sovereign"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/NationalGovernment"/>
      <owl:inverseOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/governmentGovernsOnBehalfOfSovereign"/>
   </owl:ObjectProperty>

