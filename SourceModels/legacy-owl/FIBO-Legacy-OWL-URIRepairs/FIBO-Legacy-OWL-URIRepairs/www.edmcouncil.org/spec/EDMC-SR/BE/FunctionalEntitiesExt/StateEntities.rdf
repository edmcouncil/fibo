   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/GovernmentMortgageAgency">
      <rdfs:label>Government Mortgage Agency</rdfs:label>
      <skos:definition>An agency set up by a government for the purpose of issuing mortgages.</skos:definition>
      <skos:editorialNote>These exist in the USA and may also exist now or in future in other countries. There are three such agencies in thr USA: FNMA, GNMA and FHLMC (Fannie Mae, Ginnie Mae and Freddie Mac respectively).</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/IndicatorPublishers/GovernmentBody"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/SovereignWealthFund">
      <rdfs:label>Sovereign Wealth Fund</rdfs:label>
      <skos:definition>Government investment fund funded by foreign currency reserves. Review: Also include things like International Monetary Fund. Are these a Legal Entity? See also Funds model. Move the question about Sovereign Wealth Funds to the Funds model. Looking at definitions (e.g. Wikipedia) it's a state owned investment fund. The legal entity owner is a government (or in fact a State, which is governed by a Government).</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/FunctionalEntities/FunctionalEntities/FunctionalBusinessEntity"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/StateGovernment">
      <rdfs:label>State Government</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/Government"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/NationalGovernment">
      <rdfs:label>National Government</rdfs:label>
      <skos:definition>A body which governs a state or country.</skos:definition>
      <skos:editorialNote>This is defined in relation to the act of governing that country. The physical activities of governing may be discharged through legal entities which are government bodies.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/MunicipalGovernment">
      <rdfs:label>Municipal Government</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/Government">
      <rdfs:label>Government</rdfs:label>
      <skos:definition>Something which is responsible for governing a territory, state or country.</skos:definition>
      <skos:editorialNote>This is defined as a relative thing, i.e. in relation to the act of governing. That is, a Government (state actor) acts on behalf of the state. It may have independent business entities which are state bodies in order to discharge this.</skos:editorialNote>
   </owl:Class>

   <!--Individuals-->

<owl:NamedIndividual rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/StateEntities/www.investorwords.com">
