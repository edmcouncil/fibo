<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/">
      <owl:imports rdf:resource="http://www.w3.org/2002/07/owl#"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/identity">
      <skos:definition>The class of Independent Thing which is identified as this class of Relative Thing, i.e. the thing which fulfils the role or is the party etc. defined for the Independent Thing.</skos:definition>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/RelativeThing"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/inContext">
      <skos:definition>The context in which the Relative Thing has the meaningful definition given to it.</skos:definition>
      <skos:editorialNote>For simplicity, in some instances the definition of a relative thing is given in the context of something other than a "Mediating Thing" or context in the purest sense. For example a relative thing may be defined in relation to some identifiable independent thing, as for example with the native yield calculation method for a security.</skos:editorialNote>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/RelativeThing"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/IndependentThing">
      <rdfs:label>Independent Thing</rdfs:label>
      <skos:definition>A thing in its own right.</skos:definition>
      <skos:editorialNote>SBVR: Fundamental Concept - plays roles in different perspectives In SBVR roles are also a kind of concept.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/MediatingThing">
      <rdfs:label>Mediating Thing</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/RelativeThing">
      <rdfs:label>Relative Thing</rdfs:label>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>