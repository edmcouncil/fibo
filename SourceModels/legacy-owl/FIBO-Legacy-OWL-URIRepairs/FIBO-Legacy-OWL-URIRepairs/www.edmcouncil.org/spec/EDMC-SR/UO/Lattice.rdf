<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/Abstract">
      <rdfs:label>Abstract</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/Concrete">
      <rdfs:label>Concrete</rdfs:label>
      <skos:definition>A thing which takes some physical form. This includes things which may be physically represented only in a computer system but where they are equivalent to some physical thing (such as a dematerialised security or a verbal contract). Sowa: "Physical"</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/ContinuantThing">
      <rdfs:label>Continuant Thing</rdfs:label>
      <skos:definition>A Thing which continues to exist. This is defined in opposition to the Occurrence, which is a Thing which occurs only at a specific point in time. [Reference: Sowa 2000 "Continuant"]</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/OccurentThing">
      <rdfs:label>Occurent Thing</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/TemporalThing">
      <rdfs:label>Temporal Thing</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/SpecifiedVariableQuantity"/>
   </owl:Class>

