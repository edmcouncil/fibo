			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>trade identifier</rdfs:label>
		<skos:definition>an identifier for a trade</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-fbc-pas-fpas;TradeLifecycle">
