		<fibo-fnd-rel-rel:comprises rdf:resource="https://www.gleif.org/lei/549300PN6MKI0CLP4T28"/>
		<fibo-fnd-rel-rel:isIncludedIn rdf:resource="&fibo-fbc-fct-usfsind;GlobalMarketsEntityIdentifierRegistry"/>
