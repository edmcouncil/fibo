##
# banner Subproperties domains  should not cross

SELECT DISTINCT ?error
WHERE {
  ?p1 rdfs:domain ?D1 . 
  ?p1 rdfs:subPropertyOf ?p2 .
  ?p2 rdfs:domain ?D2 .
  ?D2 rdfs:subClassOf+ ?D1 .
BIND (concat ("PRODERROR: Crossed domains. ", afn:localname(?p1), " < ", afn:localname (?p2), " but the domain ", afn:localname (?D1), " is a superclass of ", afn:localname (?D2)) AS ?error)
}
