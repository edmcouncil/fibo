	</owl:Ontology>
	
	<owl:NamedIndividual rdf:about="&fibo-tst-fnd-ctl;AssociativeControlRelation">
		<rdf:type rdf:resource="&fibo-fnd-oac-ctl;Control"/>
		<fibo-fnd-oac-ctl:hasPartyInControl rdf:resource="&fibo-tst-fnd-ctl;CaliforniaBankPartyInRole"/>
		<fibo-fnd-oac-ctl:isInControlOfThing rdf:resource="&fibo-tst-fnd-ctl;SecuritiesIncPartyInRole"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&fibo-tst-fnd-ctl;BigBankControlsGoodBankPIR">
