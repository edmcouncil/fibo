<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-be-corpx-dp "http://spec.edmcouncil.org/fibo/red/be/corporations-ext/deemed-parents/">
    <!ENTITY fibo-be-oac-cctl "http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/corporate-control/">
    <!ENTITY fibo-be-corp-corp "http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/be/corporations-ext/deemed-parents/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/corporations-ext/deemed-parents/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/corporate-control/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-be-corpx-dp;deemedToHaveParent">
      <rdfs:label>deemed to have parent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;hasControllingInterestPartyVotingShareholdingCompany"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-be-corpx-dp;DeemedParent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-corpx-dp;deemedToHaveSubsidiary">
      <rdfs:label>deemed to have subsidiary</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;hasSubsidiary"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-be-corpx-dp;DeemedSubsidiary"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-be-corpx-dp;definingOwnershipThreshold">
      <rdfs:label>defining ownership threshold</rdfs:label>
      <skos:definition>The percentage for which it is deemed that the Party is to be defined as a Parent for the purposes in question.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-be-corpx-dp;definingOwnershipThreshold.1">
      <rdfs:label>defining ownership threshold</rdfs:label>
      <skos:definition>The percentage of ownership by the other party, for which it is deemed that the Party is to be defined as a Subsidiary for the purposes in question.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-be-corpx-dp;DeemedParent">
      <rdfs:label>deemed parent</rdfs:label>
      <skos:definition>A party which is deemed to be a "Parent" for some purpose, as defined by some specific ownership threshold other than that defined for Parent i.e. other than 50% + 1 voting share.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;VotingShareholdingCompany"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-corpx-dp;DeemedSubsidiary">
      <rdfs:label>deemed subsidiary</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;ControlledCompany"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>