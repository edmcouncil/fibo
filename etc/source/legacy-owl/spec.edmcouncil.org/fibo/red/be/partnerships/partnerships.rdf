    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
    <!ENTITY fibo-be-le-cb "http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/">
    <!ENTITY fibo-be-oac-opty "http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/ownership-parties/">
    <!ENTITY fibo-fnd-oac-own "http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/">
    <!ENTITY fibo-fnd-law-lcap "http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/">
    <!ENTITY fibo-be-le-lp "http://spec.edmcouncil.org/fibo/red/be/legal-entities/legal-persons/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
    <!ENTITY fibo-be-oac-cpty "http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/control-parties/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/be/partnerships/partnerships/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/partnerships/partnerships/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/accounting-equity/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/ownership-parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/legal-persons/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/control-parties/"/>
   </owl:Ontology>


