<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-be-et-lef "http://spec.edmcouncil.org/fibo/red/be/entities-types-ext/legal-foundations/">
    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
    <!ENTITY fibo-be-oac-exec "http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/executives/">
    <!ENTITY fibo-be-le-cb "http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/be/entities-types-ext/legal-foundations/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/entities-types-ext/legal-foundations/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/executives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-be-et-lef;constitutedBy">
      <rdfs:label>constituted by</rdfs:label>
      <skos:definition>The legal document which forms the basis of the Foundation. This is the legal instrument by which it has its existence.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-cb;isConstitutedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-et-lef;Foundation"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;ArticlesOfIncorporation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-et-lef;governedByFoundationAgreement">
      <rdfs:label>governed by foundation agreement</rdfs:label>
      <skos:definition>The agreement which governs the Foundation. This is the document which governs the activities of the Foundation and the contractual relations among the principals.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;formallyConstitutedOrganizationIsGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-et-lef;Foundation"/>
      <rdfs:range rdf:resource="&fibo-be-et-lef;FoundationAgreement"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-be-et-lef;Anstalt">
      <rdfs:label>anstalt</rdfs:label>
      <skos:definition>Form of Foundation registered in Leichtenstein.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-et-lef;Foundation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-et-lef;Foundation">
      <rdfs:label>foundation</rdfs:label>
      <skos:definition>A formal foundation. Further Notes Formal definition sought.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-et-lef;FoundationAgreement">
      <rdfs:label>foundation agreement</rdfs:label>
      <skos:definition>The formal agreement by which the Foundation is governed</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-le-fbo;OrganizationCoveringAgreement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-et-lef;LegallyIncorporatedFoundation">
      <rdfs:label>legally incorporated foundation</rdfs:label>
      <skos:definition>A foundation with independent legal personhood.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-et-lef;PrivateInterestFoundation">
      <rdfs:label>private interest foundation</rdfs:label>
      <skos:definition>A form of foundation registered in Panama.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-et-lef;UnincorporatedFoundation">
      <rdfs:label>unincorporated foundation</rdfs:label>
   </owl:Class>


