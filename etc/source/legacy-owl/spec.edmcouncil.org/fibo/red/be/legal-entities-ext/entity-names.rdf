<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-be-lex-nam "http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/entity-names/">
    <!ENTITY fibo-fnd-info-info "http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-aap-agt "http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/agents/">
    <!ENTITY fibo-be-corp-corp "http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/">
    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
    <!ENTITY fibo-be-le-cb "http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/">
    <!ENTITY fibo-fnd-aap-ppl "http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/people/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/entity-names/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/entity-names/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/agents/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/people/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-be-lex-nam;humanBeingHasLegalName">
      <rdfs:label>human being has legal name</rdfs:label>
      <skos:definition>The legal name by which the person is known.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-nam;hasName"/>
      <rdfs:domain rdf:resource="&fibo-fnd-aap-ppl;Person"/>
      <rdfs:range rdf:resource="&fibo-be-lex-nam;PersonalName"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-nam;hasLegalName">
      <rdfs:label>has legal name</rdfs:label>
      <skos:definition>The registered legal name of the Incorporated Company.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-nam;hasLegalName.1"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-be-lex-nam;LegalName"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-nam;hasAlias">
      <rdfs:label>has alias</rdfs:label>
      <skos:definition>Any other name by which the Legal Entity is known but which is not its usual given name or its registered formal name.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-nam;hasName"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-nam;hasLegalName.1">
      <rdfs:label>has legal name</rdfs:label>
      <skos:definition>The name by which the legal entity is referred to in legal communications.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-nam;hasName"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-be-lex-nam;LegalName"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-nam;hasName">
      <rdfs:label>has name</rdfs:label>
      <skos:definition>The name by which the autonomous thing is known.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;details"/>
      <rdfs:domain rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-nam;hasTradingName">
      <rdfs:label>has trading name</rdfs:label>
      <skos:definition>The name by which the business is known for trading purposes.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-nam;hasName"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-nam;wasFormerlyKnownAs">
      <rdfs:label>was formerly known as</rdfs:label>
      <skos:definition>A name by which the entity has been known in the past but is not known by at the present time.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-nam;hasName"/>
      <rdfs:domain rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-nam;tradingAs">
      <rdfs:label>trading as</rdfs:label>
      <skos:definition>Alternative name/Business name by which the entity is also known</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-nam;hasName"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Name"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-be-lex-nam;fullLegalName">
      <rdfs:label>full legal name</rdfs:label>
      <skos:definition>The text which represents the full legal name of an entity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-be-lex-nam;additionalName">
      <rdfs:label>additional name</rdfs:label>
      <skos:definition>Additional or "middle" names given to the person by his or her parents or derived through tradition based on other family names.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-be-lex-nam;familyName">
      <rdfs:label>family name</rdfs:label>
      <skos:definition>The name the person derives from his or her family</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-be-lex-nam;givenName">
      <rdfs:label>given name</rdfs:label>
      <skos:definition>The name given to the person by his or her parents and unique within their immediate family.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-be-lex-nam;LegalName">
      <rdfs:label>legal name</rdfs:label>
      <skos:definition>The full legal name of an entity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;Name"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-lex-nam;PersonalName">
      <rdfs:label>personal name</rdfs:label>
      <skos:definition>The name of an individual person.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>