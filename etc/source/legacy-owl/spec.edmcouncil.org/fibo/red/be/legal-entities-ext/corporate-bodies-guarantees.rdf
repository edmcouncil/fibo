  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-be-lex-cbg;CompanyOwner">
      <rdfs:label>company owner</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-be-oac-opty;GuaranteeProvidingMember"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
