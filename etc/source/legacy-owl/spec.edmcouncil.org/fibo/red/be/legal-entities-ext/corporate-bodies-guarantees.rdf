<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-be-lex-cbg "http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/corporate-bodies-guarantees/">
    <!ENTITY fibo-be-oac-opty "http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/ownership-parties/">
    <!ENTITY fibo-be-lex-fbox "http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/formal-organizations-extended/">
    <!ENTITY fibo-be-le-cb "http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/corporate-bodies-guarantees/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/corporate-bodies-guarantees/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/ownership-and-control/ownership-parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities-ext/formal-organizations-extended/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-be-lex-cbg;bodyIncByGuaranteeIsConstitutedBy">
      <rdfs:label>body inc by guarantee is constituted by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-cb;isConstitutedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyIncorporatedWithGuarantee"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;IncorporationGuarantee"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-cbg;providedBy">
      <rdfs:label>provided by</rdfs:label>
      <skos:definition>The party which is the provider of the Guarantee.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-le-cb;guaranteeIsProvidedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;IncorporationGuarantee"/>
      <rdfs:range rdf:resource="&fibo-be-oac-opty;GuaranteeProvidingMember"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-cbg;hasGuarantor">
      <rdfs:label>has guarantor</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-fbox;hasFormalOrganizationMember"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyIncorporatedWithGuarantee"/>
      <rdfs:range rdf:resource="&fibo-be-oac-opty;GuaranteeProvidingMember"/>
      <owl:inverseOf rdf:resource="&fibo-be-oac-opty;guaranteeProvidingMemberGuarantees"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-lex-cbg;hasGuarantorCompanyOwner">
      <rdfs:label>has guarantor company owner</rdfs:label>
      <skos:definition>A Company Incorporated by Guarantee has some guarantor which is the Company Owner</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-lex-cbg;hasGuarantor"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;CompanyIncorporatedByGuarantee"/>
      <rdfs:range rdf:resource="&fibo-be-lex-cbg;CompanyOwner"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
