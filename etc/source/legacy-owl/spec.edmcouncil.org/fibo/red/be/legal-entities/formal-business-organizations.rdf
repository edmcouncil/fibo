      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/organizations/formal-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/organizations/organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
