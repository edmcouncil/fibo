   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/"/>
