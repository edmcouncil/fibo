<owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;hasObligation">
      <rdfs:label>has obligation</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-be-fct-rpt;ReportingObligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;hasObligation.1">
      <rdfs:label>has obligation</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-fct-rpt;hasObligation"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
      <rdfs:range rdf:resource="&fibo-be-fct-rpt;StatutoryReportingObligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;hasCorporateFilingObligation">
      <rdfs:label>has corporate filing obligation</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-fct-rpt;hasObligation.1"/>
      <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
      <rdfs:range rdf:resource="&fibo-be-fct-rpt;CorporateFilingObligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;identity"/>
      <rdfs:domain rdf:resource="&fibo-be-fct-rpt;ReportingEntity"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;isAbout">
      <rdfs:label>is about</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-fct-rpt;ReportingObligation"/>
      <rdfs:range rdf:resource="&fibo-be-fct-rpt;.UnionClass.4"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;isTo">
      <rdfs:label>is to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-fct-rpt;ReportingObligation"/>
      <rdfs:range rdf:resource="&fibo-be-fct-rpt;RegulatoryEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;incorporatedUnder">
      <rdfs:label>incorporated under</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-fct-rpt;StatutoryAuthority"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-fct-rpt;identity"/>
      <rdfs:domain rdf:resource="&fibo-be-fct-rpt;StatutoryReportingEntity"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-rpt;isTo.1">
      <rdfs:label>is to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-fct-rpt;isTo"/>
      <rdfs:domain rdf:resource="&fibo-be-fct-rpt;StatutoryReportingObligation"/>
      <rdfs:range rdf:resource="&fibo-be-fct-rpt;StatutoryRegulator"/>
   </owl:ObjectProperty>


