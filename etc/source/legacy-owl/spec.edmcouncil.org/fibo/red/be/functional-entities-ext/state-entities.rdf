         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/be/functional-entities-ext/state-entities/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/be/functional-entities-ext/state-entities/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/functional-entities/functional-entities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicator-publishers/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/geopolitical/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/entities-types-ext/sovereign-entities/"/>
   </owl:Ontology>


