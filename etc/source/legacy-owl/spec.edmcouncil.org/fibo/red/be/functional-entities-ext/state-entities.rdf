  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-be-fct-se;governmentMortgageAgencyTakesFormOf">
      <rdfs:label>government mortgage agency takes form of</rdfs:label>
      <skos:definition>The form which the Government Mortgage Agency takes. This is a Legal Entity and not a Natural Person, that is, it is an Artificial Person.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-fct-fct;functionalBusinessEntityTakesForm"/>
      <rdfs:domain rdf:resource="&fibo-be-fct-se;GovernmentMortgageAgency"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-se;dischargedBy">
      <rdfs:label>discharged by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-fct-se;Government"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-pub;GovernmentBody"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-se;municipalityCorrespondsTo">
      <rdfs:label>municipality corresponds to</rdfs:label>
      <skos:definition>The city or other geopolitical area that the municipality corresponds to.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-et-sov;Municipality"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-gpl;MunicipalArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-se;hasSovereigntyOver">
      <rdfs:label>has sovereignty over</rdfs:label>
      <skos:definition>The geopolitican entity (country, federal province or city) which the state embodies.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-et-sov;Sovereign"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-se;governmentGovernsOnBehalfOfSovereign">
      <rdfs:label>government governs on behalf of sovereign</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-fct-se;NationalGovernment"/>
      <rdfs:range rdf:resource="&fibo-be-et-sov;Sovereign"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-se;municipalGovernmentGoverns">
      <rdfs:label>municipal government governs</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-fct-se;MunicipalGovernment"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-gpl;MunicipalArea"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-fct-se;sovereignRepresentedByNationalGovernment">
      <rdfs:label>sovereign represented by national government</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-et-sov;Sovereign"/>
      <rdfs:range rdf:resource="&fibo-be-fct-se;NationalGovernment"/>
      <owl:inverseOf rdf:resource="&fibo-be-fct-se;governmentGovernsOnBehalfOfSovereign"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-be-fct-se;legalName">
      <rdfs:label>legal name</rdfs:label>
      <skos:definition>The legal name of the Government as a Legal Entity, e.g. "Government of the United States of America".</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-be-fct-se;GovernmentMortgageAgency">
      <rdfs:label>government mortgage agency</rdfs:label>
      <skos:definition>An agency set up by a government for the purpose of issuing mortgages.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ei-pub;GovernmentBody"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-fct-se;SovereignWealthFund">
      <rdfs:label>sovereign wealth fund</rdfs:label>
      <skos:definition>Government investment fund funded by foreign currency reserves. Review: Also include things like International Monetary Fund. Are these a Legal Entity? See also Funds model. Move the question about Sovereign Wealth Funds to the Funds model. Looking at definitions (e.g. Wikipedia) it's a state owned investment fund. The legal entity owner is a government (or in fact a State, which is governed by a Government).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-fct-se;StateGovernment">
      <rdfs:label>state government</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-be-fct-se;Government"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-fct-se;NationalGovernment">
      <rdfs:label>national government</rdfs:label>
      <skos:definition>A body which governs a state or country.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-fct-se;MunicipalGovernment">
      <rdfs:label>municipal government</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-be-fct-se;Government">
      <rdfs:label>government</rdfs:label>
      <skos:definition>Something which is responsible for governing a territory, state or country.</skos:definition>
   </owl:Class>


