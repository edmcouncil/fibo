<owl:NamedIndividual rdf:about="&fibo-be-fct-se;www.investorwords.com">
      <rdfs:label>www.investorwords.com</rdfs:label>
      <rdf:type rdf:resource="&fibo-fnd-pub-pub;WebPage"/>
   </owl:NamedIndividual>

