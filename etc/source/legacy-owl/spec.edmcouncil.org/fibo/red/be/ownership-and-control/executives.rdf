<owl:ObjectProperty rdf:about="&fibo-be-oac-exec;boardCapacityIsConferredBy">
      <rdfs:label>board capacity is conferred by</rdfs:label>
      <skos:definition>The instrument which confers the Board Capacity. This is either the Company By-laws or the Board Agreement.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;BoardCapacity"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;.UnionClass.5"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;boardMemberIsAPartyTo">
      <rdfs:label>board member is a party to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;isAPartyTo"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;BoardMember"/>
      <rdfs:range rdf:resource="&fibo-be-corp-corp;BoardAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;boardOfDirectorsAppoints">
      <rdfs:label>board of directors appoints</rdfs:label>
      <skos:definition>The board of the company has the power to appoint the individuals who are to be the public officers of the company.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;appoints"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;BoardOfDirectors"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CorporateOfficer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;companyBoardHasVestedInIt">
      <rdfs:label>company board has vested in it</rdfs:label>
      <skos:definition>The board of a company has some legal capacity vested in it.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;BoardOfDirectors"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;BoardCapacity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;boardOfDirectorsHasIdentity">
      <rdfs:label>board of directors has identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;BoardOfDirectors"/>
      <rdfs:range rdf:resource="&fibo-fnd-org-fm;Group"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;boardOfDirectorsHasMember">
      <rdfs:label>board of directors has member</rdfs:label>
      <skos:definition>The board has a number of members who collectively exercise the powers of that board.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-coll;hasConstituent"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;BoardOfDirectors"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;BoardMember"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;constrains">
      <rdfs:label>constrains</rdfs:label>
      <skos:definition>The Instrument of Incorporation (being effectively the constitution of the entity) governs and constrains the application of the bylaws of the company.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;constrains"/>
      <rdfs:domain rdf:resource="&fibo-be-le-cb;InstrumentOfIncorporation"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CompanyBylaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;companyOfficerLegalCapacityConferredBy">
      <rdfs:label>company officer legal capacity conferred by</rdfs:label>
      <skos:definition>The capacity of the public officer is conferred by one or another ot the company legal form documentation (articles of association or equivalent) or some company by-law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficerCapacity"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CompanyLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;mandatedBy">
      <rdfs:label>mandated by</rdfs:label>
      <skos:definition>The Statutory Obligation is mandated by some piece of Company Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;statutoryResponsibilityIsMandatedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CompanyStatutoryObligation"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CompanyLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;definedBy">
      <rdfs:label>defined by</rdfs:label>
      <skos:definition>The legal capacity of the Public Officer is set out in some piee of Company Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isGoverneBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficerCapacity"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CompanyLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;publicOfficerIdentity">
      <rdfs:label>public officer identity</rdfs:label>
      <skos:definition>That which is the Public Officer. This must be a natural person and also must be an adult.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;legallyDelegatedAuthorityHasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficer"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;NaturalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;corporateOfficerHasResponsibility">
      <rdfs:label>corporate officer has responsibility</rdfs:label>
      <skos:definition>The Company Public Officer is party to some statutory obligations as mandated by the applicable body of Company Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;responsiblePartyHasResponsibility"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficer"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CompanyStatutoryObligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;corporateOfficerHasResponsibilityDuty">
      <rdfs:label>corporate officer has responsibility duty</rdfs:label>
      <skos:definition>The Company Public Officer is party to some formally defined Public Officer Duty as set out on the relevant piece of Company Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;responsiblePartyHasResponsibility"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficer"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CorporateOfficerDuty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;deFactoControllingInterestPartyAppoints">
      <rdfs:label>de facto controlling interest party appoints</rdfs:label>
      <skos:definition>The Control Owner has some mechanism whereby they are able to vote and approve the board, other than through the mechanisms defined elsewhere.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;appoints"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;DeFactoControllingInterestParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;BoardMember"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;directorHasIdentity">
      <rdfs:label>director has identity</rdfs:label>
      <skos:definition>A board member (director) is typically a Natural Person, but may be any Legal Person.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;legallyDelegatedAuthorityHasIdentity"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;BoardMember"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;executiveIdentity">
      <rdfs:label>executive identity</rdfs:label>
      <skos:definition>The Executive is some adult human being.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;legallyDelegatedAuthorityHasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;Executive"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;NaturalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;legallyDelegatedAuthorityHasIdentity">
      <rdfs:label>legally delegated authority has identity</rdfs:label>
      <skos:definition>That which is in the role of Legally Controlling Party. This must be a Legal Person.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;LegallyDelegatedAuthority"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;responsiblePartyIdentity">
      <rdfs:label>responsible party identity</rdfs:label>
      <skos:definition>The Responsible Party must be some Natural Person.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;ResponsibleParty"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;NaturalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;responsiblePartyHasResponsibility">
      <rdfs:label>responsible party has responsibility</rdfs:label>
      <skos:definition>The Responsible Party is deemed to be such because it is party to some formally defined Duty.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;hasReponsibility"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;ResponsibleParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;Duty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;votingShareholderElects">
      <rdfs:label>voting shareholder elects</rdfs:label>
      <skos:definition>The shareholders approve and vote for the members of the Board.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;elects"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cctl;VotingShareholder"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;BoardMember"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;delegatesControlTo">
      <rdfs:label>delegates control to</rdfs:label>
      <skos:definition>The Organization which has management control of this Organization.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;designates"/>
      <rdfs:domain rdf:resource="&fibo-be-le-lp;LegalPerson"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;LegallyDelegatedAuthority"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;elects">
      <rdfs:label>elects</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;designates"/>
      <rdfs:domain rdf:resource="&fibo-be-le-lp;LegalPerson"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;hasCapacityCorporateOfficerCapacity">
      <rdfs:label>has capacity corporate officer capacity</rdfs:label>
      <skos:definition>The public officer of a company has some formal capacity vested in them.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficer"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CorporateOfficerCapacity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;hasCapacitySignatoryCapacity">
      <rdfs:label>has capacity signatory capacity</rdfs:label>
      <skos:definition>The party which is a Company Public Officer is also defined as being a signatory in respect of the entity, that is the officer is able to sign agreements on the part of that entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficer"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;SignatoryCapacity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;hasDelegatedControlOf">
      <rdfs:label>has delegated control of</rdfs:label>
      <skos:definition>An organization of which this organization has control via some management structure.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;LegallyDelegatedAuthority"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;hasDelegatedControlOfItself">
      <rdfs:label>has delegated control of itself</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;LegallyDelegatedAuthority"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
      <owl:inverseOf rdf:resource="&fibo-be-oac-exec;delegatesControlTo"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;hasReponsibility">
      <rdfs:label>has reponsibility</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;Duty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;hasResponsibleParty">
      <rdfs:label>has responsible party</rdfs:label>
      <skos:definition>Some party having some defined responsibility in respect of the Formal Organization</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;ResponsibleParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;hasVestedInIt">
      <rdfs:label>has vested in it</rdfs:label>
      <skos:definition>A Delegated Legal Authority is one which has the legal capacity of Delegated Legal Authority vested in it.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;hasCapacity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;LegallyDelegatedAuthority"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;DelegatedLegalAuthority"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-exec;mandatedByCompanyLaw">
      <rdfs:label>mandated by company law</rdfs:label>
      <skos:definition>The duties of a Company Public Officer are mandated by some applicable piece of Company Law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-exec;mandatedBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-exec;CorporateOfficerDuty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-exec;CompanyLaw"/>
   </owl:ObjectProperty>


