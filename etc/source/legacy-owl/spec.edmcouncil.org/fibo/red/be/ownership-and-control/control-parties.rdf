    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-be-oac-cpty;percentageDeFactoControl">
      <rdfs:label>percentage de facto control</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-be-oac-cpty;percentageDeFactoControl.1">
      <rdfs:label>percentage de facto control</rdfs:label>
      <skos:definition>The percentage of control which the party is deemed or calculated as being able to exercise over the controlled entity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>


