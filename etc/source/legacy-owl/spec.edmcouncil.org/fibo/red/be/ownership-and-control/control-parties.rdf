    -->


<owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;constitutionalContractualControlIsConferredBy">
      <rdfs:label>constitutional contractual control is conferred by</rdfs:label>
      <skos:definition>The control is conferred on the party by means of the formal covering agreement by which the organization is constituted, that is the agreement between the principals of the organization.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;contractualControlIsConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;ConstitutionalContractualControl"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;OrganizationCoveringAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;contractualControlIsConferredBy">
      <rdfs:label>contractual control is conferred by</rdfs:label>
      <skos:definition>The control is conferred on the party by means of some contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;ContractualControl"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;controlledPartyIsControlledToSomeDegreeBy">
      <rdfs:label>controlled party is controlled to some degree by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;isControlledToSomeDegreeBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;ControlledParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;courtAppoinredControlIsConferredBy">
      <rdfs:label>court appoinred control is conferred by</rdfs:label>
      <skos:definition>The control which is vested in the controlling party is conferred upon that party through some court of law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;CourtAppointedControl"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-cor;CourtOfLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;jointControllingPartyIdentity">
      <rdfs:label>joint controlling party identity</rdfs:label>
      <skos:definition>The identity of the Party is some group of entities which have formed some alliance or agreement in terms of which they jointly exercise control over the controlled organization.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;JointControllingParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;ControllingAlliance"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;limitedControllingPartyHasNatureOControl">
      <rdfs:label>limited controlling party has nature o control</rdfs:label>
      <skos:definition>The type of control for which the party has the stated percentage.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;exercisesSomeDegreeOf"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;LimitedControllingParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;LimitedDeFactoControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;exercisesContractualControl">
      <rdfs:label>exercises contractual control</rdfs:label>
      <skos:definition>Control is exercised by the party, which is conferred by some contractual agreement.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeInstrumentOfDeJureControl"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;ContractualControllingParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;ContractualControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;exercisesCourtAppointedControl">
      <rdfs:label>exercises court appointed control</rdfs:label>
      <skos:definition>Legal control exercised by the party, which is appointed by some court.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeInstrumentOfDeJureControl"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;Receiver"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;CourtAppointedControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;exercisesInvestmentBasedDeFactoControl">
      <rdfs:label>exercises investment based de facto control</rdfs:label>
      <skos:definition>The party exercises some degree of de facto control based on some investment which they hold in the entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeMeansOfDeFactoControl"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;InvestmentBasedDeFactoControllingInterestParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;InvestmentBasedDeFactoControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;hasSignificantControllingInterstParty">
      <rdfs:label>has significant controlling interst party</rdfs:label>
      <skos:definition>Some entity which owns a significant part of this company but not 50% or more.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;hasControllingInterestParty"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oacx-cptyx;hasDeJureControllingInterestParty"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;SignificantControllingInterestParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;hasTotalOwner">
      <rdfs:label>has total owner</rdfs:label>
      <skos:definition>The Formal Organization is totally owned and therefore wholly controlled by some party having 100% ownership in it.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-opty;hasConstitutionalOwner"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;TotalOwner"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;hasControllingInterestParty">
      <rdfs:label>has controlling interest party</rdfs:label>
      <skos:definition>The Formal Organization is controlled in some way by some controlling party, that is some entity which exercises such control.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;ControllingInterestParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;hasDegreeOfControl">
      <rdfs:label>has degree of control</rdfs:label>
      <skos:definition>has degree of control, (partial, majority, or complete) either alone or with other parties, with respect to some activity</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;EntityControllingParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;DegreeOfControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;hasMajorityControllingParty">
      <rdfs:label>has majority controlling party</rdfs:label>
      <skos:definition>The parent of the company, if there is one.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;isControlledToSomeDegreeBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;MajorityControllingParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;exercisesSomeDegreeOf">
      <rdfs:label>exercises some degree of</rdfs:label>
      <skos:definition>has nature or kind of control, de facto or limited de facto control</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;EntityControllingParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-ctl;DeFactoControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;exercisesSomeInvestmentBasedDeFactoControl">
      <rdfs:label>exercises some investment based de facto control</rdfs:label>
      <skos:definition>The party exercises some de facto control based on some investment in equity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeMeansOfDeFactoControl"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;ControllingLeverageParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;InvestmentBasedDeFactoControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;holdsSomeInstrumentOfDeJureControl">
      <rdfs:label>holds some instrument of de jure control</rdfs:label>
      <skos:definition>Legal or contractually based control exercised by the party.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeMeansOf"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;DeJureControllingInterestParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;DeJureControllingInterest"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;holdsSomeMeansOfDeFactoControl">
      <rdfs:label>holds some means of de facto control</rdfs:label>
      <skos:definition>The party exercises some control which is not based in law or contract but in some other means.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeMeansOf"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;DeFactoControllingInterestParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-ctl;DeFactoControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;exercisesSomeInfluenceBasedDeFactoControl">
      <rdfs:label>exercises some influence based de facto control</rdfs:label>
      <skos:definition>Exercises some influence and control other than by means of investment.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeMeansOfDeFactoControl"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;InfluenceBasedDeFactoControllingParty"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;InfluenceBasedDeFactoControl"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;holdsTheOptionToExercise">
      <rdfs:label>holds the option to exercise</rdfs:label>
      <skos:definition>Control which may optionally be exercised by the party.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;holdsSomeMeansOf"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;PartyHoldingSomeOptionForControl"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;DeJureControllingInterest"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;holdsSomeMeansOf">
      <rdfs:label>holds some means of</rdfs:label>
      <skos:definition>Some means of control held by the party.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;ControllingInterestParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-ctl;Control"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;basedOnInvestmentEquity">
      <rdfs:label>based on investment equity</rdfs:label>
      <skos:definition>Investment Based De Facto Control is based on the holding of some Investment Equity by some party.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-be-oac-cpty;InvestmentBasedDeFactoControl"/>
      <rdfs:range rdf:resource="&fibo-be-oac-opty;InvestmentEquity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-be-oac-cpty;isControlledToSomeDegreeBy">
      <rdfs:label>is controlled to some degree by</rdfs:label>
      <skos:definition>Some party which has some degree of control over the organization.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isControlledBy"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-be-oac-cpty;EntityControllingParty"/>
   </owl:ObjectProperty>


