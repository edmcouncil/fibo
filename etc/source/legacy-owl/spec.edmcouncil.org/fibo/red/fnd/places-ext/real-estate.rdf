  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-plcx-rea;.UnionClass.6">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-plcx-rea;BuiltProperty"/>
               <owl:Class rdf:about="&fibo-fnd-plcx-gph;LandParcel"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-rea;Apartment">
      <rdfs:label>apartment</rdfs:label>
      <skos:definition>A building subbdivision intended for habitation.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-plcx-rea;BuildingSubUnit"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-rea;Building">
      <rdfs:label>building</rdfs:label>
      <skos:definition>A discrete construction on its own foundations or pilings.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-plcx-rea;BuiltProperty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-rea;BuildingSubUnit">
      <rdfs:label>building sub unit</rdfs:label>
      <skos:definition>A discrete subdivision of some Building.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-rea;BuiltProperty">
      <rdfs:label>built property</rdfs:label>
      <skos:definition>Something which is constructed via construction methods; bricks and mortar.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-phy-phy;DiscretePhysicalThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-rea;Floor">
      <rdfs:label>floor</rdfs:label>
      <skos:definition>A building subdivision whose extent is the horizontal extent of the building at that height.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-rea;PropertyOwner">
      <rdfs:label>property owner</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-own;Owner"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-plcx-rea;Suite">
      <rdfs:label>suite</rdfs:label>
      <skos:definition>A subdivision of a building for which the purposes are not clearly defined or may be variable, generally consisting of a number of contiguous rooms on one or more floors.</skos:definition>
   </owl:Class>


