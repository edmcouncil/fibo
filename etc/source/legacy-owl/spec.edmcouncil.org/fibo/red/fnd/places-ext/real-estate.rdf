<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-plcx-rea "http://spec.edmcouncil.org/fibo/red/fnd/places-ext/real-estate/">
    <!ENTITY fibo-fnd-phy-phy "http://spec.edmcouncil.org/fibo/red/fnd/physical/physical/">
    <!ENTITY fibo-fnd-oac-own "http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/">
    <!ENTITY fibo-fnd-plcx-gph "http://spec.edmcouncil.org/fibo/red/fnd/places-ext/geophysical/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-plc-adr "http://spec.edmcouncil.org/fibo/red/fnd/places/addresses/">
    <!ENTITY fibo-fnd-info-info "http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/">
    <!ENTITY fibo-fnd-plc-loc "http://spec.edmcouncil.org/fibo/red/fnd/places/locations/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/real-estate/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/real-estate/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/physical/physical/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places-ext/geophysical/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/addresses/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/locations/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-plcx-rea;buildingHasPart">
      <rdfs:label>building has part</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-rea;Building"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-rea;BuildingSubUnit"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-rea;hasLocation">
      <rdfs:label>has location</rdfs:label>
      <skos:definition>The physical location of the built item or property.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-plcx-rea;BuiltProperty"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-gph;GeophysicalLocation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-rea;identifiedBy">
      <rdfs:label>identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;details"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-pos;isOf"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-rea;realEstateCoextensiveWith">
      <rdfs:label>real estate coextensive with</rdfs:label>
      <skos:definition>Real Estate corresponds to either some built item or some parcel of land.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-rea;.UnionClass.6"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-rea;hasPart">
      <rdfs:label>has part</rdfs:label>
      <skos:definition>A subdivision of the Real Estate, which is itself defined as Real Estate.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasPart"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-plcx-rea;realEstateOwnedBy">
      <rdfs:label>real estate owned by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;isOwnedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
      <rdfs:range rdf:resource="&fibo-fnd-plcx-rea;PropertyOwner"/>
   </owl:ObjectProperty>


