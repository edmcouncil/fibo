         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/organizations/formal-organizations/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/organizations/formal-organizations/">
