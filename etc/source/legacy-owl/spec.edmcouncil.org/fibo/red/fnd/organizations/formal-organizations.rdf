<owl:ObjectProperty rdf:about="&fibo-fnd-org-fm;groupHasMember">
      <rdfs:label>group has member</rdfs:label>
      <skos:definition>A member of the Entities Group. This is some Autonomous Agent.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
      <rdfs:domain rdf:resource="&fibo-fnd-org-fm;Group"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
   </owl:ObjectProperty>


