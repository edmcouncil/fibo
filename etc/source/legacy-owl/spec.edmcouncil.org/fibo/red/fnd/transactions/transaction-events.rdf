<owl:Class rdf:about="&fibo-fnd-txn-evt;ContractualUndertaking">
      <rdfs:label>contractual undertaking</rdfs:label>
      <skos:definition>A commitment, defined in a Contract, in which one Party to that Contract undertakes to do something or to deliver something or make some payment, to the other Party.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-evt;UndertakingEvent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-evt;DischargingEvent">
      <rdfs:label>discharging event</rdfs:label>
      <skos:definition>The event in which the</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;TransactionBusinessEvent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-evt;Revaluation">
      <rdfs:label>revaluation</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-evt;TransactionUndertaking">
      <rdfs:label>transaction undertaking</rdfs:label>
      <skos:definition>A contractually defined and established commitment to deliver some goods, perform some service or make some payment in cash or in kind.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-evt;Undertaking">
      <rdfs:label>undertaking</rdfs:label>
      <skos:definition>Some undertaking to act.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-occ-evt;Event"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-evt;UndertakingEvent">
      <rdfs:label>undertaking event</rdfs:label>
      <skos:definition>Something which occurs at a point in time, at which a party makes some commitment to some other party.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-evt;Undertaking"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-evt;UndertakingParty">
      <rdfs:label>undertaking party</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-rl-ena;EnablingAgent"/>
   </owl:Class>


