    <!ENTITY fibo-fnd-txn-rea "http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/">
    <!ENTITY fibo-rl-asp "http://spec.edmcouncil.org/fibo/red/fnd/relative/aspects/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-txn-evt "http://spec.edmcouncil.org/fibo/red/fnd/transactions/transaction-events/">
    <!ENTITY fibo-fnd-law-lcap "http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/">
    <!ENTITY fibo-fnd-txn-ext "http://spec.edmcouncil.org/fibo/red/fnd/transactions/transactions-extended/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-claims-events/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-claims-events/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/social-constructs/construct-aspects/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contractual-constructs/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/aspects/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/transaction-events/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-capacities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/transactions-extended/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-txn-clm;asSeenBy">
      <rdfs:label>as seen by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-rl-asp;Aspect"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-clm;AspectViewingParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-clm;asReportedBy">
      <rdfs:label>as reported by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-clm;asSeenBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-soc-asp;CommitmentAspect"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;AccountingKeeper"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-clm;definedOnThePartOf">
      <rdfs:label>defined on the part of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;REAClaim"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-rea;AccountingKeeper"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-clm;isImbalanceIn">
      <rdfs:label>is imbalance in</rdfs:label>
      <skos:definition>The imbalance in Obligations or Rights (depending on the viewpoint from which it is described) between one party and another.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;REAClaim"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-clm;hasEnd">
      <rdfs:label>has end</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-evt;DischargingEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-clm;hasStart">
      <rdfs:label>has start</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-evt;TransactionUndertaking"/>
   </owl:ObjectProperty>


