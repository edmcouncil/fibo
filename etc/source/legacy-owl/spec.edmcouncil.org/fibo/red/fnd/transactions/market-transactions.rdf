<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-txn-mkt "http://spec.edmcouncil.org/fibo/red/fnd/transactions/market-transactions/">
    <!ENTITY fibo-fnd-txn-rea "http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/">
    <!ENTITY fibo-fnd-acc-acc "http://spec.edmcouncil.org/fibo/red/fnd/accounting/accounting-main/">
    <!ENTITY fibo-fnd-txn-sup "http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/">
    <!ENTITY fibo-rl-ecr "http://spec.edmcouncil.org/fibo/red/fnd/relative/economic-resources/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/transactions/market-transactions/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/transactions/market-transactions/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/accounting-main/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/supply-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/economic-resources/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-txn-mkt;customerTransactsWithVendor">
      <rdfs:label>customer transacts with vendor</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-mkt;transactsWith"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-sup;Customer"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-sup;Vendor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-mkt;consideration">
      <rdfs:label>consideration</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;subject"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-mkt;MarketTransaction"/>
      <rdfs:range rdf:resource="&fibo-rl-ecr;EconomicResource"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-mkt;paymentTerms">
      <rdfs:label>payment terms</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;transactedUnder"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-mkt;MarketTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-mkt;MarketTransactionPaymentTerms"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-mkt;counterparty">
      <rdfs:label>counterparty</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;transactionParty"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-mkt;MarketTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-mkt;TransactionCounterparty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-mkt;principal">
      <rdfs:label>principal</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;transactionParty"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-mkt;MarketTransaction"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-mkt;TransactionPrincipal"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-mkt;governs">
      <rdfs:label>governs</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-mkt;MarketTransactionPaymentTerms"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-acc;PaymentProcess"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-txn-mkt;transactsWith">
      <rdfs:label>transacts with</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;transactsWith"/>
      <rdfs:domain rdf:resource="&fibo-fnd-txn-mkt;TransactionPrincipal"/>
      <rdfs:range rdf:resource="&fibo-fnd-txn-mkt;TransactionCounterparty"/>
   </owl:ObjectProperty>


