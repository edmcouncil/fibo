    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-txn-mkt;TransactionCounterparty">
      <rdfs:label>transaction counterparty</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;TransactionParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-mkt;MarketTransactionInvoicingTerms">
      <rdfs:label>market transaction invoicing terms</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-mkt;MarketTransaction">
      <rdfs:label>market transaction</rdfs:label>
      <skos:definition>Any transaction which defines a supply of some negotiable item in return for some Consideration. The Market Transaction has a Principal and a Counterparty, i.e. it is not symmetrical.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;EconomicTransaction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-mkt;MarketTransactionPaymentTerms">
      <rdfs:label>market transaction payment terms</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-txn-mkt;TransactionPrincipal">
      <rdfs:label>transaction principal</rdfs:label>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>