      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;hasLendingParty">
      <rdfs:label>has lending party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Lender"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;appliesTo">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-dbt;appliesTo.3"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;DebtInterest"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Debt"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;viewpoint">
      <rdfs:label>viewpoint</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;inContext"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Debt"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;MoniesOwing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;identity.1">
      <rdfs:label>identity</rdfs:label>
      <skos:definition>The lender of a loan is some form of Legal Entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Lender"/>
      <rdfs:range rdf:resource="&fibo-be-le-lp;LegalPerson"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;appliesTo.1">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Accrual"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Interest"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;governs">
      <rdfs:label>governs</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;DebtInterestPaymentTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Interest"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;appliesTo.2">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Amortization"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;partyTo">
      <rdfs:label>party to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Lender"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;DebtContractTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;owedTo">
      <rdfs:label>owed to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Debt"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Creditor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;perspective">
      <rdfs:label>perspective</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-asp;seenFromPerspectiveOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Debt"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Debtor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;isPartyTo">
      <rdfs:label>is party to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Debtor"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;MoniesOwing"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-acc-dbt;hasParty.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;partyTo.1">
      <rdfs:label>party to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Borrower"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;DebtContractTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;isSumOf">
      <rdfs:label>is sum of</rdfs:label>
      <skos:definition>The monetary amount of the credit or loan.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;MoniesOwing"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-tmp;DatedMonetaryAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;hasParty">
      <rdfs:label>has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;MoniesOwing"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Creditor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;terms">
      <rdfs:label>terms</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;contractHasTerms"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;DebtContractTermsSet"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;hasParty.1">
      <rdfs:label>has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;MoniesOwing"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Debtor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;debtAmount">
      <rdfs:label>debt amount</rdfs:label>
      <skos:definition>The amount of the finance extended as debt from one party to the other. Further notes: This is not the balance of the debt at a point in time, but the amount drawn down at a specific point in time, after which (for interest bearing debts) interest becomes payable. Semantic Modeling note This term is over-ridden for specific kinds of debt (securities, loans) and is therefore not needed in any data model. It is included here to define part of the meaning of the Debt Finance term. It is a Relationship Fact and strictly speaking should be regarded as being specialized and over-ridden by the terms in the Loan and Security classes, however it is modeled here as a "Referenceable Archetype" meaning that it appears in diagrams as a textual entry not a relationship line, and so it is not possible to formally show this specialization.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;collateralizedBy">
      <rdfs:label>collateralized by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Collateral"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;hasBorrowingParty">
      <rdfs:label>has borrowing party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Borrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;appliesTo.3">
      <rdfs:label>applies to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-acc-dbt;Interest"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-acc-dbt;accrualBasis">
      <rdfs:label>accrual basis</rdfs:label>
      <skos:definition>The basis on which interest accrues on the Debt.</skos:definition>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;DayCountBasis"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-acc-dbt;interestRate">
      <rdfs:label>interest rate</rdfs:label>
      <skos:definition>The rate of annual interest that is to be paid by the debtor to the lender on the debt.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-acc-dbt;principalRepaymentDate">
      <rdfs:label>principal repayment date</rdfs:label>
      <skos:definition>The date when the principal of the debt or loan is to have been repaid in full. Depending on the terms of the debt, or the terms of a debt security in the case of secufritized debt, this may be the date of a single payment of the debt principal or o fthe completion of scheduled partial redemption payments.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-acc-dbt;Borrower">
      <rdfs:label>borrower</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;DebtContractTermsSet">
      <rdfs:label>debt contract terms set</rdfs:label>
      <skos:definition>Terms setting out the formal rights and obligations of borrower and lender under a contract in which funds are lent from the one party to the other.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;DebtInterest">
      <rdfs:label>debt interest</rdfs:label>
      <skos:definition>An amount payable by a debtor to a creditor in consideration of a current debt amount at a given point in time.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-dbt;Interest"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;DebtInterestPaymentTermsSet">
      <rdfs:label>debt interest payment terms set</rdfs:label>
      <skos:definition>Terms for payment of Interest on any Debt.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-dbt;DebtContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;DebtPrincipalRepaymentTermsSet">
      <rdfs:label>debt principal repayment terms set</rdfs:label>
      <skos:definition>Terms for repayment on the principal of any debt.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Lender">
      <rdfs:label>lender</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Accrual">
      <rdfs:label>accrual</rdfs:label>
      <skos:definition>The build-up of interest over a period of time.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;OccurentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Amortization">
      <rdfs:label>amortization</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Collateral">
      <rdfs:label>collateral</rdfs:label>
      <skos:definition>Assets pledged in the provision of debt or loan products or securities.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Credit">
      <rdfs:label>credit</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-acc;OwingSide"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Creditor">
      <rdfs:label>creditor</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Debt">
      <rdfs:label>debt</rdfs:label>
      <skos:definition>Money which is owed by one party (the Debtor) to another party (the Creditor) at some point in time, as seen from the perspective of the Debtor.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-acc;Liability"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Debtor">
      <rdfs:label>debtor</rdfs:label>
      <skos:definition>The party which owes the debt.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;MoniesOwing">
      <rdfs:label>monies owing</rdfs:label>
      <skos:definition>A sum of money owed by one party to another party.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;MediatingThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;DebtFinance">
      <rdfs:label>debt finance</rdfs:label>
      <skos:definition>Funds loaned or extended from one party to another for some purpose. Further notes: This is a loan or a debt security principal amount. This term is distinguished from "Debt" or "Credit" in that the latter are balances at a point in time; however it is also referred to as debt for example in debt securities, since a loaned or securitized amount constitutes a debt when it is advanced (and ceases to constitute a debt when it is paid off). As such, this usually has interest payable and may have collateral. In Islamic products there is no interest. This may be a loan (realized as a draw-down on a credit facillity) or it may be a debt set up as part of the issuance of a debt security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-rl-prc;RelationshipContext"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-acc-dbt;Interest">
      <rdfs:label>interest</rdfs:label>
      <skos:definition>An amount payable by a debtor to a creditor in consideration of the credit extended to the debtor.</skos:definition>
   </owl:Class>


