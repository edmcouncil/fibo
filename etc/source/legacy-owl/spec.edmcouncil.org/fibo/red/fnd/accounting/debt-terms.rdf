<owl:ObjectProperty rdf:about="&fibo-fnd-acc-dbt;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
