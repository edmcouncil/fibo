   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/people/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/agents/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/"/>
