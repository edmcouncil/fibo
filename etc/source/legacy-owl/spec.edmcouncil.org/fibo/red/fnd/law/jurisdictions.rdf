    <!ENTITY fibo-fnd-law-jur "http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-plc-loc "http://spec.edmcouncil.org/fibo/red/fnd/places/locations/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-law-cor "http://spec.edmcouncil.org/fibo/red/fnd/law/legal-core/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/locations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law/legal-core/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;civilLawJurisdictionIsGovernedBy">
      <rdfs:label>civil law jurisdiction is governed by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;jurisdictionGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;CivilLawJurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;CivilLawSystem"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-law-jur;civilLawSystemAppliesIn"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;civilLawSystemAppliesIn">
      <rdfs:label>civil law system applies in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;appliesIn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;CivilLawSystem"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;CivilLawJurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;commonLawJurisdictionIsGovernedBy">
      <rdfs:label>common law jurisdiction is governed by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;jurisdictionGovernedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;CommonLawJurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;CommonLawSystem"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-law-jur;commonLawSystemAppliesIn"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;commonLawSystemAppliesIn">
      <rdfs:label>common law system applies in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;appliesIn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;CommonLawSystem"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;CommonLawJurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;jurisdictionGovernedBy">
      <rdfs:label>jurisdiction governed by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isGoverneBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;LegalSystem"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-law-jur;appliesIn"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;appliesIn">
      <rdfs:label>applies in</rdfs:label>
      <skos:definition>The jurisdiction which is governed under the system of law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;appliesIn.1"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;LegalSystem"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;legalSystemIsGovernedBy">
      <rdfs:label>legal system is governed by</rdfs:label>
      <skos:definition>The legal system is defined in and described by some National Constitution. Scope Note: This is rendered in FIBO Foundations as a restriction on the property isGovernedBy</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isGoverneBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;LegalSystem"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-cor;GovernmentalConstitution"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;isInForceIn">
      <rdfs:label>is in force in</rdfs:label>
      <skos:definition>The Jurisdiction in which the Statute Law has effect.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isInForceIn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;appliesIn.1">
      <rdfs:label>applies in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;governs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-rel-rel;Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-jur;hasReach">
      <rdfs:label>has reach</rdfs:label>
      <skos:definition>The geographical territory, country, federal province or city in which the jurisdiction operates.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-loc;PhysicalLocation"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-law-jur;commonName">
      <rdfs:label>common name</rdfs:label>
      <skos:definition>The name by which the Jurisdiction is commonly referred.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-law-jur;fullName">
      <rdfs:label>full name</rdfs:label>
      <skos:definition>The name of the Jurisdiction, e.g. State of New York. Usually the same as the name of a Country or State (but not for example in the case of England and Wales)</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-law-jur;CivilLawJurisdiction">
      <rdfs:label>civil law jurisdiction</rdfs:label>
      <skos:definition>A civil law jurisdiction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-jur;CivilLawSystem">
      <rdfs:label>civil law system</rdfs:label>
      <skos:definition>A system of law based on Roman Law.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-law-jur;LegalSystem"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-jur;CommonLawJurisdiction">
      <rdfs:label>common law jurisdiction</rdfs:label>
      <skos:definition>A jurisdiction based on common law.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-jur;CommonLawSystem">
      <rdfs:label>common law system</rdfs:label>
      <skos:definition>A system of law whose sources are the decisions made by judges.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-jur;Jurisdiction">
      <rdfs:label>jurisdiction</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-jur;LegalSystem">
      <rdfs:label>legal system</rdfs:label>
      <skos:definition>Some system of Law.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Abstract"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;MediatingThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-law-jur;StatuteLaw">
      <rdfs:label>statute law</rdfs:label>
      <skos:definition>Any law or body of law, passed by some competent authority within some jurisdiction and recognized and enforced in that jurisdiction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-law-cor;Law"/>
   </owl:Class>


