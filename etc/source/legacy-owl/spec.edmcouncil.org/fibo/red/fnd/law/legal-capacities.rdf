<owl:ObjectProperty rdf:about="&fibo-fnd-law-lcap;legalConstructIsConferredOn">
      <rdfs:label>legal construct is conferred on</rdfs:label>
      <skos:definition>That to upon which the conferred thing is conferred.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isConferredOn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-lcap;statutoryResponsibilityIsMandatedBy">
      <rdfs:label>statutory responsibility is mandated by</rdfs:label>
      <skos:definition>The obligation is mandated under some body of statute law.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isMandatedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-lcap;StatutoryResponsibility"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-lcap;hasCapacity">
      <rdfs:label>has capacity</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-law-lcap;isCapacityOf"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-lcap;isCapacityOf">
      <rdfs:label>is capacity of</rdfs:label>
      <skos:definition>That upon which the legal capacity has been conferred.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;legalConstructIsConferredOn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-law-lcap;isConferredBy">
      <rdfs:label>is conferred by</rdfs:label>
      <skos:definition>That which confers the construct or brings it into being.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-lcap;.UnionClass.5"/>
   </owl:ObjectProperty>


