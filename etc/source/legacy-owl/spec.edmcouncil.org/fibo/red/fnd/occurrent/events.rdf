<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-occ-evt "http://spec.edmcouncil.org/fibo/red/fnd/occurrent/events/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-plc-loc "http://spec.edmcouncil.org/fibo/red/fnd/places/locations/">
    <!ENTITY fibo-rl-ena "http://spec.edmcouncil.org/fibo/red/fnd/relative/enabling-agents/">
    <!ENTITY  "http://spec.edmcouncil.org/fibo/red/fnd/ext/snap/partial/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/events/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/occurrent/events/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/locations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/enabling-agents/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/ext/snap/partial/w3c-time"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-occ-evt;causes">
      <rdfs:label>causes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-rl-ena;EnablingAgent"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-evt;Event"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-occ-evt;agency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-evt;followedBy">
      <rdfs:label>followed by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-evt;Event"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-evt;Event"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-evt;when">
      <rdfs:label>when</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-evt;Event"/>
      <rdfs:range rdf:resource="&fibo-ext-snap-w3c-tim;Instant"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-evt;where">
      <rdfs:label>where</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-evt;Event"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-loc;Location"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-evt;agency">
      <rdfs:label>agency</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-evt;Event"/>
      <rdfs:range rdf:resource="&fibo-rl-ena;EnablingAgent"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-occ-evt;Event">
      <rdfs:label>event</rdfs:label>
      <skos:definition>Something which occurs at a given time and place.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Concrete"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;OccurentThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>