<owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;context">
      <rdfs:label>context</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-bp-bp-con;context"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-agc;BusinessAgentContext"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;dependsOn">
      <rdfs:label>depends on</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-lcy;Milestone"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;givesRiseTo">
