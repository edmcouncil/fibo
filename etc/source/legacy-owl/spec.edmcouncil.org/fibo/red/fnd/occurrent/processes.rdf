      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;businessProcessResource"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Information"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;governs">
      <rdfs:label>governs</rdfs:label>
      <skos:definition>Some business process, the carrying out of which is governed by terms in the Contract Terms Set.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;givesRiseTo.1">
      <rdfs:label>gives rise to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;givesRiseTo"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActivity"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Information"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;contactWorksFor">
