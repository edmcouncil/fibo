      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActorContactInformation"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;designatedContactDetails">
      <rdfs:label>designated contact details</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActor"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessActorContactInformation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;processRole">
      <rdfs:label>process role</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-act;role"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActor"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-occ-proc;participant"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;alternativeOutcome">
      <rdfs:label>alternative outcome</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;control"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;Decision"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;businessProcessResource">
      <rdfs:label>business process resource</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-act;activityRequiresResource"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;outcome">
      <rdfs:label>outcome</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;control"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;Decision"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;decomposesInto">
      <rdfs:label>decomposes into</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActivity"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;lead">
      <rdfs:label>lead</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;processRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActor"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessActivity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;otherRole">
      <rdfs:label>other role</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;processRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActor"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessActivity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;refers">
      <rdfs:label>refers</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-proc;refersTo"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessActivity"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Information"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;control">
      <rdfs:label>control</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessEvent"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-occ-proc;dependency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;dependency">
      <rdfs:label>dependency</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;ProcessEvent"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;hasEnd">
      <rdfs:label>has end</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-act;endsWithSome"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessEnd"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;hasStart">
      <rdfs:label>has start</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-occ-act;startsWithSome"/>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessStart"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-occ-proc;hasStep">
      <rdfs:label>has step</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-occ-proc;BusinessProcess"/>
      <rdfs:range rdf:resource="&fibo-fnd-occ-proc;ProcessEvent"/>
   </owl:ObjectProperty>


