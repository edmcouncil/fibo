<owl:DatatypeProperty rdf:about="&fibo-fnd-occ-proc;contactEmailAddress">
      <rdfs:label>contact email address</rdfs:label>
      <skos:definition>Email address of the person from the firm who will act as the contact point for the process.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-occ-proc;contactExtensionNumber">
      <rdfs:label>contact extension number</rdfs:label>
      <skos:definition>Primary Phone extension of the person from the firm who will act as the contact point for the process.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-occ-proc;contactName">
      <rdfs:label>contact name</rdfs:label>
      <skos:definition>First and Last Name of the person within the organization which fulfils this Role.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-occ-proc;contactPrimaryTelephoneNumber">
      <rdfs:label>contact primary telephone number</rdfs:label>
      <skos:definition>Primary Phone number of the person from the firm who will act as the contact point for the process.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


