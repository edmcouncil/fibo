<owl:Class rdf:about="&fibo-fnd-occ-act;Activity">
      <rdfs:label>activity</rdfs:label>
      <skos:definition>An Activity is an Event with some human agency. It is defined as something with a Time, Place, Actor and Resource(s). Also known as Event Context or just Context (see e.g DOI ontology).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;OccurentThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-rl-set;Set"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-occ-act;Actor">
      <rdfs:label>actor</rdfs:label>
      <skos:definition>An Actor is an independent entity defined in relation to some role in some process or event (or generally, in any Occurrence). Note: The Role itself is shown as a relationship fact.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-ena;EnablingAgent"/>
   </owl:Class>


