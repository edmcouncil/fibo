<owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;agreementFormalizedBy">
      <rdfs:label>agreement formalized by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-txn-rea;contractEmbodies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;varies">
      <rdfs:label>varies</rdfs:label>
      <skos:definition>Conditions Precedent vary or override some Obligation set out in the Contract</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ConditionsPrecedent"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualObligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;setsOut">
      <rdfs:label>sets out</rdfs:label>
      <skos:definition>The Contract Terms Set defines in writing the Conferred Things (Rights and Obligations, including Commitments) of each party of the Contract to the other.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;confers">
      <rdfs:label>confers</rdfs:label>
      <skos:definition>Some right conferred by the contract, by one party upon the other, for example the right to occupy some space unmolested or the right to receive some cash flows.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;mandates">
      <rdfs:label>mandates</rdfs:label>
      <skos:definition>Some obligation set out by the contract, by one party upon the other.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;varies.1">
      <rdfs:label>varies</rdfs:label>
      <skos:definition>Some Obligation defined in some other contract or instrument between the parties, which is varied in some way by this contract.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;contractualAgreementHasParty">
      <rdfs:label>contractual agreement has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-inv;agreementHasParty"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ext;ContractParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;isOne">
      <rdfs:label>is one</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualBeneficiary"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ext;ContractParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;contractualCommitmentConferredBy">
      <rdfs:label>contractual commitment conferred by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualAgreement"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-agrx-cst;confersContractualCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;mandatedby">
      <rdfs:label>mandatedby</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-lcap;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-agrx-cst;mandates"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;impliedBy">
      <rdfs:label>implied by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;impliedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualObligation"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;isAspectOf">
      <rdfs:label>is aspect of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;isAspectOf.1"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualObligation"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;isOne.1">
      <rdfs:label>is one</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualObligor"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-ext;ContractParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;obligedTo">
      <rdfs:label>obliged to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-lcp;obligedTo"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualObligor"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualBeneficiary"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;conferredBy">
      <rdfs:label>conferred by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;conferredBy.2"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualRight"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;Contract"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-agrx-cst;confers"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;impliesSome">
      <rdfs:label>implies some</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;impliesSome"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualRight"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualObligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;isAspectOf.1">
      <rdfs:label>is aspect of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;isAspectOf.2"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualRight"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;confersContractualCommitment">
      <rdfs:label>confers contractual commitment</rdfs:label>
      <skos:definition>Some Contractual Commitment which arises from the Contractual Agreement and is documented in the Contract which evidences the agreement between the parties.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-agrx-cst;ContractualAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;setsOutObligation">
      <rdfs:label>sets out obligation</rdfs:label>
      <skos:definition>The contractual obligations set out in the contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;contractTermsSetDescribes"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualObligation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-cst;setsOutRight">
      <rdfs:label>sets out right</rdfs:label>
      <skos:definition>The rights set out in the set of terms.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-soc-asp;contractTermsSetDescribes"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-ctr;ContractTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-cst;ContractualRight"/>
   </owl:ObjectProperty>


