<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-agrx-inv "http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/agreements-inverses/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-rl-pri "http://spec.edmcouncil.org/fibo/red/fnd/relative/party-inverses/">
    <!ENTITY fibo-fnd-agr-agr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/agreements/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/agreements-inverses/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/agreements-inverses/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/party-inverses/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/agreements/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-agrx-inv;agreementHasParty">
      <rdfs:label>agreement has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-agr;bilateralAgreementHasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-agr;BilateralAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-agrx-inv;AgreementParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-inv;commitmentConferredBy">
      <rdfs:label>commitment conferred by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isConferredBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-agr;MutualCommitment"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-agr;Agreement"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-agr-agr;confersCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agrx-inv;partyInRoleInContext">
      <rdfs:label>party in role in context</rdfs:label>
      <skos:definition>Some Agreement which the Party In Role is defined as being a party to.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rl-pri;partyInRoleInContext"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-agr;Agreement"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-agrx-inv;AgreementParty">
      <rdfs:label>agreement party</rdfs:label>
      <skos:definition>That which is party to some agreement.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>