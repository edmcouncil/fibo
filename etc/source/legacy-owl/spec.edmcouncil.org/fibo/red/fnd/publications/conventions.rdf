<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-pub-con "http://spec.edmcouncil.org/fibo/red/fnd/publications/conventions/">
    <!ENTITY fibo-fnd-pub-std "http://spec.edmcouncil.org/fibo/red/fnd/publications/standards/">
    <!ENTITY fibo-fnd-pub-pub "http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/publications/conventions/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/publications/conventions/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/standards/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-pub-con;industryModelConventionDefinedBy">
      <rdfs:label>industry model convention defined by</rdfs:label>
      <skos:definition>Some Industry Association which defines the convention.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-con;conventionPublishedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pub-con;IndustryModelConvention"/>
      <rdfs:range rdf:resource="&fibo-fnd-pub-std;IndustryAssociation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pub-con;standardSetBy">
      <rdfs:label>standard set by</rdfs:label>
      <skos:definition>A Standards Body which acts as the setter of the Standard.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-std;standardsSetterIdentity"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-std;standardSetBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pub-std;TechnicalStandard"/>
      <rdfs:range rdf:resource="&fibo-fnd-pub-std;StandardsBody"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pub-con;conventionPublishedBy">
      <rdfs:label>convention published by</rdfs:label>
      <skos:definition>Unlike other published information, a convention is deemed to be in the public domain and therefore published, but there is no reason for the Publisher to be known or to exist as such.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publishedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pub-con;Convention"/>
      <rdfs:range rdf:resource="&fibo-fnd-pub-pub;Publisher"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-pub-con;ISOStandard">
      <rdfs:label>i s o standard</rdfs:label>
      <skos:definition>A standard published and mandated by the International Standards Organisation (ISO).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pub-std;TechnicalStandard"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-pub-con;IndustryModelConvention">
      <rdfs:label>industry model convention</rdfs:label>
      <skos:definition>Standard conventions which exist in the public domain but are not enshrined in an existing ISO Standard are defined in this model and published as such.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pub-con;Convention"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-pub-con;Convention">
      <rdfs:label>convention</rdfs:label>
      <skos:definition>The way something is usually done.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pub-pub;PublishedInformation"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>