<owl:ObjectProperty rdf:about="&fibo-fnd-rl-pri;partyInRoleInContext">
      <rdfs:label>party in role in context</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;inContext"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-prc;RelationshipContext"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-pri;isPartyInRole">
      <rdfs:label>is party in role</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
   </owl:ObjectProperty>


