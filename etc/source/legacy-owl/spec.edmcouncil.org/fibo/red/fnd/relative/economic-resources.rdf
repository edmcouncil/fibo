<owl:Class rdf:about="&fibo-rl-ecr;EconomicContext">
      <rdfs:label>economic context</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;MediatingThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-rl-ecr;EconomicResource">
      <rdfs:label>economic resource</rdfs:label>
      <skos:definition>Anything that can bought sold or exchanged.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Concrete"/>
   </owl:Class>


