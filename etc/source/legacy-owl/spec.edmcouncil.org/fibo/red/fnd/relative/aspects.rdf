<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-rl-asp "http://spec.edmcouncil.org/fibo/red/fnd/relative/aspects/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/relative/aspects/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/relative/aspects/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-rl-asp;hasAlternativeViewpoint">
      <rdfs:label>has alternative viewpoint</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-rl-asp;Aspect"/>
      <rdfs:range rdf:resource="&fibo-rl-asp;Aspect"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-rl-asp;isAspectOf">
      <rdfs:label>is aspect of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;inContext"/>
      <rdfs:domain rdf:resource="&fibo-rl-asp;Aspect"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-rl-asp;hasOtherSide">
      <rdfs:label>has other side</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-asp;hasAlternativeViewpoint"/>
      <rdfs:domain rdf:resource="&fibo-rl-asp;Side"/>
      <rdfs:range rdf:resource="&fibo-rl-asp;Side"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-rl-asp;seenFromPerspectiveOf">
      <rdfs:label>seen from perspective of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-rl-asp;Side"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-rl-asp;Form">
      <rdfs:label>form</rdfs:label>
      <skos:definition>The form which something takes.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-rl-asp;Aspect">
      <rdfs:label>aspect</rdfs:label>
      <skos:definition>Something defined as from some perspective. Further notes: Develop this for rights and obligation perspectives as well.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-rl-asp;Side">
      <rdfs:label>side</rdfs:label>
      <skos:definition>An aspect of something that may be seen from one of two aspects, as seen from viewers at two viewpoints of that thing.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-asp;Aspect"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>