      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/quantified-resources/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
   </owl:Ontology>


