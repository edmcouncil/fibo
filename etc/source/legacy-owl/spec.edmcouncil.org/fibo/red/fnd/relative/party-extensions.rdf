<owl:ObjectProperty rdf:about="&fibo-fnd-rl-prx;organizationMemberHasIdentity">
      <rdfs:label>organization member has identity</rdfs:label>
      <skos:definition>Some entity which is a participant in the organization.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;OrganizationMember"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-prx;hasRelatedParty">
      <rdfs:label>has related party</rdfs:label>
      <skos:definition>A party which is in some relationship to the party.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-prx;hasPartyInRoleOrganizationMember">
      <rdfs:label>has party in role organization member</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-org-org;Organization"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;OrganizationMember"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-pty-pty;organizationMemberIsMemberOf"/>
   </owl:ObjectProperty>


