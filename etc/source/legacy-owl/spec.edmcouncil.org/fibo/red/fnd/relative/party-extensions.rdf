   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/relative/party-extensions/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/organizations/organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
   </owl:Ontology>


