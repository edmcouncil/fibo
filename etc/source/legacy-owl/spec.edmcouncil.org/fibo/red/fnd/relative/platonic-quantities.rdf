  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-rl-plq;Expression">
      <rdfs:label>expression</rdfs:label>
      <skos:definition>The expression of some Quantity in some form.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;Information"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;Meaning">
      <rdfs:label>meaning</rdfs:label>
      <skos:definition>The referent of some meaningful symbol.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-rl-plq;UnknowableQuantity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;ObjectiveProbability">
      <rdfs:label>objective probability</rdfs:label>
      <skos:definition>The actual probabilty of an event or situation. This is approximated to by various probabillity measures accoding to the available knowledge, but is itself unchanged.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;ProbabilityMeasure">
      <rdfs:label>probability measure</rdfs:label>
      <skos:definition>The measure of an unknown objective probabilty, this will come closer to the actual but unknown objective probability acoording to the information available.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-rl-plq;Expression"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;Quantity">
      <rdfs:label>quantity</rdfs:label>
      <skos:definition>Some knowable or unknowable quantity which is expressed in some way, the expression being distinct from the Quantity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Abstract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;SpecifiedVariableQuantity">
      <rdfs:label>specified variable quantity</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-rl-plq;Quantity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;Symbol">
      <rdfs:label>symbol</rdfs:label>
      <skos:definition>A symbol which is held to express some meaning.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;UnknowableQuantity">
      <rdfs:label>unknowable quantity</rdfs:label>
      <skos:definition>Some quantity which, while it is held to exist, is not directly apprehendable to senses or to measurement. This quantity may have some symbol or measure which seeks to capture it, but which is held not to be able to capture it fully.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;VariableSpecification">
      <rdfs:label>variable specification</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-rl-plq;Specification"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-rl-plq;Specification">
      <rdfs:label>specification</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
   </owl:Class>


