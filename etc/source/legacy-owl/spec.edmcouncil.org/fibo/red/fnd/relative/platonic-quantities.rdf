  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;expresses">
      <rdfs:label>expresses</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;Expression"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-plq;Quantity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;expresses.1">
      <rdfs:label>expresses</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rl-plq;expresses"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;ProbabilityMeasure"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-plq;ObjectiveProbability"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;expressedAs">
      <rdfs:label>expressed as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;details"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;Quantity"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-plq;Expression"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-rl-plq;expresses"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;specifies">
      <rdfs:label>specifies</rdfs:label>
      <skos:definition>The thing which is specified by the Specification.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;Specification"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;expresses.2">
      <rdfs:label>expresses</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rl-plq;expresses"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;Symbol"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-plq;Meaning"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;specifiedAs">
      <rdfs:label>specified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rl-plq;expressedAs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;SpecifiedVariableQuantity"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-plq;VariableSpecification"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-rl-plq;specifies.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;mutableTo">
      <rdfs:label>mutable to</rdfs:label>
      <skos:definition>Each way in which the variable quantity may be expressed, is mutable to another equivalent way in which that quantity may also be expressed while remaining the same quantity.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;VariableSpecification"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-plq;VariableSpecification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-rl-plq;specifies.1">
      <rdfs:label>specifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rl-plq;specifies"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rl-plq;expresses"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-plq;VariableSpecification"/>
      <rdfs:range rdf:resource="&fibo-fnd-rl-plq;SpecifiedVariableQuantity"/>
   </owl:ObjectProperty>


