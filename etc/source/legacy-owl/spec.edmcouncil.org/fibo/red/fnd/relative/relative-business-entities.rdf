<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-rl-rbe "http://spec.edmcouncil.org/fibo/red/fnd/relative/relative-business-entities/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-oac-own "http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/">
    <!ENTITY fibo-fnd-aap-agt "http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/agents/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/relative/relative-business-entities/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/relative/relative-business-entities/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/ownership-and-control/ownership/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/agents/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-rl-rbe;relativeBusinessEntityDefinedInContextOf">
      <rdfs:label>relative business entity defined in context of</rdfs:label>
      <skos:definition>The context in which the Relative Business Entity is defined and in which some entity performs the defined role.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;inContext"/>
      <rdfs:domain rdf:resource="&fibo-rl-rbe;RelativeBusinessEntity"/>
      <rdfs:range rdf:resource="&fibo-rl-rbe;BusinessContext"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-rl-rbe;takesEntityForm">
      <rdfs:label>takes entity form</rdfs:label>
      <skos:definition>The form which a Relative Business Entity takes. This may be an Autonomous Agent.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;takesForm"/>
      <rdfs:domain rdf:resource="&fibo-rl-rbe;RelativeBusinessEntity"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-rl-rbe;BusinessContext">
      <rdfs:label>business context</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;MediatingThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-rl-rbe;RelativeBusinessEntity">
      <rdfs:label>relative business entity</rdfs:label>
      <skos:definition>A business entity defined in some relative sense, for example by reference to its role in some process or its overall role or function in the marketplace or in the world as a whole.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>