<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-rl-coll "http://spec.edmcouncil.org/fibo/red/fnd/relative/collections/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-rl-set "http://spec.edmcouncil.org/fibo/red/fnd/relative/set-membership/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/relative/collections/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/relative/collections/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/set-membership/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-rl-coll;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;identity"/>
      <rdfs:domain rdf:resource="&fibo-rl-coll;Constituent"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-rl-coll;hasConstituent">
      <rdfs:label>has constituent</rdfs:label>
      <skos:definition>A constituent of the Collection, that is one of the things that is collected up in bring the collection into being. Further notes: Some kinds of collection are defined in terms of collections of relative things (e.g. basket constituents) with qualifying facts, while others may be defined simply as sets of independent things. It is also possible to have a collection of collections. This is covered by the identity criterion for the Constituent being any Independent Thing, including Collection.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
      <rdfs:domain rdf:resource="&fibo-rl-coll;Collection"/>
      <rdfs:range rdf:resource="&fibo-rl-coll;Constituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-rl-coll;hasMember">
      <rdfs:label>has member</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
      <rdfs:domain rdf:resource="&fibo-rl-coll;Collection"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-rl-coll;Collection">
      <rdfs:label>collection</rdfs:label>
      <skos:definition>A set of things collected together for some purpose.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-set;Set"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-rl-coll;Constituent">
      <rdfs:label>constituent</rdfs:label>
      <skos:definition>Something which makes up part of a collection, formalized in terms of its existence as a member of that collection.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>