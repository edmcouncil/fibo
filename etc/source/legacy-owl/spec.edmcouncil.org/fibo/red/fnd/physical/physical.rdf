<owl:Class rdf:about="&fibo-fnd-phy-phy;ContinuousPhysicalStuff">
      <rdfs:label>continuous physical stuff</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-phy-phy;PhysicalItem"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-phy-phy;DiscretePhysicalThing">
      <rdfs:label>discrete physical thing</rdfs:label>
      <skos:definition>Some physical item with well defined physical extent.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-phy-phy;PhysicalItem">
      <rdfs:label>physical item</rdfs:label>
      <skos:definition>Something you can touch.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Concrete"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:Class>


