<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-bus-ecr "http://spec.edmcouncil.org/fibo/red/fnd/business/marketable-economic-resource/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-rl-ecr "http://spec.edmcouncil.org/fibo/red/fnd/relative/economic-resources/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-bus-ser "http://spec.edmcouncil.org/fibo/red/fnd/business/services/">
    <!ENTITY fibo-fnd-phy-phy "http://spec.edmcouncil.org/fibo/red/fnd/physical/physical/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/business/marketable-economic-resource/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/business/marketable-economic-resource/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/economic-resources/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/business/services/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/physical/physical/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-bus-ecr;form">
      <rdfs:label>form</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-ecr;takesMaterialForm"/>
      <rdfs:domain rdf:resource="&fibo-fnd-bus-ecr;Good"/>
      <rdfs:range rdf:resource="&fibo-fnd-phy-phy;PhysicalItem"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-bus-ecr;takesForm">
      <rdfs:label>takes form</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-ecr;takesMaterialForm"/>
      <rdfs:domain rdf:resource="&fibo-fnd-bus-ecr;MarketableService"/>
      <rdfs:range rdf:resource="&fibo-fnd-bus-ser;Service"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-bus-ecr;form.1">
      <rdfs:label>form</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-ecr;takesMaterialForm"/>
      <rdfs:domain rdf:resource="&fibo-fnd-bus-ecr;NegotiableCommodity"/>
      <rdfs:range rdf:resource="&fibo-fnd-phy-phy;PhysicalItem"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-bus-ecr;unitOfMeasure">
      <rdfs:label>unit of measure</rdfs:label>
      <skos:definition>The unit in which the underlying commodity is measured. Further Notes This is modeled as text at this level in the model. It may be possible to model individual types of unit in relation to individual types of commodity, e.g. the various units of measurement for oil, grain and so on, though these are usually one default worldwide one type of commodity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-bus-ecr;Good">
      <rdfs:label>good</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-bus-ecr;BundleOfResources"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-bus-ecr;MarketableService">
      <rdfs:label>marketable service</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;OccurentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-bus-ecr;BundleOfResources">
      <rdfs:label>bundle of resources</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-rl-ecr;EconomicResource"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-bus-ecr;NegotiableCommodity">
      <rdfs:label>negotiable commodity</rdfs:label>
      <skos:definition>Goods or raw materials which may be bought or sold in some marketplace.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>