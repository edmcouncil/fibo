  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-agr-agr;agreementHasPartyInRole">
      <rdfs:label>agreement has party in role</rdfs:label>
      <skos:definition>A party to the Agreement.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-agr;Agreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agr-agr;bilateralAgreementHasPartyInRole">
      <rdfs:label>bilateral agreement has party in role</rdfs:label>
      <skos:definition>A party to the Bilateral Agreement. There are exactly two of parties to the Agreement.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-agr;BilateralAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agr-agr;multilateralAgreementHasPartyInRole">
      <rdfs:label>multilateral agreement has party in role</rdfs:label>
      <skos:definition>A party to the Multilateral Agreement. There are at least three parties to the Agreement,</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-agr;MultilateralAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-agr-agr;confersCommitment">
      <rdfs:label>confers commitment</rdfs:label>
      <skos:definition>Some Commitment which arises from the Agreement and exists as a result of that Agreement.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;confers"/>
      <rdfs:domain rdf:resource="&fibo-fnd-agr-agr;Agreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-agr;MutualCommitment"/>
   </owl:ObjectProperty>


