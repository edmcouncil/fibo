<owl:ObjectProperty rdf:about="&fibo-fnd-lawx-rch;civilJurisdictionGeographicalReach">
      <rdfs:label>civil jurisdiction geographical reach</rdfs:label>
      <skos:definition>The geopolitical entity (usually country) over which the Civil Jurisdiction has its reach.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;hasReach"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;CivilLawJurisdiction"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;GeopoliticalEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-lawx-rch;statuteLawHasGeographicalReach">
      <rdfs:label>statute law has geographical reach</rdfs:label>
      <skos:definition>The geographical extent of the jurisdiction in which the Statute Law has effect.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;hasReach"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-law-jur;isInForceIn"/>
      <rdfs:domain rdf:resource="&fibo-fnd-law-jur;StatuteLaw"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;GeopoliticalEntity"/>
   </owl:ObjectProperty>


