         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/documentation/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/documentation/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/corporate-bodies/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-fnd-info-doc;DocumentVersionType">
      <skos:definition>Type or status of a document, specifically Draft or final. Not the same as versioning of a revision-controlled formal document.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Final</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Draft</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;authorOf">
      <rdfs:label>author of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;originator"/>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-doc;FormalControlledDocument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;issuedBy">
      <rdfs:label>issued by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;originator"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;Certificate"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;issuedTo">
      <rdfs:label>issued to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;Certificate"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-uo-rl;identity"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;ControlDocument"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Document"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;about">
      <rdfs:label>about</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;DocumentInformation"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-doc;FormalControlledDocument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;issuerOf">
      <rdfs:label>issuer of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-doc;FormalControlledDocument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;becomes">
      <rdfs:label>becomes</rdfs:label>
      <skos:definition>A document may in the course of its life become another document, for example a draft becoming a later draft or becoming a formal document, or a new revision of a formally revision controlled document.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;informationFlow"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-info;Document"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-info;Document"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;becomes.1">
      <rdfs:label>becomes</rdfs:label>
      <skos:definition>Signing and bringing into effect of the Draft Contract to become a formal, written Contract.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;becomesFormalizedAs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;DraftContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-agr-ctr;WrittenContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;becomesFormalizedAs">
      <rdfs:label>becomes formalized as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;becomes"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;DraftDocument"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-doc;FormalDocument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;hsaPart">
      <rdfs:label>hsa part</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;hasPart"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;DraftDocument"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-doc;DraftDocumentPart"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;formalStatementBy">
      <rdfs:label>formal statement by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;originator"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;FormalStatement"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;from">
      <rdfs:label>from</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;originator"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;Memorandum"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-doc;to"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;to">
      <rdfs:label>to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;Memorandum"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;writes">
      <rdfs:label>writes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-doc;Memorandum"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;onThePartOf">
      <rdfs:label>on the part of</rdfs:label>
      <skos:definition>The legal entity by whom or on whose behalf the Notice is written.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;originator"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;Notice"/>
      <rdfs:range rdf:resource="&fibo-be-le-cb;BodyCorporate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;reportAuthorBusinessOrganization">
      <rdfs:label>report author business organization</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-doc;originator"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-doc;Report"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-doc;originator">
      <rdfs:label>originator</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-info;WrittenInformation"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-info-doc;controlDate">
      <rdfs:label>control date</rdfs:label>
      <skos:definition>The date at which the controlling aspect of the document takes effect, for example the valid date for a permit, the sign-off date for a written document that is required to proceed with a process or activity.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-doc;version">
      <rdfs:label>version</rdfs:label>
      <skos:definition>The formal Revision of the document, as recorded on the document and on any relevant Revision Management System.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-doc;title">
      <rdfs:label>title</rdfs:label>
      <skos:definition>The formal title of the Document.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


