<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-info-sch "http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-info-info "http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/">
    <!ENTITY fibo-fnd-info-ids "http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/">
    <!ENTITY fibo-fnd-plc-cty "http://spec.edmcouncil.org/fibo/red/fnd/places/countries/">
    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
    <!ENTITY fibo-sec-sec-cls "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-classification/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/identifiers/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-classification/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;CountryCodeScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;CountryCode"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;identifies">
      <rdfs:label>identifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-ids;identifies"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;CountryCode"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;identifiedBy">
      <rdfs:label>identified by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-ids;hasIdentifier"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-cty;Country"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;CountryCode"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-sch;identifies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;sectorClassificationSchemeDefines">
      <rdfs:label>sector classification scheme defines</rdfs:label>
      <skos:definition>A sector classification code defined under the scheme</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;classificationSchemeDefines"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;SectorClassificationScheme"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;OrganizationIndustrySectorClassification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;defines.1">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;classificationSchemeDefines"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;SectorClassificationScheme"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-cls;IndustrySectorClassification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;classifies">
      <rdfs:label>classifies</rdfs:label>
      <skos:definition>The thing which is classified by the classification code. This may be any independent thing, concrete or abstract and continuant or occurrent.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;Classification"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;defines.2">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;BusinessCalendarScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;BusinessCalendarCode"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;classificationSchemeDefines">
      <rdfs:label>classification scheme defines</rdfs:label>
      <skos:definition>A classification code defined under the scheme</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;ClassificationScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;Classification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;defines.3">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;CurrencyCodeScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;CurrencyCode"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;SchematicCode"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;Scheme"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-sch;defines.4">
      <rdfs:label>defines</rdfs:label>
      <skos:definition>The scheme sets out the nature, structure and requirements of a set of Schematic Codes.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-info-sch;Scheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-sch;SchematicCode"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-info-sch;code">
      <rdfs:label>code</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-sch;schemeReference">
      <rdfs:label>scheme reference</rdfs:label>
      <skos:definition>The reference by which the scheme is identified, such as a URI.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-info-sch;Class1">
      <rdfs:label>class1</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;CountryCode">
      <rdfs:label>country code</rdfs:label>
      <skos:definition>A recognized, standardized code by which a country can be uniquely identified.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;SchematicCode"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;CountryCodeScheme">
      <rdfs:label>country code scheme</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;Scheme"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;SectorClassificationScheme">
      <rdfs:label>sector classification scheme</rdfs:label>
      <skos:definition>Scheme defining the structure and semantics of a set of classification codes which identify different industry sectors and (optionally) sub-sectors.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;ClassificationScheme"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;BusinessCalendarCode">
      <rdfs:label>business calendar code</rdfs:label>
      <skos:definition>The Calendar for a particular business center, which defines what are the working days in that center.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;BusinessCalendarScheme">
      <rdfs:label>business calendar scheme</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;Classification">
      <rdfs:label>classification</rdfs:label>
      <skos:definition>Some Classification of some Thing to identify what type of Thing it is (usually within a particular class of Things such as financial securities or organisations).</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;ClassificationScheme">
      <rdfs:label>classification scheme</rdfs:label>
      <skos:definition>The Scheme defining how the Classification is determined. This may be a URI or some other unique identification of the Scheme.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;CurrencyCode">
      <rdfs:label>currency code</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;CurrencyCodeScheme">
      <rdfs:label>currency code scheme</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;RegistrationCode">
      <rdfs:label>registration code</rdfs:label>
      <skos:definition>Any code by which something in formally registered, for example for the purposes of legal identification.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;Information"/>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;ContinuantThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;SchematicCode">
      <rdfs:label>schematic code</rdfs:label>
      <skos:definition>A code which is defined in some scheme as having some meaning.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-sch;Scheme">
      <rdfs:label>scheme</rdfs:label>
      <skos:definition>The scheme by which the schematic code is identified.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>