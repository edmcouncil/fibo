<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-info-msg "http://spec.edmcouncil.org/fibo/red/fnd/information/messaging/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-info-info "http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/messaging/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/messaging/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-msg;madeBy">
      <rdfs:label>made by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-msg;requestedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-msg;Application"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-msg;Applicant"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-msg;hasSubject">
      <rdfs:label>has subject</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-msg;Message"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-msg;requestedBy">
      <rdfs:label>requested by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-msg;Request"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-msg;RequestingParty"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-info-msg;Applicant">
      <rdfs:label>applicant</rdfs:label>
      <skos:definition>The party making some application.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-msg;RequestingParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-msg;Application">
      <rdfs:label>application</rdfs:label>
      <skos:definition>A formal application by some party to some other party, in which the requesting party sets out some requirement which they request to be fulfilled by the other party.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-msg;Request"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-msg;Message">
      <rdfs:label>message</rdfs:label>
      <skos:definition>Some message communicating something from some party to some other party.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;Information"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-msg;Request">
      <rdfs:label>request</rdfs:label>
      <skos:definition>A request by some party to some other party for some thing or service.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-msg;RequestingParty">
      <rdfs:label>requesting party</rdfs:label>
      <skos:definition>The party making some request.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>