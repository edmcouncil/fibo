  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-info-idni;ISO3LetterCountryCodeScheme">
      <rdfs:label>i s o 3 letter country code scheme</rdfs:label>
      <skos:definition>[add reference of scheme as ISO Standard plus sub-set]</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pub-con;ISOStandard"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;CountryCodeScheme"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-idni;ISOCountryCode">
      <rdfs:label>i s o country code</rdfs:label>
      <skos:definition>The country code standard defined by ISO as ISO 3166. Note that this comes in several defined formats. For dta management purposes the precise format would need to be identified.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;CountryCode"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
