<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-info-idni "http://spec.edmcouncil.org/fibo/red/fnd/information/iso3166-identifiers/">
    <!ENTITY fibo-fnd-info-sch "http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/">
    <!ENTITY fibo-fnd-plc-cty "http://spec.edmcouncil.org/fibo/red/fnd/places/countries/">
    <!ENTITY fibo-fnd-pub-con "http://spec.edmcouncil.org/fibo/red/fnd/publications/conventions/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/iso3166-identifiers/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/iso3166-identifiers/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/conventions/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-idni;identifier">
      <rdfs:label>identifier</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;identifiedBy"/>
      <rdfs:domain rdf:resource="&fibo-fnd-plc-cty;Country"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-idni;ISOCountryCode"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-idni;identifies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idni;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idni;ISO3LetterCountryCodeScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-idni;ISOCountryCode"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idni;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idni;ISOCountryCode"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-idni;ISO3LetterCountryCodeScheme"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-idni;defines"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-idni;identifies">
      <rdfs:label>identifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;identifies"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-idni;ISOCountryCode"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>


