<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-info-tem "http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/">
    <!ENTITY fibo-uo-lat "http://spec.edmcouncil.org/fibo/red/uo/lattice/">
    <!ENTITY fibo-fnd-info-info "http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
    <!ENTITY fibo-fnd-math-math "http://spec.edmcouncil.org/fibo/red/fnd/math/math/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-fnd-info-sch "http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/math/math/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-info-tem;analyzes">
      <rdfs:label>analyzes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-tem;AnalyticalInformation"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-math;TimedParameter"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-tem;derivedFrom">
      <rdfs:label>derived from</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;expressedAs"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-tem;AnalyticalInformation"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-tem;.UnionClass.6"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-tem;makesReferenceTo">
      <rdfs:label>makes reference to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;derivedFrom"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-tem;AnalyticalInformation"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-math;TimedParameter"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-tem;includes">
      <rdfs:label>includes</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-info-tem;AnalyticsCalculationModel"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-math;Formula"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-tem;hasStatus">
      <rdfs:label>has status</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;details"/>
      <rdfs:domain rdf:resource="&fibo-uo-rl;IndependentThing"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-tem;Status"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-tem;isOf">
      <rdfs:label>is of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-info;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-tem;Status"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-info-tem;hasStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-info-tem;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;defines.4"/>
      <rdfs:domain rdf:resource="&fibo-fnd-info-tem;RatingScheme"/>
      <rdfs:range rdf:resource="&fibo-fnd-info-tem;Rating"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-info-tem;tense">
      <rdfs:label>tense</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Tense"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-tem;tense.1">
      <rdfs:label>tense</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Tense"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-fnd-info-tem;tense.2">
      <rdfs:label>tense</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Tense"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-info-tem;.UnionClass.6">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-math-math;Formula"/>
               <owl:Class rdf:about="&fibo-fnd-info-tem;AnalyticsCalculationModel"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-tem;AnalyticalInformation">
      <rdfs:label>analytical information</rdfs:label>
      <skos:definition>Information about some item at some time past, present or future. This takes the form of some measure about the thing being analyzed; the measure will have some values at different times as shown in the Math grammar for dated parameters.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;TimedParameter"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;Information"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-tem;AnalyticsCalculationModel">
      <rdfs:label>analytics calculation model</rdfs:label>
      <skos:definition>A model used for formal calculation of parameters.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;MathematicalModel"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-tem;TimeSensitiveInformation">
      <rdfs:label>time sensitive information</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-tem;HistoricalInformation">
      <rdfs:label>historical information</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;OccurentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-tem;Rating">
      <rdfs:label>rating</rdfs:label>
      <skos:definition>A schematic code which is used to express an opinion about something, as expressed by some agent. This may take different values at different times, as the agent expressing that opinion may revise it from time to time.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-tem;TimeSensitiveInformation"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;SchematicCode"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-tem;RatingScheme">
      <rdfs:label>rating scheme</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-sch;Scheme"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-info-tem;Status">
      <rdfs:label>status</rdfs:label>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>