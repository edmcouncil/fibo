<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-pty-rl "http://spec.edmcouncil.org/fibo/red/fnd/parties/roles/">
    <!ENTITY fibo-fnd-aap-ppl "http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/people/">
    <!ENTITY fibo-fnd-rl-prc "http://spec.edmcouncil.org/fibo/red/fnd/relative/party-role-context/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-org-org "http://spec.edmcouncil.org/fibo/red/fnd/organizations/organizations/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/roles/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/people/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/party-role-context/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/organizations/organizations/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-fnd-pty-pty;organizationMemberIsMemberOf">
      <rdfs:label>organization member is member of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-rl;hasRole"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isMemberOf"/>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;OrganizationMember"/>
      <rdfs:range rdf:resource="&fibo-fnd-org-org;Organization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pty-pty;hasParty">
      <rdfs:label>has party</rdfs:label>
      <skos:definition>an independent party associated with an agreement, contract, policy, regulation, or other business arrangement</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
      <rdfs:domain rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pty-pty;isAPartyTo">
      <rdfs:label>is a party to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
      <rdfs:range rdf:resource="&fibo-fnd-rel-rel;Thing"/>
      <owl:inverseOf rdf:resource="&fibo-fnd-pty-pty;hasParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pty-pty;hasIdentity">
      <rdfs:label>has identity</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-fnd-pty-pty;hasPartyInRole">
      <rdfs:label>has party in role</rdfs:label>
      <skos:definition>The role fulfilled by a party in a relationship context.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-fnd-rl-prc;RelationshipContext"/>
      <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-fnd-pty-pty;partyHasEffectiveDate">
      <rdfs:label>party has effective date</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;dateAndTime"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-fnd-pty-pty;.UnionClass.8">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-org-org;Organization"/>
               <owl:Class rdf:about="&fibo-fnd-aap-ppl;Person"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-pty-pty;IndependentParty">
      <rdfs:label>independent party</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-pty-pty;OrganizationMember">
      <rdfs:label>organization member</rdfs:label>
      <skos:definition>Some entity which is a member of some organization.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-fnd-pty-pty;PartyInRole">
      <rdfs:label>party in role</rdfs:label>
      <skos:definition>Any autonomous entity which is defined as being the party in some context, for example the party to a contract or a transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-rl;AgentInRole"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>