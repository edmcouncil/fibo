    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-uo-rl;identity">
      <rdfs:label>identity</rdfs:label>
      <skos:definition>The class of Independent Thing which is identified as this class of Relative Thing, i.e. the thing which fulfils the role or is the party etc. defined for the Independent Thing.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-uo-rl;RelativeThing"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-uo-rl;inContext">
      <rdfs:label>in context</rdfs:label>
      <skos:definition>The context in which the Relative Thing has the meaningful definition given to it.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-uo-rl;RelativeThing"/>
      <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
