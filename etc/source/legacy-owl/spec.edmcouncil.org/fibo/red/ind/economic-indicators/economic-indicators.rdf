<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-ind-ei-ei "http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicators/">
    <!ENTITY fibo-uo-rl "http://spec.edmcouncil.org/fibo/red/uo/relatives/">
    <!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-ind-ind-val "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators-values/">
    <!ENTITY fibo-fnd-pub-pub "http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/">
    <!ENTITY fibo-fnd-math-math "http://spec.edmcouncil.org/fibo/red/fnd/math/math/">
    <!ENTITY fibo-fnd-plc-cty "http://spec.edmcouncil.org/fibo/red/fnd/places/countries/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicators/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicators/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators-values/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/math/math/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/places/countries/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;economicRateAppliesTo">
      <rdfs:label>economic rate applies to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;parameterAppliesTo"/>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;EconomicRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-ei;Economy"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;value">
      <rdfs:label>value</rdfs:label>
      <skos:definition>The value of the GDP, as quoted for a given period.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;GrossDomesticProduct"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-val;GDPValue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;value.1">
      <rdfs:label>value</rdfs:label>
      <skos:definition>The value of the inflation rate, as quoted for a given period.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;InflationRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-val;InflationRateValue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;isOf">
      <rdfs:label>is of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;isAbout"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ei-ei;economicRateAppliesTo"/>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;InflationRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-ei;Economy"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;publishedFor">
      <rdfs:label>published for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;isAbout"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;characterizes"/>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;InflationRate"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;hasPercentageValue">
      <rdfs:label>has percentage value</rdfs:label>
      <skos:definition>The value of the unemployment rate for the stated period.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;UnemploymentRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-val;UnemploymentRateValue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;isOf.1">
      <rdfs:label>is of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;isAbout"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ei-ei;economicRateAppliesTo"/>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;UnemploymentRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-ei;Economy"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ei-ei;isEconomyOf">
      <rdfs:label>is economy of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-ind-ei-ei;Economy"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;GeopoliticalEntity"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-ind-ei-ei;abbreviation">
      <rdfs:label>abbreviation</rdfs:label>
      <skos:definition>The recognized abbreviation of the Index, e.g. RPI.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-ind-ei-ei;hasMeasurementDate">
      <rdfs:label>has measurement date</rdfs:label>
      <skos:definition>The date at which the published figure applies.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-ind-ei-ei;name">
      <rdfs:label>name</rdfs:label>
      <skos:definition>The commonly identified name of the economic rate.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-ind-ei-ei;excludesEnergyAndFood">
      <rdfs:label>excludes energy and food</rdfs:label>
      <skos:definition>Whether the index excludes energy and food prices.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-ind-ei-ei;EconomicRate">
      <rdfs:label>economic rate</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ind-ind;MarketBaseRate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-ei;Economy">
      <rdfs:label>economy</rdfs:label>
      <skos:definition>A geographically distinct web of economic relationships.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-ei;GrossDomesticProduct">
      <rdfs:label>gross domestic product</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ei-ei;EconomicRate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-ei;InflationRate">
      <rdfs:label>inflation rate</rdfs:label>
      <skos:definition>The annual percent change in consumer prices compared with the previous year's consumer prices, for a given country.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ei-ei;UnemploymentRate">
      <rdfs:label>unemployment rate</rdfs:label>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>