<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-ind-ir-ir "http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rates/">
    <!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
    <!ENTITY fibo-md-tmp-ins "http://spec.edmcouncil.org/fibo/red/md/temporal-core/instrument-temporal-terms/">
    <!ENTITY fibo-ind-ind-val "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators-values/">
    <!ENTITY  "http://spec.edmcouncil.org/fibo/red/fnd/utilities/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rates/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rates/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/md/temporal-core/instrument-temporal-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators-values/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/analytics"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-ind-ir-ir;tenor">
      <rdfs:label>tenor</rdfs:label>
      <skos:definition>The length of time for which the IBOR is quoted, e.g. 3 months, 6 months and so on.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterBankOfferRate"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;KnownDuration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-ir;value">
      <rdfs:label>value</rdfs:label>
      <skos:definition>The value of the interest rate.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterestReferenceRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-val;InterestRateTimedValue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-ir;refersTo">
      <rdfs:label>refers to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/analytics/"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-ins;spreadIsBetween"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;MarketRateSpread"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-ir;BaseRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-ir;refersTo.1">
      <rdfs:label>refers to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-ins;spreadIsBetween"/>
      <rdfs:subPropertyOf rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/analytics/"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;MarketRateSpread"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-ir;InterBankOfferRate"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-ind-ir-ir;effectiveDate">
      <rdfs:label>effective date</rdfs:label>
      <skos:definition>The date from which the rate is effective.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-ind-ir-ir;rateResetTimeOfDay">
      <rdfs:label>rate reset time of day</rdfs:label>
      <skos:definition>The time of day when the rate is reset e.g. 11:00</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;timeHoursAndMinutes"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-ind-ir-ir;rateName">
      <rdfs:label>rate name</rdfs:label>
      <skos:definition>The common name for the Interest Rate e.g. LIBOR</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-ind-ir-ir;BaseRate">
      <rdfs:label>base rate</rdfs:label>
      <skos:definition>The base rate set by a central bank for a given currency.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ir-ir;InterestReferenceRate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-ir;InterBankOfferRate">
      <rdfs:label>inter bank offer rate</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-ir;InterestReferenceRate">
      <rdfs:label>interest reference rate</rdfs:label>
      <skos:definition>A rate of Interest paid by or agreed among some bank or set of banks.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ind-ind;MarketBaseRate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-ir;MarketRateSpread">
      <rdfs:label>market rate spread</rdfs:label>
      <skos:definition>A rate formed from the difference between an interest rate and a lending rate, each for a given duration.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ind-ind;MarketSpread"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-ir;OvernightInterestRate">
      <rdfs:label>overnight interest rate</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-ir;USFederalFundsRate">
      <rdfs:label>u s federal funds rate</rdfs:label>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>