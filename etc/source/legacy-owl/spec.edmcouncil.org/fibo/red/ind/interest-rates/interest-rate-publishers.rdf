  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-ind-ir-pub;BaseRateAuthority">
      <rdfs:label>base rate authority</rdfs:label>
      <skos:definition>The authority responsible for setting the base rate for a given currency. This would normally be a central bank.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ir-pub;InterestRateAuthority"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-pub;CentralBank">
      <rdfs:label>central bank</rdfs:label>
      <skos:definition>A bank which regulates the monetary supply for a given economy and currency.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ir-pub;MonetaryAuthority"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-pub;InterbankRateAuthority">
      <rdfs:label>interbank rate authority</rdfs:label>
      <skos:definition>The authority responsible for setting the Interbank lending rate.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-pub;InterestRateAuthority">
      <rdfs:label>interest rate authority</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-rl;AgentInRole"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-pub;MarketDataProvider">
      <rdfs:label>market data provider</rdfs:label>
      <skos:definition>An entity which provides data or information about the financial markets. This is usually either a publishing house or a specialized market data vendor.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ind-ind;FinancialInformationPublisher"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-ir-pub;MonetaryAuthority">
      <rdfs:label>monetary authority</rdfs:label>
      <skos:definition>Some formal organization tasked with some or all of the tasks relating the monetary policy, regulation and supply for some currency or in some country or group of countries. Editorial Note: Added because not all the functions of rate setting etc. are always managed by a Central Bank, and because in the OWL, there needed to be a class in this ontology itself which can be the domain of the property indicating by whom the Interbank Rate Authority is convened (since this is no longer or not necessarily the Central Bank as originally modeled).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:NamedIndividual rdf:about="&fibo-ind-ir-pub;TedSpread">
      <rdfs:label>ted spread</rdfs:label>
      <rdf:type rdf:resource="&fibo-ind-ir-ir;MarketRateSpread"/>
   </owl:NamedIndividual>

</rdf:RDF>