<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-ind-ir-pub "http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rate-publishers/">
    <!ENTITY fibo-fnd-pty-rl "http://spec.edmcouncil.org/fibo/red/fnd/parties/roles/">
    <!ENTITY fibo-be-le-fbo "http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/">
    <!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-ind-ir-ir "http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rates/">
    <!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rate-publishers/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rate-publishers/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/roles/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/interest-rates/interest-rates/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <skos:definition>The legal entity which is the authority for setting and publishing the base rate.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ir-pub;.1"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;BaseRateAuthority"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-pub;CentralBank"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;baseRateSetBy">
      <rdfs:label>base rate set by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ir-pub;setBy"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;BaseRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-pub;BaseRateAuthority"/>
      <owl:inverseOf rdf:resource="&fibo-ind-ir-pub;setsBaseRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;regulatesMoneySupplyFor">
      <rdfs:label>regulates money supply for</rdfs:label>
      <skos:definition>The currency issued by the central bank and for which it is responsible for the money supply.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;CentralBank"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;convenedBy">
      <rdfs:label>convened by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;InterbankRateAuthority"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-pub;MonetaryAuthority"/>
      <owl:inverseOf rdf:resource="&fibo-ind-ir-pub;.2"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;interbankRatePublishedBy">
      <rdfs:label>interbank rate published by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ind-ind;isPublishedBy"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterBankOfferRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-pub;MarketDataProvider"/>
      <owl:inverseOf rdf:resource="&fibo-ind-ir-pub;publishesInterbankRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;interbankRateSetBy">
      <rdfs:label>interbank rate set by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ir-pub;setBy"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterBankOfferRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-pub;InterbankRateAuthority"/>
      <owl:inverseOf rdf:resource="&fibo-ind-ir-pub;setsInterbankRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;.1">
      <rdfs:label/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-rl;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;InterestRateAuthority"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;.2">
      <rdfs:label/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;appoints"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;MonetaryAuthority"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-pub;InterbankRateAuthority"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;publishesInterbankRate">
      <rdfs:label>publishes interbank rate</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ind-ind;publishes"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;MarketDataProvider"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-ir;InterBankOfferRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;setBy">
      <rdfs:label>set by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isProvidedBy"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-ir;InterestReferenceRate"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-pub;InterestRateAuthority"/>
      <owl:inverseOf rdf:resource="&fibo-ind-ir-pub;sets"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;sets">
      <rdfs:label>sets</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;provides"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;InterestRateAuthority"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-ir;InterestReferenceRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;setsBaseRate">
      <rdfs:label>sets base rate</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ir-pub;sets"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;BaseRateAuthority"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-ir;BaseRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ir-pub;setsInterbankRate">
      <rdfs:label>sets interbank rate</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-ir-pub;sets"/>
      <rdfs:domain rdf:resource="&fibo-ind-ir-pub;InterbankRateAuthority"/>
      <rdfs:range rdf:resource="&fibo-ind-ir-ir;InterBankOfferRate"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
