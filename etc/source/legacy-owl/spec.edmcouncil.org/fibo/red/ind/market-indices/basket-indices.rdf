    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-ind-mi-bi;isPriceOf">
      <rdfs:label>is price of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-mi-bi;isPriceOf.1"/>
      <rdfs:domain rdf:resource="&fibo-ind-mi-bi;EquityReferenceIndex"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;EquityReferenceIndexBasket"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-mi-bi;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;identity.2"/>
      <rdfs:domain rdf:resource="&fibo-ind-mi-bi;EquityIndexConstituent"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-eqi;PubliclyTradedShare"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-mi-bi;hasConstituent">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-mi-bi;hasConstituent.1"/>
      <rdfs:domain rdf:resource="&fibo-ind-mi-bi;EquityReferenceIndexBasket"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;EquityIndexConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-mi-bi;value">
      <rdfs:label>value</rdfs:label>
      <skos:definition>The value of the benchmark at the stated date or time.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-ind-mi-bi;ReferenceIndex"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-val;ReferenceIndexDatedValue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-mi-bi;isPriceOf.1">
      <rdfs:label>is price of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-ind-mi-bi;ReferenceIndex"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;ReferenceIndexBasket"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-mi-bi;hasConstituent.1">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;hasConstituent.1"/>
      <rdfs:domain rdf:resource="&fibo-ind-mi-bi;ReferenceIndexBasket"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;ReferenceIndexConstituent"/>
   </owl:ObjectProperty>


