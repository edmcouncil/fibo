<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-ind-mi-bi "http://spec.edmcouncil.org/fibo/red/ind/market-indices/basket-indices/">
    <!ENTITY fibo-sec-sec-bsk "http://spec.edmcouncil.org/fibo/red/sec/securities/baskets/">
    <!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
    <!ENTITY fibo-fnd-pub-pub "http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/">
    <!ENTITY fibo-ind-ind-val "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators-values/">
    <!ENTITY fibo-sec-eq-eqi "http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/ind/market-indices/basket-indices/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/ind/market-indices/basket-indices/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/baskets/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators-values/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/"/>
   </owl:Ontology>


