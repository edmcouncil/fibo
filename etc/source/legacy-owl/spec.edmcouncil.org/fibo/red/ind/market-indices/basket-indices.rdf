  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-ind-mi-bi;EquityIndexConstituent">
      <rdfs:label>equity index constituent</rdfs:label>
      <skos:definition>A constituent of the Equity Index.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-mi-bi;ReferenceIndexConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-mi-bi;EquityReferenceIndex">
      <rdfs:label>equity reference index</rdfs:label>
      <skos:definition>A benchmark or reference index the constituents of which are exclusively Equity securities.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-mi-bi;ReferenceIndex"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-mi-bi;EquityReferenceIndexBasket">
      <rdfs:label>equity reference index basket</rdfs:label>
      <skos:definition>The basket of securities which makes up the Equities Reference Index.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-ind-mi-bi;ReferenceIndexBasket"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-mi-bi;ReferenceIndex">
      <rdfs:label>reference index</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-ind-ind-ind;MarketBaseRate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-mi-bi;ReferenceIndexBasket">
      <rdfs:label>reference index basket</rdfs:label>
      <skos:definition>The basket of securities which makes up a published index.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;BasketOfSecurities"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-ind-mi-bi;ReferenceIndexConstituent">
      <rdfs:label>reference index constituent</rdfs:label>
      <skos:definition>A security which makes up part of a Reference Index.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;SecuritiesBasketConstituent"/>
   </owl:Class>


