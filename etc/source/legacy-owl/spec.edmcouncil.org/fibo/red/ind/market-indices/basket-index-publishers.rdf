<owl:ObjectProperty rdf:about="&fibo-ind-mi-pub;publishedBy">
      <rdfs:label>published by</rdfs:label>
      <skos:definition>The publisher of the rate such as Standard and Poors. This is identified as a kind of market data vendor.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publishedBy"/>
      <rdfs:domain rdf:resource="&fibo-ind-mi-bi;ReferenceIndex"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-pub;IndexProvider"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-mi-pub;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publisherIdentity"/>
      <rdfs:domain rdf:resource="&fibo-ind-mi-pub;IndexProvider"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-mi-pub;isAbleToTrade">
      <rdfs:label>is able to trade</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-ind-mi-pub;IndexProvider"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;ReferenceIndexBasket"/>
   </owl:ObjectProperty>


