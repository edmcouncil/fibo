    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-ind-ind-ind;financialInformationPublisherHasIdentity">
      <rdfs:label>financial information publisher has identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publisherIdentity"/>
      <rdfs:domain rdf:resource="&fibo-ind-ind-ind;FinancialInformationPublisher"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ind-ind;marketSpreadIsBetween">
      <rdfs:label>market spread is between</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/analytics/"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-ins;spreadIsBetween"/>
      <rdfs:domain rdf:resource="&fibo-ind-ind-ind;MarketSpread"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-ind;MarketReferentialParameter"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ind-ind;isVolatilityOf">
      <rdfs:label>is volatility of</rdfs:label>
      <skos:definition>The market rate to which the volatility measure applies and of which it is a measure.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;parameterAppliesTo"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
      <rdfs:domain rdf:resource="&fibo-ind-ind-ind;Volatility"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-ind;MarketBaseRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ind-ind;isPublishedBy">
      <rdfs:label>is published by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;isProvidedBy"/>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publishedBy"/>
      <rdfs:domain rdf:resource="&fibo-ind-ind-ind;MarketReferentialParameter"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-ind;FinancialInformationPublisher"/>
      <owl:inverseOf rdf:resource="&fibo-ind-ind-ind;publishes"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-ind-ind-ind;publishes">
      <rdfs:label>publishes</rdfs:label>
      <skos:definition>Information published by the publisher, in the form of some market referential parameter such as an economic index or indicator.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;provides"/>
      <rdfs:domain rdf:resource="&fibo-ind-ind-ind;FinancialInformationPublisher"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-ind;MarketReferentialParameter"/>
   </owl:ObjectProperty>


