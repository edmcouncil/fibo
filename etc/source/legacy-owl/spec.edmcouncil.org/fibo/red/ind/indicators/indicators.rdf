   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/publications/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/math/math/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relations/relations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/analytics"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/md/temporal-core/instrument-temporal-terms/"/>
   </owl:Ontology>


