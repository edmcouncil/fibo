<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-loan-typ-stu "http://spec.edmcouncil.org/fibo/red/loan/loan-types/student-loans/">
    <!ENTITY fibo-loan-typ-per "http://spec.edmcouncil.org/fibo/red/loan/loan-types/personal-loans/">
    <!ENTITY fibo-loan-loan-bt "http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loan-types/student-loans/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loan-types/student-loans/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loan-types/personal-loans/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-loan-typ-stu;hasBorrower">
      <rdfs:label>has borrower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-typ-per;hasBorrower.1"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-stu;StudentLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-per;PersonalLoanBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-stu;setOutTermsFor">
      <rdfs:label>set out terms for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-bt;setsOutTermsFor"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-stu;StudentLoanContract"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-stu;StudentLoan"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-loan-typ-stu;StudentBorrower">
      <rdfs:label>student borrower</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-loan-typ-per;PersonalLoanBorrower"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-stu;StudentLoan">
      <rdfs:label>student loan</rdfs:label>
      <skos:definition>A loan provided for the purposes of education.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;Loan"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-stu;StudentLoanContract">
      <rdfs:label>student loan contract</rdfs:label>
      <skos:definition>The contract for a Student Loan.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;LoanContract"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
