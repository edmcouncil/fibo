<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-loan-typ-sec "http://spec.edmcouncil.org/fibo/red/loan/loan-types/secured-loans/">
    <!ENTITY fibo-loan-loan-bt "http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/">
    <!ENTITY fibo-loan-loan-col "http://spec.edmcouncil.org/fibo/red/loan/loans/loans-collateral/">
    <!ENTITY fibo-loan-loan-gty "http://spec.edmcouncil.org/fibo/red/loan/loans/loans-guaranties/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loan-types/secured-loans/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loan-types/secured-loans/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-collateral/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-guaranties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-loan-typ-sec;HowSecuredSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Guaranty</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Collateral</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-loan-typ-sec;coveredBy">
      <rdfs:label>covered by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-typ-sec;CollateralizedSecuredLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-col;LoanCollateralAgreement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-sec;guaranteedBy">
      <rdfs:label>guaranteed by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-loan-gty;guaranteedBy"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-sec;GuaranteedLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-gty;LoanGuarantor"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-loan-typ-sec;howSecured">
      <rdfs:label>how secured</rdfs:label>
      <rdfs:range rdf:resource="&fibo-loan-typ-sec;HowSecuredSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-sec;secured">
      <rdfs:label>secured</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-sec;howSecured.1">
      <rdfs:label>how secured</rdfs:label>
      <rdfs:range rdf:resource="&fibo-loan-typ-sec;HowSecuredSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-sec;howSecured.2">
      <rdfs:label>how secured</rdfs:label>
      <rdfs:range rdf:resource="&fibo-loan-typ-sec;HowSecuredSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-typ-sec;secured.1">
      <rdfs:label>secured</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-loan-typ-sec;CollateralizedSecuredLoan">
      <rdfs:label>collateralized secured loan</rdfs:label>
      <skos:definition>Loan in which some asset is provided as collateral to the loan.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-typ-sec;SecuredLoan"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-sec;GuaranteedLoan">
      <rdfs:label>guaranteed loan</rdfs:label>
      <skos:definition>Loan secured by guaranty.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-loan-typ-sec;SecuredLoan">
      <rdfs:label>secured loan</rdfs:label>
      <skos:definition>A loan which is secured by some security in the form either of collateral or of guaranty.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-loan-loan-bt;Loan"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>