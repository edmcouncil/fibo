    <!ENTITY fibo-loan-loan-reg "http://spec.edmcouncil.org/fibo/red/loan/loans/loans-regulatory/">
    <!ENTITY fibo-loan-typ-mtg "http://spec.edmcouncil.org/fibo/red/loan/loan-types/mortgage-loans/">
    <!ENTITY fibo-loan-loan-bt "http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/">
    <!ENTITY fibo-loan-loan-col "http://spec.edmcouncil.org/fibo/red/loan/loans/loans-collateral/">
