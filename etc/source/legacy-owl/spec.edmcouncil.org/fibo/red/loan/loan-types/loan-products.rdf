    -->


<owl:ObjectProperty rdf:about="&fibo-loan-typ-lpr;isAbout">
      <rdfs:label>is about</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-loan-reg;LoanProductRepresentations"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-lpr;LoanProduct"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-lpr;becomes">
      <rdfs:label>becomes</rdfs:label>
      <skos:definition>If accepted by a potential borrower, the Loan Product is embodied as an actual loan on a given date and under given conditions.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-typ-lpr;LoanProduct"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;Loan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-lpr;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-typ-lpr;LoanProduct"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-bt;LoanContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-lpr;hasPreconditions">
      <rdfs:label>has preconditions</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-typ-lpr;LoanProduct"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-crf;LoanPrecedentConditions"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-lpr;becomes.1">
      <rdfs:label>becomes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-typ-lpr;becomes"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-lpr;MortgageProduct"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;MortgageLoan"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-lpr;defines.1">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-typ-lpr;defines"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-lpr;MortgageProduct"/>
      <rdfs:range rdf:resource="&fibo-loan-typ-mtg;MortgageLoanContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-typ-lpr;specifies">
      <rdfs:label>specifies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-loan-typ-lpr;LoanProduct"/>
      <rdfs:range rdf:resource="&fibo-loan-loan-col;LoanCollateral"/>
   </owl:ObjectProperty>


