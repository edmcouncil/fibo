         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/loan/loan-types/loan-products/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/loan/loan-types/loan-products/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/accounting-main/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-regulatory/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loan-types/mortgage-loans/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loan-basic-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/loan/loans/loans-collateral/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/credit-facilities/"/>
   </owl:Ontology>


