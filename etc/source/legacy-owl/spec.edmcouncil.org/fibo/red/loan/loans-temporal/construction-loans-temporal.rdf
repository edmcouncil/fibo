<owl:ObjectProperty rdf:about="&fibo-loan-tmp-con;loanAdvanceToDate">
      <rdfs:label>loan advance to date</rdfs:label>
      <skos:definition>The amount of principal advanced to date.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-loan-tmp-con;ConstructionLoanSnapshot"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-loan-tmp-con;hasSnapshot">
      <rdfs:label>has snapshot</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-loan-tmp-sts;hasSnapshot"/>
      <rdfs:domain rdf:resource="&fibo-loan-typ-con;ConstructionLoan"/>
      <rdfs:range rdf:resource="&fibo-loan-tmp-con;ConstructionLoanSnapshot"/>
   </owl:ObjectProperty>


