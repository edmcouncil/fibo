<owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ltv;informationDate">
      <rdfs:label>information date</rdfs:label>
      <skos:definition>The date at which this valuation was carried out.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-loan-tmp-ltv;value">
      <rdfs:label>value</rdfs:label>
