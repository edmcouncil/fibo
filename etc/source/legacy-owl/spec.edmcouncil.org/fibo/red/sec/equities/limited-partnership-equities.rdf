<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/equities/limited-partnership-equities/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/equities/limited-partnership-equities/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-restrictions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contractual-constructs/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/partnerships/partnerships/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-eq-lpe;limitedPartnerhipUnitHasHolderLimitedPartner">
      <rdfs:label>limited partnerhip unit has holder limited partner</rdfs:label>
      <skos:definition>The holder of the Limited Partnership Unit is a Limited Partner.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;heldBy"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-lpe;LimitedPartnershipUnit"/>
      <rdfs:range rdf:resource="&fibo-be-ptr-ptr;LimitedPartner"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-lpe;investorType">
      <rdfs:label>investor type</rdfs:label>
      <skos:definition>Type of investor that is allowed to hold the security.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-eq-lpe;LimitedPartnershipUnitRestriction"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-rst;InvestorType"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-lpe;limitedPartnershipUnitConfers">
      <rdfs:label>limited partnership unit confers</rdfs:label>
      <skos:definition>Voting rights conferred by the Limited Partnership Unit.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-cst;confers"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-lpe;LimitedPartnershipUnit"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-eqi;VotingRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-lpe;definesRestrictions">
      <rdfs:label>defines restrictions</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-rst;contractuallyDefinesRestrictions"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-lpe;LimitedPartnershipUnit"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-lpe;LimitedPartnershipUnitRestriction"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-eq-lpe;paymentStatus">
      <rdfs:label>payment status</rdfs:label>
      <skos:definition>Payment Status of the Unit.</skos:definition>
      <rdfs:range rdf:resource="&fibo-sec-eq-eqi;SecurityPaymentStatus"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-eq-lpe;restricted">
      <rdfs:label>restricted</rdfs:label>
      <skos:definition>Yes = Restricted; No = Free.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-eq-lpe;LimitedPartnershipUnit">
      <rdfs:label>limited partnership unit</rdfs:label>
      <skos:definition>A limited partnership is a form of partnership similar to a general partnership, except that in addition to one or more general partners (GPs) there are one or more limited partners (LPs).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;TransferableContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-eq-lpe;LimitedPartnershipUnitRestriction">
      <rdfs:label>limited partnership unit restriction</rdfs:label>
      <skos:definition>Restrictions on ownership or transfer of the Units.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-rst;SecurityContractualRestriction"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
