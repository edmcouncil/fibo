<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-eq-dr "http://spec.edmcouncil.org/fibo/red/sec/equities/depositary-receipts/">
    <!ENTITY fibo-be-fct-ban "http://spec.edmcouncil.org/fibo/red/be/functional-entities-ext/banks/">
    <!ENTITY fibo-sec-sec-sec "http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
    <!ENTITY fibo-sec-sec-iss "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/">
    <!ENTITY fibo-fnd-pty-pty "http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/">
    <!ENTITY fibo-fnd-law-jur "http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/">
    <!ENTITY fibo-sec-eq-eqi "http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/equities/depositary-receipts/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/equities/depositary-receipts/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/functional-entities-ext/banks/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/parties/parties/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-eq-dr;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-dr;DepositaryReceiptIssuer"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-dr;DepositaryBank"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-dr;authorizedUnder">
      <rdfs:label>authorized under</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-sec;legallyRecordedIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-dr;InternationalDepositaryReceipt"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-dr;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-eq-dr;InternationalDepositaryReceipt"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-eqi;PubliclyTradedShare"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-eq-dr;issuer">
      <rdfs:label>issuer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;issuer"/>
      <rdfs:domain rdf:resource="&fibo-sec-eq-dr;InternationalDepositaryReceipt"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-dr;DepositaryReceiptIssuer"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-eq-dr;multiplier">
      <rdfs:label>multiplier</rdfs:label>
      <skos:definition>The number of underlying shares (whether multiple or fractional) represented by a single Depositary Receipt.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-eq-dr;numberOfCertificatesIssued">
      <rdfs:label>number of certificates issued</rdfs:label>
      <skos:definition>The number of certificates issued to the general public.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-eq-dr;DepositaryBank">
      <rdfs:label>depositary bank</rdfs:label>
      <skos:definition>A bank which is set up and authorized to issue Depositary Receipts.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-be-fct-ban;Bank"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-eq-dr;DepositaryReceiptIssuer">
      <rdfs:label>depositary receipt issuer</rdfs:label>
      <skos:definition>The party which is the issuer of a Depository Receipt in some jurisdiciton.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-iss;SecurityIssuer"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-eq-dr;InternationalDepositaryReceipt">
      <rdfs:label>international depositary receipt</rdfs:label>
      <skos:definition>Certificate which represent ownership of an underlying Equity.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sec;ExchangeTradedSecurity"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>