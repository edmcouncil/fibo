<owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;identity.4"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;DebtInstrumentPoolConstituent"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;representsAnInterestIn">
      <rdfs:label>represents an interest in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;representsAnInterestInDebtPool"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;DebtInstrumentPoolEquity"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;DebtInstrumentPool"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;hasConstituent">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;hasConstituent.3"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;DebtInstrumentPool"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;DebtInstrumentPoolConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;identity.2"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;DebtPoolConstituent"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;DebtFinance"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;hasConstituent.1">
      <rdfs:label>has constituent</rdfs:label>
      <skos:definition>The credit which forms part of the pool. This is Debt from the point of view of the party to which the credit is extended, and credit from the point of view of a holder of the pool or some share in that pool.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;hasConstituent.2"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;DebtPool"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;DebtPoolConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;takesForm">
      <rdfs:label>takes form</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-own;asssetTakesForm"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;DebtPoolAsset"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;DebtPool"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;identity.2">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-coll;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;PoolConstituent"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;seenFromPerspectiveOf">
      <rdfs:label>seen from perspective of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-dbt;perspective"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;PoolDebt"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;PoolDebtor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;identity.3">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasIdentity"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;PoolDebtor"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;Pool"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;hasConstituent.2">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-coll;hasConstituent"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;Pool"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;PoolConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;sPVHoldsPool">
      <rdfs:label>s p v holds pool</rdfs:label>
      <skos:definition>A Special Purpose Vehicle always holds some kind of pool. This may be a debt pool, an asset pool (fund) or some pool of securities from which it issues Participation Notes.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-be-fct-spv;SpecialPurposeVehicle"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;Pool"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;identity.4">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;identity.2"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;SecuritiesPoolConstituent"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;hasConstituent.3">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;hasConstituent.2"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;SecuritiesPool"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;SecuritiesPoolConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;representsAnInterestInDebtPool">
      <rdfs:label>represents an interest in debt pool</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;representsAnInterestInPool"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;AssetPoolEquity"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;DebtPool"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-pls;representsAnInterestInPool">
      <rdfs:label>represents an interest in pool</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-aeq;equityRepresentsAnInterestIn"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-pls;PoolEquity"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-pls;Pool"/>
   </owl:ObjectProperty>


