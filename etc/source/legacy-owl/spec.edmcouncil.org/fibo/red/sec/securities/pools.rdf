  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-sec-pls;AssetPoolEquity">
      <rdfs:label>asset pool equity</rdfs:label>
      <skos:definition>Some share of the value of an asset pool, such as a debt pool.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;PoolEquity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;DebtInstrumentPool">
      <rdfs:label>debt instrument pool</rdfs:label>
      <skos:definition>A pool investment consisting of a collection of debt instruments, such as bonds.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;SecuritiesPool"/>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;DebtPool"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;DebtInstrumentPoolConstituent">
      <rdfs:label>debt instrument pool constituent</rdfs:label>
      <skos:definition>The Debt Instrument Pool Constituent is a Tradable Debt Security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;DebtPoolConstituent"/>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;SecuritiesPoolConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;DebtInstrumentPoolEquity">
      <rdfs:label>debt instrument pool equity</rdfs:label>
      <skos:definition>Some share of the value of an debt instrument pool, such as the pool underlying a CDO.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;AssetPoolEquity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;DebtPool">
      <rdfs:label>debt pool</rdfs:label>
      <skos:definition>A pool of debt, such as a pool of loans or mortgages.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;Pool"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;DebtPoolAsset">
      <rdfs:label>debt pool asset</rdfs:label>
      <skos:definition>An asset which combines a number of individual assets of the same kind into a pool. The pool will generally have information about it which is some aggregation of the corresponding information about the constituents of that pool, such as aggregated mortage paydown or default values. Comment: What about mobile homes?</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-own;Asset"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;DebtPoolConstituent">
      <rdfs:label>debt pool constituent</rdfs:label>
      <skos:definition>Something which makes up the debt in a Debt Pool.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;PoolConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;Pool">
      <rdfs:label>pool</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-uo-lat;Concrete"/>
      <rdfs:subClassOf rdf:resource="&fibo-rl-coll;Collection"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;PoolConstituent">
      <rdfs:label>pool constituent</rdfs:label>
      <skos:definition>Something which makes up one of the components of a pool.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-coll;Constituent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;PoolDebt">
      <rdfs:label>pool debt</rdfs:label>
      <skos:definition>Debt issued by some pool.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-dbt;Debt"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;PoolDebtor">
      <rdfs:label>pool debtor</rdfs:label>
      <skos:definition>The pool which holds the Pool Debt.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-dbt;Debtor"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;PoolEquity">
      <rdfs:label>pool equity</rdfs:label>
      <skos:definition>A share or proportion of the capital gains in some pool investment such as a fund or a debt asset pool.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-acc-aeq;Equity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;SecuritiesPool">
      <rdfs:label>securities pool</rdfs:label>
      <skos:definition>A pool of securities purchased by an SPV or other entity for the purpose of issuing notes against those securities.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-pls;SecuritiesPoolConstituent">
      <rdfs:label>securities pool constituent</rdfs:label>
      <skos:definition>The Securities Pool Constituent is a Tradable Security.</skos:definition>
   </owl:Class>


