<owl:ObjectProperty rdf:about="&fibo-sec-sec-bsk;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-coll;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-bsk;BasketConstituent"/>
      <rdfs:range rdf:resource="&fibo-uo-rl;IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-bsk;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-bsk;BasketOfIndicesConstituent"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;ReferenceIndex"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-bsk;hasConstituent">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;hasConstituent.2"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-bsk;BasketOfIndices"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-bsk;BasketOfIndicesConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-bsk;hasConstituent.1">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;hasConstituent.2"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-bsk;BasketOfSecurities"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-bsk;SecuritiesBasketConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-bsk;hasConstituent.2">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-rl-coll;hasConstituent"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-bsk;Basket"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-bsk;BasketConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-bsk;hasConstituent.3">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;hasConstituent.2"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-bsk;MixedBasket"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-bsk;.UnionClass.3"/>
