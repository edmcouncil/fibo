    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
    <!ENTITY fibo-sec-sec-lst "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-listings/">
