<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/relatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/relative/mereological/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/info-core/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/publications/conventions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-listings/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contracts-extensions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/languages/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agents-and-people/agents/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/law/jurisdictions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/security-assets/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-sec-sec-sec;Optionality">
      <skos:definition>Whether something is Mandatory or Optional.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Optional</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mandatory</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-sec-sec-sec;SecurityDeliveryTypeSelection">
      <skos:definition>Type of delivery as used in settlement terms for a financial security.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Other</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cash</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-sec-sec;deemedRiskCurrency">
      <rdfs:label>deemed risk currency</rdfs:label>
      <skos:definition>The currency in which any risks relating to the instrument are seen to take place. Further notes: This is a risk specific term identified during SME Reviews of common securities terms. It was agreed at a review with a risk firm that this term is applicable within the context of risk management only and is not a fundamental fact about the security. It therefore needs to be disposed in some way to show it in the context of risk.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sec;denominationCurrency">
      <rdfs:label>denomination currency</rdfs:label>
      <skos:definition>The default currency in which units of the Security are denominated.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sec;hasStructuredName">
      <rdfs:label>has structured name</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;SecurityStructuredName"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sec;legallyRecordedIn">
      <rdfs:label>legally recorded in</rdfs:label>
      <skos:definition>Jurisdiction (country, county, state, province, city) in which the security is legally recorded for regulatory and/or tax purposes.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;hasGoverningJurisdiction"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sec;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;contractCounterpartyIdentity"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-ast;SecurityHolder"/>
      <rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-sec;hasDefaultSettlementTerms">
      <rdfs:label>has default settlement terms</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;SecuritySettlementConvention"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;bearer">
      <rdfs:label>bearer</rdfs:label>
      <skos:definition>The owner is not registered in the books of the issuer or of the registrar.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;denominationIncrement">
      <rdfs:label>denomination increment</rdfs:label>
      <skos:definition>The multiples above the smallest possible denomination (Default Lot Size) in which the security can be held.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;fASTApplicable">
      <rdfs:label>f a s t applicable</rdfs:label>
      <skos:definition>Whether the security is to be held at the transfer agent as part of the FAST (Fully Automated Securities Transfer) program. Further notes: Term is specific to DTCC / US context. Is there a more generic meaning similar to this which applies across different jurisdictions? If not this term should be defined with reference to this specific context.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;fractionalEligible">
      <rdfs:label>fractional eligible</rdfs:label>
      <skos:definition>Whether the security can contain Fractional Share/amount component.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;interchangeable">
      <rdfs:label>interchangeable</rdfs:label>
      <skos:definition>Whether or not the security is freely interchangeable with other of the same securities. Yes indicates that the security is interchangeable; no indicates that it is not.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;issueDate">
      <rdfs:label>issue date</rdfs:label>
      <skos:definition>Date that the Security was issued.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;listed">
      <rdfs:label>listed</rdfs:label>
      <skos:definition>Whether or not the security is listed on some exchange or trading facility.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;registered">
      <rdfs:label>registered</rdfs:label>
      <skos:definition>Securities are recorded in the name of the owner on the books of the issuer or the issuer's registrar.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;fullName">
      <rdfs:label>full name</rdfs:label>
      <skos:definition>Full name of the security</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;language">
      <rdfs:label>language</rdfs:label>
      <skos:definition>Language in which the security name is expressed.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-info-lang;LanguageSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;shortName">
      <rdfs:label>short name</rdfs:label>
      <skos:definition>Short name of the security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;allowsContractualSettlement">
      <rdfs:label>allows contractual settlement</rdfs:label>
      <skos:definition>Whether the market allows contractual settlement. This is a settlement term related to the market.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;preferredSettlementCurrency">
      <rdfs:label>preferred settlement currency</rdfs:label>
      <skos:definition>Preferred currency in which the instrument is to be settled. Securities may be settled in a currency other than this.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-sec-sec;settlementBusinessDays">
      <rdfs:label>settlement business days</rdfs:label>
      <skos:definition>Default number of business days for settlement e.g. T+1, T+3 and so on.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-sec-sec;.UnionClass.6">
      <skos:definition>Partial payment on the scheduled dates, expressed as an amount or as a percentage (but not both).</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-fnd-acc-cur;MoneyAmount"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-sec;ExchangeTradedSecurity">
      <rdfs:label>exchange traded security</rdfs:label>
      <skos:definition>Tradable security traded on an exchange or other trading venue.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sec;TradingRegisteredSecurity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-sec;Security">
      <rdfs:label>security</rdfs:label>
      <skos:definition>A financial instrument which can be bought or sold. This may be traded either over the counter or through an exchange or other trading venue.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;TransferableContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-sec;SecurityStructuredName">
      <rdfs:label>security structured name</rdfs:label>
      <skos:definition>The formal, structured name of a Security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-info;StructuredName"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-sec;SecurityNote">
      <rdfs:label>security note</rdfs:label>
      <skos:definition>An individually denominated note within a security or tranche of a security, where this is issued in notes of different denominations.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-rl-mer;Part"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-sec;SecuritySettlementConvention">
      <rdfs:label>security settlement convention</rdfs:label>
      <skos:definition>Default Terms for the settlement of a Security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sec;SettlementConvention"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-sec;SettlementConvention">
      <rdfs:label>settlement convention</rdfs:label>
      <skos:definition>A convention for settlement of a trade or transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-pub-con;Convention"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-sec;Stake">
      <rdfs:label>stake</rdfs:label>
      <skos:definition>The holding of some portion of some venture or something of value, by some party. The stake may be represented by some form of formal document such as a share or some debt unit.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>


