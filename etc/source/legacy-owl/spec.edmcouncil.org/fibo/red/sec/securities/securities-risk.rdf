<owl:ObjectProperty rdf:about="&fibo-sec-sec-rsk;countryOfRisk">
      <rdfs:label>country of risk</rdfs:label>
      <skos:definition>The country where any risks are inherent in the hierarcrhy that the issuer is part of.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-sec-iss;SecurityIssuer"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rsk;countryOfEconomicRisk">
      <rdfs:label>country of economic risk</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-rsk;countryOfRisk"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rsk;countryOfPoliticalRisk">
      <rdfs:label>country of political risk</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-rsk;countryOfRisk"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-rsk;incomeSourceCountry">
      <rdfs:label>income source country</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-cty;Country"/>
   </owl:ObjectProperty>


