<owl:ObjectProperty rdf:about="&fibo-sec-sec-cls;hasClassification">
      <rdfs:label>has classification</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-fi;FinancialInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-cls;SecurityClassification"/>
      <owl:inverseOf rdf:resource="&fibo-sec-sec-cls;classifies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-cls;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;classificationSchemeDefines"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-cls;SecurityClassificationScheme"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-cls;SecurityClassification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-cls;classifies">
      <rdfs:label>classifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;classifies"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-cls;SecurityClassification"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-fi;FinancialInstrument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-cls;classifies.1">
      <rdfs:label>classifies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;classifies"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-cls;IndustrySectorClassification"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-cls;hasClassification.1">
      <rdfs:label>has classification</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-cls;hasClassification"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-cls;SecurityClassification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-sec-cls;hasSectorClassification">
      <rdfs:label>has sector classification</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-cls;hasClassification"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-cls;IndustrySectorClassification"/>
   </owl:ObjectProperty>


