      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-id;ProprietarySecurityIdentificationCode"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-ind;SEDOLCode">
      <rdfs:label>s e d o l code</rdfs:label>
