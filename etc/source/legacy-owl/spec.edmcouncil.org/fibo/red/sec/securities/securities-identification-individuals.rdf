<owl:Class rdf:about="&fibo-sec-sec-ind;ISINIdentifier">
      <rdfs:label>i s i n identifier</rdfs:label>
      <skos:definition>An identifier defined under the ISO 6166 standard.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-id;SecurityIdentifier"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-sec-ind;RICCode">
      <rdfs:label>r i c code</rdfs:label>
