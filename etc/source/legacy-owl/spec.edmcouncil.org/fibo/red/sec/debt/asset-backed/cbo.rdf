<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-dbt-ab-cbo "http://spec.edmcouncil.org/fibo/red/sec/debt/asset-backed/cbo/">
    <!ENTITY fibo-sec-sec-pls "http://spec.edmcouncil.org/fibo/red/sec/securities/pools/">
    <!ENTITY fibo-sec-dbt-bond-bond "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-common/">
    <!ENTITY fibo-sec-dbt-ab-cdo "http://spec.edmcouncil.org/fibo/red/sec/debt/asset-backed/cdo/">
    <!ENTITY fibo-sec-dbt-ab-pbs "http://spec.edmcouncil.org/fibo/red/sec/debt/asset-backed/pool-backed-securities/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/debt/asset-backed/cbo/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/debt/asset-backed/cbo/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/pools/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-common/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/asset-backed/cdo/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/asset-backed/pool-backed-securities/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cbo;hasConstituent">
      <rdfs:label>has constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-pls;hasConstituent"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cbo;BondPool"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-bond;Bond"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cbo;isTrancheOf">
      <rdfs:label>is tranche of</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-cdo;isTrancheOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cbo;CBOInstrument"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cbo;BondPool"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-dbt-ab-cbo;BondPool">
      <rdfs:label>bond pool</rdfs:label>
      <skos:definition>A pool investment consisting of a collection of Bonds.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-pls;DebtInstrumentPool"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cbo;CBODeal">
      <rdfs:label>c b o deal</rdfs:label>
      <skos:definition>An issue of Collateralized Bond Obligation notes.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-pbs;CollateralizedObligationDeal"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cbo;CBOInstrument">
      <rdfs:label>c b o instrument</rdfs:label>
      <skos:definition>A multitranche debt structure similar in some respects to a collateralized mortgage obligation (CMO) structure. Typically low-rated bonds rather than mortgages serve as the collateral. The organization creating and promoting the structure usually holds the underlying equity and may also collect a fee.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cdo;CashCDOTranche"/>
   </owl:Class>


  <!--
