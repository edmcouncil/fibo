<rdfs:Datatype rdf:about="&fibo-sec-dbt-dbt-iss;DebtIssuancePurpose">
      <skos:definition>Purpose for the Issuance of a Debt Security. Also defines processes to be followed in some instances.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Advanced Refunding Method</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remarketing</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Refunding</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">New Money</rdf:first>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


