<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-iss;oID">
      <rdfs:label>o i d</rdfs:label>
      <skos:definition>Discount from par value at the time a bond or other debt instrument is issued.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-iss;debtIssuancePurpose">
      <rdfs:label>debt issuance purpose</rdfs:label>
      <skos:definition>Purpose of the Issue of a Debt Security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-iss;bankQualified">
      <rdfs:label>bank qualified</rdfs:label>
      <skos:definition>Designation given to a public purpose bond offering by the issuer if it reasonably expects to issue in the calendar year of such offering no more than $10 million par amount of bonds of the type required to be included in making such calculation under the Internal Revenue Code.  When purchased by a commercial bank for its portfolio, the bank may receive an 80% tax deduction for the interest cost of carry for the issue.  A bond that is bank qualified is also known as a qualified tax-exempt obligation.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-iss;debtIssuancePurpose.2">
      <rdfs:label>debt issuance purpose</rdfs:label>
      <skos:definition>Purpose of the Issue of a Municipal Debt Security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-iss;DebtIssuancePurpose"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-iss;bankQualified.1">
      <rdfs:label>bank qualified</rdfs:label>
      <skos:definition>Designation given to a public purpose bond offering by the issuer if it reasonably expects to issue in the calendar year of such offering no more than $10 million par amount of bonds of the type required to be included in making such calculation under the Internal Revenue Code.  When purchased by a commercial bank for its portfolio, the bank may receive an 80% tax deduction for the interest cost of carry for the issue.  A bond that is bank qualified is also known as a qualified tax-exempt obligation.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-iss;debtIssuancePurpose.3">
      <rdfs:label>debt issuance purpose</rdfs:label>
      <skos:definition>Purpose of the Issue of a Municipal Debt Security.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-iss;purpose">
      <rdfs:label>purpose</rdfs:label>
      <skos:definition>The purpose for which the SPV was set up, which is to issue securities.</skos:definition>
      <rdfs:range rdf:resource="&fibo-be-fct-spv;SPVPurposeSelection"/>
   </owl:DatatypeProperty>


