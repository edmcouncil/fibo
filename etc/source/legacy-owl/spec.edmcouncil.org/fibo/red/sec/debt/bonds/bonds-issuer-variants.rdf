<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-dbt-bond-iss "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-issuer-variants/">
    <!ENTITY fibo-sec-sec-iss "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/">
    <!ENTITY fibo-sec-dbt-bond-bond "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-common/">
    <!ENTITY fibo-be-corp-corp "http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-be-fct-se "http://spec.edmcouncil.org/fibo/red/be/functional-entities-ext/state-entities/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-issuer-variants/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-issuer-variants/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-common/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/corporations/corporations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/functional-entities-ext/state-entities/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-iss;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-iss;CorporateBondIssuer"/>
      <rdfs:range rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-iss;issuer">
      <rdfs:label>issuer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;issuer"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-iss;CorporateBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-iss;CorporateBondIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-iss;issuer.1">
      <rdfs:label>issuer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;issuer"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-iss;GovernmentBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-iss;GovernmentDebtIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-iss;identifiedAs.1">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-iss;GovernmentDebtIssuer"/>
      <rdfs:range rdf:resource="&fibo-be-fct-se;NationalGovernment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-iss;identifiedAs.2">
      <rdfs:label>identified as</rdfs:label>
      <skos:definition>The Issuer of a Municipal Debt is a Municipality, which is identified as a State Actor governing on the part of a Municipal Area or City. This is equivalent to a Legal Entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;identity"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-iss;MunicipalDebtIssuer"/>
      <rdfs:range rdf:resource="&fibo-be-fct-se;MunicipalGovernment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-iss;issuer.2">
      <rdfs:label>issuer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-iss;issuer"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-iss;MunicipalBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-iss;MunicipalDebtIssuer"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-bond-iss;legalOpinion">
      <rdfs:label>legal opinion</rdfs:label>
      <skos:definition>Whether legal opinion exists for municipal securities.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-dbt-bond-iss;CorporateBond">
      <rdfs:label>corporate bond</rdfs:label>
      <skos:definition>A debt security issued by a corporation, as opposed to those issued by a government or municipality.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-bond;Bond"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-iss;CorporateBondIssuer">
      <rdfs:label>corporate bond issuer</rdfs:label>
      <skos:definition>A Company or Corporation, acting in the capacity of an Issuer.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-iss;SecurityIssuer"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-iss;GovernmentBond">
      <rdfs:label>government bond</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-iss;GovernmentDebtIssuer">
      <rdfs:label>government debt issuer</rdfs:label>
      <skos:definition>A national Government, acting in the capacity of an Issuer.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-iss;MunicipalBond">
      <rdfs:label>municipal bond</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-iss;MunicipalDebtIssuer">
      <rdfs:label>municipal debt issuer</rdfs:label>
      <skos:definition>A Municipality, acting in the capacity of an Issuer. The Issuer of a Municipal Debt is a Municipality, which is identified as a State Actor governing on the part of a Municipal Area or City. This is equivalent to a Legal Entity.</skos:definition>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>