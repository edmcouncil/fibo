<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-gty;policyDetails">
      <rdfs:label>policy details</rdfs:label>
      <skos:definition>Details of the Insurance Policy which make up this Guaranty.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-dbt-gty;loCDetails">
      <rdfs:label>lo c details</rdfs:label>
      <skos:definition>Details of the Letter of Credit</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
   </owl:DatatypeProperty>


