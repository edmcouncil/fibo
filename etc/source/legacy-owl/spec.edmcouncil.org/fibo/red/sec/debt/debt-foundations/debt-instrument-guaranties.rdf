<owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;hasCollateral">
      <rdfs:label>has collateral</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;takesFormOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;CollateralizedGuaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-dbt;Collateral"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;hasGuarantor">
      <rdfs:label>has guarantor</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;hasGuarantor"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;GovernmentGuaranty"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-gty;StateGuarantor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;form">
      <rdfs:label>form</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;takesFormOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;InsuranceBackedGuaranty"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-gty;SecuritiesGuarantyInsurancePolicy"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;hasGuarantor.1">
      <rdfs:label>has guarantor</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;hasGuarantor"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;JointGuaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-gty;Guarantor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;lien">
      <rdfs:label>lien</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;takesFormOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;LienGuaranty"/>
      <rdfs:range rdf:resource="&fibo-fnd-plc-loc;RealEstate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;form.1">
      <rdfs:label>form</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;takesFormOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;LetterOfCreditGuaranty"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-gty;LetterOfCredit"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;issuedBy">
      <rdfs:label>issued by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasPartyInRole"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;LetterOfCredit"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-dbt-gty;LetterOfCreditIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-dbt-gty;identifiedAs">
      <rdfs:label>identified as</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-gty;guarantorIdentity"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-dbt-gty;StateGuarantor"/>
      <rdfs:range rdf:resource="&fibo-be-et-sov;Sovereign"/>
   </owl:ObjectProperty>


