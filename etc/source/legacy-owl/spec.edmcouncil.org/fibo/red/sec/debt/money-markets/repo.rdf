<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-dbt-mm-repo "http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/repo/">
    <!ENTITY fibo-fnd-agrx-ext "http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contracts-extensions/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
    <!ENTITY fibo-fnd-agr-ctr "http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/repo/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/sec/debt/money-markets/repo/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contracts-extensions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements/contracts/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-repo;repurchaseAgreeementHasLender">
      <rdfs:label>repurchase agreeement has lender</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-mm-repo;repurchaseAgreementHasParty"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-repo;RepurchaseAgreement"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-mm-repo;RepoLender"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-repo;period">
      <rdfs:label>period</rdfs:label>
      <skos:definition>The duration of the repurchase agreement, where there is one.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-repo;RepurchaseAgreement"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-repo;repurchaseAgreementHasParty">
      <rdfs:label>repurchase agreement has party</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-ext;hasParty"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-repo;RepurchaseAgreement"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-mm-repo;RePoParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-repo;definedFromPerspectiveOfRepoBorrower">
      <rdfs:label>defined from perspective of repo borrower</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-repo;LiabilityRepurchaseAgreement"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-mm-repo;RepoBorrower"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-repo;definedFromPerspectiveOfRepoLender">
      <rdfs:label>defined from perspective of repo lender</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-repo;AssetRepurchaseAgreement"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-mm-repo;RepoLender"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-mm-repo;repurchaseAgreementHasBorrower">
      <rdfs:label>repurchase agreement has borrower</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-mm-repo;repurchaseAgreementHasParty"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-mm-repo;RepurchaseAgreement"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-mm-repo;RepoBorrower"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-mm-repo;openEnded">
      <rdfs:label>open ended</rdfs:label>
      <skos:definition>Whether the repo is open ended.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-dbt-mm-repo;AssetRepurchaseAgreement">
      <rdfs:label>asset repurchase agreement</rdfs:label>
      <skos:definition>A Repurchase Agreement defined from the point of view of the lender, where it is seen as an asset repo.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-mm-repo;RepurchaseAgreement"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-mm-repo;LiabilityRepurchaseAgreement">
      <rdfs:label>liability repurchase agreement</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-mm-repo;RePoParty">
      <rdfs:label>re po party</rdfs:label>
      <skos:definition>A party to a repurchase agreement.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agrx-ext;ContractParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-mm-repo;RepoBorrower">
      <rdfs:label>repo borrower</rdfs:label>
      <skos:definition>The borrower in the repurchase agreement.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-mm-repo;RePoParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-mm-repo;RepoLender">
      <rdfs:label>repo lender</rdfs:label>
      <skos:definition>The lender in the repurchase agreement.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-mm-repo;RepurchaseAgreement">
      <rdfs:label>repurchase agreement</rdfs:label>
      <skos:definition>A repurchase agreement (or repo) is an agreement between two parties whereby one party lends the other a security at a specified price with a commitment to take the security back at a later date for another specified price.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;BilateralContract"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>