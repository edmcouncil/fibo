<owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfl;periodicAmortizationAmount">
      <rdfs:label>periodic amortization amount</rdfs:label>
      <skos:definition>The monetary amount of each amortization payment in the Amortization Schedule.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfl;BondAmortizationPaymentTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfl;lockoutPeriod">
      <rdfs:label>lockout period</rdfs:label>
      <skos:definition>The lockout period refers to the amount of time for which a callable security cannot be called and only interest coupon payments are received by the security holder. For example, with a 10-year noncall 3-year ("10nc3") debt security, the security cannot be called for the first three years.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfl;BondCallTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;DatePeriod"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfl;premium">
      <rdfs:label>premium</rdfs:label>
      <skos:definition>A premium paid to the bond owner when the bond is called.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-cfl;BondCallTermsSet"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-cfl;couponPaymentCurrency">
