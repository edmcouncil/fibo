<rdfs:Datatype rdf:about="&fibo-sec-dbt-bond-cfl;AmortizationType">
      <skos:definition>The different types of principal payments applicable to a debt security.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Accreting</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Principal Plus Interest</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Principal Only</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Interest Only</rdf:first>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-sec-dbt-bond-cfl;RateBasisType">
      <skos:definition>The type of basis for a percentage quoted figure, as used in specifying Bond Call percentage amounts. REVIEW: Does this same meaningful term have wider applicability?</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Percentage Par</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Percentage CAV</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


