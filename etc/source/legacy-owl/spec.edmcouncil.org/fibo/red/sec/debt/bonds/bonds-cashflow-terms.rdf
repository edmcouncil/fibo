   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;IndexLinkedPrincipalDeterminationTermsSet">
      <rdfs:label>index linked principal determination terms set</rdfs:label>
      <skos:definition>Terms for payment of the principal. These link the principal amounts to some underlying index or interest rate.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-cfl;BondAmortizationPaymentTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;InflationBondVariableCouponTermsSet">
      <rdfs:label>inflation bond variable coupon terms set</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;MandatorySinkingFundSchedule">
      <rdfs:label>mandatory sinking fund schedule</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;MandatorySinkingFundScheduleListing">
      <rdfs:label>mandatory sinking fund schedule listing</rdfs:label>
      <skos:definition>A full schedule of dates and payments for a Mandatory Sinking Fund style of Call.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;MandatorySinkingFundTermsSet">
      <rdfs:label>mandatory sinking fund terms set</rdfs:label>
      <skos:definition>Terms relating to a Mandatory Sinking Fund call of the issue.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-cfl;SinkingFundAmortizationTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;PartialCallFeature">
      <rdfs:label>partial call feature</rdfs:label>
      <skos:definition>These are terms whereby the issuer can recall part of the issue on scheduled dates but there is no specific amount defined for those dates. Consenus:Yes</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-cfl;BondCallTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;PrincipalPaymentCalculationFormula">
      <rdfs:label>principal payment calculation formula</rdfs:label>
      <skos:definition>A formula for calculation of principal payments.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;Formula"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;PrincipalPaymentFormulaExpression">
      <rdfs:label>principal payment formula expression</rdfs:label>
      <skos:definition>The expression of the formula by which the payment of principal is formally described.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;FormulaExpression"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;RedemptionPayment">
      <rdfs:label>redemption payment</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;RedemptionPaymentDate">
      <rdfs:label>redemption payment date</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;RedemptionSchedule">
      <rdfs:label>redemption schedule</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;RedemptionScheduleListing">
      <rdfs:label>redemption schedule listing</rdfs:label>
      <skos:definition>Schedule for payments of defined amounts of the Principal on a Debt Security.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;ScheduledCouponPayment">
      <rdfs:label>scheduled coupon payment</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;ScheduledCouponPaymentDate">
      <rdfs:label>scheduled coupon payment date</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;SingleCouponTermsSet">
      <rdfs:label>single coupon terms set</rdfs:label>
      <skos:definition>A set of coupon terms which remain the same through the life of the Bond. This is the default and is the case when there is not a Step Coupon or Conditional Coupon.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;SinkingFundAmortizationTermsSet">
      <rdfs:label>sinking fund amortization terms set</rdfs:label>
      <skos:definition>Terms for the paydown of principal in a sinking fund type of amortizing security.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;StepDate">
      <rdfs:label>step date</rdfs:label>
      <skos:definition>The date from which the next set of coupon terms specified in the Step Schedule takes effect.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;StepEvent">
      <rdfs:label>step event</rdfs:label>
      <skos:definition>The event whereby a set of fixed coupon terms comes into force as specified in the Step Schedule.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;StepSchedule">
      <rdfs:label>step schedule</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;StepScheduleListing">
      <rdfs:label>step schedule listing</rdfs:label>
      <skos:definition>A schedule of dates and interest rates which take effect on those dates. At present these are assumed to always be ficxed interest rates, and are modeled as such.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;SteppedCouponTermsSet">
      <rdfs:label>stepped coupon terms set</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;VariableInterestCalculationFormula">
      <rdfs:label>variable interest calculation formula</rdfs:label>
      <skos:definition>Formula for the calculation of variable interest.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;VariableInterestExpression">
      <rdfs:label>variable interest expression</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;ZeroCouponBondCall">
      <rdfs:label>zero coupon bond call</rdfs:label>
      <skos:definition>Call of a Zero Coupon bond.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-cfl;CallOfIssue"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;ZeroCouponTermsSet">
      <rdfs:label>zero coupon terms set</rdfs:label>
      <skos:definition>Terms for payment of Interest on a Bond, where this pays a zero rate of interest.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-cfl;DebtFixedCouponTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;ZeroCouponAndOIDBondCallTermsSet">
      <rdfs:label>zero coupon and o i d bond call terms set</rdfs:label>
      <skos:definition>Terms defining the Call options for a Zero Coupon or OID Bond.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;FirstRegularCouponDate">
      <rdfs:label>first regular coupon date</rdfs:label>
      <skos:definition>The scheduled date of the first regular coupon payment, that is the start of payments according to a regular repeating series of coupon periods, after any non standard (long or short) first coupon period in the schedule.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-tim-tim;ContractuallyDeterminedDate"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;LastRegularCouponDate">
      <rdfs:label>last regular coupon date</rdfs:label>
      <skos:definition>The scheduled date of the last regular coupon payment, that is the last of the payments that are made according to a regular repeating series of coupon periods, before any non standard (long or short) last coupon period in the schedule.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-cfl;StubLength">
      <rdfs:label>stub length</rdfs:label>
      <skos:definition>The length of the Stub Period as a temporal duration (a number of days).</skos:definition>
   </owl:Class>


