<owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupport">
      <rdfs:label>provides prepayment support</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyCMO"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyCMO"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;specifiesTrigger">
      <rdfs:label>specifies trigger</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyJumpTranche"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;JumpZTriggerEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;revertsOn">
      <rdfs:label>reverts on</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyRegularJumpZ"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;JumpZTriggerEventReversal"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;hasUnderlyingPool">
      <rdfs:label>has underlying pool</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-cdo;isTrancheOf"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyCMO"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-mbs;AgencyMBSPool"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;specifiesReverseTrigger">
      <rdfs:label>specifies reverse trigger</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;JumpZTriggerEventReversal"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;JumpZTriggerEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;supportedBy">
      <rdfs:label>supported by</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;PlannedAmortizationClassBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PlannedAmortizationClassBond"/>
      <owl:inverseOf rdf:resource="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;specifies">
      <rdfs:label>specifies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;PlannedAmortizationClassBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PACTrancheAmortizationSchedule"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor">
      <rdfs:label>provides prepayment support for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupport"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;PlannedAmortizationClassBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PlannedAmortizationClassBond"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;takesPrepaymentAfter">
      <rdfs:label>takes prepayment after</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-cmo;supportedBy"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-1Class"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-2Class"/>
      <owl:inverseOf rdf:resource="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;SupportTranche"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-2Class"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor.1">
      <rdfs:label>provides prepayment support for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-2Class"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-1Class"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;takesPrepaymentAfter.1">
      <rdfs:label>takes prepayment after</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-cmo;supportedBy"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-2Class"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-3Class"/>
      <owl:inverseOf rdf:resource="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor.2"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor.2">
      <rdfs:label>provides prepayment support for</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-dbt-ab-cmo;providesPrepaymentSupportFor"/>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-3Class"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;PAC-2Class"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-sec-dbt-ab-cmo;specifies.1">
      <rdfs:label>specifies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-ab-cmo;TACTranche"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-cmo;TACTrancheAmortizationSchedule"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-ab-cmo;trancheType">
      <rdfs:label>tranche type</rdfs:label>
      <skos:definition>The type of tranche for the CMO. Many different structures are used in practice, including stable PAC bonds or risky IOs and POs. There are floaters and inverse floaters. There are also Z-bonds, which are analogous to zero-coupon bonds.</skos:definition>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-mbs;MBSTrancheTypeSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-ab-cmo;maximumPrepaymentSpeed">
      <rdfs:label>maximum prepayment speed</rdfs:label>
      <skos:definition>The prepayment range under which the PAC schedule must remain, in PSA.</skos:definition>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-mbs;PaymentSpeed"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-ab-cmo;pricingSpeed">
      <rdfs:label>pricing speed</rdfs:label>
      <skos:definition>The prepayment speed specified fo the TAC Tranche at the outset of the deal, in PSA.</skos:definition>
      <rdfs:range rdf:resource="&fibo-sec-dbt-ab-mbs;PaymentSpeed"/>
   </owl:DatatypeProperty>


