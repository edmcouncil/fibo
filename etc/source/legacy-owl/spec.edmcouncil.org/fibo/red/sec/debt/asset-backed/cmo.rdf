  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyCMO">
      <rdfs:label>agency c m o</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-pbs;CollateralizedObligationInstrument"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyIOTranche">
      <rdfs:label>agency i o tranche</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyCMO"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyJumpTranche">
      <rdfs:label>agency jump tranche</rdfs:label>
      <skos:definition>A tranche where if there is some sort of trigger event reached then the holders of the tranche will begin to receive payments.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyJumpZTranche">
      <rdfs:label>agency jump z tranche</rdfs:label>
      <skos:definition>A Jump Z tranche is like a Z tranche but if there is some sort of trigger event reached then the holders of the Jump Z tranche will begin to receive payments. Regular non-Sticky Jump Z tranches maintain their changed status only while the trigger event is in effect, and revert to their old payment status once the trigger event has passed.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyJumpTranche"/>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyZTranche"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyPOTranche">
      <rdfs:label>agency p o tranche</rdfs:label>
      <skos:definition>Principal Only tranche. This tranche will only pay principal.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyRegularJumpZ">
      <rdfs:label>agency regular jump z</rdfs:label>
      <skos:definition>Regular non-Sticky Jump Z tranches maintain their changed status only while the trigger event is in effect, and revert to their old payment status once the trigger event has passed.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyJumpZTranche"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyStickyJumpZ">
      <rdfs:label>agency sticky jump z</rdfs:label>
      <skos:definition>"Sticky" Jump Z tranches maintain the payment priority of a Jump Z tranche until they are retired.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;AgencyZTranche">
      <rdfs:label>agency z tranche</rdfs:label>
      <skos:definition>A tranche that does not receive payments while other tranches remain.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;CMODeal">
      <rdfs:label>c m o deal</rdfs:label>
      <skos:definition>An issue of Collateralized mortgaged Obligations.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-pbs;CollateralizedObligationDeal"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;IOette">
      <rdfs:label>i oette</rdfs:label>
      <skos:definition>REVIEW: Is this a kind of IO Tranche.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyIOTranche"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;JumpZTriggerEvent">
      <rdfs:label>jump z trigger event</rdfs:label>
      <skos:definition>The event which triggers the Jump Z</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;TriggerEvent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;JumpZTriggerEventReversal">
      <rdfs:label>jump z trigger event reversal</rdfs:label>
      <skos:definition>The reversal of the event which triggers the Jump Z</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;PACPOTranche">
      <rdfs:label>p a c p o tranche</rdfs:label>
      <skos:definition>Planned Amortization Class principal only tranche.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;PlannedAmortizationClassBond"/>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-cmo;AgencyPOTranche"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;PACTrancheAmortizationSchedule">
      <rdfs:label>p a c tranche amortization schedule</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-cfl;RedemptionSchedule"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;PAC-1Class">
      <rdfs:label>p a c-1 class</rdfs:label>
      <skos:definition>Planned Amortization Class tranche. PAC-1 is the most senior Planned Amortization Class tranche.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;PAC-2Class">
      <rdfs:label>p a c-2 class</rdfs:label>
      <skos:definition>Planned Amortization Class tranche. PAC-2 refers to a support tranche that is given a scheduled payment structure like a PAC bond.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;PAC-3Class">
      <rdfs:label>p a c-3 class</rdfs:label>
      <skos:definition>Planned Amortization Class tranche. Additional support tranche with scheduled payments.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;PAC-ZTranche">
      <rdfs:label>p a c- z tranche</rdfs:label>
      <skos:definition>Planned Amortization Class tranche which is also a Z tranche, that is Definition needed.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;PlannedAmortizationClassBond">
      <rdfs:label>planned amortization class bond</rdfs:label>
      <skos:definition>Planned Amortization Class tranche.This is a tranche where the principal payment must follow a certain schedule.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;SuperPOTranche">
      <rdfs:label>super p o tranche</rdfs:label>
      <skos:definition>Principal Only tranche. This tranche will only pay principal. Not clear how this is distinct from generic PO.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;SupportTranche">
      <rdfs:label>support tranche</rdfs:label>
      <skos:definition>A tranche which provides payment support to a PAC Tranche.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-uo-rl;RelativeThing"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;TACTranche">
      <rdfs:label>t a c tranche</rdfs:label>
      <skos:definition>Targeted Amortization Class. This is related to a PAC tranche and has a payment schedule geared towards a specified prepayment speed (called the pricing speed). Agency CMO</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-ab-mbs;TranchedMBSInstrument"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;TACTrancheAmortizationSchedule">
      <rdfs:label>t a c tranche amortization schedule</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-ab-cmo;TriggerEvent">
      <rdfs:label>trigger event</rdfs:label>
      <skos:definition>Any event where some value passes some threashold. Or some other type of business event. This is not restricted to "trigger" in the sense of a value passing a threshold. Can also be an seen such as a CDO manager going into bankruptcy.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-occ-evt;Event"/>
   </owl:Class>


