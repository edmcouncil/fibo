<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-sec-dbt-bond-strip "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/strips/">
    <!ENTITY fibo-sec-dbt-dbt-ins "http://spec.edmcouncil.org/fibo/red/sec/debt/debt-foundations/debt-instruments/">
    <!ENTITY fibo-sec-dbt-bond-bond "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-common/">
