    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-sec-dbt-bond-strip;isStripOfUnderlying">
      <rdfs:label>is strip of underlying</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-dbt-bond-strip;STRIP"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-bond;Bond"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-sec-dbt-bond-strip;interestStrip">
      <rdfs:label>interest strip</rdfs:label>
      <skos:definition>The STRIP instrument is a strip of the Interest of the underlying Debt Instrument.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-bond-strip;principalStrip">
      <rdfs:label>principal strip</rdfs:label>
      <skos:definition>The STRIP instrument is not a strip of the Principal of the underlying Debt Instrument.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-bond-strip;interestStrip.1">
      <rdfs:label>interest strip</rdfs:label>
      <skos:definition>The STRIP instrument is not a strip of the Interest of the underlying Debt Instrument.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-bond-strip;principalStrip.1">
      <rdfs:label>principal strip</rdfs:label>
      <skos:definition>The STRIP instrument is a strip of the Principal of the underlying Debt Instrument.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-bond-strip;interestStrip.2">
      <rdfs:label>interest strip</rdfs:label>
      <skos:definition>Whether the STRIP instrument is a strip of the Interest of the underlying Debt Instrument.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-sec-dbt-bond-strip;principalStrip.2">
      <rdfs:label>principal strip</rdfs:label>
      <skos:definition>Whether the STRIP instrument is a strip of the Principal of the underlying Debt Instrument.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


