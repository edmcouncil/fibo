    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-sec-dbt-bond-strip;InterestSTRIP">
      <rdfs:label>interest s t r i p</rdfs:label>
      <skos:definition>A Debt Instrument which consists of the interest component alone, of a Debt Security.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-bond-strip;STRIP"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-strip;PrincipalSTRIP">
      <rdfs:label>principal s t r i p</rdfs:label>
      <skos:definition>A Debt Instrument which consists of the principal component alone, of a Debt Security.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-sec-dbt-bond-strip;STRIP">
      <rdfs:label>s t r i p</rdfs:label>
      <skos:definition>Separate Trading of Registered Interest and Principal of Securities. A Debt Instrument which consists of either the interest component alone, or the principal component alone, of a bond.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-dbt-dbt-ins;TradableDebtInstrument"/>
   </owl:Class>


