<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-md-tmp-sts "http://spec.edmcouncil.org/fibo/red/md/temporal-core/security-trading-statuses/">
    <!ENTITY fibo-fnd-info-tem "http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/">
    <!ENTITY fibo-md-tmp-tmp "http://spec.edmcouncil.org/fibo/red/md/temporal-core/temporal-concepts/">
    <!ENTITY fibo-sec-sec-lst "http://spec.edmcouncil.org/fibo/red/sec/securities/securities-listings/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-sec-sec-sec "http://spec.edmcouncil.org/fibo/red/sec/securities/securities/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/md/temporal-core/security-trading-statuses/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/md/temporal-core/security-trading-statuses/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/md/temporal-core/temporal-concepts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-listings/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-md-tmp-sts;ExchangeSecurityStatusSelection">
      <skos:definition>The types of status that a security can have on a trading exchange.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">traded</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">halted</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-md-tmp-sts;SecurityLifecycleStatusSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Issued</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When Issued</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When Distributed</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-md-tmp-sts;SecurityStatusSelection">
      <skos:definition>The kinds of trading status that any tradable security may have.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Inactive</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Active</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-md-tmp-sts;hasStatus">
      <rdfs:label>has status</rdfs:label>
      <skos:definition>The trading status of this listing i.e. this security on this exchange.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-sts;hasTradingStatus"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-lst;SecurityListing"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-sts;ExchangeSecurityTradingStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-sts;hasStatus.1">
      <rdfs:label>has status</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-tem;hasStatus"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-sts;SecurityLifecycleStatus"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-sts;describes">
      <rdfs:label>describes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-tem;isOf"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-sts;Worthless"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-sts;hasTradingStatus">
      <rdfs:label>has trading status</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-tem;hasStatus"/>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-sts;SecurityTradingStatus"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;tense">
      <rdfs:label>tense</rdfs:label>
      <skos:definition>The status at the present time.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Tense"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;delisted">
      <rdfs:label>delisted</rdfs:label>
      <skos:definition>Whether the security is currently delisted on that exchange or trading facility.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;securityTradingStatus">
      <rdfs:label>security trading status</rdfs:label>
      <skos:definition>Status on an exchange. Exchanged Delisted Other?</skos:definition>
      <rdfs:range rdf:resource="&fibo-md-tmp-sts;ExchangeSecurityStatusSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;status">
      <rdfs:label>status</rdfs:label>
      <rdfs:range rdf:resource="&fibo-md-tmp-sts;SecurityLifecycleStatusSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;securityStatus">
      <rdfs:label>security status</rdfs:label>
      <skos:definition>The status of the security within its lifecycle. Active Inactive</skos:definition>
      <rdfs:range rdf:resource="&fibo-md-tmp-sts;SecurityStatusSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;worthless">
      <rdfs:label>worthless</rdfs:label>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;statusDate">
      <rdfs:label>status date</rdfs:label>
      <skos:definition>Date at which the status of the security is taken.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-sts;tense.1">
      <rdfs:label>tense</rdfs:label>
      <skos:definition>The status determined at some point in the past.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Tense"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-md-tmp-sts;CurrentStatus">
      <rdfs:label>current status</rdfs:label>
      <skos:definition>The status at the present time.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-md-tmp-tmp;StatusCodeValue"/>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-info-tem;Status"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-sts;ExchangeSecurityTradingStatus">
      <rdfs:label>exchange security trading status</rdfs:label>
      <skos:definition>The trading status of a security on a given trading exchange.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-sts;SecurityLifecycleStatus">
      <rdfs:label>security lifecycle status</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-sts;SecurityTradingStatus">
      <rdfs:label>security trading status</rdfs:label>
      <skos:definition>The status of the security in terms of whether it is trading or not, and any special considerations relating to trading.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-sts;Worthless">
      <rdfs:label>worthless</rdfs:label>
      <skos:definition>Announcement by the regulator that the security has become worthless.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-md-tmp-sts;PastStatus">
      <rdfs:label>past status</rdfs:label>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>