   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/md/temporal-core/credit-ratings/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/schemes/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/information/temporal-info/"/>
