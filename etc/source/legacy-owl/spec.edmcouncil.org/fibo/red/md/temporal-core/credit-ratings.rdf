      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities-issuance-terms/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/be/legal-entities/formal-business-organizations/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/securities/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/uo/lattice/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-md-tmp-crr;CreditWatchDirectionSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upgrade</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Downgrade</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>

   <rdfs:Datatype rdf:about="&fibo-md-tmp-crr;CreditWatchOutlookSelection">
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stable</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On Watch</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;definedIn">
      <rdfs:label>defined in</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-info-sch;definedIn"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-crr;InvestmentCreditRating"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;CreditRatingScheme"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;rates">
      <rdfs:label>rates</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-crr;rates.1"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-crr;IssueCreditRating"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-iss;OfferIssue"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;rated">
      <rdfs:label>rated</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-sec-sec-sec;Security"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;InstrumentCreditRating"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;issuedBy">
      <rdfs:label>issued by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publishedBy"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-crr;InvestmentCreditRating"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;CreditRatingIssuer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;rates.1">
      <rdfs:label>rates</rdfs:label>
      <skos:definition>The Credit rating rates something, for example a legal entity, an instrument or some such. The thing that can be rated is any type of Continuant Thing.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;isAbout"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-crr;InvestmentCreditRating"/>
      <rdfs:range rdf:resource="&fibo-uo-lat;ContinuantThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;rates.2">
      <rdfs:label>rates</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-crr;rates.1"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-crr;InstrumentCreditRating"/>
      <rdfs:range rdf:resource="&fibo-sec-sec-sec;Security"/>
      <owl:inverseOf rdf:resource="&fibo-md-tmp-crr;rated"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;rates.3">
      <rdfs:label>rates</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-md-tmp-crr;rates.1"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-crr;OrganizationCreditRating"/>
      <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-md-tmp-crr;hasIdentity">
      <rdfs:label>has identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-pub-pub;publisherIdentity"/>
      <rdfs:domain rdf:resource="&fibo-md-tmp-crr;CreditRatingIssuer"/>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;CreditRatingAgency"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-md-tmp-crr;watchDirection">
      <rdfs:label>watch direction</rdfs:label>
      <skos:definition>The direction in which the credit rating is expected to move, for something which has a credit rating on watch.</skos:definition>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;CreditWatchDirectionSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-md-tmp-crr;watchOutlook">
      <rdfs:label>watch outlook</rdfs:label>
      <skos:definition>The expected outlook for the rated entity.</skos:definition>
      <rdfs:range rdf:resource="&fibo-md-tmp-crr;CreditWatchOutlookSelection"/>
   </owl:DatatypeProperty>


