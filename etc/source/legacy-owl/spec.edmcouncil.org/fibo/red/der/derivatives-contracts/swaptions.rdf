<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-ctr-swn "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaptions/">
    <!ENTITY fibo-fnd-agrx-cst "http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contractual-constructs/">
    <!ENTITY fibo-der-ctr-der "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/">
    <!ENTITY fibo-fnd-txn-rea "http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-der-ctr-swp "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/">
    <!ENTITY fibo-der-ctr-otc "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaptions/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaptions/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/agreements-ext/contractual-constructs/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/transactions/rea-transactions/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-ctr-swn;isRightTo">
      <rdfs:label>is right to</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-swn;LegalSwapOption"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-swp;SwapTransaction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-swn;confers">
      <rdfs:label>confers</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agrx-cst;confers"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-swn;SwaptionContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-swn;LegalSwapOption"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-swn;hasExerciseProcedure">
      <rdfs:label>has exercise procedure</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-swn;SwaptionContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-swn;SwaptionExerciseProcedrure"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-swn;hasPremium">
      <rdfs:label>has premium</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-swn;SwaptionContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-swn;SwaptionPremium"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-swn;premiumAmount">
      <rdfs:label>premium amount</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-swn;SwaptionPremium"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-swn;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-txn-rea;transactionEmbodiesEconomicAgreement"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-swn;SwaptionTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-swn;SwaptionContract"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-ctr-swn;paymentDate">
      <rdfs:label>payment date</rdfs:label>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-ctr-swn;LegalSwapOption">
      <rdfs:label>legal swap option</rdfs:label>
      <skos:definition>The option, that is the right but not the obligation, to enter into a Swap transaction at some time in the future under terms defined in the Swaption contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-agrx-cst;ContractualOption"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swn;SwaptionContract">
      <rdfs:label>swaption contract</rdfs:label>
      <skos:definition>Option taken on interest rate swaps. These are FX interest rate option.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swn;SwaptionExerciseProcedrure">
      <rdfs:label>swaption exercise procedrure</rdfs:label>
      <skos:definition>Procedure for the exercise of the Swaption.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swn;SwaptionPremium">
      <rdfs:label>swaption premium</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swn;SwaptionTransaction">
      <rdfs:label>swaption transaction</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeTransaction"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>