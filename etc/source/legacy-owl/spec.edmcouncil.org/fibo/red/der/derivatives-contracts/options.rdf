   <owl:Class rdf:about="&fibo-der-ctr-opt;FixedStrikeTermsSet">
      <rdfs:label>fixed strike terms set</rdfs:label>
      <skos:definition>Contractual terms setting out a fixed, pre-agreed price or level at which the option is to be struck.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-opt;OptionStrikeTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-opt;FloatingLookbackFormulaExpression">
