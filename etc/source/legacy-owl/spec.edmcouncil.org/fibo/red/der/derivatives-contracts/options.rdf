   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-opt;PremiumPercentageOfNotionalExpression">
      <rdfs:label>premium percentage of notional expression</rdfs:label>
      <skos:definition>The expression of the premium as a percentage of the notional value of the transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-opt;OptionPremiumExpression"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-opt;PremiumPricePerOptionExpression">
