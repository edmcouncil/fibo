      <rdfs:subClassOf rdf:resource="&fibo-fnd-agr-ctr;ContractCounterparty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-opt;OptionBuyerCommitment">
      <rdfs:label>option buyer commitment</rdfs:label>
      <skos:definition>Commitments, if any, on the part of the holder (buyer) of the Option.</skos:definition>
