<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-ctr-dft "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-cashflow-terms/">
    <!ENTITY fibo-der-ctr-swp "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/">
    <!ENTITY fibo-fnd-math-math "http://spec.edmcouncil.org/fibo/red/fnd/math/math/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-cashflow-terms/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-cashflow-terms/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/math/math/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-ctr-dft;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-dft;CashflowFormulaArgument"/>
      <rdfs:range rdf:resource="&fibo-fnd-math-math;TimedParameter"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-dft;hasArgument">
      <rdfs:label>has argument</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;hasArgument"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-dft;CashflowFormulaExpression"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-dft;CashflowFormulaArgument"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-dft;expression">
      <rdfs:label>expression</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-math-math;expression.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ctr-dft;CashflowFormula"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-dft;CashflowFormulaExpression"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ctr-dft;hasFormulaComponent">
      <rdfs:label>has formula component</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ctr-dft;ParametricCashflowTermsSet"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-dft;CashflowFormula"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-ctr-dft;CashflowFormula">
      <rdfs:label>cashflow formula</rdfs:label>
      <skos:definition>A formula which forms part of a set of cashflow terms.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;Formula"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-dft;CashflowFormulaArgument">
      <rdfs:label>cashflow formula argument</rdfs:label>
      <skos:definition>The argument of a cxashflow formula expression.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;FormulaArgument"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-dft;CashflowFormulaExpression">
      <rdfs:label>cashflow formula expression</rdfs:label>
      <skos:definition>A formula expression for a Cashflow Formula.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;FormulaExpression"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-dft;CashflowTermsSet">
      <rdfs:label>cashflow terms set</rdfs:label>
      <skos:definition>A set of terms setting out a cashflow structure of payments committed to by one party to a contract, expressed in mathematical terms.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapLegTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-dft;ParametricCashflowTermsSet">
      <rdfs:label>parametric cashflow terms set</rdfs:label>
      <skos:definition>Terms for a set of cashflows defined according to a mathematical formula</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-dft;CashflowTermsSet"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>