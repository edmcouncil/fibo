<owl:Class rdf:about="&fibo-der-ctr-swp;FixedPaymentLeg">
      <rdfs:label>fixed payment leg</rdfs:label>
      <skos:definition>The fixed payment leg of a Swap such as a Dividend Swap.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;PaymentLeg"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;FixedPaymentStream">
      <rdfs:label>fixed payment stream</rdfs:label>
      <skos:definition>The fixed payment leg of a Swap such as a Dividend Swap.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;Swapstream"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;FloatingFundingLeg">
      <rdfs:label>floating funding leg</rdfs:label>
      <skos:definition>A floating leg or interest stream in the role of being a funding leg.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;LegPayer">
      <rdfs:label>leg payer</rdfs:label>
      <skos:definition>The party responsible for making the payments defined by this Leg. FpML: 'A reference to the party responsible for making the payments defined by this structure.'</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;LegReceiver">
      <rdfs:label>leg receiver</rdfs:label>
      <skos:definition>The party that receives the payments in a Swap Leg. FpML: 'A reference to the party that receives the payments corresponding to this structure.'</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;LegStrikeCommitment">
      <rdfs:label>leg strike commitment</rdfs:label>
      <skos:definition>A commitment to strike a swap deal as defined in the strike leg terms.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapLegCommitment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;PayingLeg">
      <rdfs:label>paying leg</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapDirectionalLeg"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;PaymentCommitment">
      <rdfs:label>payment commitment</rdfs:label>
      <skos:definition>A commitment to make some payment in exchange for some good or benefit.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;PaymentLeg">
      <rdfs:label>payment leg</rdfs:label>
      <skos:definition>A leg in which payments are made without reference to some underlying.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapLeg"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;PaymentStreamPaymentSchedule">
      <rdfs:label>payment stream payment schedule</rdfs:label>
      <skos:definition>The schedule of payments on a funding leg of a Swap.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-tim-tim;Schedule"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;RealizedVariableCommitment">
      <rdfs:label>realized variable commitment</rdfs:label>
      <skos:definition>A commitment to pay out on the swap leg according to the terms defined for a realized variable swap leg.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;ReceiveLegPayer">
      <rdfs:label>receive leg payer</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;LegPayer"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;ReceiveLegReceiver">
      <rdfs:label>receive leg receiver</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;LegReceiver"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;ReceivingLeg">
      <rdfs:label>receiving leg</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;StrikeLeg">
      <rdfs:label>strike leg</rdfs:label>
      <skos:definition>The abstract leg implied by the strike in a statistical swap.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapConfirmation">
      <rdfs:label>swap confirmation</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCInstrumentConfirmation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapContract">
      <rdfs:label>swap contract</rdfs:label>
      <skos:definition>An exchange of streams of payments or other commitments over time according to specified terms.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapDirectionalLeg">
      <rdfs:label>swap directional leg</rdfs:label>
      <skos:definition>A swap leg defined soleley in terms of how it is seen by the Holder, i.e. whether it is the payment made to get in to the deal or the payments received as a result of participating in the deal.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;TransactionEventAspect"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapHolder">
      <rdfs:label>swap holder</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapLeg">
      <rdfs:label>swap leg</rdfs:label>
      <skos:definition>One leg of a Swap Transaction, as defined in the Swap Contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;TransactionEvent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapLegCommitment">
      <rdfs:label>swap leg commitment</rdfs:label>
      <skos:definition>Commitment to make the cashflow or payment defined in a Swap Leg.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;ContractualEconomicCommitment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapLegTermsSet">
      <rdfs:label>swap leg terms set</rdfs:label>
      <skos:definition>Contractual terms setting out the agreed cashflow, strike, calculation conditions or other features of any one leg of a Swap transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;EconomicContractTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapLegTermsSet.1">
      <rdfs:label>swap leg terms set</rdfs:label>
      <skos:definition>The contractual terms for an individual leg of a Swap. These form part of the overall Swap terms.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapTermsSet">
      <rdfs:label>swap terms set</rdfs:label>
      <skos:definition>The legal contractual terms of a Swap Transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeTransactionTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;Swapstream">
      <rdfs:label>swapstream</rdfs:label>
      <skos:definition>One leg of a Swap Transaction, as defined in the Swap Contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-dft;CashflowTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapstreamEvent">
      <rdfs:label>swapstream event</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-sch;ScheduledEvent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapParty">
      <rdfs:label>swap party</rdfs:label>
      <skos:definition>A party to a Swap transaction and therefore a legal party to the Swap contract that embodies that transaction.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-txn-rea;TransactionParty"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ctr-swp;SwapTransaction">
      <rdfs:label>swap transaction</rdfs:label>
      <skos:definition>A Swap Transaction is a transaction between two equal parties in which some negotiable item is provided on each side. The parties are not Principal and Counterparty but two equal parties.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otcd;OTCDerivativeTransaction"/>
   </owl:Class>


