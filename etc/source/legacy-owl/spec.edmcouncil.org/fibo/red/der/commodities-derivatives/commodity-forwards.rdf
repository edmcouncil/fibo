<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-com-for "http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodity-forwards/">
    <!ENTITY fibo-der-ctr-for "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/">
    <!ENTITY fibo-der-com-cmc "http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-contracts/">
    <!ENTITY fibo-der-com-del "http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-delivery/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodity-forwards/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodity-forwards/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/commodities-derivatives/commodities-delivery/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-com-for;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;embodies.2"/>
      <rdfs:domain rdf:resource="&fibo-der-com-for;CommodityForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-com-for;CommodityForwardContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-for;closedOutBy">
      <rdfs:label>closed out by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;closedOutBy"/>
      <rdfs:domain rdf:resource="&fibo-der-com-del;CommodityContractCashCloseout"/>
      <rdfs:range rdf:resource="&fibo-der-com-for;CommodityForwardContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-for;referenceRate">
      <rdfs:label>reference rate</rdfs:label>
      <skos:definition>Is actually a price of something.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-com-for;CommodityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-for;side">
      <rdfs:label>side</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;side.1"/>
      <rdfs:domain rdf:resource="&fibo-der-com-for;CommodityForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-com-del;CommodityForwardDelivery"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-for;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-com-cmc;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-com-for;CommodityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-com-cmc;CommodityUnderlying"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-com-for;hasCalculationAgent">
      <rdfs:label>has calculation agent</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-com-for;CashSettledForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-com-for;CommodityForwardCalculationAgent"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-com-for;cashSettled">
      <rdfs:label>cash settled</rdfs:label>
      <skos:definition>The contract is settled for cash i.e. closed out against a similar but reverse contact.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-com-for;CashSettledForwardContract">
      <rdfs:label>cash settled forward contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-for;CommodityForwardContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-for;CommodityForwardCalculationAgent">
      <rdfs:label>commodity forward calculation agent</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-for;CommodityForwardContract">
      <rdfs:label>commodity forward contract</rdfs:label>
      <skos:definition>Definition needed.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-com-cmc;CommodityDerivativeContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-com-for;CommodityForwardTransaction">
      <rdfs:label>commodity forward transaction</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardTransaction"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>