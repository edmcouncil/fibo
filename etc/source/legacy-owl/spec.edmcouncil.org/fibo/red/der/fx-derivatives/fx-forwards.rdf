<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-fx-for "http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-forwards/">
    <!ENTITY fibo-der-ctr-otc "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/">
    <!ENTITY fibo-der-ctr-for "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/">
    <!ENTITY fibo-der-fx-fxc "http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-contracts/">
    <!ENTITY fibo-der-fx-swp "http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-swaps/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-der-fx-spot "http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-spots/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-forwards/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-forwards/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/otc-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-contracts/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/fx-derivatives/fx-spots/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-fx-for;exchangedCurrencyOne">
      <rdfs:label>exchanged currency one</rdfs:label>
      <skos:definition>FpML has a term exchangedCurrency1 but this refers to the currency flow not the currency as in USD, CDN etc. FpML definition for exchangedCurrency1: 'This is the first of the two currency flows that define a single leg of a standard foreign exchange transaction.'</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;exchangedCurrencyTwo">
      <rdfs:label>exchanged currency two</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;Currency"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;establishes">
      <rdfs:label>establishes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;establishes.1"/>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-for;ForwardCashDeliveryCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;establishes.1">
      <rdfs:label>establishes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;establishes"/>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-for;ForwardCashSettlementCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;hasPart">
      <rdfs:label>has part</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardOutright"/>
      <rdfs:range rdf:resource="&fibo-der-fx-spot;FxSpot"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;hasPart.1">
      <rdfs:label>has part</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardOutright"/>
      <rdfs:range rdf:resource="&fibo-der-fx-swp;FxSwap"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-fx-for;FxForwardContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;side">
      <rdfs:label>side</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-for;ForwardCashDelivery"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-fx-for;side.1">
      <rdfs:label>side</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-fx-for;FxForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ctr-for;ForwardCashSettlementTermsSet"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-fx-for;forwardRate">
      <rdfs:label>forward rate</rdfs:label>
      <skos:definition>The rate at which the seller currency is purchased in the buyer currency.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-fx-for;fxForwardRate">
      <rdfs:label>fx forward rate</rdfs:label>
      <skos:definition>'The rate of exchange between the two currencies.'</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;number"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-fx-for;FXForwardOutrightContract">
      <rdfs:label>f x forward outright contract</rdfs:label>
      <skos:definition>A forward contract in the forex market that locks in the price at which an entity can buy or sell a currency on a future date. Also known as "outright forward currency transaction", "forward outright" or "FX forward".</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-fx-fxc;ForeignExchangeDerivativeContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-otc;OTCInstrumentContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-fx-for;FxForwardContract">
      <rdfs:label>fx forward contract</rdfs:label>
      <skos:definition>A agreement to deliver and settle a given amount of currency in one currency, in exchange for a given amount in another currency, at an agreed date in the future and at an agreed rate of exchange.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-fx-for;FxForwardOutright">
      <rdfs:label>fx forward outright</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-fx-fxc;OTCFxProduct"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-fx-for;FxForwardTransaction">
      <rdfs:label>fx forward transaction</rdfs:label>
      <skos:definition>A transaction in which cash is exchanged in two currencies at an agreed rate at an agreed date in the future.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardTransaction"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
