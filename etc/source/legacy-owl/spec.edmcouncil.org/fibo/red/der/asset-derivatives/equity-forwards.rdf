<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-ass-eqf "http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/equity-forwards/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
    <!ENTITY fibo-der-ctr-for "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/">
    <!ENTITY fibo-der-ass-ass "http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-derivatives/">
    <!ENTITY fibo-der-ctr-der "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/">
    <!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/">
    <!ENTITY fibo-der-ass-eqs "http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/equity-swaps/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/equity-forwards/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/equity-forwards/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/forwards/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-derivatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/accounting/currency-amount/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/equity-swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<rdfs:Datatype rdf:about="&fibo-der-ass-eqf;MethodOfAdjustmentSelection">
      <skos:definition>Method by which adjustments will be made to the forward contract should one or more of a number of extraordinary events occur.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Options Exchange</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Calculation Agent</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdfs:Datatype>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;dividendAdjustment">
      <rdfs:label>dividend adjustment</rdfs:label>
      <skos:definition>One or more Dividend Adjustment Periods, which are used to calculate the Deviation between Expected Dividend and Actual Dividend in that Period.'</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;DividendAdjustmentPeriod"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;effectiveDate">
      <rdfs:label>effective date</rdfs:label>
      <skos:definition>The Effective date for a forward starting derivative. This is the starting date for the contract. Further notes: FpML: 'Effective date for a forward starting derivative. If this element is not present, the effective date is the trade date. Note that FpML has this typed as "adjustableDate or relativeDate". Our Determined Date is wider in scope than this and may need to be narrowed down depending on what is the true range of ways in which this date may be defined in a forward contract. Note that the optionality identified in the FpML definition is not reflected here. The assumption is that this exists as a legal fact, and that if that legal fact is not reflected in the FpML message then the trade date is taken to be the (legal) Effective Date. Therefore this term always exists from a legal perspective.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Date"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;notionalAmount">
      <rdfs:label>notional amount</rdfs:label>
      <skos:definition>DEFINITION NEEDED FpML: 'The notional amount.' (thanks!)</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;hasBuyer">
      <rdfs:label>has buyer</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;hasBuyer"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;EquityForwardBuyer"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;hasConditions">
      <rdfs:label>has conditions</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqs;DividendConditions"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;commitsTo">
      <rdfs:label>commits to</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;commitsTo.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardDeliveryCommitment"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;SecurityForwardDelivery"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;embodies.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-ass;SecurityForwardDelivery"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;EquityForwardDeliveryCommitment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;embodies.1">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;embodies.2"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;side">
      <rdfs:label>side</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;side"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;EquityForwardCashSettlement"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;side.1">
      <rdfs:label>side</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;side.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;SecurityForwardDelivery"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;hasSeller">
      <rdfs:label>has seller</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;hasSeller"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;EquityForwardSeller"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-ass;EquityObservable"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;undertakenBy">
      <rdfs:label>undertaken by</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;undertakenBy"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityValuation"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;EquityValuationCalculationAgent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-eqf;establishes">
      <rdfs:label>establishes</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-for;establishes.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-eqf;EquityForwardContract"/>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;EquityForwardDeliveryCommitment"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-ass-eqf;priceSource">
      <rdfs:label>price source</rdfs:label>
      <skos:definition>The Price source to be used to determine the settlement price.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-ass-eqf;settlementMethodElection">
      <rdfs:label>settlement method election</rdfs:label>
      <skos:definition>How the forward will be settled.</skos:definition>
      <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-ass-eqf;methodOfAdjustment">
      <rdfs:label>method of adjustment</rdfs:label>
      <skos:definition>Method by which adjustments will be made to the forward contract should one or more of a number of extraordinary events occur.</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-ass-eqf;MethodOfAdjustmentSelection"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-ass-eqf;numberOfUnits">
      <rdfs:label>number of units</rdfs:label>
      <skos:definition>The price per asset, index or basket observed on the trade or effective date.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;nonNegativeNumber"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-ass-eqf;DividendAdjustmentPeriod">
      <rdfs:label>dividend adjustment period</rdfs:label>
      <skos:definition>A period used to calculate the Deviation between Expected Dividend and Actual Dividend in that Period.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-tim-tim;DatePeriod"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityForwardBuyer">
      <rdfs:label>equity forward buyer</rdfs:label>
      <skos:definition>The party that buys this forwward contract, that is the party which pays for this instrument and receives the rights defined by it.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardContractBuyer"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityForwardCashSettlement">
      <rdfs:label>equity forward cash settlement</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardCashSettlementTermsSet"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityForwardContract">
      <rdfs:label>equity forward contract</rdfs:label>
      <skos:definition>Definition needed</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ass-ass;EquityDerivativeContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityForwardDeliveryCommitment">
      <rdfs:label>equity forward delivery commitment</rdfs:label>
      <skos:definition>The commitment to deliver an equity underlying at the maturity of an OTC Equity Forward contract.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardDeliveryCommitment"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityForwardSeller">
      <rdfs:label>equity forward seller</rdfs:label>
      <skos:definition>The party that sells or writes this forward instrument, that is the party which grants the rights defined by this instrument and in return receives a payment for it.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardContractSeller"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityForwardTransaction">
      <rdfs:label>equity forward transaction</rdfs:label>
      <skos:definition>A transaction in which one party commits at the present to delivery of an equity security or securities or cash equivalent in the future.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;ForwardTransaction"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquitySecurityDelivery">
      <rdfs:label>equity security delivery</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ass-ass;UnderlyingSecurityDelivery"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityValuation">
      <rdfs:label>equity valuation</rdfs:label>
      <skos:definition>The act of valuation of an equity underlying (share, basket or index) during the life of an OTC equity option or forward.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;UnderlyingValuation"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-eqf;EquityValuationCalculationAgent">
      <rdfs:label>equity valuation calculation agent</rdfs:label>
      <skos:definition>The entity which carries out the valuation of an asset underlying an equity forward or option, or from which the valuation of the underlying is to be taken.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-for;CalculationAgent"/>
   </owl:Class>


