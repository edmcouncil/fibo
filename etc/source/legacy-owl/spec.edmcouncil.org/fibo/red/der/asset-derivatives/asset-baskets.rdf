<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-ass-bas "http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-baskets/">
    <!ENTITY fibo-sec-sec-bsk "http://spec.edmcouncil.org/fibo/red/sec/securities/baskets/">
    <!ENTITY fibo-der-ass-ass "http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-derivatives/">
    <!ENTITY fibo-ind-mi-bi "http://spec.edmcouncil.org/fibo/red/ind/market-indices/basket-indices/">
    <!ENTITY fibo-sec-eq-eqi "http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-baskets/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-baskets/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/securities/baskets/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/asset-derivatives/asset-derivatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/market-indices/basket-indices/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/equities/equity-instruments/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-ass-bas;constituent">
      <rdfs:label>constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;hasConstituent"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;BasketOfEquityIndices"/>
      <rdfs:range rdf:resource="&fibo-der-ass-bas;EquitiesBasketIndexConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;identity.2"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;EquitiesBasketShareConstituent"/>
      <rdfs:range rdf:resource="&fibo-sec-eq-eqi;PubliclyTradedShare"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;identity.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;EquitiesBasketIndexConstituent"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;EquityReferenceIndex"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;constituent.1">
      <rdfs:label>constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;hasConstituent.1"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;EquitiesSingleNameBasket"/>
      <rdfs:range rdf:resource="&fibo-der-ass-bas;EquitiesBasketShareConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;constituent.2">
      <rdfs:label>constituent</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-sec-sec-bsk;hasConstituent.3"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;EquitiesMixedBasket"/>
      <rdfs:range rdf:resource="&fibo-der-ass-bas;.UnionClass.4"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;defines">
      <rdfs:label>defines</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;EquityBasketOptionAnnex"/>
      <rdfs:range rdf:resource="&fibo-der-ass-bas;EquitiesBasketShareConstituent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;specifiesCompositionOf">
      <rdfs:label>specifies composition of</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;EquityBasketOptionAnnex"/>
      <rdfs:range rdf:resource="&fibo-der-ass-bas;EquitiesSingleNameBasket"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;identity.2">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ass-ass;identity.3"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;EquityBasketObservable"/>
      <rdfs:range rdf:resource="&fibo-der-ass-bas;EquitiesSingleNameBasket"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-ass-bas;hasMember">
      <rdfs:label>has member</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-ind-mi-bi;hasConstituent"/>
      <rdfs:domain rdf:resource="&fibo-der-ass-bas;SetOfEquityIndexConstituents"/>
      <rdfs:range rdf:resource="&fibo-ind-mi-bi;EquityIndexConstituent"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-ass-bas;numberOfShares">
      <rdfs:label>number of shares</rdfs:label>
      <skos:definition>The number of this share that makes up this component of the index.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="&fibo-der-ass-bas;percentage">
      <rdfs:label>percentage</rdfs:label>
      <skos:definition>The percentage of the basket which this constituent makes up.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;percentage"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-ass-bas;.UnionClass.4">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesBasketShareConstituent"/>
               <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesBasketIndexConstituent"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;.UnionClass.5">
      <skos:definition>Any basket of equity consituents. This may be a basket of shares, of equity indices or a basket which is mixture of both.</skos:definition>
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="&fibo-der-ass-bas;BasketOfEquityIndices"/>
               <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesSingleNameBasket"/>
               <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesMixedBasket"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;BasketOfEquityIndices">
      <rdfs:label>basket of equity indices</rdfs:label>
      <skos:definition>A basket of indices which are all equity-only indices.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;BasketOfIndices"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesBasketIndexConstituent">
      <rdfs:label>equities basket index constituent</rdfs:label>
      <skos:definition>An index which makes up a part of a Basket.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;BasketOfIndicesConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesBasketShareConstituent">
      <rdfs:label>equities basket share constituent</rdfs:label>
      <skos:definition>A security which makes up a part of the Basket.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;SecuritiesBasketConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesMixedBasket">
      <rdfs:label>equities mixed basket</rdfs:label>
      <skos:definition>A basket which has a mix of constituents which are shares and equity indices.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;MixedBasket"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;EquitiesSingleNameBasket">
      <rdfs:label>equities single name basket</rdfs:label>
      <skos:definition>A basket of securities consisting only of equities securities (publicly issued and traded shares).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-sec-sec-bsk;BasketOfSecurities"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;EquityBasketObservable">
      <rdfs:label>equity basket observable</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ass-ass;EquityObservable"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;EquityBasketOptionAnnex">
      <rdfs:label>equity basket option annex</rdfs:label>
      <skos:definition>An annex to an Equities Basket Option, setting out in detail the composition of the Basket.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-ass-bas;SetOfEquityIndexConstituents">
      <rdfs:label>set of equity index constituents</rdfs:label>
      <skos:definition>A defined set of the constituents of some named index.</skos:definition>
   </owl:Class>


  <!--
