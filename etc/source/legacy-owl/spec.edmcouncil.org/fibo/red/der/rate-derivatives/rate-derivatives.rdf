<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-rd-rd "http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/rate-derivatives/">
    <!ENTITY fibo-der-ctr-der "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
    <!ENTITY fibo-der-rd-irs "http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/ir-swaps/">
    <!ENTITY fibo-ind-ei-ei "http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicators/">
    <!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/rate-derivatives/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/rate-derivatives/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/derivatives-basics/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/ir-swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicators/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/indicators/indicators/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-rd-rd;hasUnderlying">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-rd-rd;hasUnderlying.2"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-rd;IndexDerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-rd-rd;EconomicRateObservable"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-rd;hasUnderlying.1">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-rd-rd;hasUnderlying.2"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-rd;InterestRateDerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-rd-rd;InterestRateObservable"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-rd;indexTenor">
      <rdfs:label>index tenor</rdfs:label>
      <skos:definition>The tenor of the index, that is the length of time for which the index is quoted, e.g. 3 months, 6 months and so on.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-rd-rd;InterestRateObservable"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;KnownDateDuration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-rd;hasUnderlying.2">
      <rdfs:label>has underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-rd;RateBasedDerivativeContract"/>
      <rdfs:range rdf:resource="&fibo-der-rd-rd;RateObservable"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-rd;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-rd-rd;identity.1"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-rd;EconomicRateObservable"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-ei;EconomicRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-rd;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;identity.1"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-rd;RateObservable"/>
      <rdfs:range rdf:resource="&fibo-ind-ind-ind;MarketReferentialParameter"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-rd-rd;rateTreatment">
      <rdfs:label>rate treatment</rdfs:label>
      <skos:definition>The specification of any rate conversion which needs to be applied to the observed rate before being used in any calculations.</skos:definition>
      <rdfs:range rdf:resource="&fibo-der-rd-irs;SwapInterestRateTreatmentSelection"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-rd-rd;EconomicRateObservable">
      <rdfs:label>economic rate observable</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-rd-rd;RateObservable"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-rd;IndexDerivativeContract">
      <rdfs:label>index derivative contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-rd-rd;RateBasedDerivativeContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-rd;InterestRateDerivativeContract">
      <rdfs:label>interest rate derivative contract</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-rd;InterestRateObservable">
      <rdfs:label>interest rate observable</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-rd;RateBasedDerivativeContract">
      <rdfs:label>rate based derivative contract</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;DerivativeContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-rd;RateObservable">
      <rdfs:label>rate observable</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-der;ObservableUnderlying"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
