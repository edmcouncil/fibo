<!DOCTYPE rdf:RDF [
    <!ENTITY fibo-der-rd-ins "http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/inflation-swaps/">
    <!ENTITY fibo-fnd-math-math "http://spec.edmcouncil.org/fibo/red/fnd/math/math/">
    <!ENTITY fibo-der-ctr-swp "http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/">
    <!ENTITY fibo-der-rd-rd "http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/rate-derivatives/">
    <!ENTITY fibo-der-rd-irs "http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/ir-swaps/">
    <!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/">
    <!ENTITY fibo-sec-dbt-bond-cfv "http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-cashflow-variants/">
    <!ENTITY fibo-ind-ei-ei "http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicators/">
    <!ENTITY fibo-fnd-tim-tim "http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/">
]>

<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/inflation-swaps/">

   <owl:Ontology rdf:about="http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/inflation-swaps/">
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/math/math/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/derivatives-contracts/swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/rate-derivatives/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/der/rate-derivatives/ir-swaps/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/utilities/business-facing-types/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/sec/debt/bonds/bonds-cashflow-variants/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/ind/economic-indicators/economic-indicators/"/>
      <owl:imports rdf:resource="http://spec.edmcouncil.org/fibo/red/fnd/date-time/date-time/"/>
   </owl:Ontology>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:ObjectProperty rdf:about="&fibo-der-rd-ins;identity">
      <rdfs:label>identity</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationIndexFallBackBond"/>
      <rdfs:range rdf:resource="&fibo-sec-dbt-bond-cfv;InflationBond"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;lag">
      <rdfs:label>lag</rdfs:label>
      <skos:definition>Details needed. Inserted per telecon (Derivatives PoC) from Andrew Jacobs. Identify the corresponding term in FpML and use that here.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationIndexUnderlying"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;DurationDescriptionInCalendarTemporalUnit"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;identity.1">
      <rdfs:label>identity</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-rd-rd;identity"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationIndexUnderlying"/>
      <rdfs:range rdf:resource="&fibo-ind-ei-ei;InflationRate"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;specifies">
      <rdfs:label>specifies</rdfs:label>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationIndexUnderlying"/>
      <rdfs:range rdf:resource="&fibo-der-rd-ins;InflationIndexFallBackBond"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;embodies">
      <rdfs:label>embodies</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;embodies"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationSwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-rd-ins;InflationSwapContract"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;hasLeg">
      <rdfs:label>has leg</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;hasLeg"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationSwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-rd-irs;FixedInterestLeg"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;hasLeg.1">
      <rdfs:label>has leg</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;hasLeg"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationSwapTransaction"/>
      <rdfs:range rdf:resource="&fibo-der-rd-ins;InflationLeg"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;governs">
      <rdfs:label>governs</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-swp;governs.1"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationSwapContract"/>
      <rdfs:range rdf:resource="&fibo-der-rd-ins;InflationSwapTransaction"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-rd-ins;underlying">
      <rdfs:label>underlying</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-der-rd-rd;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-rd-ins;InflationSwapContract"/>
      <rdfs:range rdf:resource="&fibo-der-rd-ins;InflationIndexUnderlying"/>
   </owl:ObjectProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:DatatypeProperty rdf:about="&fibo-der-rd-ins;fallbackBondApplicable">
      <rdfs:label>fallback bond applicable</rdfs:label>
      <skos:definition>The applicability of a fallback bond as defined in the 2006 ISDA Inflation Derivatives Definitions, sections 1.3 and 1.8. Yes implies a value of true.</skos:definition>
      <rdfs:range rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
   </owl:DatatypeProperty>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->



  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


<owl:Class rdf:about="&fibo-der-rd-ins;InflationIndexFallBackBond">
      <rdfs:label>inflation index fall back bond</rdfs:label>
      <skos:definition>A fallback bond as defined in the 2006 ISDA Inflation Derivatives Definitions, sections 1.3 and 1.8.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-fnd-math-math;ReferenceEntity"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-ins;InflationIndexUnderlying">
      <rdfs:label>inflation index underlying</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-rd-rd;EconomicRateObservable"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-ins;InflationLeg">
      <rdfs:label>inflation leg</rdfs:label>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapLeg"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-ins;InflationSwapContract">
      <rdfs:label>inflation swap contract</rdfs:label>
      <skos:definition>Draft definition: This is a swap instrument which makes reference to an inflation index.</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-rd-rd;IndexDerivativeContract"/>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapContract"/>
   </owl:Class>

   <owl:Class rdf:about="&fibo-der-rd-ins;InflationSwapTransaction">
      <rdfs:label>inflation swap transaction</rdfs:label>
      <skos:definition>A transaction in which one leg is based on an inflation rate and the other leg is based on an interest rate (usually or always fixed interest).</skos:definition>
      <rdfs:subClassOf rdf:resource="&fibo-der-ctr-swp;SwapTransaction"/>
   </owl:Class>


  <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->


</rdf:RDF>