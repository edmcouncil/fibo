      <rdfs:subPropertyOf rdf:resource="&fibo-der-ctr-der;hasUnderlying"/>
      <rdfs:domain rdf:resource="&fibo-der-misc-cfd;ContractForDifference"/>
      <rdfs:range rdf:resource="&fibo-fnd-oac-own;Asset"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-misc-cfd;holder">
      <rdfs:label>holder</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;hasCounterparty"/>
      <rdfs:domain rdf:resource="&fibo-der-misc-cfd;ContractForDifference"/>
      <rdfs:range rdf:resource="&fibo-der-misc-cfd;CFDHolder"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-misc-cfd;hasCounterparty">
      <rdfs:label>has counterparty</rdfs:label>
      <rdfs:subPropertyOf rdf:resource="&fibo-fnd-agr-ctr;hasPrincipal"/>
      <rdfs:domain rdf:resource="&fibo-der-misc-cfd;ContractForDifference"/>
      <rdfs:range rdf:resource="&fibo-der-misc-cfd;CFDCounterparty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="&fibo-der-misc-cfd;rollingFrequency">
      <rdfs:label>rolling frequency</rdfs:label>
      <skos:definition>In principle they can be rolled on any agreed basis. Usually no firm idea on when the parties want to settle, will hold untilo they feel like terminating, nbut there's no reason why this might not be made weekly or monthly.</skos:definition>
      <rdfs:domain rdf:resource="&fibo-der-misc-cfd;ContractForDifference"/>
      <rdfs:range rdf:resource="&fibo-fnd-tim-tim;Frequency"/>
   </owl:ObjectProperty>


