<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/Agents/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Roles/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PartyRoleContext/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/organizationMemberIsMemberOf">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/isMemberOf"/>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Roles/hasRole"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/OrganizationMember"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/Organization"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/hasParty">
      <skos:definition>an independent party associated with an agreement, contract, policy, regulation, or other business arrangement</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/has"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/Thing"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/IndependentParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/isAPartyTo">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/IndependentParty"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/Thing"/>
      <owl:inverseOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/hasParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyInRoleHasIdentity">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Relations/Relations/hasIdentity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/IndependentParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/hasPartyInRole">
      <skos:definition>The role fulfilled by a party in a relationship context.</skos:definition>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PartyRoleContext/RelationshipContext"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/partyHasEffectiveDate">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/dateAndTime"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/Anon-14250">
      <owl:equivalentClass>
         <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/People/Person"/>
               <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/Organization"/>
            </owl:unionOf>
         </owl:Class>
      </owl:equivalentClass>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/IndependentParty">
      <rdfs:label>Independent Party</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/OrganizationMember">
      <rdfs:label>Organization Member</rdfs:label>
      <skos:definition>Some entity which is a member of some organization.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole">
      <rdfs:label>Party In Role</rdfs:label>
      <skos:definition>Any autonomous entity which is defined as being the party in some context, for example the party to a contract or a transaction.</skos:definition>
      <skos:editorialNote>A Party is any entity which can be meaningfully said to be "Party to" something. The contexts in which something can be said to be party to something include: - Contracts - Transactions - Credit versus Debt - Nostro and Vostro accounts</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Roles/AgentInRole"/>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>