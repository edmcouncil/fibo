<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/illegalCartelHasMember">
      <skos:definition>Some organization which is a member of the cartel.</skos:definition>
      <skos:editorialNote>Cartels are described as groups specifically of formal organizations.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/organizationHasMember"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/IllegalCartel"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/FormalOrganizations/FormalOrganization"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/Club">
      <rdfs:label>Club</rdfs:label>
      <skos:definition>An informal organization formed to pursue some common interest among its members.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/LegitimateOrganization"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/CrimeSyndicate">
      <rdfs:label>Crime Syndicate</rdfs:label>
      <skos:definition>An informal grouping formed for the purposes of organized criminal activities.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/IllegalOrganization"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/IllegalCartel">
      <rdfs:label>Illegal Cartel</rdfs:label>
      <skos:definition>A collection of companies that come together to manipulate the market in some way. e.g. price fixing.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/IllegalOrganization">
      <rdfs:label>Illegal Organization</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/Organization"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/Organizations/LegitimateOrganizations/LegitimateOrganization">
      <rdfs:label>Legitimate Organization</rdfs:label>
      <skos:definition>An organization which exists to serve some lawful purpose.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>