<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Control/Control">
      <rdfs:label>Control</rdfs:label>
      <skos:definition>The term &lt;i&gt;control&lt;/i&gt; (including the terms &lt;i&gt;controlling, controlled by&lt;/i&gt; and &lt;i&gt;under common control with&lt;/i&gt; ) means the possession, direct or indirect, of the power to direct or cause the direction of the management and policies of a person, whether through the ownership of voting shares, by contract, or otherwise.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/SocialConstructs/SocialConstructs/SocialConstruct"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Control/ControllingParty">
      <rdfs:label>Controlling Party</rdfs:label>
      <skos:definition>Party which exercises some form of control in some context.</skos:definition>
      <skos:editorialNote>At this level of abstraction it is not defined whether the control is some degree of controlling interest, or some level of actual control (asserted or calculated) in some entity.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Control/DeFactoControl">
      <rdfs:label>De Facto Control</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Control/Control"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-SR/FND/OwnershipAndControl/Control/DeJureControl">
      <rdfs:label>De Jure Control</rdfs:label>
      <skos:definition>control that is formalized in law, or codified in some legal instrument</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCapacities/LegalConstruct"/>
   </owl:Class>

