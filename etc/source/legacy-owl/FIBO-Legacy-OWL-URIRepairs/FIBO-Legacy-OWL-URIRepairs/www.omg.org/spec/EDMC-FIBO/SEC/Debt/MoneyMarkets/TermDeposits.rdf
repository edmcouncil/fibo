   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/"/>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/termDuration">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/TermDeposit"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Time/Time/Duration"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/settlementTerms">
      <skos:definition>The contractual terms specifying the obligations of the certificate holder for the specified duration of that deposit.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/contractHasTerms"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/TermDeposit"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/TermDepositSettlementTerms"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/termDepositRate">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/amount">
      <rdfs:range rdf:resource="http://www.omg.org/spec/FIBO/Foundation/Accounting/CurrencyAmount/MoneyAmount"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/TermDeposit">
      <rdfs:label>Term Deposit</rdfs:label>
      <skos:definition>A money deposit at a bank that cannot be withdrawn for a certain "term" or period of time. When the term is over it can be withdrawn or it can be held for another term. The longer the term the better the yield on the money. A certificate of deposit is a time deposit product.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCInstrumentContract"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/MoneyMarkets/TermDeposits/TermDepositSettlementTerms">
      <rdfs:label>Term Deposit Settlement Terms</rdfs:label>
      <skos:definition>Contractual terms for the settlement of a Term Deposit.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCContractSettlementTerms"/>
   </owl:Class>

   <!--Individuals-->

