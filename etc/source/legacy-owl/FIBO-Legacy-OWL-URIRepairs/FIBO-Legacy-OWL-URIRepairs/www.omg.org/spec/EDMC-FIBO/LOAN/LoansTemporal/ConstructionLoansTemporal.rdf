         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/ConstructionLoansTemporal/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/ConstructionLoansTemporal/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/LoansStatus/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/ConstructionLoansTemporal/hasSnapshot">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/LoansStatus/hasSnapshot"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoanTypes/ConstructionLoans/ConstructionLoan"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/ConstructionLoansTemporal/ConstructionLoanSnapshot"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/ConstructionLoansTemporal/loanAdvanceToDate">
      <rdfs:range rdf:resource="http://www.omg.org/spec/FIBO/Foundation/Accounting/CurrencyAmount/MoneyAmount"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/ConstructionLoansTemporal/ConstructionLoanSnapshot">
      <rdfs:label>Construction Loan Snapshot</rdfs:label>
      <skos:definition>Information at a point in time, specific to a Construction Loan.</skos:definition>
      <skos:editorialNote>This covers the "Loan advance to date" amount, which will have a current value on any given day and which will vary over time, as milestones are passed and additional advances drawn down.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/LOAN/LoansTemporal/LoansStatus/LoanSnapshotOrStatus"/>
   </owl:Class>

