<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesClassification/SecurityClassification">
      <rdfs:label>Security Classification</rdfs:label>
      <skos:definition>The classification of the type of security, e.g. Equity, Debt, Short Term Note etc.</skos:definition>
      <skos:editorialNote>Not the same as the Industry Classification identifying the industry sector associated with the Security.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/Classification"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesClassification/SecurityClassificationScheme">
      <rdfs:label>Security Classification Scheme</rdfs:label>
      <skos:definition>The Scheme defining the classification of a security i..e what type of security it is (for example the ISO 10962 CFI lassification Scheme). This may be referred to by means of a URI or some other means depending on the convention adopted on implementation.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/Schemes/ClassificationScheme"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesClassification/IndustrySectorClassification">
      <rdfs:label>Industry Sector Classification</rdfs:label>
      <skos:definition>An Industry Sector Classification, identifying industries according to some Scheme. This may be applied to a Security, an Organization or some other thing or activity.</skos:definition>
   </owl:Class>

