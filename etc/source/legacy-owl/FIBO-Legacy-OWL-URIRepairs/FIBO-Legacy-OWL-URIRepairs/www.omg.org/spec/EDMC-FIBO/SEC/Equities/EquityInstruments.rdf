         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/">

   <owl:Ontology rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/ShareholderEquity/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/InstrumentContracts/"/>
   </owl:Ontology>

   <!--Datatypes-->

<rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/DistributionMechanism">
      <skos:definition>The method by which amounts owing to a holder of an equity or a fund, are distributed, e.g. by payment of a monetary amount or by reinvestment in the same company or fund. Note that this may be defined in a contract or instrument, in which case it will be referred to in contractual terms. In many cases it is not defined in the product, but will be defined in a Corporate Action message at the time that the distribution is made. In this case, this selection will be referred to in the Corporate Action / Event message.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Payment</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reinvestment</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Security</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/SecurityPaymentStatus">
      <skos:definition>The Payment Status of a Unit or other Security.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nil</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Partly</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fully</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/Seniority">
      <skos:definition>The seniority of a tranche of equity or debt. The precise implications of the different levels of seniority are defined for the instrument class in question.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Senior</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Junior</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Other</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/VotingRightsTypeSelection">
      <skos:definition>Detailed definition of the voting rights given to the holder of a class of Shares or other Equity Instruments, as a result of holding those securities.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ordinary</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extraordinary</rdf:first>
                        <rdf:rest>
                           <rdf:Description>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subordinated</rdf:first>
                              <rdf:rest>
                                 <rdf:Description>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One Per Holder</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description>
                                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Restricted</rdf:first>
                                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                       </rdf:Description>
                                    </rdf:rest>
                                 </rdf:Description>
                              </rdf:rest>
                           </rdf:Description>
                        </rdf:rest>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/hasHolder">
      <skos:definition>A party which holds the publicly issued share.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecurityAssets/holder"/>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/holder"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/PubliclyTradedShare"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/CorporateOwnership/PublicShareholder"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/shareConfers">
      <skos:definition>The right of a stockholder to vote on matters of corporate policy as well as on who is to compose the board of directors. Rights can be of junior or senior rank (precedence of ranks).</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgreementsExt/ContractualConstructs/confers"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/Share"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/VotingRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/holder">
      <skos:definition>A party which holds the share.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Agreements/Contracts/heldBy"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/Share"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/CorporateOwnership/Shareholder"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/stockholderEquityRepresents">
      <skos:definition>The Monetary Amount (capital amount) represented by the Stockholder Equity. This is the total equity in the firm in the form of shares (that is, the total equity other than additional paid in capital).</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Accounting/AccountingEquity/equityTakesMaterialForm"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/ShareholderEquity"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/FIBO/Foundation/Accounting/CurrencyAmount/MoneyAmount"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersExactlyOneVotingRight">
      <skos:definition>The share confers exactly one voting right on the Holder.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersSome"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/SingleVotingShare"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/VotingRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersNo">
      <skos:definition>The share confers no voting rights on the Holder.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/shareConfers"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/NonVotingShare"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/VotingRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersOwnershipOfEquity">
      <skos:definition>Equity represented by the Share and owned by the Holder of the Share in proportion to the amount of the Issue that they hold.</skos:definition>
      <skos:editorialNote>A person or organization that holds at least a partial share of stock is called a shareholder.</skos:editorialNote>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/Share"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/ShareholderEquity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersOwnershipOfIssuedEquity">
      <skos:definition>A share confers on the holder the legal right of ownership of issued equity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersOwnershipOfEquity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/PubliclyTradedShare"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/PubliclyIssuedEquity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersOwnershipOfPrivateEquity">
      <skos:definition>Equity represented by the Private Equity security and owned by the Holder of the share in proportion to the amount of the issue that they hold.</skos:definition>
      <skos:editorialNote>A person or organization that holds at least a partial share of stock is called a shareholder.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersOwnershipOfEquity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/PrivatelyHeldShare"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/PrivateEquity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersSome">
      <skos:definition>The share confers one or more voting rights on the Holder.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/shareConfers"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/VotingShare"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/VotingRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersTwoOrMoreVotingRight">
      <skos:definition>The share confers multiple voting rights on the Holder.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/confersSome"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/MultipleVotingShare"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/VotingRight"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/holdsSomeShareholderEquity">
      <skos:definition>The shareholder holds some of the issued shareholder equity in the company, by way of the shares that they hold.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/CorporateOwnership/shareholderHoldsEquity"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/OwnershipAndControl/CorporateOwnership/Shareholder"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/ShareholderEquity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/isSeniorTo">
      <skos:definition>The share has a higher seniority than the related share, meaning that it gives the holder a higher claim on the assets of the issuing entity in the event of the winding up of that entity.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/isSeniorToAnotherClassOf"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/Share"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/Share"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/isSeniorToAnotherClassOf">
      <skos:definition>A class of equity may have a higher claim on the assets of the issuing entity in the event of the winding up of that entity than another class of equity. This is referred to as taking precedence or priority over that other class of equity. It is also referred to as the first class being senior to that other class of issued equity.</skos:definition>
      <skos:editorialNote>This information is also embodied in the property of Priority for the class of equity. These are two indications of the same information, relating to the equity and the share that gives ownership of that equity, respectively.</skos:editorialNote>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/ShareholderEquity"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/ShareholderEquity"/>
   </owl:ObjectProperty>

