   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/numberOfSharesInIssue">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/wholeNumber"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Equities/EquityInstruments/nonPaidAmount">
