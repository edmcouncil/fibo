      <owl:inverseOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/Spots/embodies"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/FxDerivatives/FxSpots/benefitSubject">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/subject"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/FxDerivatives/FxSpots/FxSpotTransaction"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/SupplyTransactions/NegotiableCash"/>
