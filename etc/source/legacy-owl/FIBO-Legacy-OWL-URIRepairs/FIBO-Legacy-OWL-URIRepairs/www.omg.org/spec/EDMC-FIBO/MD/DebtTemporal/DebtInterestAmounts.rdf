      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/DebtTemporal/DebtInterestAmounts/TreasuryBillReferenceRate"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/DebtTemporal/DebtInterestAmounts/TreasuryBillInterestRateValue"/>
