   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/MD/DebtTemporal/DebtInterestAmounts/hasCoupon.3">
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/DebtTemporal/DebtInterestAmounts/bondHasCoupon"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Debt/Bonds/BondsCashflowVariants/VariableCouponBond"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/MD/DebtTemporal/DebtInterestAmounts/BondVariableCoupon"/>
