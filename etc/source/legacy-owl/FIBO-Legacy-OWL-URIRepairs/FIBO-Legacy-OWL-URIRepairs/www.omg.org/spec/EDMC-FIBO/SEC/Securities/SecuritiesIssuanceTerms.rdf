   <!--Datatypes-->

<rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesIssuanceTerms/SecurityOfferingDistributionType">
      <skos:definition>The distribution type of a securities Offering. Primary Offering - The original sale of a company's securities, in which the proceeds from the sale are received directly by the company. Secondary Offering - An Offering of a security which has been previously issued.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Primary</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secondary</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesIssuanceTerms/SecurityOfferingSaleMethod">
      <skos:definition>Method for sale of a new security offering.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Competitive</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Negotiated</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                     </rdf:Description>
                  </rdf:rest>
               </rdf:Description>
            </owl:oneOf>
         </rdfs:Datatype>
      </owl:equivalentClass>
   </rdf:Description>

   <rdf:Description rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesIssuanceTerms/SecurityOfferingType">
      <skos:definition>The way in which a Securities Issue is offered.</skos:definition>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">New</rdf:first>
                  <rdf:rest>
                     <rdf:Description>
