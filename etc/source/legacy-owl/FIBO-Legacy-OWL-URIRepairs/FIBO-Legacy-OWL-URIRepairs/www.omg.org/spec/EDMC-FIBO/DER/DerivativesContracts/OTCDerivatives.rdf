<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCDerivatives/OTCDerivativeContract">
      <rdfs:label>OTC Derivative Contract</rdfs:label>
      <skos:definition>A derivative contract which is struck between two parties "Over the Counter".</skos:definition>
      <skos:editorialNote>This is both a Derivative Contract (not all of which are struck OTC) and an OTC contract (not all of which are Derivatives.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/DerivativesBasics/DerivativeContract"/>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCInstruments/OTCInstrumentContract"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCDerivatives/OTCDerivativeTransaction">
      <rdfs:label>OTC Derivative Transaction</rdfs:label>
      <skos:definition>A derivative transaction which is struck over the counter between two parties.</skos:definition>
      <skos:editorialNote>This is both a derivative transaction and a Covered Transaction, that is, the OTC transaction is transacted under some previously agreed Master Agreement between the two parties.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/TransactionsExtended/CoveredTransaction"/>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/DerivativesBasics/DerivativeTransaction"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/DER/DerivativesContracts/OTCDerivatives/OTCProduct">
      <rdfs:label>OTC Product</rdfs:label>
      <skos:definition>An Over the Counter Derivative. This is made up of one or more transactions, with a corresponding contract or contracts.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Production/Production"/>
   </owl:Class>

