<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/MarketIndicators/EconomicRate">
      <rdfs:label>Economic Rate</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsParameters/MarketBaseRate"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/MarketIndicators/Economy">
      <rdfs:label>Economy</rdfs:label>
      <skos:definition>A geographically distinct web of economic relationships.</skos:definition>
      <skos:editorialNote>This may be a national economy (relating to a country); a regional economy, or the economy of a state or some other intra-national entity. It need not always be geographical in the strictest sense, for example Second Life has an economy. For the purposes of this model, it refers only to economies invilving monetqry exchange i.e. not the broader sense used by evolutionary biologists etc.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/IndependentThing"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/MarketIndicators/GrossDomesticProduct">
      <rdfs:label>Gross Domestic Product</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/MarketIndicators/EconomicRate"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/MarketIndicators/InflationRate">
      <rdfs:label>Inflation Rate</rdfs:label>
      <skos:definition>The annual percent change in consumer prices compared with the previous year's consumer prices, for a given country.</skos:definition>
      <skos:editorialNote>Always either includes or excludes: Energy prices Food prices ALL inflation rates cite whether or not they exclude energy and food prices. If nothing stated it is assumed they include them.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndicators/MarketIndicators/UnemploymentRate">
