   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/EquityIndexConstituent">
      <rdfs:label>Equity Index Constituent</rdfs:label>
      <skos:definition>A constituent of the Equity Index.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/ReferenceIndexConstituent"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/EquityReferenceIndex">
      <rdfs:label>Equity Reference Index</rdfs:label>
      <skos:definition>A benchmark or reference index the constituents of which are exclusively Equity securities.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/ReferenceIndex"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/EquityReferenceIndexBasket">
      <rdfs:label>Equity Reference Index Basket</rdfs:label>
      <skos:definition>The basket of securities which makes up the Equities Reference Index.</skos:definition>
      <skos:editorialNote>This is the set of securities of which the collective price, when combined as defined for this basket, is the Reference Index value.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/ReferenceIndexBasket"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/ReferenceIndex">
      <rdfs:label>Reference Index</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/pre-IND/Indicators/IndicatorsParameters/MarketBaseRate"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/ReferenceIndexBasket">
      <rdfs:label>Reference Index Basket</rdfs:label>
      <skos:definition>The basket of securities which makes up a published index.</skos:definition>
      <skos:editorialNote>For most securities terms all that matters is that the index is a number put out by a publisher, However, we need to also model indices themselves, and there are also certain classes of equity derivative which refer to individual components of an index. Therefore an index is now modeled in terms of the basket of which it is made up. Note: Not to be confused with a Basket Of Indices, which would be a basket whose constituents are indices. By contrast, this term is the basket of securities which makes up a basket of securities.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/BasketOfSecurities"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/pre-IND/MarketIndices/BasketIndices/ReferenceIndexConstituent">
      <rdfs:label>Reference Index Constituent</rdfs:label>
      <skos:definition>A security which makes up part of a Reference Index.</skos:definition>
      <skos:editorialNote>A Reference Index as defined in this model is an index of securities, and therefore does not include credit indices, economic indicators or any other kind of index. Therefore this kind of thing which is a Reference Index Constituent is a kind of Securities Basket Constituent, i.e. a security with some composition information and current pricing.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Baskets/SecuritiesBasketConstituent"/>
   </owl:Class>

   <!--Individuals-->

