   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/hasTaxTreatment">
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/FinancialInstruments/FinancialInstrument"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/FinancialInstrumentTaxTreatment"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/hasTaxTreatment.1">
