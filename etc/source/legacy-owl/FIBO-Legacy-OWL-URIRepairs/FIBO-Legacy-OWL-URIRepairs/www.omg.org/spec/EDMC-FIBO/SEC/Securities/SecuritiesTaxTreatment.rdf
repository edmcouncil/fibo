      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/Securities/Security"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/SecurityTaxTreatment"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/exempt">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/yesOrNo"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/frankedRate">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/percentage"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/securityTaxCode">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/FinancialInstrumentTaxTreatment">
      <rdfs:label>Financial Instrument Tax Treatment</rdfs:label>
      <skos:definition>The treatment of the financial instrument for taxation purposes. This is the most general form of tax treatment for financial instruments, including cash.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Accounting/Taxation/TaxTreatment"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/SecurityTaxTreatment">
      <rdfs:label>Security Tax Treatment</rdfs:label>
      <skos:definition>Amount of money due to the government or tax authority, according to various pre-defined parameters such as thresholds or income.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/SEC/Securities/SecuritiesTaxTreatment/FinancialInstrumentTaxTreatment"/>
   </owl:Class>

