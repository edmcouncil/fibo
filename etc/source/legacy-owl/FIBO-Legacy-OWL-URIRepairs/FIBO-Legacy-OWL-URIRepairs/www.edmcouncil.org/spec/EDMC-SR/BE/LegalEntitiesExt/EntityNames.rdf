<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Corporations/Corporations/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/People/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Organizations/Organizations/"/>
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/humanBeingHasLegalName">
      <skos:definition>The legal name by which the person is known.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasName"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/People/Person"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/PersonalName"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasLegalName">
      <skos:definition>The registered legal name of the Incorporated Company.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasLegalName.1"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Corporations/Corporations/IncorporatedCompany"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/LegalName"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasAlias">
      <skos:definition>Any other name by which the Legal Entity is known but which is not its usual given name or its registered formal name.</skos:definition>
      <skos:editorialNote>Added at SME Review, to meet AML requirements.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasName"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasLegalName.1">
      <skos:definition>The name by which the legal entity is referred to in legal communications.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasName"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/LegalName"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasName">
      <skos:definition>The name by which the autonomous thing is known.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/details"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasTradingName">
      <skos:definition>The name by which the business is known for trading purposes.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasName"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/wasFormerlyKnownAs">
      <skos:definition>A name by which the entity has been known in the past but is not known by at the present time.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasName"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Name"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/tradingAs">
      <skos:definition>Alternative name/Business name by which the entity is also known</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/hasName"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Corporations/Corporations/IncorporatedCompany"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Name"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/fullLegalName">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/familyName">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/givenName">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/additionalName">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/LegalName">
      <rdfs:label>Legal Name</rdfs:label>
      <skos:definition>The full legal name of an entity.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/Name"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/LegalEntitiesExt/EntityNames/PersonalName">
      <rdfs:label>Personal Name</rdfs:label>
      <skos:definition>The name of an individual person.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>