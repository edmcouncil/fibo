<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/Venues/venueLocatedAt">
      <skos:definition>The place which is identified as the Venue.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/identity"/>
