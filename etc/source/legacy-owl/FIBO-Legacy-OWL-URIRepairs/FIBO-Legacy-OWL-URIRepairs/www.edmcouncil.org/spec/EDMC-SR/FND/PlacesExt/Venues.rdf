<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/PlacesExt/Venues/Venue">
      <rdfs:label>Venue</rdfs:label>
      <skos:definition>A place where something happens, described in the context of the thing which happens.</skos:definition>
      <skos:editorialNote>This is the most primitive concept possible. Another level of detail would have a kind of Venue whose "location" identity is a Legal Jurisdiction.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/RelativeThing"/>
   </owl:Class>

