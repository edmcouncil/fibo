<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/LegalPersons/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/constitutedBy">
      <skos:definition>The legal document which forms the basis of the Foundation. This is the legal instrument by which it has its existence.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/isConstitutedBy"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/Foundation"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/InstrumentOfIncorporation"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/governedByFoundationAgreement">
      <skos:definition>The agreement which governs the Foundation. This is the document which governs the activities of the Foundation and the contractual relations among the principals.</skos:definition>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/formallyConstitutedOrganizationIsGovernedBy"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/Foundation"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/FoundationAgreement"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/Anstalt">
      <rdfs:label>Anstalt</rdfs:label>
      <skos:definition>Form of Foundation registered in Leichtenstein.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/Foundation"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/Foundation">
      <rdfs:label>Foundation</rdfs:label>
      <skos:definition>A formal foundation. Further Notes Formal definition sought.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/FoundationAgreement">
      <rdfs:label>Foundation Agreement</rdfs:label>
      <skos:definition>The formal agreement by which the Foundation is governed</skos:definition>
      <skos:editorialNote>This is the or any agreement among the principals, as distinct from the or any founding charter.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/OrganizationCoveringAgreement"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/LegallyIncorporatedFoundation">
      <rdfs:label>Legally Incorporated Foundation</rdfs:label>
      <skos:definition>A foundation with independent legal personhood.</skos:definition>
      <skos:editorialNote>The mechanism for this to be possible is typical in Civil Law (e.g. mainland European) jurisdictions. In Common Law jurisdictions the term Foundation is more typically descriptive of the activity and tax status of an entity not of its legal personhood or lack of it.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/CorporateBodies/BodyCorporate"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/PrivateInterestFoundation">
      <rdfs:label>Private Interest Foundation</rdfs:label>
      <skos:definition>A form of foundation registered in Panama.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/EntitiesTypesExt/LegalFoundations/UnincorporatedFoundation">
      <rdfs:label>Unincorporated Foundation</rdfs:label>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>