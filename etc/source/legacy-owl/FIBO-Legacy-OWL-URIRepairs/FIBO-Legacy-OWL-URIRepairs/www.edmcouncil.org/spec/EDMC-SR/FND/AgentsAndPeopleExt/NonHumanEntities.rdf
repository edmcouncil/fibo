<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgentsAndPeopleExt/NonHumanEntities/MechanicalRobot">
      <rdfs:label>Mechanical Robot</rdfs:label>
      <skos:definition>Some robot which has physical extent.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgentsAndPeopleExt/NonHumanEntities/Robot"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgentsAndPeopleExt/NonHumanEntities/NonHumanAnimal">
      <rdfs:label>Non Human Animal</rdfs:label>
      <skos:definition>Some animal other than a member of the species /homo/ /sapiens/</skos:definition>
      <skos:editorialNote>Included for completeness and scope of the model. Completes the range of entities that come under the grouping "Autonomous Entity". Note that there are legal moves around the concept of "Non human persons" (self aware animals such as dolphins). These are not covered in the present model. It would be necessary to add these only if we were to extend this model to describe the rights extended to persons which may include this category.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgentsAndPeopleExt/NonHumanEntities/Robot">
      <rdfs:label>Robot</rdfs:label>
      <skos:definition>Any self actualizing artificial entity.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgentsAndPeopleExt/NonHumanEntities/SoftwareAgent">
      <rdfs:label>Software Agent</rdfs:label>
      <skos:definition>Some self-actualizing routine or set of routine which does not have physical extent.</skos:definition>
   </owl:Class>

