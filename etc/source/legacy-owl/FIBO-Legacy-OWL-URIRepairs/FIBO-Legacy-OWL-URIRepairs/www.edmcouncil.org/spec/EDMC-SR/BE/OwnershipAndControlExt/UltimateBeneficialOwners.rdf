   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/UltimateBeneficialOwners/identity">
      <skos:definition>ultimate Beneficial Owner is any kind of Human Being.</skos:definition>
      <skos:editorialNote>Questions around minor versus adult human being. Assume so far that ...</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/OwnershipPartiesExtras/entityOwningPartyIdentity"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/UltimateBeneficialOwners/UltimateBeneficialOwner"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/AgentsAndPeople/People/Person"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/UltimateBeneficialOwners/hasUltimateBeneficialOwner">
      <skos:definition>The ultimate holders of the assets of a hierarchy of companies. These may be people or other legal entities.</skos:definition>
      <skos:editorialNote>Defined by law as the owning or controlling of more than (a threshold e.g. 25% or 10% of the entity, as defined in the individual AML laws in the applicable jurisdiction). Review: Determine whether this is any human being or an adult human being; else OK.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/CorporateOwnershipExtras/partHeldByShareholder"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Corporations/Corporations/IncorporatedCompany"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/UltimateBeneficialOwners/UltimateBeneficialOwner"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/UltimateBeneficialOwners/isUltimateBeneficialOwnerOf">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/UltimateBeneficialOwners/UltimateBeneficialOwner"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/Corporations/Corporations/IncorporatedCompany"/>
      <owl:inverseOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/OwnershipAndControlExt/UltimateBeneficialOwners/hasUltimateBeneficialOwner"/>
   </owl:ObjectProperty>

