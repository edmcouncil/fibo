<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/">
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Publications/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/standardSetBy">
      <skos:definition>The agency by which the standard is set.</skos:definition>
      <skos:editorialNote>This may be one of a range of types of entity; this relationship is defined in terms of some entity standing in the role of Standards Setter.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Publications/publishedBy"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/TechnicalStandard"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/StandardsSetter"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/standardsSetterIdentity">
      <skos:definition>The entity which performs the role of Standards Setter.</skos:definition>
      <skos:editorialNote>This could be any type of Formal Organization.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Publications/publisherIdentity"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/StandardsSetter"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/bodyAcronym">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/acronym">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <owl:DatatypeProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/standardName">
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Utilities/BusinessFacingTypes/text"/>
   </owl:DatatypeProperty>

   <!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/IndustryAssociation">
      <rdfs:label>Industry Association</rdfs:label>
      <skos:definition>An organization which has membership drawn from members of some industry and acts to serve the interests of those members.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/LegalEntities/FormalBusinessOrganizations/FormallyConstitutedOrganization"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/StandardsBody">
      <rdfs:label>Standards Body</rdfs:label>
      <skos:definition>An organization dedicated exclusively to the propagation of standards.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/StandardsSetter">
      <rdfs:label>Standards Setter</rdfs:label>
      <skos:definition>An agent which acts to set standards such as message standards, programming standards and so on. This may in principal be any kind of business entity.</skos:definition>
      <skos:editorialNote>Note that by virtue of its activities the Standards Setter is also a Publisher, that is it publishes versions of the standards that it sets. However the activity of setting a standard is not itself a direct specialization of the activity of publishing, rather it is a by-product of that activity. This is why there is an apparently redundant parent relationship with Publisher.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Publications/Publisher"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Standards/TechnicalStandard">
      <rdfs:label>Technical Standard</rdfs:label>
      <skos:definition>An established norm or requirement about technical systems; a published document formally setting out such established norm.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Publications/Publications/PublishedInformation"/>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>