   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/LawExt/JuridicialReach/">
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/"/>
      <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Locations/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/LawExt/JuridicialReach/civilJurisdictionGeographicalReach">
      <skos:definition>The geopolitical entity (usually country) over which the Civil Jurisdiction has its reach.</skos:definition>
      <skos:editorialNote>This is typically one country, since civil law applies to the country which makes the laws.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/hasReach"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/CivilLawJurisdiction"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Countries/GeopoliticalEntity"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/LawExt/JuridicialReach/statuteLawHasGeographicalReach">
      <skos:definition>The geographical extent of the jurisdiction in which the Statute Law has effect.</skos:definition>
      <skos:editorialNote>This is usually but not always a Country. It may also be a part of a country (England-and-Wales) or a State or Province within a Federal country, or it may be the federal country itself if the statute is a federal law.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/isInForceIn"/>
      <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/hasReach"/>
      <rdfs:domain rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/Jurisdictions/StatuteLaw"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Places/Countries/GeopoliticalEntity"/>
   </owl:ObjectProperty>

