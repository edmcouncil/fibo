<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Production/EventProgram">
      <rdfs:label>Event Program</rdfs:label>
      <skos:definition>A planned sequence of events.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Production/Program"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Production/Production">
      <rdfs:label>Production</rdfs:label>
      <skos:definition>A combination of an event and a context, in which something or some range of things are produced, created or issued.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/Lattice/OccurentThing"/>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/MediatingThing"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Production/ProductionProgram">
      <rdfs:label>Production Program</rdfs:label>
      <skos:definition>A planned set of Productions.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Production/Program">
      <rdfs:label>Program</rdfs:label>
      <skos:definition>A set of activities and / or events set up at one time by some agency with the intent to have those activities and / or events take place at some point in the future.</skos:definition>
      <skos:editorialNote>Modeled as a set of Productions. It may be necessary in future to distinguish programs which are a set of productions (this class) from programs which are a set of other types of activity.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Occurrent/Activities/Activity"/>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/SetMembership/Set"/>
   </owl:Class>

