   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/specifiedAs">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expressedAs"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/SpecifiedVariableQuantity"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/VariableSpecification"/>
      <owl:inverseOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/specifies.1"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/mutableTo">
      <skos:definition>Each way in which the variable quantity may be expressed, is mutable to another equivalent way in which that quantity may also be expressed while remaining the same quantity.</skos:definition>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/VariableSpecification"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/VariableSpecification"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/specifies.1">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expresses"/>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/specifies"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/VariableSpecification"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/SpecifiedVariableQuantity"/>
   </owl:ObjectProperty>

