<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expresses">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/isAbout"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/Expression"/>
