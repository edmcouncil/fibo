   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expresses.1">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expresses"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/ProbabilityMeasure"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/ObjectiveProbability"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expressedAs">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/details"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/Quantity"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/Expression"/>
      <owl:inverseOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expresses"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/specifies">
      <skos:definition>The thing which is specified by the Specification.</skos:definition>
      <skos:editorialNote>This may be a Continuant Thing or an Occurrent Thing. It is always an Independent Thing on the assumption that the relative nature of a Relative Thing is not specified but rather the independent thing is specified in whatever relative context the specification is applied.</skos:editorialNote>
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Information/InfoCore/isAbout"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/Specification"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expresses.2">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/expresses"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/PlatonicQuantities/Symbol"/>
