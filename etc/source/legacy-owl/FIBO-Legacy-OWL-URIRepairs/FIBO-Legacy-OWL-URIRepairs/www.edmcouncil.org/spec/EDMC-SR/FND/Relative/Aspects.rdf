<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
         xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xml:base="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/">

   <owl:Ontology rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/">
      <owl:imports rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/"/>
   </owl:Ontology>

   <!--Datatypes-->

<!--Object Properties-->

<owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/hasAlternativeViewpoint">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Aspect"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Aspect"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/isAspectOf">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/inContext"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Aspect"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/IndependentThing"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/hasOtherSide">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/hasAlternativeViewpoint"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Side"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Side"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/seenFromPerspectiveOf">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Side"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole"/>
   </owl:ObjectProperty>

   <!--Data Properties-->

<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Form">
      <rdfs:label>Form</rdfs:label>
      <skos:definition>The form which something takes.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/UO/OntologicalDegree/RelativeThing"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Aspect">
      <rdfs:label>Aspect</rdfs:label>
      <skos:definition>Something defined as from some perspective. Further notes: Develop this for rights and obligation perspectives as well.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Side">
      <rdfs:label>Side</rdfs:label>
      <skos:definition>An aspect of something that may be seen from one of two aspects, as seen from viewers at two viewpoints of that thing.</skos:definition>
      <skos:editorialNote>This may be a concrete (two dimensional) thing, or an abstract thing which can be seen from two sides.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Relative/Aspects/Aspect"/>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>