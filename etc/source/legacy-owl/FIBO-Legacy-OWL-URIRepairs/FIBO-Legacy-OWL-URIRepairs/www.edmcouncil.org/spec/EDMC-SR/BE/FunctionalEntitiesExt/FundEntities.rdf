<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/FundEntities/FCP">
      <rdfs:label>FCP</rdfs:label>
      <skos:definition>A French fund vehicle type. Designation and definition needed.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/BE/FunctionalEntities/FunctionalEntities/FunctionalBusinessEntity"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/FundEntities/FundsSPV">
      <rdfs:label>Funds SPV</rdfs:label>
      <skos:definition>A Special Purpose Vehicle (SPV) set up specifically to create an investment fund. It is set up by a company or a group of companies for this purpose. The SPV is formed for a specific reason and exists for a specific period of time and is then disbanded.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/SPVs/SpecialPurposeVehicle"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/BE/FunctionalEntitiesExt/FundEntities/SICAF">
      <rdfs:label>SICAF</rdfs:label>
