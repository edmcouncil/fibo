<!--Annotation Properties-->

<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/AspectViewingParty">
      <rdfs:label>Aspect Viewing Party</rdfs:label>
      <skos:definition>The party from the perspective of which the aspect is defined.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Parties/Parties/PartyInRole"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/ContractualTransactionObligation">
      <rdfs:label>Contractual Transaction Obligation</rdfs:label>
      <skos:definition>An obligation which is one perspective on a commitment which is both a contractual commitment and a transaction-related economic commitment.</skos:definition>
      <skos:editorialNote>REA supports the definition of transactions both within and between distinct legal entities or persons. This element forms part of the definition of REA transaction concepts which are between entities.</skos:editorialNote>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/REATransactionObligation"/>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/AgreementsExt/ContractualConstructs/ContractualObligation"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/DebtClaim">
      <rdfs:label>Debt Claim</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/REAClaim"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/EquityClaim">
      <rdfs:label>Equity Claim</rdfs:label>
      <skos:definition>Needs to be unpicked Equity represents ownership in some venture. You own a piece of the assets of the venture, which gives you a claim on those assets. there is parallel to the discharging of obligations and rights in a transaction, in there here, the obligation/right continues to exist until or unless the venture is wound up.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/REATransactionObligation">
      <rdfs:label>REA Transaction Obligation</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/SocialConstructs/ConstructAspects/Obligation"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/REATransactionRight">
      <rdfs:label>REA Transaction Right</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/SocialConstructs/ConstructAspects/Right"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/CashflowCommitment">
      <rdfs:label>Cashflow Commitment</rdfs:label>
      <skos:definition>A legal or contractual commitment to some cash flows in the future.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/TransactionsExtended/ContractuallyDefinedBenefit"/>
   </owl:Class>

