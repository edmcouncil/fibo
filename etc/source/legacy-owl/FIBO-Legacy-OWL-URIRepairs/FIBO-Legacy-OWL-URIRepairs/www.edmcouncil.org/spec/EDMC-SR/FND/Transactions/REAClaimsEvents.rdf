      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/AspectViewingParty"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/asReportedBy">
      <rdfs:subPropertyOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/asSeenBy"/>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/SocialConstructs/ConstructAspects/CommitmentAspect"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/AccountingKeeper"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/definedOnThePartOf">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/REAClaim"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/AccountingKeeper"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/isImbalanceIn">
      <skos:definition>The imbalance in Obligations or Rights (depending on the viewpoint from which it is described) between one party and another.</skos:definition>
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/REAClaim"/>
      <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/Law/LegalCapacities/LegalConstruct"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/hasEnd">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/TransactionEvent"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/TransactionEvents/DischargingEvent"/>
   </owl:ObjectProperty>

   <owl:ObjectProperty rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REAClaimsEvents/hasStart">
      <rdfs:domain rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/REATransactions/TransactionEvent"/>
      <rdfs:range rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Transactions/TransactionEvents/TransactionUndertaking"/>
   </owl:ObjectProperty>

