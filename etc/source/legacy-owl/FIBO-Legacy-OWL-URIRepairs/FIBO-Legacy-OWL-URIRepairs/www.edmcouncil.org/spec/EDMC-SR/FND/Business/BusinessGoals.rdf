<!--Classes-->

<owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/BusinessGoal">
      <rdfs:label>Business Goal</rdfs:label>
      <skos:definition>A Goal is a state or condition of the enterprise to be brought about or sustained through appropriate Means.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/EDMC-SR/FND/GoalsAndObjectives/Goals/Goal"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/CorporateGoal">
      <rdfs:label>Corporate Goal</rdfs:label>
      <skos:definition>A business goal in the furtherance of the aims and profitability of a corporation.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/BusinessGoal"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/FinancialWellbeing">
      <rdfs:label>Financial Wellbeing</rdfs:label>
      <skos:definition>The overall balance sheet and well being of the organisation.</skos:definition>
      <rdfs:subClassOf rdf:resource="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/InvestmentGoal"/>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/InvestmentGoal">
      <rdfs:label>Investment Goal</rdfs:label>
      <skos:definition>A goal for which is to be achieved by investments within an investment institution.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/LendingGoal">
      <rdfs:label>Lending Goal</rdfs:label>
      <skos:definition>Reasons for extending loans and credit, i.e. the organisation has a goal to create capital growth by extending credit or making loans.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/MarketInvestmentGoal">
      <rdfs:label>Market Investment Goal</rdfs:label>
      <skos:definition>A business goal driving investment activities as distinct from day to day commercial trading activities.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/ProjectGoal">
      <rdfs:label>Project Goal</rdfs:label>
      <skos:definition>Some stated goal of a project as set out in the project planning documentation.</skos:definition>
      <skos:editorialNote>Project risks are framed in terms of their impact on stated project goals. Examples including meeting deadlines, meeting cost projections, and so on.</skos:editorialNote>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/Reputation">
      <rdfs:label>Reputation</rdfs:label>
      <skos:definition>The goal of a business to maintain or enhance its reputation.</skos:definition>
   </owl:Class>

   <owl:Class rdf:about="http://www.edmcouncil.org/spec/EDMC-SR/FND/Business/BusinessGoals/TransactionProcessingGoal">
      <rdfs:label>Transaction Processing Goal</rdfs:label>
      <skos:definition>The goal of processing transactions in which the organization is engaged, without error and within the stated time constraints.</skos:definition>
   </owl:Class>

   <!--Individuals-->

</rdf:RDF>