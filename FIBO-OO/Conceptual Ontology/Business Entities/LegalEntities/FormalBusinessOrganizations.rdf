    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/establishedPrimaryAddressBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;establishedPrimaryAddressBy">
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;PrimaryAddressRelation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/establishedLocationBy"/>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/establishedPrimaryAddressOf -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;establishedPrimaryAddressOf">
        <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;PrimaryAddressRelation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/establishedLocationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/establishedRegisteredAddressBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;establishedRegisteredAddressBy">
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;RegisteredAddressRelation"/>
        <owl:inverseOf rdf:resource="&fibo-be-le-fbo;establishesRegisteredAddress"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/establishedLocationBy"/>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/establishedRegisteredAddressOf -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;establishedRegisteredAddressOf">
        <rdfs:range rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;RegisteredAddressRelation"/>
        <owl:inverseOf rdf:resource="&fibo-be-le-fbo;hasEstablishedRegisteredAddress"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/establishedLocationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/establishesPrimaryAddress -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;establishesPrimaryAddress">
        <owl:inverseOf rdf:resource="&fibo-be-le-fbo;establishedPrimaryAddressBy"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/establishesLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/establishesRegisteredAddress -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;establishesRegisteredAddress">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/establishesLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/hasEquity -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasEquity">
        <rdfs:label>has equity</rdfs:label>
        <skos:definition>the formally constituted organization has some form of stockholders equity</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-fnd-acc-aeq;StockholdersEquity"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/hasEstablishedPrimaryAddress -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasEstablishedPrimaryAddress">
        <owl:inverseOf rdf:resource="&fibo-be-le-fbo;establishedPrimaryAddressOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/hasEstablishedLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/hasEstablishedRegisteredAddress -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasEstablishedRegisteredAddress">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/AddressRelations/hasEstablishedLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/hasInternalEntity -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasInternalEntity">
        <rdfs:label>has internal entity</rdfs:label>
        <skos:definition>the organization has some part which is formally defined as an Internal Entity, for example for reporting, management or accounting purposes</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:range rdf:resource="&fibo-be-le-fbo;OrganizationalSubUnit"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-le-fbo;hasSubUnit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/hasOperatingAddress -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasOperatingAddress">
        <rdfs:label>has operating address</rdfs:label>
        <skos:definition>address at which the organization carries out its operations</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/LegalEntities/FormalBusinessOrganizations/hasPrimaryAddress -->

    <owl:ObjectProperty rdf:about="&fibo-be-le-fbo;hasPrimaryAddress">
        <rdfs:label>has primary address</rdfs:label>
        <skos:definition>The main address at which communications to the organization may be addressed; its headquarters address.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-le-fbo;FormallyConstitutedOrganization"/>
        <owl:inverseOf rdf:resource="&fibo-be-le-fbo;primaryAddressOf"/>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
