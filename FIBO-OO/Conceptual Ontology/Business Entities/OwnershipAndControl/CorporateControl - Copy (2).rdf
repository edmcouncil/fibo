    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/globalUltimateParentOfRelation -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;globalUltimateParentOfRelation">
        <owl:inverseOf rdf:resource="&fibo-be-oac-cctl;globalUltimateParentageBy"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingPartyOfRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/globalUltimateParentageBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;globalUltimateParentageBy">
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;GlobalUltimateParent"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;GlobalUltimateParentRelation"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/globalUltimateParentageOf -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;globalUltimateParentageOf">
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;GlobalUltimateParentRelation"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;Subsidiary"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/hasAffiliate -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;hasAffiliate">
        <skos:definition>has a party which directly, or indirectly through one or more intermediaries, controls, or is controlled by, or is under common control with the company.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;hasControllingInterestParty"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;Affiliate"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/hasDomesticUltimateParent -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;hasDomesticUltimateParent">
        <skos:definition>the organization recognized as the ultimate parent of the company within its country or jurisdiction of incorporation, if it has one</skos:definition>
        <skos:editorialNote>In the case of companies that are subsidiaries of another company that itself has a parent, this identifies the organization at the top of the hierarchy of organizations in the country of registration. Adapted from consensus definition of Ultimate Parent, now that this is split into national and global parent.</skos:editorialNote>
        <fibo-fnd-utl-av:adaptedFrom>consensus definition of ultimate parent, with the split between domestic and global parent</fibo-fnd-utl-av:adaptedFrom>
        <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;hasMajorityControllingParty"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;DomesticUltimateParent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/hasGlobalUltimateParent -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;hasGlobalUltimateParent">
        <fibo-fnd-utl-av:adaptedFrom>consensus definition of ultimate parent, with the split between domestic and global parent</fibo-fnd-utl-av:adaptedFrom>
        <skos:editorialNote>In the case of companies that are subsidiaries of another company that itself has a parent, this identifies the organization at the top of the hierarchy, world-wide. Adapted from consensus definition of Ultimate Parent, now that this is split into national and global parent.</skos:editorialNote>
        <skos:definition>the organization recognized as the ultimate parent of the company, if it has one</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;isControlledOverFiftyPercentBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/hasMajorityControllingInterestParty -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;hasMajorityControllingInterestParty">
        <skos:editorialNote>This is defined as company or other Formal Organization which owns a controlling stake of greater than 50 percent (50 percent plus one voting share or above) in this company.</skos:editorialNote>
        <skos:definition>the parent organization of the company, if there is one.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cpty;hasSignificantControllingInterestParty"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestCompany"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/hasMajorityOwnedSubsidiary -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;hasMajorityOwnedSubsidiary">
        <skos:definition> a majority owned subsidiary of the company, in this case, where there is above 50 percent (50 percent plus one share) ownership of the shares.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;WhollyOwnedSubsidiary"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;hasSubsidiary"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/hasSubsidiary -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;hasSubsidiary">
        <skos:definition>&quot; a subsidiary of the company, that is an affiliae controlled by the company directly, or indirectly through one or more intermediaries.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;Subsidiary"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/holdsControllingInterestIn -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;holdsControllingInterestIn">
        <skos:definition>a company in which the voting shareholder owns some voting equity stake.</skos:definition>
        <rdfs:range rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;VotingShareholder"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-opty;holdsAnInterestIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/holdsMajorityControllingInterestIn -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;holdsMajorityControllingInterestIn">
        <skos:definition>the incorporated company in which the voting shareholder holds fifty percent or more of the voting share equity.</skos:definition>
        <rdfs:range rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;VotingShareholder"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;holdsControllingInterestIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/holdsSignificantControllingInterestIn -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;holdsSignificantControllingInterestIn">
        <skos:definition>the incorporated company in which the voting shareholder holds a significant proportion (but not fifty percent or more) of the voting share equity.</skos:definition>
        <rdfs:range rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;VotingShareholder"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;holdsControllingInterestIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/isControlledOverFiftyPercentBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;isControlledOverFiftyPercentBy">
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-ctl;isControlledBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/isControlledTotallyBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;isControlledTotallyBy">
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;isControlledOverFiftyPercentBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/isWhollyOwnedBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;isWhollyOwnedBy">
        <skos:definition>the formally constituted organization that has 100 percent ownership and control over a given incorporated company</skos:definition>
        <rdfs:domain rdf:resource="&fibo-be-corp-corp;IncorporatedCompany"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;TotalControllingInterestCompany"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;hasMajorityControllingInterestParty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/overFiftyPercentControlledPartyOfRelation -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;overFiftyPercentControlledPartyOfRelation">
        <owl:inverseOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestOf"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-ctl;controlledPartyOfRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/overFiftyPercentControllingInterestBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;overFiftyPercentControllingInterestBy">
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestCompany"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestRelation"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-ctl;controlBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/overFiftyPercentControllingInterestOf -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;overFiftyPercentControllingInterestOf">
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestRelation"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;Subsidiary"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-ctl;controlOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/overFiftyPercentControllingPartyOfRelation -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;overFiftyPercentControllingPartyOfRelation">
        <owl:inverseOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestBy"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-oac-ctl;controllingPartyOfRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/totalControlledPartyOfRelation -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;totalControlledPartyOfRelation">
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControlledPartyOfRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/totalControllingInterestBy -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;totalControllingInterestBy">
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;TotalControllingInterestCompany"/>
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;TotalControllingInterestRelation"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestBy"/>
        <owl:inverseOf rdf:resource="&fibo-be-oac-cctl;totalControllingPartyOfRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/totalControllingInterestOf -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;totalControllingInterestOf">
        <rdfs:domain rdf:resource="&fibo-be-oac-cctl;TotalControllingInterestRelation"/>
        <rdfs:range rdf:resource="&fibo-be-oac-cctl;WhollyOwnedSubsidiary"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestOf"/>
        <owl:inverseOf rdf:resource="&fibo-be-oac-cctl;totalControlledPartyOfRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/totalControllingPartyOfRelation -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;totalControllingPartyOfRelation">
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingPartyOfRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/totallyControlsInterestIn -->

    <owl:ObjectProperty rdf:about="&fibo-be-oac-cctl;totallyControlsInterestIn">
        <rdfs:subPropertyOf rdf:resource="&fibo-be-oac-cctl;controlsOverFiftyPercentInterestIn"/>
        <owl:inverseOf rdf:resource="&fibo-be-oac-cctl;isControlledTotallyBy"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&fibo-fnd-pty-rl;isPartyInRole"/>
            <rdf:Description rdf:about="&fibo-be-oac-cctl;totalControllingPartyOfRelation"/>
            <rdf:Description rdf:about="&fibo-be-oac-cctl;totalControllingInterestOf"/>
            <rdf:Description rdf:about="&fibo-fnd-pty-rl;hasPartyIdentity"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/Corporations/Corporations/IncorporatedCompany -->

    <owl:Class rdf:about="&fibo-be-corp-corp;IncorporatedCompany">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cctl;hasMajorityControllingInterestParty"/>
                <owl:onClass rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestCompany"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cpty;hasControllingInterestParty"/>
                <owl:onClass rdf:resource="&fibo-be-oac-cctl;VotingShareholder"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cpty;hasSignificantControllingInterestParty"/>
                <owl:onClass rdf:resource="&fibo-be-oac-cctl;SignificantShareholdingCompany"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cctl;isWhollyOwnedBy"/>
                <owl:onClass rdf:resource="&fibo-be-oac-cctl;TotalControllingInterestCompany"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <fibo-fnd-utl-av:explanatoryNote>The restrictions defined herein extend the definition of incorporated company to link it to external entities that hold shares in it.</fibo-fnd-utl-av:explanatoryNote>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/Affiliate -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;Affiliate">
        <rdfs:label>affiliate</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-be-oac-cpty;MajorityControllingParty"/>
                    <rdf:Description rdf:about="&fibo-be-oac-cctl;ControlledCompany"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <skos:definition rdf:datatype="&xsd;string">an affiliate of, or a person affiliated with, a specific person is a person that directly, or indirectly through one or more intermediaries, controls, or is controlled by, or is under common control with, the person specified.</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/ControlledCompany -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;ControlledCompany">
        <rdfs:label>Controlled Company</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-fnd-oac-ctl;ControlledParty"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;hasRole"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-be-oac-cpty;isControlledToSomeDegreeBy"/>
                        <owl:allValuesFrom rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>A company over which another incorporated company has some degree of control by way of ownership of voting shares</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/DomesticUltimateParent -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;DomesticUltimateParent">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestCompany"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/DomesticUltimateParentRelation -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;DomesticUltimateParentRelation">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestRelation"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/GlobalUltimateParent -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;GlobalUltimateParent">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestCompany"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/GlobalUltimateParentRelation -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;GlobalUltimateParentRelation">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cctl;globalUltimateParentageOf"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-oac-cctl;ControlledCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cctl;globalUltimateParentageBy"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-oac-cctl;VotingShareholdingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/JointVenturePartner -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;JointVenturePartner">
        <rdfs:label>Joint Venture Partner</rdfs:label>
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cpty;EntityControllingParty"/>
        <skos:definition>A party which has the role of Joint Venture Partner to some venture.</skos:definition>
        <skos:editorialNote>This is part of ongoing work - legal definitions sought.</skos:editorialNote>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/NonWhollyOwnedSubsidiary -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;NonWhollyOwnedSubsidiary">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;Subsidiary"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/OverFiftyPercentControllingInterestCompany -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestCompany">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;VotingShareholdingCompany"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/OverFiftyPercentControllingInterestRelation -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestRelation">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cpty;EquityBasedDeJureControl"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestOf"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-oac-cctl;ControlledCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cctl;overFiftyPercentControllingInterestBy"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-oac-cctl;VotingShareholdingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/SignificantShareholdingCompany -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;SignificantShareholdingCompany">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cpty;SignificantControllingInterestParty"/>
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;VotingShareholdingCompany"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/Subsidiary -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;Subsidiary">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;ControlledCompany"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/TotalControllingInterestCompany -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;TotalControllingInterestCompany">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cpty;TotalOwner"/>
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestCompany"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/BE/OwnershipAndControl/CorporateControl/TotalControllingInterestRelation -->

    <owl:Class rdf:about="&fibo-be-oac-cctl;TotalControllingInterestRelation">
        <rdfs:subClassOf rdf:resource="&fibo-be-oac-cctl;OverFiftyPercentControllingInterestRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-be-oac-cctl;totalControllingInterestBy"/>
                <owl:someValuesFrom rdf:resource="&fibo-be-oac-cctl;VotingShareholdingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
