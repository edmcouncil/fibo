    <owl:DatatypeProperty rdf:about="&fibo-be-corp-corp;hasDateOfIncorporation">
        <rdfs:label>has date of incorporation</rdfs:label>
        <skos:definition>The date of formation of the company. This is identified as the formal date of registration in company registration documents. Further Notes: Generally known as Date of Incorporation.</skos:definition>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-be-corp-corp;hasDateOfRegistration">
        <rdfs:label>has date of incorporation</rdfs:label>
        <skos:definition>Date at which the corporation is registered in some jurisdiction for regulatory and / or for tax purposes. Further Notes: This is analogous to a person registering for paying taxes somewhere, regardless of their birth, citizenship and so on.</skos:definition>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&fibo-be-corp-corp;hasLegalFormAbbreviation">
        <rdfs:label>has legal form abbreviation</rdfs:label>
        <skos:definition>The precise form of the incorporated company abbreviation as defined in the jurisdiction in which it is registered, for example LLC, LLP, Ltd, PLC, Corp.</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-utl-bt;text"/>
    </owl:DatatypeProperty>


