    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/Asset -->

    <owl:Class rdf:about="&fibo-fnd-oac-own;Asset">
        <rdfs:label>asset</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&fibo-fnd-pty-rl;ThingInRole"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&fibo-fnd-pty-rl;hasPartyIdentity"/>
                        <owl:onClass>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&fibo-fnd-oac-own;isOwnedBy"/>
                                <owl:onClass rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
                                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                            </owl:Restriction>
                        </owl:onClass>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasAcquisitionDate"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&fibo-fnd-oac-own;takesForm"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
