    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/isAssetOf -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isAssetOf">
        <rdfs:label>is an asset of</rdfs:label>
        <skos:definition>identifies the party that owns the asset</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/isOwnedBy -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isOwnedBy">
        <rdfs:label>is owned by</rdfs:label>
        <owl:inverseOf rdf:resource="&fibo-fnd-oac-own;owns"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/isOwnedThing -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isOwnedThing">
        <owl:inverseOf rdf:resource="&fibo-fnd-oac-own;ownershipOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/isOwnedThingInRole -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isOwnedThingInRole">
        <owl:inverseOf rdf:resource="&fibo-fnd-oac-own;ownershipOfThing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/isOwningParty -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isOwningParty">
        <owl:inverseOf rdf:resource="&fibo-fnd-oac-own;ownershipBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/isOwningPartyInRole -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;isOwningPartyInRole">
        <owl:inverseOf rdf:resource="&fibo-fnd-oac-own;ownershipByParty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/ownershipBy -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;ownershipBy">
        <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Ownership"/>
        <rdfs:range rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;relates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/ownershipByParty -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;ownershipByParty">
        <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Ownership"/>
        <rdfs:range rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;relates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/ownershipOf -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;ownershipOf">
        <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Ownership"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;relates"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/ownershipOfThing -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;ownershipOfThing">
        <rdfs:domain rdf:resource="&fibo-fnd-oac-own;Ownership"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;relates"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/owns -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;owns">
        <rdfs:label>owns</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">(1) to have (something) as one&apos;s own, possess, (2) to admit or acknowledge that something is the case or that one feels a certain way</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&fibo-fnd-oac-own;isOwningParty"/>
            <rdf:Description rdf:about="&fibo-fnd-oac-own;ownershipOf"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&fibo-fnd-pty-rl;isInRole"/>
            <rdf:Description rdf:about="&fibo-fnd-oac-own;isOwningPartyInRole"/>
            <rdf:Description rdf:about="&fibo-fnd-oac-own;ownershipOfThing"/>
            <rdf:Description rdf:about="&fibo-fnd-pty-rl;hasPartyIdentity"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/OwnershipAndControl/Ownership/takesForm -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-oac-own;takesForm">
        <rdfs:label>takes form</rdfs:label>
        <skos:definition>identifies the form the entity takes</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
    </owl:ObjectProperty>
    


