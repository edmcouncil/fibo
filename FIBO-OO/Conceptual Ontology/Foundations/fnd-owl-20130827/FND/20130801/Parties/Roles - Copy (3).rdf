    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/hasPartyIdentity -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-pty-rl;hasPartyIdentity">
        <rdfs:range rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
        <rdfs:domain rdf:resource="&fibo-fnd-pty-rl;AgentInRole"/>
        <owl:inverseOf rdf:resource="&fibo-fnd-pty-rl;isPartyInRole"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/hasRole -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-pty-rl;hasRole">
        <rdfs:label>has role</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">provides a means for relating a person, organization, group, or other entity to a role that entity plays in some relationship and context</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-pty-rl;Role"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/isPartyInRole -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-pty-rl;isPartyInRole"/>
    


