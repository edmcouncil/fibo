    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/establishedLocationBy -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-adr;establishedLocationBy">
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;Address"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;AddressRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/establishedLocationOf -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-adr;establishedLocationOf">
        <rdfs:range rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;AddressRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/establishesLocation -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-adr;establishesLocation">
        <owl:inverseOf rdf:resource="&fibo-fnd-plc-adr;establishedLocationBy"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;relates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasEstablishedLocation -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-adr;hasEstablishedLocation">
        <owl:inverseOf rdf:resource="&fibo-fnd-plc-adr;establishedLocationOf"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;relates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasLocation -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-adr;hasLocation">
        <owl:inverseOf rdf:resource="&fibo-fnd-plc-adr;locationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/locationOf -->

    <owl:ObjectProperty rdf:about="&fibo-fnd-plc-adr;locationOf">
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&fibo-fnd-plc-adr;establishesLocation"/>
            <rdf:Description rdf:about="&fibo-fnd-plc-adr;establishedLocationOf"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


