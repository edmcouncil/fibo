    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/AddressElement -->

    <rdfs:Datatype rdf:about="&fibo-fnd-plc-adr;AddressElement">
        <rdfs:label>address element</rdfs:label>
        <owl:equivalentClass rdf:resource="&xsd;string"/>
        <skos:definition>A textual element giving part of some postal address and referring to some physical aspect of the place to which the address refers</skos:definition>
    </rdfs:Datatype>
    


