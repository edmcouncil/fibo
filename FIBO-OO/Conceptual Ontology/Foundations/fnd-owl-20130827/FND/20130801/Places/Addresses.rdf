    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasAddressComponent -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasAddressComponent">
        <rdfs:label>has address component</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The postal address has as part of it some distinct textual element which performs some distinct function within the overall address such as referring to some specific physical place, built property feature or post office box.</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;Address"/>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasCountryName -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasCountryName">
        <rdfs:label>has country name</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The name of the country in which the address is situated, in some format which may be recognized in that or other countries. </skos:definition>
        <fibo-fnd-utl-av:termOrigin rdf:datatype="&xsd;anyURI">http://tools.ietf.org/html/rfc6350</fibo-fnd-utl-av:termOrigin>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;hasAddressComponent"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasLocality -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasLocality">
        <rdfs:label>has locality</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">That part of a written address which uniquely references some town, city or other urban area within the overall address. </skos:definition>
        <fibo-fnd-utl-av:termOrigin rdf:datatype="&xsd;anyURI">http://tools.ietf.org/html/rfc6350</fibo-fnd-utl-av:termOrigin>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;hasAddressComponent"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasPOBox -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasPOBox">
        <rdfs:label>has post office box</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Address element giving the Post Office Box number in the form of digits or letters plus digits.</skos:definition>
        <fibo-fnd-utl-av:termOrigin rdf:datatype="&xsd;anyURI">http://tools.ietf.org/html/rfc6350</fibo-fnd-utl-av:termOrigin>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;hasAddressComponent"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasPostalCode -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasPostalCode">
        <rdfs:label>has postal code</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The postal code for an address, in a format recognized by the postal authorities in the country in which the address is situated. </skos:definition>
        <fibo-fnd-utl-av:termOrigin rdf:datatype="&xsd;anyURI">http://tools.ietf.org/html/rfc6350</fibo-fnd-utl-av:termOrigin>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;hasAddressComponent"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasStateAbbreviation -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasStateAbbreviation">
        <rdfs:label>has state abbreviation</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Address element giving the formal abbreviation of the state or province (in federal countries) in which the address is situated. </skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;hasAddressComponent"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasStateName -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasStateName">
        <rdfs:label>has state name</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Address element giving the name of the state or province (in federal countries) in which the address is situated.</skos:definition>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;hasAddressComponent"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/EDMC-FIBO/FND/Places/Addresses/hasStreetAddress -->

    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-adr;hasStreetAddress">
        <rdfs:label>has street address</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Address element giving the building name or number and the street in which the address is situated. </skos:definition>
        <fibo-fnd-utl-av:termOrigin rdf:datatype="&xsd;anyURI">http://tools.ietf.org/html/rfc6350</fibo-fnd-utl-av:termOrigin>
        <rdfs:range rdf:resource="&fibo-fnd-plc-adr;AddressElement"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-adr;hasAddressComponent"/>
    </owl:DatatypeProperty>
    


