	<fibo-ln-ln-ln:PublicRecordCategory rdf:about="&fibo-ln-ln-ln;_PublicRecordCategory_foreclosure">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>foreclosure</rdfs:label>
	</fibo-ln-ln-ln:PublicRecordCategory>
	
	<fibo-ln-ln-ln:PublicRecordCategory rdf:about="&fibo-ln-ln-ln;_PublicRecordCategory_garnishment">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>garnishment</rdfs:label>
	</fibo-ln-ln-ln:PublicRecordCategory>
	
	<fibo-ln-ln-ln:PublicRecordCategory rdf:about="&fibo-ln-ln-ln;_PublicRecordCategory_lien">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>lien</rdfs:label>
	</fibo-ln-ln-ln:PublicRecordCategory>
	
	<fibo-ln-ln-ln:PublicRecordCategory rdf:about="&fibo-ln-ln-ln;_PublicRecordCategory_marital">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>marital</rdfs:label>
	</fibo-ln-ln-ln:PublicRecordCategory>
	
	<fibo-ln-ln-ln:PublicRecordCategory rdf:about="&fibo-ln-ln-ln;_PublicRecordCategory_personalProperty">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>personal property</rdfs:label>
	</fibo-ln-ln-ln:PublicRecordCategory>
	
	<fibo-ln-ln-ln:PublicRecordCategory rdf:about="&fibo-ln-ln-ln;_PublicRecordCategory_realEstate">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>real estate</rdfs:label>
	</fibo-ln-ln-ln:PublicRecordCategory>
	
	<fibo-ln-ln-ln:UnderwritingAutomation rdf:about="&fibo-ln-ln-ln;_UnderwritingAutomation_automatic">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>manual</rdfs:label>
		<skos:definition rdf:datatype="&xsd;string">Underwriting was performed using an automated underwriting system.</skos:definition>
	</fibo-ln-ln-ln:UnderwritingAutomation>
	
	<fibo-ln-ln-ln:UnderwritingAutomation rdf:about="&fibo-ln-ln-ln;_UnderwritingAutomation_manual">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>manual</rdfs:label>
		<skos:definition rdf:datatype="&xsd;string">Underwriting was performed manually</skos:definition>
	</fibo-ln-ln-ln:UnderwritingAutomation>
	
	<fibo-ln-ln-ln:UnderwritingDecision rdf:about="&fibo-ln-ln-ln;_UnderwritingDecision_approve">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>approve</rdfs:label>
	</fibo-ln-ln-ln:UnderwritingDecision>
	
	<fibo-ln-ln-ln:UnderwritingDecision rdf:about="&fibo-ln-ln-ln;_UnderwritingDecision_deny">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>deny</rdfs:label>
	</fibo-ln-ln-ln:UnderwritingDecision>
	
	<fibo-ln-ln-ln:UnderwritingDecision rdf:about="&fibo-ln-ln-ln;_UnderwritingDecision_eligible">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
		<rdfs:label>eligible</rdfs:label>
	</fibo-ln-ln-ln:UnderwritingDecision>
	
	<fibo-ln-ln-ln:UnderwritingDecision rdf:about="&fibo-ln-ln-ln;_UnderwritingDecision_ineligible">
