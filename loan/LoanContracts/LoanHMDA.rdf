		<rdf:type rdf:resource="&owl;NamedIndividual"/>
	</fibo-ln-ln-hmda:HMDA_Disposition>
	
	<fibo-ln-ln-hmda:HMDA_Disposition rdf:about="&fibo-ln-ln-hmda;_HMDA_Disposition_LoanPurchasedByYourInstitution">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
	</fibo-ln-ln-hmda:HMDA_Disposition>
	
	<fibo-ln-ln-hmda:HMDA_Disposition rdf:about="&fibo-ln-ln-hmda;_HMDA_Disposition_PreapprovalRequestApprovedButNotAccepted">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
	</fibo-ln-ln-hmda:HMDA_Disposition>
	
	<fibo-ln-ln-hmda:HMDA_Disposition rdf:about="&fibo-ln-ln-hmda;_HMDA_Disposition_PreapprovalRequestDenied">
		<rdf:type rdf:resource="&owl;NamedIndividual"/>
	</fibo-ln-ln-hmda:HMDA_Disposition>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-hmda;hasHMDA_DispositionDate">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label>HMDA disposition date</rdfs:label>
		<skos:definition>the date associated with the HMDA Disposition for a HMDA Covered Loan</skos:definition>
	</owl:ObjectProperty>

</rdf:RDF>