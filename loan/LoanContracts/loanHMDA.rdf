	<owl:Class
		rdf:about="&hmda;HMDA_CoveredLoanContract">
		<rdfs:subClassOf
			rdf:resource="&loan;MortgageLoanContract"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&lnu;rel-produces_I"/>
				<owl:someValuesFrom
					rdf:resource="&loan;ObligationToReport"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<skos:definition
			rdf:datatype="&xs;string">A closed-end mortgage loan or open-end line of credit that is not an excluded transaction for HMDA reporting under US section 1003.3(c) of the Revised Home Mortgage Disclosure Act of 2015. Definition adapted from the Revised HMDA regulatory text.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&hmda;HMDA_Disposition">
		<rdfs:subClassOf
			rdf:resource="&arr-cls;Classifier"/>
		<skos:definition
			rdf:datatype="&xs;string">A type of action taken regarding an application for a HMDA covered loan.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&hmda;HMDA_PreApprovalRequest">
		<rdfs:subClassOf
			rdf:resource="&loan;PreApprovalRequest"/>
		<skos:definition
			rdf:datatype="&xs;string">A PreApproval Request for a home purchase loan up to a certain amount, and subject to certain non-credit related conditions.  This commitment is entered into after a comprehensive analysis of the credit worthiness of the borrower is carried out.  Adapted from the 2015 Revised HMDA regulation.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&hmda;HMDA_Report">
		<rdfs:subClassOf
			rdf:resource="&lnu;Report"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xs;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&lnu;ContactInfo"/>
				<owl:onProperty
					rdf:resource="&lnu;hasIndividualContactInfo"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xs;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass
					rdf:resource="&lnu;ContactInfo"/>
				<owl:onProperty
					rdf:resource="&lnu;hasOrgContactInfo"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:minQualifiedCardinality
					rdf:datatype="&xs;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onClass>
					<owl:Class>
						<owl:intersectionOf>
							<rdf:Description>
								<rdf:first
									rdf:resource="&fibo-be-le-lei;LEICapableEntity"/>
								<rdf:rest>
									<rdf:Description>
										<rdf:first
											rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
										<rdf:rest
											rdf:resource="&rdf;nil"/>
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:intersectionOf>
					</owl:Class>
				</owl:onClass>
				<owl:onProperty
					rdf:resource="&lnu;rel-isSubmittedBy"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&loan;hasNumberOfEntries"/>
				<owl:someValuesFrom
					rdf:resource="&xs;positiveInteger"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty
					rdf:resource="&lnu;rel-isSubmittedTo"/>
				<owl:someValuesFrom
					rdf:resource="&fibo-fbc-fct-rga;RegulatoryAgency"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<skos:definition
			rdf:datatype="&xs;string">A report prepared to satisfy HMDA regulatory reporting requirements as described US section 1003.3(c) of the Revised Home Mortgage Disclosure Act of 2015. Definition adapted from the Revised HMDA regulatory text.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xs;string">The filter class on the hasReportingAgent restriction comes from the hasIdentity restriction on FinancialServiceProvider, which, unfortunately,  is a PartyInRole.</skos:editorialNote>
	</owl:Class>
	
