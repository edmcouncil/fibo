	<loan:fibo-ConstructionType
		rdf:about="&loan;_manufactured">
		<rdf:type
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&xs;string">A factory built dwelling built in compliance with the Federal Manufactured Home Construction and Safety Standards in effect at the time the home was manufactured as evidenced by the HUD label.</skos:definition>
	</loan:fibo-ConstructionType>
	
	<loan:fibo-ConstructionType
		rdf:about="&loan;_mobileHome">
		<rdf:type
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&xs;string">Dwelling unit constructed on a base frame which features wheels and axles to be used in transporting home from place to place, does not meet HUD code.</skos:definition>
	</loan:fibo-ConstructionType>
	
	<loan:fibo-ConstructionType
		rdf:about="&loan;_modular">
		<rdf:type
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&xs;string">A factory built dwelling not on a permanent chassis.</skos:definition>
	</loan:fibo-ConstructionType>
	
	<loan:fibo-DwellingCapacity
		rdf:about="&loan;_multiFamily">
		<rdf:type
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&xs;string">5 or more units</skos:definition>
	</loan:fibo-DwellingCapacity>
	
	<loan:fibo-ConstructionType
		rdf:about="&loan;_onFrameModular">
		<rdf:type
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&xs;string">A factory built dwelling on a permanent chassis which does not have a HUD label.</skos:definition>
	</loan:fibo-ConstructionType>
	
	<loan:fibo-DwellingCapacity
		rdf:about="&loan;_singleFamily">
		<rdf:type
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&xs;string">1-4 units</skos:definition>
	</loan:fibo-DwellingCapacity>
	
	<loan:fibo-ConstructionType
		rdf:about="&loan;_siteBuilt">
		<rdf:type
			rdf:resource="&owl;Thing"/>
		<skos:definition
			rdf:datatype="&xs;string">Describes construction process, indicating that most elements are created at the homes permanent site. May include some prefabricated components.</skos:definition>
	</loan:fibo-ConstructionType>
	
	<owl:Class
