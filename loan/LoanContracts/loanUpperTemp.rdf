										</owl:someValuesFrom>
									</owl:Restriction>
								</rdf:first>
								<rdf:rest
									rdf:resource="&rdf;nil"/>
							</rdf:Description>
						</rdf:rest>
					</rdf:Description>
				</owl:intersectionOf>
			</owl:Class>
