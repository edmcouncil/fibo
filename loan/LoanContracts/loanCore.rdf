		<rdfs:range
			rdf:resource="&xs;positiveInteger"/>
		<skos:definition
			rdf:datatype="&xs;string">Relates something  to a value within an index created with the intent of measuring the credit worthiness of a borrower.</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ln-ln-ln;hasLender">
