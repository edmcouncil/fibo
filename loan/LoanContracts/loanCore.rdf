			rdf:resource="&fibo-ln-ln-up;cur-hasAmount"/>
		<skos:definition
			rdf:datatype="&xs;string">Relates to the maximum amount of funds that may be borrowed from an account (e.g. for line of credit).</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ln-ln-ln;hasCreditReportingAgency">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-ln-ln-up;hasParticipant"/>
		<skos:definition
			rdf:datatype="&xs;string">Relates something to a party which is prodiving credit report(s) e.g. needed to assess the risk of a loan</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-ln-ln-ln;hasCreditScore">
