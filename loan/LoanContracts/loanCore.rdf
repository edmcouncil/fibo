		<rdfs:range
			rdf:resource="&fibo-fnd-aap-ppl;Person"/>
		<skos:definition
			rdf:datatype="&xs;string">Relates something, typically  a loan contract, to a person that initially originates or creates it.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ln-ln-ln;hasPartyBeingChecked">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-ln-ln-up;hasParticipant"/>
