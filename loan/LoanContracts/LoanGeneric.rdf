	<owl:ObjectProperty rdf:about="&fibo-fbc-dae-dbt;hasOutstandingAmount">
		<fibo-fnd-utl-av:synonym>has actual unpaid balance</fibo-fnd-utl-av:synonym>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-fbc-fct-breg;isSubdivisionOf">
		<owl:inverseOf rdf:resource="&fibo-fbc-fct-breg;hasSubdivision"/>
		<skos:editorialNote>This should be added to the BusinessRegistries ontology</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;ChargeCategory">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-arr-cls;Classifier"/>
		<rdfs:label>charge cateogry</rdfs:label>
		<skos:definition>a category indicating what a charge is for (e.g. closing costs, interest)</skos:definition>
		<fibo-fnd-utl-av:usageNote>Use with LineItem, (has ChargeCategory instance), (hasNumericalValue for number of units) and (hasCost for the amount of money)</fibo-fnd-utl-av:usageNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;ContactInformation">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:onClass rdf:resource="&fibo-fnd-aap-ppl;LegallyCapableAdult"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:onClass rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:onClass rdf:resource="&fibo-fnd-plc-adr;PostalAddress"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:onClass rdf:resource="&fibo-ln-ln-lgn;EmailAddress"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:onClass rdf:resource="&fibo-ln-ln-lgn;PhoneNumber"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isClassifiedBy"/>
				<owl:someValuesFrom rdf:resource="&fibo-ln-ln-lgn;ContactPurpose"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>contact information</rdfs:label>
		<skos:definition>a way to make contact with a person or organization for a particular reason regarding something.  The details may be role-specific, so the information cannot always go on the individual or organization</skos:definition>
		<skos:editorialNote>This approach avoids the need to have multiple properties to connect different things to ContactInformation use for different purposes.</skos:editorialNote>
		<skos:editorialNote>Topic: BE</skos:editorialNote>
		<fibo-fnd-utl-av:usageNote>Make sure to check the existing instances of ContactPurpose and use those before creating new ones.</fibo-fnd-utl-av:usageNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;ContactPurpose">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-arr-cls;Classifier"/>
		<rdfs:label>contact purpose</rdfs:label>
		<skos:definition>a virtual address for communication via phone</skos:definition>
		<skos:editorialNote>Future consideration: move this property to somewehre in BE.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;EmailAddress">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-plc-adr;VirtualAddress"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/UtilitiesExt/Values/hasValue"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>email address</rdfs:label>
		<skos:definition>a virtual address for communication via email</skos:definition>
		<skos:editorialNote>Future consideration: move this property to Addresses ontology (fibo-fnd-plc-adr).</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;IndividualContactInformation">
		<rdfs:subClassOf rdf:resource="&fibo-ln-ln-lgn;ContactInformation"/>
		<rdfs:label>individual contact information</rdfs:label>
		<skos:definition>contact information for an individual</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;InformationProduct">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:label>information product</rdfs:label>
		<skos:definition>a product that is informational in nature, rather than physical</skos:definition>
		<skos:editorialNote>Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas).</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;LineItem">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-ln-ln-lgn;hasUnitCost"/>
				<owl:onClass rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
				<owl:onDataRange rdf:resource="&xsd;decimal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;has"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&fibo-fnd-pas-pas;Product">
							</rdf:Description>
							<rdf:Description rdf:about="&fibo-ln-ln-lgn;ChargeCategory">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-ln-ln-lgn;hasCost"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>line item</rdfs:label>
		<skos:definition>a description of a product or service including its unit cost, number of units and total cost</skos:definition>
		<skos:editorialNote>Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas).</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;OrganizationContactInformation">
		<rdfs:subClassOf rdf:resource="&fibo-ln-ln-lgn;ContactInformation"/>
		<rdfs:label>organization contact information</rdfs:label>
		<skos:definition>contact information for an organization</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;PaymentHistory">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-arr-doc;Record"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-dae-dbt;hasOutstandingAmount"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-pas-psch;Payment"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payment history</rdfs:label>
		<skos:definition>a record of actual payments of principal, interest, and other related amounts made by a borrower to a lender or servicer in order to fulfill their re-payment obligation</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote>Just like a PaymentSchedule except that the dates and amounts are actuals.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;PaymentSchedule">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-dt-fd;Schedule"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasMember"/>
				<owl:someValuesFrom rdf:resource="&fibo-ln-ln-lgn;ScheduledPayment"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>payment schedule</rdfs:label>
		<skos:definition>a Schedule of amounts of principal and interest that are due over the term of the loan in order to fully pay off the obligation</skos:definition>
		<skos:editorialNote>Topic: Payments
Not using psch:PaymentSchedule because it uses PaymentEvent which is an OccurrenceKind which does not make sense. Also, it should use hasMember not comprises to put items into a Collection. Ripe for coagulation.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;PhoneNumber">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-plc-adr;VirtualAddress"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/FND/UtilitiesExt/Values/hasValue"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>phone number</rdfs:label>
		<skos:definition>a virtual address for communication via phone</skos:definition>
		<skos:editorialNote>Future consideration: move this property to Addresses ontology (fibo-fnd-plc-adr).</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&fibo-ln-ln-lgn;ScheduledPayment">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-dt-fd;AdHocScheduleEntry"/>
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-psch;PaymentObligation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-pas-psch;hasPaymentAmount"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-ln-ln-lgn;hasScheduledUnpaidBalance"/>
				<owl:someValuesFrom rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>scheduled payment</rdfs:label>
		<skos:definition>a scheduled payment occurrence that is part of a payment schedule covering the term of the loan</skos:definition>
		<skos:editorialNote>Topic: Payments</skos:editorialNote>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-lgn;hasContactInformation">
		<rdfs:label>has contact information</rdfs:label>
		<rdfs:range rdf:resource="&fibo-ln-ln-lgn;ContactInformation"/>
		<skos:definition>relates an individual or an organization to its contact information</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-lgn;hasCost">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-cur;hasMonetaryAmount"/>
		<rdfs:label>has cost</rdfs:label>
		<skos:definition>relates an item to its cost</skos:definition>
		<skos:editorialNote>Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas).</skos:editorialNote>
		<fibo-fnd-utl-av:explanatoryNote>This can entail adding up other costs (e.g. 4 units * a unit price)</fibo-fnd-utl-av:explanatoryNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-lgn;hasScheduledUnpaidBalance">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label>has scheduled unpaid balance</rdfs:label>
		<rdfs:range rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<skos:definition>relates to what the balance should be after a scheduled payment is made according to contract terms</skos:definition>
		<skos:editorialNote>Topic: Payments</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-lgn;hasUnitCost">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-acc-cur;hasMonetaryAmount"/>
		<rdfs:label>has unit cost</rdfs:label>
		<skos:definition>relates an item to its unit cost</skos:definition>
		<skos:editorialNote>Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas).</skos:editorialNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-lgn;isPerformedBy">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasParty"/>
		<rdfs:label>is performed by</rdfs:label>
		<skos:definition>relates an occurrence to a party that fully or partially performs the activity or process corresponding to the occurrence (e.g. who performed an inspection)</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>https://www.merriam-webster.com/dictionary/perform</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&fibo-ln-ln-lgn;worksFor">
		<rdfs:label>works for</rdfs:label>
		<rdfs:domain rdf:resource="&fibo-fnd-aap-ppl;LegallyCapablePerson"/>
		<rdfs:range rdf:resource="&fibo-fnd-pty-pty;IndependentParty"/>
		<skos:definition>relates a person to the organization or company that they work for either as an employee or contractor</skos:definition>
		<fibo-fnd-utl-av:usageNote>Use this property to connect appraisers to the appraisal management company.</fibo-fnd-utl-av:usageNote>
	</owl:ObjectProperty>

</rdf:RDF>